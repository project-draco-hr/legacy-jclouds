{
  injector=Guice.createInjector(new RestModule(),new AzureBlobRestClientModule(),new ExecutorServiceModule(sameThreadExecutor(),sameThreadExecutor()),new AbstractModule(){
    protected void configure(){
      Names.bindProperties(binder(),checkNotNull(new AzureBlobPropertiesBuilder(ACCOUNT,KEY)).build());
      bind(TransformingHttpCommandExecutorService.class).toInstance(createMock(TransformingHttpCommandExecutorService.class));
    }
  }
);
  filter=injector.getInstance(SharedKeyLiteAuthentication.class);
}
