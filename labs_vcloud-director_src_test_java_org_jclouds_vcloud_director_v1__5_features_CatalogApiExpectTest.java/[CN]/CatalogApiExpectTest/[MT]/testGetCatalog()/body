{
  HttpRequest catalogRequest=HttpRequest.builder().method("GET").endpoint(endpoint + "/catalog/7212e451-76e1-4631-b2de-ba1dfd8080e4").addHeader("Accept","*/*").addHeader("x-vcloud-authorization",token).build();
  HttpResponse catalogResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/catalog/catalog.xml",VCloudDirectorMediaType.CATALOG + ";version=1.5")).build();
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,catalogRequest,catalogResponse);
  Catalog expected=catalog();
  URI catalogURI=URI.create(endpoint + "/catalog/7212e451-76e1-4631-b2de-ba1dfd8080e4");
  assertEquals(api.getCatalogApi().getCatalog(catalogURI),expected);
}
