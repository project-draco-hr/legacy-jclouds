{
  StorageController ideController=StorageController.builder().name("IDE Controller").bus(StorageBus.IDE).attachISO(1,0,"VBoxGuestAdditions_").build();
  VmSpec vmSpec=VmSpec.builder().id("").name("").memoryMB(512).osTypeId("").controller(ideController).forceOverwrite(true).cleanUpMode(CleanupMode.Full).build();
  InstallGuestAdditions installGuestAdditions=new InstallGuestAdditions(vmSpec,VBOX_VERISION);
  assertEquals(installGuestAdditions.render(OsFamily.UNIX),"installModuleAssistantIfNeeded || return 1\n" + "mount -t iso9660 /dev/cdrom1 /mnt\n" + "/mnt/VBoxLinuxAdditions.run --nox11\n");
}
