{
  Method method=TerremarkVCloudExpressAsyncClient.class.getMethod("addNode",URI.class,String.class,String.class,int.class,AddNodeOptions[].class);
  HttpRequest request=processor.createRequest(method,URI.create("https://vcloud/extensions/internetService/12"),"10.2.2.2","name",22);
  assertRequestLineEquals(request,"POST https://vcloud/extensions/internetService/12/nodeServices HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"Accept: application/vnd.tmrk.vCloud.nodeService+xml\n");
  assertPayloadEquals(request,Strings2.toStringAndClose(getClass().getResourceAsStream("/CreateNodeService-test2.xml")),"application/vnd.tmrk.vCloud.nodeService+xml",false);
  assertResponseParserClassEquals(method,request,ParseSax.class);
  assertSaxResponseParserClassEquals(method,NodeHandler.class);
  assertFallbackClassEquals(method,null);
  checkFilters(request);
}
