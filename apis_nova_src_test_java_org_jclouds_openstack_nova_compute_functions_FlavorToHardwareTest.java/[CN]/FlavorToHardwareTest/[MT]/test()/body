{
  Hardware flavor=convertFlavor();
  Hardware tempFlavor=new HardwareBuilder().ids("1").name("256 MB Server").processors(ImmutableList.of(new Processor(1.0,1.0))).ram(256).volumes(ImmutableList.of(new VolumeBuilder().type(Volume.Type.LOCAL).size(10.0f).durable(true).bootDevice(true).build())).uri(new URI("http://servers.api.openstack.org/1234/flavors/1")).build();
  assertEquals(flavor,tempFlavor);
}
