{
  Hardware existingHardware=new HardwareBuilder().id("us-sw-1/Small 1GB").providerId("Small 1GB").ram(1024).volume(new VolumeImpl(Float.valueOf(61440),true,true)).location(zone).build();
  Image existingImage=new ImageBuilder().id("us-sw-1/sdc:sdc:centos-5.7:1.2.1").operatingSystem(OperatingSystem.builder().family(OsFamily.LINUX).description("foobuntu").build()).providerId("sdc:sdc:centos-5.7:1.2.1").description("foobuntu").status(Image.Status.AVAILABLE).location(zone).build();
  checkHardwareAndImageStatus(existingHardware,existingHardware,existingImage.getId(),existingImage.getOperatingSystem(),existingImage);
