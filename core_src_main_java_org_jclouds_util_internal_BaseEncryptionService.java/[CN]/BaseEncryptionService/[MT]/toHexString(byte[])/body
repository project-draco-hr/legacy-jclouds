{
  byte[] hex=new byte[2 * raw.length];
  int index=0;
  for (  byte b : raw) {
    int v=b & 0xFF;
    hex[index++]=HEX_CHAR_TABLE[v >>> 4];
    hex[index++]=HEX_CHAR_TABLE[v & 0xF];
  }
  try {
    return new String(hex,"ASCII");
  }
 catch (  UnsupportedEncodingException e) {
    throw new RuntimeException(e);
  }
}
