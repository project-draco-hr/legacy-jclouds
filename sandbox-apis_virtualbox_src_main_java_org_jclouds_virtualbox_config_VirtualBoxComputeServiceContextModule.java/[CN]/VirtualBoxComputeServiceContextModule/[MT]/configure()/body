  }
).to(IMachineToHardware.class);
  bind(new TypeLiteral<Function<IMachine,Image>>(){
  }
).to(IMachineToImage.class);
  bind(new TypeLiteral<Supplier<Location>>(){
  }
).to(OnlyLocationOrFirstZone.class);
}
