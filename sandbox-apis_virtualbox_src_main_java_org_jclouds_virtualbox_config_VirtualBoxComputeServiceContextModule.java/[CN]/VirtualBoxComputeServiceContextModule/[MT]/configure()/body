  }
).to((Class)IdentityFunction.class);
  bind(new TypeLiteral<Function<Image,Image>>(){
  }
).to((Class)IdentityFunction.class);
  bind(new TypeLiteral<Function<IMachine,Hardware>>(){
  }
).to(IMachineToHardware.class);
