{
  return wrapFuture(connection.listOwnedBuckets(),new Function<SortedSet<BucketMetadata>,org.jclouds.blobstore.domain.ListResponse<? extends ResourceMetadata>>(){
    public org.jclouds.blobstore.domain.ListResponse<? extends ResourceMetadata> apply(    SortedSet<BucketMetadata> from){
      return new ListResponseImpl<ResourceMetadata>(Iterables.transform(from,bucket2ResourceMd),null,null,false);
    }
  }
);
}
