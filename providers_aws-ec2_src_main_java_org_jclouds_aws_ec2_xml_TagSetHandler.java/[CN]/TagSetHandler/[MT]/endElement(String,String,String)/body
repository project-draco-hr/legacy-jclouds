{
  if (qName.equals("item")) {
    inItem=false;
    if (key != null) {
      result.put(key,Strings.nullToEmpty(value));
    }
  }
  if (inItem) {
    if (qName.equals("key")) {
      key=currentText.toString().trim();
    }
 else     if (qName.equals("value")) {
      value=currentText.toString().trim();
    }
  }
}
