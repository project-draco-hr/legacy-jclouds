{
  return new EC2HardwareBuilder(InstanceType.M2_4XLARGE).ram(70041).processors(ImmutableList.of(new Processor(8.0,3.25))).volumes(ImmutableList.<Volume>of(new VolumeImpl(10.0f,"/dev/sda1",true,false),new VolumeImpl(840.0f,"/dev/sdb",false,false),new VolumeImpl(840.0f,"/dev/sdc",false,false))).is64Bit(true);
}
