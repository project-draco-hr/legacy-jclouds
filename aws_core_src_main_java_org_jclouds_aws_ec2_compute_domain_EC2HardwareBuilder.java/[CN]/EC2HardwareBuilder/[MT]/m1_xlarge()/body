{
  return new EC2HardwareBuilder(InstanceType.M1_XLARGE).ram(15360).processors(ImmutableList.of(new Processor(4.0,2.0))).volumes(ImmutableList.<Volume>of(new VolumeImpl(10.0f,"/dev/sda1",true,false),new VolumeImpl(420.0f,"/dev/sdb",false,false),new VolumeImpl(420.0f,"/dev/sdc",false,false),new VolumeImpl(420.0f,"/dev/sdd",false,false),new VolumeImpl(420.0f,"/dev/sde",false,false))).is64Bit(true);
}
