{
  return new EC2HardwareBuilder(InstanceType.CC1_4XLARGE).ram(23 * 1024).processors(ImmutableList.of(new Processor(4.0,4.0),new Processor(4.0,4.0))).volumes(ImmutableList.<Volume>of(new VolumeImpl(10.0f,"/dev/sda1",true,false),new VolumeImpl(840.0f,"/dev/sdb",false,false),new VolumeImpl(840.0f,"/dev/sdc",false,false)));
}
