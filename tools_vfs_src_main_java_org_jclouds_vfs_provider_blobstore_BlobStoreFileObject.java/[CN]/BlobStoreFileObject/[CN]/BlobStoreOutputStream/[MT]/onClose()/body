{
  try {
    blob.setPayload(file);
    Payloads.calculateMD5(blob);
    logger.info(String.format(">> put: %s/%s %d bytes",getContainer(),getNameTrimLeadingSlashes(),blob.getPayload().getContentMetadata().getContentLength()));
    String tag=context.putBlob(getContainer(),blob);
    logger.info(String.format("<< tag %s: %s/%s",tag,getContainer(),getNameTrimLeadingSlashes()));
  }
  finally {
    file.delete();
  }
}
