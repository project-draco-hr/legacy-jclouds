{
  try {
    blob.setData(file);
    blob.generateMD5();
    logger.info(String.format(">> put: %s/%s %d bytes",getContainer(),getNameTrimLeadingSlashes(),blob.getContentLength()));
    String tag=blobStore.putBlob(getContainer(),blob);
    logger.info(String.format("<< tag %s: %s/%s",tag,getContainer(),getNameTrimLeadingSlashes()));
  }
  finally {
    file.delete();
  }
}
