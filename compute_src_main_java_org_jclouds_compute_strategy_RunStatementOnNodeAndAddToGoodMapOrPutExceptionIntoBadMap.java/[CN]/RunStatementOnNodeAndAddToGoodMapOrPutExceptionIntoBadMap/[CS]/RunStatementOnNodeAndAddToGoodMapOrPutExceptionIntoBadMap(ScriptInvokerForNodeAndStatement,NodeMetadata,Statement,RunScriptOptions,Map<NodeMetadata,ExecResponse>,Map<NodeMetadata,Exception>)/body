{
  this.statement=checkNotNull(statement,"statement");
  this.scriptInvokerForNodeAndStatement=checkNotNull(scriptInvokerForNodeAndStatement,"scriptInvokerForNodeAndStatement");
  this.node=node;
  this.badNodes=checkNotNull(badNodes,"badNodes");
