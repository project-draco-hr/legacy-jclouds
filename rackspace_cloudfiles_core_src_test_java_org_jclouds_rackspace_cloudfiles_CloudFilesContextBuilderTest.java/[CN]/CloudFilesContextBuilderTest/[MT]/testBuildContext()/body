{
  CloudContext<CloudFilesBlobStore> context=new CloudFilesContextBuilder("id","secret").withModules(new StubCloudFilesConnectionModule(),new StubRackspaceAuthenticationModule()).buildContext();
  assertEquals(context.getClass(),CloudFilesContextImpl.class);
  assertEquals(context.getAccount(),"id");
  assertEquals(context.getEndPoint(),URI.create("http://localhost/rackspacestub/cloudfiles"));
}
