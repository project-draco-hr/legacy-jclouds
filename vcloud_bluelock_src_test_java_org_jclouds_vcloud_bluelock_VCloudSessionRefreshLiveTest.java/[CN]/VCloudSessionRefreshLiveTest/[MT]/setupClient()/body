{
  identity=checkNotNull(System.getProperty("bluelock-vclouddirector.identity"),"bluelock-vclouddirector.identity");
  String credential=checkNotNull(System.getProperty("bluelock-vclouddirector.credential"),"bluelock-vclouddirector.credential");
  Properties props=new Properties();
  props.setProperty(PROPERTY_SESSION_INTERVAL,40 + "");
  context=new ComputeServiceContextFactory().createContext("bluelock-vclouddirector",identity,credential,ImmutableSet.<Module>of(new Log4JLoggingModule()),props);
  connection=VCloudClient.class.cast(context.getProviderSpecificContext().getApi());
}
