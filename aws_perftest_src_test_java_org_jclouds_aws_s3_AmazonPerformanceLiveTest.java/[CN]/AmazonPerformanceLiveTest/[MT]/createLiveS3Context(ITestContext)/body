{
  exec=Executors.newCachedThreadPool();
  if (testContext.getAttribute("jclouds.test.identity") != null) {
    s3=new AmazonS3Client(new BasicAWSCredentials((String)testContext.getAttribute("jclouds.test.identity"),(String)testContext.getAttribute("jclouds.test.credential")));
  }
 else {
    throw new RuntimeException("not configured properly");
  }
}
