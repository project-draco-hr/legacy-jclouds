{
  if (response.getContent() == null)   return null;
  try {
    return parse(Utils.toStringAndClose(response.getContent()));
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
 finally {
    try {
      response.getContent().close();
    }
 catch (    IOException e) {
      Throwables.propagate(e);
    }
  }
}
