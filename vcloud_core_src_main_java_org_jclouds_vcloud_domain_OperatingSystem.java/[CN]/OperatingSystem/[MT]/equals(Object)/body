{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (getClass() != obj.getClass())   return false;
  OperatingSystem other=(OperatingSystem)obj;
  if (description == null) {
    if (other.description != null)     return false;
  }
 else   if (!description.equals(other.description))   return false;
  if (edit == null) {
    if (other.edit != null)     return false;
  }
 else   if (!edit.equals(other.edit))   return false;
  if (id == null) {
    if (other.id != null)     return false;
  }
 else   if (!id.equals(other.id))   return false;
  if (info == null) {
    if (other.info != null)     return false;
  }
 else   if (!info.equals(other.info))   return false;
  if (vmwOsType == null) {
    if (other.vmwOsType != null)     return false;
  }
 else   if (!vmwOsType.equals(other.vmwOsType))   return false;
  return true;
}
