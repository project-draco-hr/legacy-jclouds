{
  Map<String,Location> locations=Maps.newLinkedHashMap();
  for (  String zone : availabilityZoneToRegionMap.keySet()) {
    locations.put(zone,new LocationImpl(LocationScope.ZONE,zone,zone,availabilityZoneToRegionMap.get(zone)));
  }
  for (  String region : availabilityZoneToRegionMap.values()) {
    locations.put(region,new LocationImpl(LocationScope.REGION,region,region,null));
  }
  return locations;
}
