{
  InputStream is=getClass().getResourceAsStream("/task-unsupported.xml");
  Task result=factory.create(injector.getInstance(TaskHandler.class)).parse(is);
  Task expects=Task.builder().id("3904-1002").startTime(dateService.iso8601DateParse("2010-05-26T08:08:08.000+08:00")).endTime(dateService.iso8601DateParse("2010-05-26T08:09:09.000+08:00")).status(Task.Status.NONE).type("application/vnd.vmware.vcloud.task+xml").href(URI.create("https://api.savvis.net/rest/api/v0.8/task/3904-1002")).error(TaskError.builder().vendorSpecificErrorCode("3000").minorErrorCode(-1).majorErrorCode(500).message("your requested task id is not found. Please contact Savvis Administrator for further assistance/clarification ").build()).build();
  assertEquals(result.toString(),expects.toString());
}
