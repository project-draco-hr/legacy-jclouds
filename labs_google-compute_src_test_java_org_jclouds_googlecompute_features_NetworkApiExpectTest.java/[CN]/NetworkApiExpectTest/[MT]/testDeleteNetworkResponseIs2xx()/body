{
  HttpRequest delete=HttpRequest.builder().method("DELETE").endpoint("https://www.googleapis" + ".com/compute/v1beta13/projects/myproject/networks/jclouds-test").addHeader("Accept","application/json").addHeader("Authorization","Bearer " + TOKEN).build();
  HttpResponse deleteResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/operation.json")).build();
  NetworkApi api=requestsSendResponses(requestForScopes(COMPUTE_SCOPE),TOKEN_RESPONSE,delete,deleteResponse).getNetworkApiForProject("myproject");
  assertEquals(api.delete("jclouds-test"),new ParseOperationTest().expected());
}
