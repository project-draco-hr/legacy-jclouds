{
  return new Predicate<OsFamilyVersion64Bit>(){
    @Override public boolean apply(    OsFamilyVersion64Bit input){
      return input.family == OsFamily.RHEL || (input.family == OsFamily.UBUNTU && !input.version.equals("9.10") || !input.is64Bit) || input.family == OsFamily.WINDOWS || input.family == OsFamily.CENTOS;
    }
  }
;
}
