{
  UserApi api=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,authenticatedGET().endpoint(endpoint + "/v2.0/users/e021dfd758eb44a89f1c57c8ef3be8e2").build(),HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/user_details.json",APPLICATION_JSON)).build()).getUserApi().get();
  User user=api.get("e021dfd758eb44a89f1c57c8ef3be8e2");
  assertNotNull(user);
  assertEquals(user,User.builder().name("nova").id("e021dfd758eb44a89f1c57c8ef3be8e2").build());
}
