{
  final String templateId="/vAppTemplate/vappTemplate-ef4415e6-d413-4cbb-9262-f9bbec5f2ea9";
  URI uri=URI.create(endpoint + templateId);
  VAppTemplateApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",templateId + "/action/disableDownload").httpRequestBuilder().build(),new VcloudHttpResponsePrimer().httpResponseBuilder().build()).getVAppTemplateApi();
  assertNotNull(api);
  api.disableDownload(uri);
}
