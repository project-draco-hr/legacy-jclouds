{
  final String templateId="/vAppTemplate/vappTemplate-ef4415e6-d413-4cbb-9262-f9bbec5f2ea9";
  URI uri=URI.create(endpoint + templateId);
  VAppTemplateApi api=orderedRequestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",templateId + "/metadata").xmlFilePayload("/vapptemplate/metadata.xml",METADATA).acceptMedia(TASK).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vapptemplate/error400.xml",ERROR).httpResponseBuilder().statusCode(400).build()).getVAppTemplateApi();
  api.getMetadataApi().merge(uri,exampleMetadata());
}
