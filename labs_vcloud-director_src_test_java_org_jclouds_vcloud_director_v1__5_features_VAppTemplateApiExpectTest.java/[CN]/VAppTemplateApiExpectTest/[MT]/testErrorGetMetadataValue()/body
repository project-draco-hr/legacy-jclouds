{
  final String templateId="/vAppTemplate/vappTemplate-ef4415e6-d413-4cbb-9262-f9bbec5f2ea9";
  URI uri=URI.create(endpoint + templateId);
  VAppTemplateApi api=orderedRequestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",templateId + "/metadata/12345").acceptMedia(METADATA_ENTRY).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vapptemplate/error403.xml",ERROR).httpResponseBuilder().statusCode(403).build()).getVAppTemplateApi();
  assertNull(api.getMetadataApi(uri).get("12345"));
}
