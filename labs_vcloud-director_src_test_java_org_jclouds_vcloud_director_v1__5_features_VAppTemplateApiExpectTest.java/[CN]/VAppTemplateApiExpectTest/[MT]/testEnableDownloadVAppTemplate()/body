{
  final String templateId="/vAppTemplate/vappTemplate-ef4415e6-d413-4cbb-9262-f9bbec5f2ea9";
  URI uri=URI.create(endpoint + templateId);
  VAppTemplateApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",templateId + "/action/enableDownload").acceptMedia(TASK).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/task/task.xml",TASK).httpResponseBuilder().build()).getVAppTemplateApi();
  assertNotNull(api);
  Task task=api.enableDownload(uri);
  assertNotNull(task);
}
