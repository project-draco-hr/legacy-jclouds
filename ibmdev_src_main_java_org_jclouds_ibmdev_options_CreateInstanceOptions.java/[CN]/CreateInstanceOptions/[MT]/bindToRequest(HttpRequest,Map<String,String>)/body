{
  Map<String,Object> postData=Maps.newLinkedHashMap();
  postData.putAll(postParams);
  postData.put("publicKey",publicKey);
  if (volumeID != null)   postData.put("volumeID",volumeID);
  if (configurationData.size() > 0)   postData.put("configurationData",configurationData);
  if (ip != null)   postData.put("ip",ip);
  super.bindToRequest(request,postData);
}
