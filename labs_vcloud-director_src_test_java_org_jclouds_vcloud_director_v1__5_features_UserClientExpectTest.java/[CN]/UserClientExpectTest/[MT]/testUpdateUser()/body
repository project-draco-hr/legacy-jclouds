{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT","/admin/user/b37223f3-8792-477a-820f-334998f61cd6").xmlFilePayload("/user/updateUserSource.xml",VCloudDirectorMediaType.USER).acceptMedia(VCloudDirectorMediaType.USER).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/user/updateUser.xml",VCloudDirectorMediaType.USER).httpResponseBuilder().build());
  User source=updateUserSource();
  User expected=updateUser();
  assertEquals(client.getUserClient().updateUser(userRef.getHref(),source),expected);
}
