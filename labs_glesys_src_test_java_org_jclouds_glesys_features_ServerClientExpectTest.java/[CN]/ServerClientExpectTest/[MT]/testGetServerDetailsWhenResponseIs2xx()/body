{
  ServerClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint("https://api.glesys.com/server/details/format/json").addHeader("Accept","application/json").addHeader("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").addFormParam("includestate","true").addFormParam("serverid","xm3276891").build(),HttpResponse.builder().statusCode(200).payload(payloadFromResource("/server_details.json")).build()).getServerClient();
