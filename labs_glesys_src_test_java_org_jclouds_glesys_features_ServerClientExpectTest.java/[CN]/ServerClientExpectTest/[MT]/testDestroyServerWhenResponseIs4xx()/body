{
  ServerClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint(URI.create("https://api.glesys.com/server/destroy/format/json")).headers(ImmutableMultimap.<String,String>builder().put("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").build()).payload(newUrlEncodedFormPayload(ImmutableMultimap.<String,String>builder().put("serverid","server777").put("keepip","false").build())).build(),HttpResponse.builder().statusCode(401).build()).getServerClient();
  client.destroyServer("server777",DestroyServerOptions.Builder.discardIp());
}
