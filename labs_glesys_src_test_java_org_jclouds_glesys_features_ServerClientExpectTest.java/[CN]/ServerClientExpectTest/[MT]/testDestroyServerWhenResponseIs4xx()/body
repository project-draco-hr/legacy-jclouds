{
  ServerClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint("https://api.glesys.com/server/destroy/format/json").addHeader("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").addFormParam("serverid","server777").addFormParam("keepip","false").build(),HttpResponse.builder().statusCode(401).build()).getServerClient();
  client.destroyServer("server777",DestroyServerOptions.Builder.discardIp());
}
