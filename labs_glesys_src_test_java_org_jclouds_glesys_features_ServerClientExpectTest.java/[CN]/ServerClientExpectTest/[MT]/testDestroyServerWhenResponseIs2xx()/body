{
  ServerClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint("https://api.glesys.com/server/destroy/format/json").addHeader("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").addFormParam("serverid","server777").addFormParam("keepip","true").build(),HttpResponse.builder().statusCode(200).build()).getServerClient();
  client.destroyServer("server777",DestroyServerOptions.Builder.keepIp());
}
