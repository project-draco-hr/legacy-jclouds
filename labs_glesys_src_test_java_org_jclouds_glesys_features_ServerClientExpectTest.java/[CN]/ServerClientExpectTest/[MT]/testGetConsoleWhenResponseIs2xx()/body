{
  ServerClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint("https://api.glesys.com/server/console/format/json").addHeader("Accept","application/json").addHeader("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").addFormParam("serverid","server322").build(),HttpResponse.builder().statusCode(200).payload(payloadFromResource("/server_console.json")).build()).getServerClient();
  Console expected=Console.builder().host("79.99.2.147").port(59478).password("1476897311").protocol("vnc").build();
  assertEquals(client.getConsole("server322"),expected);
}
