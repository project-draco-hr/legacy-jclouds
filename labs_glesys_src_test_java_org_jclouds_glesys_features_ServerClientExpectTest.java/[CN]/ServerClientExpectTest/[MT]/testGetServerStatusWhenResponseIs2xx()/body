{
  ServerClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint("https://api.glesys.com/server/status/format/json").addHeader("Accept","application/json").addHeader("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").addFormParam("serverid","xm3276891").build(),HttpResponse.builder().statusCode(206).payload(payloadFromResource("/server_status.json")).build()).getServerClient();
  assertEquals(client.getServerStatus("xm3276891"),expectedServerStatus());
}
