{
  ServerClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint("https://api.glesys.com/server/console/format/json").addHeader("Accept","application/json").addHeader("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").addFormParam("serverid","server322").build(),HttpResponse.builder().statusCode(404).build()).getServerClient();
