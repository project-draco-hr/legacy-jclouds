{
  Set<Hardware> sizes=Sets.newHashSet();
  for (  int cpus : new int[]{1,2,4,8})   for (  int ram : new int[]{512,1024,2048,4096,8192,16384}) {
    String id=String.format("cpu=%d,ram=%s,disk=%d",cpus,ram,10);
    sizes.add(new HardwareImpl(id,null,id,null,null,ImmutableMap.<String,String>of(),ImmutableList.of(new Processor(cpus,1.0)),ram,10,ImagePredicates.any()));
  }
  return sizes;
}
