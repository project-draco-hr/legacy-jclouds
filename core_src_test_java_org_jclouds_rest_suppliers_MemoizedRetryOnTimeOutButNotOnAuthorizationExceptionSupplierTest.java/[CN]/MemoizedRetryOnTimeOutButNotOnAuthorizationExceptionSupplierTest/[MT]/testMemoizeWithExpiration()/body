{
  CountingSupplier countingSupplier=new CountingSupplier();
  Supplier<Integer> memoizedSupplier=new MemoizedRetryOnTimeOutButNotOnAuthorizationExceptionSupplier(new AtomicReference<AuthorizationException>(),countingSupplier,75,TimeUnit.MILLISECONDS);
  checkExpiration(countingSupplier,memoizedSupplier);
}
