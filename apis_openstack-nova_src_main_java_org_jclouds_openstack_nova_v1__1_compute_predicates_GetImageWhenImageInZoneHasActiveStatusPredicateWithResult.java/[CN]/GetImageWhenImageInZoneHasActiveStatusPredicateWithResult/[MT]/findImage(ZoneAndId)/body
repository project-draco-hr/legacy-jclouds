{
  return Iterables.tryFind(client.getImageClientForZone(zoneAndId.getZone()).listImagesInDetail(),new Predicate<org.jclouds.openstack.nova.v1_1.domain.Image>(){
    @Override public boolean apply(    org.jclouds.openstack.nova.v1_1.domain.Image input){
      return input.getId().equals(zoneAndId.getId());
    }
  }
).orNull();
}
