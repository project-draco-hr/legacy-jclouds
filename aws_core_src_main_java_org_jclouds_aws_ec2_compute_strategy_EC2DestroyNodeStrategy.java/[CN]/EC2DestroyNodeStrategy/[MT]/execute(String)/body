{
  String[] parts=parseHandle(id);
  String region=parts[0];
  String instanceId=parts[1];
  ec2Client.getInstanceServices().terminateInstancesInRegion(region,instanceId);
  return instanceStateTerminated.apply(Iterables.getOnlyElement(Iterables.concat(ec2Client.getInstanceServices().describeInstancesInRegion(region,instanceId))));
}
