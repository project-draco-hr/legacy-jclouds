{
  injector=Guice.createInjector(new AbstractModule(){
    @Override protected void configure(){
      bind(URI.class).annotatedWith(SDN.class).toInstance(URI.create("http://services.nirvanix.com/ws"));
      bindConstant().annotatedWith(Jsr330.named(PROPERTY_JSON_DEBUG)).to(true);
    }
    @SuppressWarnings("unused") @Provides @Singleton protected SDNAuthentication provideCloud(    RestClientFactory factory){
      return factory.create(SDNAuthentication.class);
    }
  }
,new RestModule(),new Log4JLoggingModule(),new ExecutorServiceModule(new WithinThreadExecutorService()),new JavaUrlHttpCommandExecutorServiceModule());
}
