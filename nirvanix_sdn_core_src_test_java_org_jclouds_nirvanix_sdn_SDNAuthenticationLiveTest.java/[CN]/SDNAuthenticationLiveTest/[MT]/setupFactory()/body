{
  String endpoint="http://services.nirvanix.com";
  identity=checkNotNull(System.getProperty("jclouds.test.identity"),"jclouds.test.identity");
  credential=checkNotNull(System.getProperty("jclouds.test.credential"),"jclouds.test.credential");
  ContextSpec<SDNAuthClient,SDNAuthAsyncClient> contextSpec=contextSpec("test",endpoint,"1",identity,credential,SDNAuthClient.class,SDNAuthAsyncClient.class);
  context=createContextBuilder(contextSpec,ImmutableSet.<Module>of(new Log4JLoggingModule(),new ExecutorServiceModule(sameThreadExecutor(),sameThreadExecutor()))).buildContext();
}
