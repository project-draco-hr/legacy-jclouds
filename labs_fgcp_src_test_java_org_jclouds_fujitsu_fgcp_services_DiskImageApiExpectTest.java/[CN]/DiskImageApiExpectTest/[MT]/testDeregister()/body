{
  HttpRequest request=buildGETWithQuery("Action=UnregisterDiskImage" + "&diskImageId=IMG_A1B2C3_1234567890ABCD");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/UnregisterDiskImage-response.xml")).build();
  DiskImageApi api=requestSendsResponse(request,response).getDiskImageApi();
  api.deregister("IMG_A1B2C3_1234567890ABCD");
}
