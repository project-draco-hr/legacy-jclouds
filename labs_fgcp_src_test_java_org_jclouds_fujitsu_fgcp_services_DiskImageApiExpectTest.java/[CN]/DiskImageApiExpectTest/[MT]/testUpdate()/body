{
  HttpRequest request=buildGETWithQuery("Action=UpdateDiskImageAttribute" + "&diskImageId=IMG_A1B2C3_1234567890ABCD" + "&updateLcId=en"+ "&attributeName=updateName"+ "&attributeValue=new-name");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/UpdateDiskImageAttribute-response.xml")).build();
  DiskImageApi api=requestSendsResponse(request,response).getDiskImageApi();
  api.update("IMG_A1B2C3_1234567890ABCD","en","updateName","new-name");
}
