{
  for (  String imageId : imagesToDeregister)   client.deregisterImageInRegion(regionId,imageId);
  for (  String snapshotId : snapshotsToDelete)   view.unwrap(EC2ApiMetadata.CONTEXT_TOKEN).getApi().getElasticBlockStoreServices().deleteSnapshotInRegion(regionId,snapshotId);
  super.tearDownContext();
}
