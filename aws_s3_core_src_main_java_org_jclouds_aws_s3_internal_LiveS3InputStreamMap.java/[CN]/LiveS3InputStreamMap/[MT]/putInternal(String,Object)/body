{
  S3Object object=new S3Object(s);
  try {
    InputStream returnVal=containsKey(s) ? get(s) : null;
    object.setData(o);
    object.generateETag();
    connection.putObject(bucket,object).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS);
    return returnVal;
  }
 catch (  Exception e) {
    Utils.<S3RuntimeException>rethrowIfRuntimeOrSameType(e);
    throw new S3RuntimeException(String.format("Error adding object %1$s:%2$s",bucket,object),e);
  }
}
