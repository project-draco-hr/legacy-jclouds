{
  if (vApp.getChildren().getVms().size() == 0)   return ImmutableSet.of();
  Builder<String> ips=ImmutableSet.builder();
  Vm vm=Iterables.get(vApp.getChildren().getVms(),0);
  NetworkConnectionSection networkConnectionSection=findNetworkConnectionSectionForVApp.apply(vApp);
  if (networkConnectionSection != null) {
    for (    NetworkConnection connection : networkConnectionSection.getNetworkConnections()) {
      if (connection.getIpAddress() != null)       ips.add(connection.getIpAddress());
      if (connection.getExternalIpAddress() != null)       ips.add(connection.getExternalIpAddress());
    }
  }
 else {
    for (    ResourceAllocationSettingData net : filter(findVirtualHardwareSectionForVApp.apply(vApp).getItems(),CIMPredicates.resourceTypeIn(ResourceType.ETHERNET_ADAPTER))) {
    }
  }
  return ips.build();
}
