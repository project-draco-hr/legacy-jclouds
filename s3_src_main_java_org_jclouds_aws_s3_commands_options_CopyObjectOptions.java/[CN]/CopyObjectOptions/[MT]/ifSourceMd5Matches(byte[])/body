{
  checkState(getIfNoneMatch() == null,"ifMd5DoesntMatch() is not compatible with ifMd5Matches()");
  checkState(getIfModifiedSince() == null,"ifModifiedSince() is not compatible with ifMd5Matches()");
  replaceHeader("x-amz-copy-source-if-match",String.format("\"%1$s\"",S3Utils.toHexString(checkNotNull(md5,"md5"))));
  return this;
}
