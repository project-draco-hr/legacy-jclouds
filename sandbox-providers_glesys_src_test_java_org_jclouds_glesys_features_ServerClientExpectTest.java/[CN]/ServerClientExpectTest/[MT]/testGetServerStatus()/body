{
  createMock("status","POST",200,"/server_status.json",serverIdOnly).getServerStatus(serverId);
  createMock("status","POST",200,"/server_status.json",serverIdOnly,ServerStatusOptions.Builder.state()).getServerStatus(serverId,ServerStatusOptions.Builder.state());
