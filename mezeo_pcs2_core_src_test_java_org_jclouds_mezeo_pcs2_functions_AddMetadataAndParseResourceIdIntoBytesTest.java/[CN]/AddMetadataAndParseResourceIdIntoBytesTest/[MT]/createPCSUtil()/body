{
  PCSUtil connection=createMock(PCSUtil.class);
  expect(connection.put(eq(URI.create("http://localhost/contents/7F143552-AAF5-11DE-BBB0-0BC388ED913B/metadata/foo")),eq("bar"))).andReturn(true);
  expect(connection.put(eq(URI.create("http://localhost/contents/7F143552-AAF5-11DE-BBB0-0BC388ED913B/metadata/biz")),eq("baz"))).andReturn(true);
  replay(connection);
  return connection;
}
