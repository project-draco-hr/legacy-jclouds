{
  PCSUtil connection=createMock(PCSUtil.class);
  final Future<Void> voidF=createMock(Future.class);
  expect(connection.put(eq(URI.create("http://localhost/contents/7F143552-AAF5-11DE-BBB0-0BC388ED913B/metadata/foo")),eq("bar"))).andReturn(voidF);
  expect(connection.put(eq(URI.create("http://localhost/contents/7F143552-AAF5-11DE-BBB0-0BC388ED913B/metadata/biz")),eq("baz"))).andReturn(voidF);
  replay(connection);
  return connection;
}
