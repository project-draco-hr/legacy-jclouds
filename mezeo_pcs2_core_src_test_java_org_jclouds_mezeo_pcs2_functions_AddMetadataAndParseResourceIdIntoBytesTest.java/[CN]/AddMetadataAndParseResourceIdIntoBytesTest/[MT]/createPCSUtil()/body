{
  PCSUtil connection=createMock(PCSUtil.class);
  final Future<Void> voidF=createMock(Future.class);
  expect(connection.putMetadata(eq("7F143552-AAF5-11DE-BBB0-0BC388ED913B"),eq("foo"),eq("bar"))).andReturn(voidF);
  expect(connection.putMetadata(eq("7F143552-AAF5-11DE-BBB0-0BC388ED913B"),eq("biz"),eq("baz"))).andReturn(voidF);
  replay(connection);
  return connection;
}
