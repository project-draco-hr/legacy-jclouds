{
  GetString get=factory.createGetString(httpServletRequest.getParameter("uri"));
  client.submit(get);
  Writer writer=httpServletResponse.getWriter();
  try {
    writer.write(get.get().trim());
  }
 catch (  Exception e) {
    throw new ServletException(e);
  }
  writer.flush();
  writer.close();
}
