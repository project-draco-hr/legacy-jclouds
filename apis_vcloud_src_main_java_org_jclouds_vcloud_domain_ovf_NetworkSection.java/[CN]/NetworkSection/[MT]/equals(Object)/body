{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  NetworkSection other=(NetworkSection)obj;
  if (info == null) {
    if (other.info != null)     return false;
  }
 else   if (!info.equals(other.info))   return false;
  if (networks == null) {
    if (other.networks != null)     return false;
  }
 else   if (!networks.equals(other.networks))   return false;
  return true;
}
