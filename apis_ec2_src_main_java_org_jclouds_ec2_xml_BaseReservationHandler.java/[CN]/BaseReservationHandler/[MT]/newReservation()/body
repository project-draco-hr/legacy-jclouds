{
  String region=getRequest() != null ? AWSUtils.findRegionInArgsOrNull(getRequest()) : null;
  if (region == null)   region=defaultRegion.get();
  Reservation<? extends RunningInstance> info=new Reservation<RunningInstance>(region,groupIds,instances,ownerId,requesterId,reservationId);
  this.groupIds=Sets.newLinkedHashSet();
  this.instances=Sets.newLinkedHashSet();
  this.ownerId=null;
  this.requesterId=null;
  this.reservationId=null;
  return info;
}
