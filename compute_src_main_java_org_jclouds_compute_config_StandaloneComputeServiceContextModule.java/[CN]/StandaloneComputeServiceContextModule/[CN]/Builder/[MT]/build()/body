{
  return new StandaloneComputeServiceContextModule(){
    @Override protected Class<? extends AddNodeWithTagStrategy> defineAddNodeWithTagStrategy(){
      return addNodeWithTagStrategy;
    }
    @Override protected Class<? extends DestroyNodeStrategy> defineDestroyNodeStrategy(){
      return destroyNodeStrategy;
    }
    @Override protected Class<? extends GetNodeMetadataStrategy> defineGetNodeMetadataStrategy(){
      return getNodeMetadataStrategy;
    }
    @Override protected Class<? extends Supplier<Set<? extends Hardware>>> defineHardwareSupplier(){
      return hardwareSupplier;
    }
    @Override protected Class<? extends Supplier<Set<? extends Image>>> defineImageSupplier(){
      return imageSupplier;
    }
    @Override protected Class<? extends ListNodesStrategy> defineListNodesStrategy(){
      return listNodesStrategy;
    }
    @Override protected Class<? extends RebootNodeStrategy> defineRebootNodeStrategy(){
      return rebootNodeStrategy;
    }
    protected Class<? extends Supplier<Set<? extends Location>>> defineLocationSupplier(){
      return locationSupplier;
    }
    @Override protected void configure(){
      for (      Module module : modules)       install(module);
      super.configure();
    }
  }
;
}
