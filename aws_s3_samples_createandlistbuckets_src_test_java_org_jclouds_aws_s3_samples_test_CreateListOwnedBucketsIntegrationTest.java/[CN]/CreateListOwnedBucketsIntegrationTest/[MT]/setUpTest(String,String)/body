{
  AWSAccessKeyId=AWSAccessKeyId != null ? AWSAccessKeyId : sysAWSAccessKeyId;
  AWSSecretAccessKey=AWSSecretAccessKey != null ? AWSSecretAccessKey : sysAWSSecretAccessKey;
  if ((AWSAccessKeyId != null) && (AWSSecretAccessKey != null))   context=S3ContextFactory.createS3Context(AWSAccessKeyId,AWSSecretAccessKey,new LiveS3ConnectionModule());
 else   context=S3ContextFactory.createContext("stub","stub").withHttpAddress("stub").withModule(new StubS3ConnectionModule()).build();
}
