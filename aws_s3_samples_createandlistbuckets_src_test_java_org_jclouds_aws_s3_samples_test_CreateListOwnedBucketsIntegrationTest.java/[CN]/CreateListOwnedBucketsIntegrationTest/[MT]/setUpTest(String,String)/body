{
  AWSAccessKeyId=AWSAccessKeyId != null ? AWSAccessKeyId : sysAWSAccessKeyId;
  AWSSecretAccessKey=AWSSecretAccessKey != null ? AWSSecretAccessKey : sysAWSSecretAccessKey;
  if ((AWSAccessKeyId != null) && (AWSSecretAccessKey != null))   context=S3ContextBuilder.newBuilder(AWSAccessKeyId,AWSSecretAccessKey).withSaxDebug().relaxSSLHostname().withModules(new Log4JLoggingModule()).buildContext();
 else   context=S3ContextFactory.createS3Context("stub","stub",new StubS3BlobStoreModule());
}
