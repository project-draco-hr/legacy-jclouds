{
  T object;
  while (true) {
    while (pool.size() == 0) {
      try {
        Thread.sleep(100);
      }
 catch (      InterruptedException e) {
        throw new RuntimeException(e);
      }
    }
    object=pool.poll();
    boolean returnToPool=true;
    try {
      if (object != null) {
        returnToPool=false;
        return object;
      }
    }
 catch (    Exception e) {
    }
 finally {
      if (returnToPool) {
        addToPool(object);
      }
    }
  }
}
