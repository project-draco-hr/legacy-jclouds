{
switch (usedObjectStrategy) {
case THROW_AWAY:
    return new Destroyer<T>(){
      public void destroy(      T object){
        createNewObject();
        removeObject(object);
      }
    }
;
case REUSE:
  return new Destroyer<T>(){
    public void destroy(    T object){
      addToPool(object);
    }
  }
;
}
return null;
}
