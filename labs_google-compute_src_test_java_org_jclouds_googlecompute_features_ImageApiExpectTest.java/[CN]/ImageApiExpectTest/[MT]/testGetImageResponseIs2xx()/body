{
  HttpRequest get=HttpRequest.builder().method("GET").endpoint("https://www.googleapis" + ".com/compute/v1beta13/projects/google/images/centos-6-2-v20120326").addHeader("Accept","application/json").addHeader("Authorization","Bearer " + TOKEN).build();
  HttpResponse operationResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/image_get.json")).build();
  ImageApi imageApi=requestsSendResponses(requestForScopes(COMPUTE_READONLY_SCOPE),TOKEN_RESPONSE,get,operationResponse).getImageApiForProject("google");
  assertEquals(imageApi.get("centos-6-2-v20120326"),new ParseImageTest().expected());
}
