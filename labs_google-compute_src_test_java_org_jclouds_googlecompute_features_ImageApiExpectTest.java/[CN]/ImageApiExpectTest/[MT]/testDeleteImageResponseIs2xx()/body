{
  HttpRequest delete=HttpRequest.builder().method("DELETE").endpoint("https://www.googleapis" + ".com/compute/v1beta13/projects/myproject/images/centos-6-2-v20120326").addHeader("Accept","application/json").addHeader("Authorization","Bearer " + TOKEN).build();
  HttpResponse deleteResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/operation.json")).build();
  ImageApi imageApi=requestsSendResponses(requestForScopes(COMPUTE_SCOPE),TOKEN_RESPONSE,delete,deleteResponse).getImageApiForProject("myproject");
  assertEquals(imageApi.delete("centos-6-2-v20120326"),new ParseOperationTest().expected());
}
