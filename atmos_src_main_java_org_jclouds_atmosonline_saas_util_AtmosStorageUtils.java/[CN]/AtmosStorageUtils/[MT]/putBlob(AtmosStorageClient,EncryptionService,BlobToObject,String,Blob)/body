{
  final String path=container + "/" + blob.getMetadata().getName();
  deleteAndEnsureGone(sync,path);
  if (blob.getMetadata().getContentMD5() != null)   blob.getMetadata().getUserMetadata().put("content-md5",encryptionService.hex(blob.getMetadata().getContentMD5()));
  sync.createFile(container,blob2Object.apply(blob));
  return path;
}
