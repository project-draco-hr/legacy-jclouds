{
  if (this == obj)   return true;
  if (obj == null || getClass() != obj.getClass())   return false;
  VmNIC that=VmNIC.class.cast(obj);
  return Objects.equal(this.tag,that.tag) && Objects.equal(this.ip,that.ip) && Objects.equal(this.netmask,that.netmask)&& Objects.equal(this.gateway,that.gateway);
}
