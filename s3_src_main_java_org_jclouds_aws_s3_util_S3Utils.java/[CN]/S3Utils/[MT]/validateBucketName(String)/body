{
  checkNotNull(bucketName,"bucketName");
  checkArgument(bucketName.matches("^[a-z0-9].*"),"bucket name must start with a number or letter");
  checkArgument(bucketName.matches("^[-_.a-z0-9]+"),"bucket name can only contain lowercase letters, numbers, periods (.), underscores (_), and dashes (-)");
  checkArgument(bucketName.length() > 2 && bucketName.length() < 256,"bucket name must be between 3 and 255 characters long");
  checkArgument(!IP_PATTERN.matcher(bucketName).matches(),"bucket name cannot be ip address style");
  return bucketName;
}
