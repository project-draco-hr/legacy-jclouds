{
  return NodeMetadataBuilder.fromNodeMetadata(backendNodeMetadata).tags(jsonMetadata.tags).group(jsonMetadata.group).userMetadata(jsonMetadata.userMetadata).credentials(LoginCredentials.builder().user(jsonMetadata.user).privateKey(jsonMetadata.privateKey).password(jsonMetadata.password).authenticateSudo(jsonMetadata.authenticateSudo).build()).build();
}
