{
  if (millis == 0l)   this.state=state;
 else   service.execute(new Runnable(){
    @Override public void run(){
      try {
        Thread.sleep(millis);
      }
 catch (      InterruptedException e) {
        Throwables.propagate(e);
      }
      StubNodeMetadata.this.state=state;
    }
  }
);
}
