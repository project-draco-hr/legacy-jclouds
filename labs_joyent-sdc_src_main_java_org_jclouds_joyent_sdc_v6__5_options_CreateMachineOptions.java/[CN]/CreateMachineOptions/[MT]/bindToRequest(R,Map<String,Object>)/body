{
  MachineRequest machine=new MachineRequest(checkNotNull(postParams.get("name"),"name parameter not present").toString(),checkNotNull(postParams.get("package"),"package parameter not present").toString(),checkNotNull(postParams.get("dataset"),"dataset parameter not present").toString());
  if (metadata.size() > 0)   machine.metadata=metadata;
  if (tag.size() > 0)   machine.tag=tag;
  return bindToRequest(request,machine);
}
