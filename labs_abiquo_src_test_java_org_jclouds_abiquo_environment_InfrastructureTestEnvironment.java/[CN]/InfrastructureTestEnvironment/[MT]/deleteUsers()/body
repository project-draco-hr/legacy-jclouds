{
  if (user != null) {
    String nick=user.getNick();
    user.delete();
    assertNull(enterprise.findUser(UserPredicates.nick(nick)));
  }
  if (enterpriseAdmin != null) {
    String nick=enterpriseAdmin.getNick();
    enterpriseAdmin.delete();
    assertNull(enterprise.findUser(UserPredicates.nick(nick)));
  }
}
