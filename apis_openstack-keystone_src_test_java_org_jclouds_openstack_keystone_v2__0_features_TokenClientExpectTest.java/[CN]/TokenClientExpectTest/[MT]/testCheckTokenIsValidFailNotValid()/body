{
  TokenClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,standardRequestBuilder(endpoint + "/v2.0/tokens/sometokenorother").method("HEAD").headers(ImmutableMultimap.of("X-Auth-Token",authToken)).build(),standardResponseBuilder(404).build()).getTokenClient().get();
  assertFalse(client.isValid("sometokenorother"));
}
