{
  TokenClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,HttpRequest.builder().method("HEAD").endpoint(endpoint + "/v2.0/tokens/sometokenorother").addHeader("X-Auth-Token",authToken).build(),HttpResponse.builder().statusCode(404).build()).getTokenClient().get();
  assertFalse(client.isValid("sometokenorother"));
}
