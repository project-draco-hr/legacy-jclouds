{
  HttpResponse listResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/describe_loadbalancers.xml","text/xml")).build();
  ELBApi apiWhenExist=requestSendsResponse(list,listResponse);
  assertEquals(apiWhenExist.getLoadBalancerApi().list().get(0).toString(),new DescribeLoadBalancersResponseTest().expected().toString());
}
