{
  if (nodeElement.getIdproperty() != null)   getProject().setProperty(nodeElement.getIdproperty(),createdNode.getProviderId());
  if (nodeElement.getHostproperty() != null)   getProject().setProperty(nodeElement.getHostproperty(),ipOrEmptyString(createdNode.getPublicAddresses()));
  if (nodeElement.getPasswordproperty() != null && !isKeyAuth(createdNode))   getProject().setProperty(nodeElement.getPasswordproperty(),createdNode.getCredentials().key);
  if (nodeElement.getUsernameproperty() != null)   getProject().setProperty(nodeElement.getUsernameproperty(),createdNode.getCredentials().account);
}
