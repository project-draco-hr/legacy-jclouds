{
  NodeMetadata metadata=node instanceof NodeMetadata ? NodeMetadata.class.cast(node) : computeService.getNodeMetadata(node.getHandle());
  log(String.format("   node id=%s, name=%s, tag=%s, location=%s, state=%s, publicIp=%s, privateIp=%s, extra=%s",metadata.getId(),metadata.getName(),metadata.getTag(),metadata.getLocation(),metadata.getState(),ComputeTaskUtils.ipOrEmptyString(metadata.getPublicAddresses()),ipOrEmptyString(metadata.getPrivateAddresses()),metadata.getExtra()));
}
