{
switch (action) {
case CREATE:
case GET:
case DESTROY:
case REBOOT:
    if (nodeElement != null) {
switch (action) {
case CREATE:
        create(computeService);
      break;
case GET:
    get(computeService);
  break;
case DESTROY:
destroy(computeService);
break;
case REBOOT:
reboot(computeService);
break;
}
}
 else {
this.log("missing node element for action: " + action,Project.MSG_ERR);
}
break;
case LIST:
list(computeService);
break;
case LIST_DETAILS:
listDetails(computeService);
break;
case LIST_IMAGES:
listImages(computeService);
break;
case LIST_SIZES:
listSizes(computeService);
break;
case LIST_LOCATIONS:
listLocations(computeService);
break;
default :
this.log("bad action: " + action,Project.MSG_ERR);
}
}
