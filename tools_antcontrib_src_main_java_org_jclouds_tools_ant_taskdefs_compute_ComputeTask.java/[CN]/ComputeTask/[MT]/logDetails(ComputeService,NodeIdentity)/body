{
  NodeMetadata metadata=computeService.getNodeMetadata(node.getId());
  log(String.format("   node id=%s, name=%s, state=%s, publicIp=%s, privateIp=%s",metadata.getId(),node.getName(),metadata.getState(),ipOrEmptyString(metadata.getPublicAddresses()),ipOrEmptyString(metadata.getPrivateAddresses())));
}
