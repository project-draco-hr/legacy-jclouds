{
  ServerMetadata metadata=computeService.getServerMetadata(server.getId());
  log(String.format("   server id=%s, name=%s, state=%s, publicIp=%s, privateIp=%s",metadata.getId(),server.getName(),metadata.getState(),ipOrEmptyString(metadata.getPublicAddresses()),ipOrEmptyString(metadata.getPrivateAddresses())));
}
