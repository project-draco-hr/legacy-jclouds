{
  return new MapMaker().makeComputingMap(new Function<URI,ComputeServiceContext>(){
    @Override public ComputeServiceContext apply(    URI from){
      try {
        return new ComputeServiceContextFactory().createContext(from,defaultModulesProvider.get(),defaultPropertiesProvider.get());
      }
 catch (      IOException e) {
        throw new RuntimeException(e);
      }
    }
  }
);
}
