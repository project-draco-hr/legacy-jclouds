{
  log("list images");
  for (  Image image : computeService.listImages()) {
    log(String.format("   image location=%s, id=%s, name=%s, version=%s, arch=%s, osfam=%s, osdesc=%s, desc=%s",image.getLocation(),image.getProviderId(),image.getName(),image.getVersion(),image.getArchitecture(),image.getOsFamily(),image.getOsDescription(),image.getDescription()));
  }
}
