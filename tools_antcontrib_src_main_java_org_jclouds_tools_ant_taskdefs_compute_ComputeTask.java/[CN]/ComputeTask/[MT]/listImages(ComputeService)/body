{
  log("list images");
  for (  Image image : computeService.listImages()) {
    log(String.format("   image location=%s, id=%s, version=%s, arch=%s, osfam=%s, desc=%s",image.getLocation(),image.getId(),image.getVersion(),image.getArchitecture(),image.getOsFamily(),image.getOsDescription()));
  }
}
