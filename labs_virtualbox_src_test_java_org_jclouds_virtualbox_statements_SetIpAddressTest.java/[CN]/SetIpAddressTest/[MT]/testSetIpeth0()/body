{
  NetworkInterfaceCard networkInterfaceCard=NetworkInterfaceCard.builder().slot(0L).addNetworkAdapter(NetworkAdapter.builder().staticIp("127.0.0.1").networkAttachmentType(NetworkAttachmentType.NAT).build()).build();
  SetIpAddress setIpAddressStmtm=new SetIpAddress(networkInterfaceCard);
  assertEquals("ifconfig eth0 127.0.0.1;",setIpAddressStmtm.render(OsFamily.UNIX));
}
