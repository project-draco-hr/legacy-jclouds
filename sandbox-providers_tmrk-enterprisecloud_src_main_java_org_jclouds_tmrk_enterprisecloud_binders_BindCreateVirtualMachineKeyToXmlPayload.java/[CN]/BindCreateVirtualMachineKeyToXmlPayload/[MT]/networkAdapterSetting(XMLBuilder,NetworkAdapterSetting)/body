{
  String href=setting.getNetwork().getHref().toString();
  String name=setting.getNetwork().getName();
  String type=setting.getNetwork().getType();
  builder.e("NetworkAdapter").e("Network").a("href",href).a("name",name).a("type",type).up().e("IpAddress").t(setting.getIpAddress()).up();
  return builder;
}
