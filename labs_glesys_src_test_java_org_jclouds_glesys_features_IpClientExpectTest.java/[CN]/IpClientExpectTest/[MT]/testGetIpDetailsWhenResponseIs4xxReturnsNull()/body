{
  IpClient client=requestSendsResponse(HttpRequest.builder().method("GET").endpoint("https://api.glesys.com/ip/details/ipaddress/31.192.227.37/format/json").addHeader("Accept","application/json").addHeader("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").build(),HttpResponse.builder().statusCode(404).build()).getIpClient();
  assertEquals(client.getIp("31.192.227.37"),null);
}
