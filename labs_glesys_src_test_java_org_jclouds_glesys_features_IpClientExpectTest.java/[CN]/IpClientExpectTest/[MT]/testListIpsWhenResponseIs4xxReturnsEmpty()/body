{
  IpClient client=requestSendsResponse(HttpRequest.builder().method("GET").endpoint("https://api.glesys.com/ip/listown/format/json").addHeader("Accept","application/json").addHeader("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").build(),HttpResponse.builder().statusCode(404).build()).getIpClient();
  assertTrue(client.listIps().isEmpty());
}
