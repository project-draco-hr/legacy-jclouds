{
  setupCredentials();
  Properties overrides=setupProperties();
  client=new ComputeServiceContextFactory(setupRestProperties()).createContext(provider,ImmutableSet.<Module>of(new Log4JLoggingModule()),overrides).getComputeService();
  socketTester=new RetryablePredicate<IPSocket>(new InetSocketAddressConnect(),300,1,TimeUnit.SECONDS);
  sshFactory=Guice.createInjector(getSshModule()).getInstance(Factory.class);
}
