{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/admin/network/b466c0c5-8a5c-4335-b703-a2e2e6b5f3e1").xmlFilePayload("/network/admin/updateNetworkSource.xml",VCloudDirectorMediaType.ORG_NETWORK).acceptMedia(VCloudDirectorMediaType.TASK).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/network/admin/updateNetworkTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=updateNetworkTask();
  assertEquals(client.getAdminNetworkClient().updateNetwork(networkRef.getHref(),updateNetwork()),expected);
}
