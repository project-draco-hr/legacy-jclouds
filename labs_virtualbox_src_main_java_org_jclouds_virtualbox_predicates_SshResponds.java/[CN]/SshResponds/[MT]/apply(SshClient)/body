{
  try {
    client.connect();
    if (client.exec("id").getExitStatus() == 0) {
      return true;
    }
  }
 catch (  SshException e) {
    logger.trace("No response from ssh daemon connecting to %s: %s",client,e.getMessage());
  }
 finally {
    if (client != null) {
      client.disconnect();
    }
  }
  return false;
}
