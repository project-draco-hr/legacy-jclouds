{
  HttpRequest request=buildGETWithQuery("Action=CreateAddressPool" + "&pipFrom=192.168.0.0" + "&pipTo=192.168.30.0");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/CreateAddressPool-response.xml")).build();
  VirtualDCApi api=requestSendsResponse(request,response).getVirtualDCApi();
  api.createAddressPool("192.168.0.0","192.168.30.0");
}
