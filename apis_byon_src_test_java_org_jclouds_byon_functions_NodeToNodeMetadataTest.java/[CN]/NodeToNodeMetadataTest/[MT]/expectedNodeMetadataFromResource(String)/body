{
  Location location=expectedLocationFromResource(resource);
  return new NodeMetadataBuilder().ids("cluster-1").group("hadoop").name("cluster-1").location(location).state(NodeState.RUNNING).operatingSystem(new OperatingSystemBuilder().description("redhat").family(OsFamily.RHEL).arch("x86").version("5.3").build()).publicAddresses(ImmutableSet.of("cluster-1.mydomain.com")).credentials(new Credentials("myUser",NodesFromYamlTest.key)).adminPassword("happy bear").build();
}
