{
  return new NodeMetadataBuilder().ids("cluster-" + id).group("hadoop").name("cluster-" + id).loginPort(loginPort).hostname("cluster-" + id + ".mydomain.com").location(location).userMetadata(ImmutableMap.of("Name","foo")).tags(ImmutableSet.of("vanilla")).status(Status.RUNNING).operatingSystem(OperatingSystem.builder().description("redhat").family(OsFamily.RHEL).arch("x86").version("5.3").build()).publicAddresses(ImmutableSet.of("cluster-" + id + ".mydomain.com")).credentials(LoginCredentials.builder().user("myUser").privateKey(NodesFromYamlTest.key).password("happy bear").authenticateSudo(true).build()).build();
}
