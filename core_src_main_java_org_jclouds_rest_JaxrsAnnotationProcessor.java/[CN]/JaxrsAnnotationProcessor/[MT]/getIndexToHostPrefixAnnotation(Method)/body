{
  Map<Integer,Set<Annotation>> indexToHostPrefixAnnotation=Maps.filterValues(methodToIndexOfParamToHostPrefixParamAnnotations.get(method),new Predicate<Set<Annotation>>(){
    public boolean apply(    Set<Annotation> input){
      return input.size() == 1;
    }
  }
);
  if (indexToHostPrefixAnnotation.size() > 1) {
    throw new IllegalStateException(String.format("You must not specify more than one @HostPrefixParam annotation on: %s; found %s",method.toString(),indexToHostPrefixAnnotation));
  }
  return indexToHostPrefixAnnotation;
}
