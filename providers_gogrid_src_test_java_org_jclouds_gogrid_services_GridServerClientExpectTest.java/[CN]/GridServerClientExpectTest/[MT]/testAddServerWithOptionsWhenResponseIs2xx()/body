{
  HttpRequest listGridServers=HttpRequest.builder().method("GET").endpoint(URI.create("https://api.gogrid.com/api/grid/server/add?" + "v=1.6&name=serverName&server.ram=memory&image=img55&ip=127.0.0.1&isSandbox=true&description=fooy&" + "sig=e9aafd0a5d4c69bb24536be4bce8a528&api_key=identity")).build();
  HttpResponse listGridServersResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/test_get_server_list.json","application/json")).build();
  GoGridClient addServerWithOptionsWorked=requestSendsResponse(listGridServers,listGridServersResponse);
  assertEquals(addServerWithOptionsWorked.getServerServices().addServer("serverName","img55","memory","127.0.0.1",new AddServerOptions().asSandboxType().withDescription("fooy")).toString(),new ParseServerTest().expected().toString());
}
