{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("DELETE",vAppId).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/deleteVAppTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=deleteVAppTask();
  assertEquals(api.getVAppApi().deleteVApp(vAppURI),expected);
}
