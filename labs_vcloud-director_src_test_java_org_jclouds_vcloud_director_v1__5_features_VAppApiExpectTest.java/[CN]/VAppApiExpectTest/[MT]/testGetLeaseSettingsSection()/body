{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vAppId + "/leaseSettingsSection").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getLeaseSettingsSection.xml",VCloudDirectorMediaType.LEASE_SETTINGS_SECTION).httpResponseBuilder().build());
  LeaseSettingsSection expected=getLeaseSettingsSection();
  assertEquals(api.getVAppApi().getLeaseSettingsSection(vAppURI),expected);
}
