{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT",vAppId + "/startupSection").xmlFilePayload("/vApp/modifyStartupSection.xml",VCloudDirectorMediaType.STARTUP_SECTION).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/modifyStartupSectionTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  StartupSection section=null;
  Task expected=modifyStartupSectionTask();
  assertEquals(api.getVAppApi().modifyStartupSection(vAppURI,section),expected);
}
