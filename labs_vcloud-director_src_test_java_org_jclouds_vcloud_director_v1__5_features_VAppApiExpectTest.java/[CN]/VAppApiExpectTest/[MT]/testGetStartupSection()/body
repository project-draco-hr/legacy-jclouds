{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vAppId + "/startupSection").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getStartupSection.xml",VCloudDirectorMediaType.STARTUP_SECTION).httpResponseBuilder().build());
  StartupSection expected=getStartupSection();
  assertEquals(api.getVAppApi().getStartupSection(vAppURI),expected);
}
