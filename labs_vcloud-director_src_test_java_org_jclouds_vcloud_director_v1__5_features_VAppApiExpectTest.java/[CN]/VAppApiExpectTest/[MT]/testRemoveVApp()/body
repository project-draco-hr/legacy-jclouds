{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("DELETE",vAppId).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/removeVAppTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=removeVAppTask();
  assertEquals(api.getVAppApi().removeVApp(vAppURI),expected);
}
