{
  VCloudDirectorApi api=orderedRequestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vAppId).acceptMedia(VCloudDirectorMediaType.VAPP).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/vApp.xml",VCloudDirectorMediaType.VAPP).httpResponseBuilder().build());
  VApp expected=getVApp();
  assertEquals(api.getVAppApi().getVApp(vAppURI),expected);
}
