{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",vAppId + "/power/action/powerOff").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/powerOffTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=powerOffTask();
  assertEquals(api.getVAppApi().powerOff(vAppURI),expected);
}
