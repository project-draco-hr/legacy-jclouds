{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT",vAppId + "/startupSection").xmlFilePayload("/vApp/editStartupSection.xml",VCloudDirectorMediaType.STARTUP_SECTION).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/editStartupSectionTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  StartupSection section=null;
  Task expected=editStartupSectionTask();
  assertEquals(api.getVAppApi().editStartupSection(vAppURI,section),expected);
}
