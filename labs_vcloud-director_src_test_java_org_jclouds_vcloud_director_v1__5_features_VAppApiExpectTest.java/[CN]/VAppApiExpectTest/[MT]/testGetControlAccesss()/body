{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vAppId + "/controlAccess").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getControlAccess.xml",VCloudDirectorMediaType.VAPP).httpResponseBuilder().build());
  ControlAccessParams expected=getControlAccessParams();
  assertEquals(api.getVAppApi().getControlAccess(vAppURI),expected);
}
