{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vAppId + "/controlAccess").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getAccessControl.xml",VCloudDirectorMediaType.VAPP).httpResponseBuilder().build());
  ControlAccessParams expected=getAccessControlParams();
  assertEquals(api.getVAppApi().getAccessControl(vAppURI),expected);
}
