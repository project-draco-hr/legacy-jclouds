{
  if (qName.equals("Vdc")) {
    vDC=new LinkImpl(attributes.getValue(attributes.getIndex("name")),attributes.getValue(attributes.getIndex("type")),URI.create(attributes.getValue(attributes.getIndex("href"))));
  }
 else   if (qName.equals("Network")) {
    putLink(availableNetworks,attributes);
  }
 else   if (qName.equals("ResourceEntity")) {
    putLink(resourceEntities,attributes);
  }
}
