{
  HttpRequest request=signer.signGetBlob("container","name",120);
  assertRequestLineEquals(request,"GET http://storage/container/name?" + "temp_url_sig=4759d99d13c826bba0af2c9f0c526ca53c95abaf&temp_url_expires=123456909 HTTP/1.1");
  assertFalse(request.getHeaders().containsKey("X-Auth-Token"));
  assertPayloadEquals(request,null,null,false);
  assertEquals(request.getFilters().size(),0);
}
