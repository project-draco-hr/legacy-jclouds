{
  DriveMetrics.Builder builder=new DriveMetrics.Builder();
  if (from.containsKey(key + ":read:bytes"))   builder.readBytes(new Long(from.get(key + ":read:bytes")));
  if (from.containsKey(key + ":read:requests"))   builder.readRequests(new Long(from.get(key + ":read:requests")));
  if (from.containsKey(key + ":write:bytes"))   builder.writeBytes(new Long(from.get(key + ":write:bytes")));
  if (from.containsKey(key + ":write:requests"))   builder.writeRequests(new Long(from.get(key + ":write:requests")));
  return builder.build();
}
