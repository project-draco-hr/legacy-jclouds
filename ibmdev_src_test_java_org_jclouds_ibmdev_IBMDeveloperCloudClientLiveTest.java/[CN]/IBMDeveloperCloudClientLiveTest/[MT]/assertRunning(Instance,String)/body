{
  instance=connection.getInstance(instance.getId());
  try {
    assertIpHostAndStatusACTIVE(instance);
    assertConsistent(instance,name);
  }
 catch (  NullPointerException e) {
    System.err.println(instance);
    throw e;
  }
catch (  AssertionError e) {
    System.err.println(instance);
    throw e;
  }
}
