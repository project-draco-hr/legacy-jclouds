{
  return new Predicate<OsFamilyVersion64Bit>(){
    @Override public boolean apply(    OsFamilyVersion64Bit input){
      return (input.family != OsFamily.WINDOWS && !input.is64Bit) || input.family == OsFamily.RHEL || (input.family == OsFamily.UBUNTU && input.version.equals("11.04")) || (input.family == OsFamily.CENTOS && input.version.matches("5.[23]")) || (input.family == OsFamily.WINDOWS && input.version.equals("2008")) || (input.family == OsFamily.WINDOWS && input.version.equals("2008 R2") && !input.is64Bit);
    }
  }
;
}
