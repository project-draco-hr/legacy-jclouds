{
  Location location=client.getLocationById(URI.create("/cloudapi/ecloud/locations/1"));
  Location expected=Location.builder().href(URI.create("/cloudapi/ecloud/locations/1")).name("Terremark - Richardson").type("application/vnd.tmrk.cloud.location").friendlyName("Terremark - Richardson").locode("DAC").iso3166("US-TX").build();
  assertEquals(location,expected);
}
