{
  RESTLink link=checkNotNull(target.searchLink(ParentLinkName.DATACENTER),ValidationErrors.MISSING_REQUIRED_LINK + " " + ParentLinkName.DATACENTER);
  ExtendedUtils utils=(ExtendedUtils)context.getUtils();
  HttpResponse response=utils.getAbiquoHttpClient().get(link);
  ParseXMLWithJAXB<DatacenterDto> parser=new ParseXMLWithJAXB<DatacenterDto>(utils.getXml(),TypeLiteral.get(DatacenterDto.class));
  datacenter=wrap(context,Datacenter.class,parser.apply(response));
  return datacenter;
}
