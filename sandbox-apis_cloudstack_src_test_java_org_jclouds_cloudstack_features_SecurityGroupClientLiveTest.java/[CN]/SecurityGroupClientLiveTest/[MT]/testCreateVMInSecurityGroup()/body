{
  if (!securityGroupsSupported)   return;
  vm=VirtualMachineClientLiveTest.createVirtualMachineWithSecurityGroupInZone(zone.getId(),group.getId(),client,jobComplete,virtualMachineRunning);
  if (vm.getPassword() != null)   password=vm.getPassword();
  checkSSH(new IPSocket(vm.getIPAddress(),22));
}
