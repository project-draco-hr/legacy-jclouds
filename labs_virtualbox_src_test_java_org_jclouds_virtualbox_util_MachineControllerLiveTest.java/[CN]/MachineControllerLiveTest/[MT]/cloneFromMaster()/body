{
  IMachine source=getVmWithGuestAdditionsInstalled();
  CloneSpec cloneSpec=CloneSpec.builder().vm(masterSpec.getVmSpec()).network(masterSpec.getNetworkSpec()).master(source).linked(true).build();
  return new CloneAndRegisterMachineFromIMachineIfNotAlreadyExists(manager,workingDir,machineUtils).apply(cloneSpec);
}
