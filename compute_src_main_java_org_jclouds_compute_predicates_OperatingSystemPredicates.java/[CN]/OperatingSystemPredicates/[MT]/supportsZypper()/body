{
  return new Predicate<OperatingSystem>(){
    @Override public boolean apply(    OperatingSystem os){
      if (os.getFamily() != null) {
switch (os.getFamily()) {
case SUSE:
          return true;
      }
    }
    for (    String toMatch : ImmutableSet.of(os.getName(),os.getDescription()))     if (toMatch != null && toMatch.toLowerCase().indexOf("suse") != -1)     return true;
    return false;
  }
  @Override public String toString(){
    return "supportsZypper()";
  }
}
;
}
