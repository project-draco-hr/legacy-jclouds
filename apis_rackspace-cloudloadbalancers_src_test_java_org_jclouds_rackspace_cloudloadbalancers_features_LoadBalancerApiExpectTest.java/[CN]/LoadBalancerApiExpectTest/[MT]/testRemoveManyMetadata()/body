{
  URI endpoint=URI.create("https://dfw.loadbalancers.api.rackspacecloud.com/v1.0/123123/loadbalancers/2000/metadata?id=23&id=24");
  LoadBalancerApi api=requestsSendResponses(rackspaceAuthWithUsernameAndApiKey,responseWithAccess,authenticatedGET().method("DELETE").endpoint(endpoint).replaceHeader("Accept",MediaType.WILDCARD).build(),HttpResponse.builder().statusCode(200).build()).getLoadBalancerApiForZone("DFW");
  assertTrue(api.removeMetadata(2000,ImmutableList.<Integer>of(23,24)));
}
