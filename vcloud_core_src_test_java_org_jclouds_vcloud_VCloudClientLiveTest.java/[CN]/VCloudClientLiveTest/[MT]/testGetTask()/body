{
  org.jclouds.vcloud.domain.TasksList response=connection.getDefaultTasksList().get(10,TimeUnit.SECONDS);
  assertNotNull(response);
  assertNotNull(response.getLocation());
  assertNotNull(response.getTasks());
  for (  Task t : response.getTasks()) {
    assertEquals(connection.getTask(t.getLocation()).get(30,TimeUnit.SECONDS).getLocation(),t.getLocation());
  }
}
