{
  identity=checkNotNull(System.getProperty("test." + provider + ".identity"),"test." + provider + ".identity must be set.  ex. apiKey");
  credential=checkNotNull(System.getProperty("test." + provider + ".credential"),"test." + provider + ".credential must be set.  ex. secretKey");
  endpoint=System.getProperty("test." + provider + ".endpoint");
  apiversion=System.getProperty("test." + provider + ".apiversion");
}
