{
  super(call("setupPublicCurl"),saveHttpResponseTo(download,"{tmp}{fs}",vboxGuestAdditionsIso),exec(String.format("mount -o loop {tmp}{fs}%s %s",vboxGuestAdditionsIso,mountPoint)),call("installModuleAssistantIfNeeded"),exec(String.format("%s%s",mountPoint,"/VBoxLinuxAdditions.run")),exec(String.format("umount %s",mountPoint)));
}
