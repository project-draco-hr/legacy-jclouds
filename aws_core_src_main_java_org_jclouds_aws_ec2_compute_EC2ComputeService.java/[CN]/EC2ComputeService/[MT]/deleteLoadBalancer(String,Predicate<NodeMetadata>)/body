{
  String region=parseHandle(Iterables.get(Iterables.filter(super.listNodesDetailsMatching(NodePredicates.all()),Predicates.and(filter,Predicates.not(NodePredicates.TERMINATED))),0).getHandle())[0];
  ec2Client.getElasticLoadBalancerServices().deleteLoadBalancerInRegion(region,loadBalancerName);
}
