{
  try {
    String sha1=on(':').join(fixedLength(2).split(hex(CryptoStreams.sha1(KeyFactory.getInstance("RSA").generatePrivate(privateKey).getEncoded()))));
    return sha1;
  }
 catch (  InvalidKeySpecException e) {
    throw propagate(e);
  }
catch (  NoSuchAlgorithmException e) {
    throw propagate(e);
  }
}
