{
  checkArgument(checkNotNull(input,"input") instanceof Set,"this binder is only valid for Set!");
  Set<byte[]> md5s=(Set<byte[]>)input;
  StringBuilder builder=new StringBuilder();
  builder.append("{\"checksums\":{");
  for (  byte[] md5 : md5s)   builder.append(String.format("\"%s\":null,",encryptionService.hex(md5)));
  builder.deleteCharAt(builder.length() - 1);
  builder.append("}}");
  request.getHeaders().replaceValues(HttpHeaders.CONTENT_TYPE,ImmutableSet.of(MediaType.APPLICATION_JSON));
  super.bindToRequest(request,builder.toString());
}
