{
  Set<Annotation> keys=new HashSet<Annotation>();
  List<Annotation> parameterAnnotations=Lists.newArrayList(method.getParameterAnnotations()[index]);
  Collection<Annotation> filtered=Collections2.filter(parameterAnnotations,new Predicate<Annotation>(){
    public boolean apply(    Annotation input){
      return input.annotationType().equals(clazz);
    }
  }
);
  for (  Annotation annotation : filtered) {
    keys.add(annotation);
  }
  return keys;
}
