{
  if (invocation.getInvokable().getEnclosingType().getRawType().isAnnotationPresent(Path.class))   uriBuilder.appendPath(invocation.getInvokable().getEnclosingType().getRawType().getAnnotation(Path.class).value());
  if (invocation.getInvokable().isAnnotationPresent(Path.class))   uriBuilder.appendPath(invocation.getInvokable().getAnnotation(Path.class).value());
  return getPathParamKeyValues(invocation);
}
