{
  Optional<Consumes> accept=Optional.fromNullable(invocation.getInvokable().getAnnotation(Consumes.class)).or(Optional.fromNullable(invocation.getInterfaceType().getAnnotation(Consumes.class)));
  return (accept.isPresent()) ? ImmutableSet.copyOf(accept.get().value()) : ImmutableSet.<String>of();
}
