{
  Map<Integer,Set<Annotation>> indexToPayloadAnnotation=filterValues(toRefine.getUnchecked(method).asMap(),new Predicate<Set<Annotation>>(){
    public boolean apply(    Set<Annotation> input){
      return input.size() == 1;
    }
  }
);
  return ImmutableSortedMap.copyOf(indexToPayloadAnnotation);
}
