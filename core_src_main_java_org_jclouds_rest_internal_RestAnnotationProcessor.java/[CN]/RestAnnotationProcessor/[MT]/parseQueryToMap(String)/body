{
  Multimap<String,String> map=LinkedListMultimap.create();
  if (in == null) {
  }
 else   if (in.indexOf('&') == -1) {
    map.put(in,null);
  }
 else {
    String[] parts=HttpUtils.urlDecode(in).split("&");
    for (int partIndex=0; partIndex < parts.length; partIndex++) {
      int indexOfFirstEquals=parts[partIndex].indexOf('=');
      String key=indexOfFirstEquals == -1 ? parts[partIndex] : parts[partIndex].substring(0,indexOfFirstEquals);
      String value=indexOfFirstEquals == -1 ? null : parts[partIndex].substring(indexOfFirstEquals + 1);
      map.put(key,value);
    }
  }
  return map;
}
