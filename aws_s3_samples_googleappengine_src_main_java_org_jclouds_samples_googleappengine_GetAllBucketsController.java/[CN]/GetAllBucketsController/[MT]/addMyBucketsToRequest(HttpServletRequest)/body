{
  System.err.println(context.getAccount() + ":" + context.getEndPoint());
  SortedSet<BucketMetadata> myBucketMetadata=context.getApi().listOwnedBuckets().get(10,TimeUnit.SECONDS);
  SortedSet<BucketResult> myBuckets=Sets.newTreeSet(Iterables.transform(myBucketMetadata,metadataToBucketResultProvider.get()));
  request.setAttribute("buckets",myBuckets);
}
