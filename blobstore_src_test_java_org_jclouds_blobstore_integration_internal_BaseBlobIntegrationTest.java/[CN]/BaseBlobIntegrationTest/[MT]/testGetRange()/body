{
  String containerName=getContainerName();
  try {
    String key="apples";
    addObjectAndValidateContent(containerName,key);
    Blob object1=context.getBlobStore().getBlob(containerName,key,range(0,5));
    assertEquals(BlobStoreUtils.getContentAsStringOrNullAndClose(object1),TEST_STRING.substring(0,6));
    Blob object2=context.getBlobStore().getBlob(containerName,key,range(6,TEST_STRING.length()));
    assertEquals(BlobStoreUtils.getContentAsStringOrNullAndClose(object2),TEST_STRING.substring(6,TEST_STRING.length()));
  }
  finally {
    returnContainer(containerName);
  }
}
