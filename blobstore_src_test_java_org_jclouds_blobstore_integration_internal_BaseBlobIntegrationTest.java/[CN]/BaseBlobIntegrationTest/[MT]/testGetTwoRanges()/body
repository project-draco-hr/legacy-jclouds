{
  String containerName=getContainerName();
  try {
    String key="apples";
    addObjectAndValidateContent(containerName,key);
    Blob object=context.getBlobStore().getBlob(containerName,key,range(0,5).range(6,TEST_STRING.length()));
    assertEquals(getContentAsStringOrNullAndClose(object),TEST_STRING);
  }
  finally {
    returnContainer(containerName);
  }
}
