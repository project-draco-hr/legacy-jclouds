{
  if (qName.equals("Org")) {
    int typeIndex=attributes.getIndex("type");
    if (typeIndex != -1) {
      if (attributes.getValue(typeIndex).equals(ORG_XML)) {
        int index=attributes.getIndex("href");
        if (index != -1) {
          org=URI.create(attributes.getValue(index));
        }
      }
    }
  }
}
