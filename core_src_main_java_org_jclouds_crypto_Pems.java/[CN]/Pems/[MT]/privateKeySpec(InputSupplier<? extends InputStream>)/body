{
  return fromPem(supplier,new PemProcessor<KeySpec>(ImmutableMap.<String,ResultParser<KeySpec>>of(PRIVATE_PKCS1_MARKER,new ResultParser<KeySpec>(){
    @Override public KeySpec parseResult(    byte[] bytes) throws IOException {
      return new PKCS1EncodedPrivateKeySpec(bytes).getKeySpec();
    }
  }
,PRIVATE_PKCS8_MARKER,new ResultParser<KeySpec>(){
    @Override public KeySpec parseResult(    byte[] bytes) throws IOException {
      return new PKCS8EncodedKeySpec(bytes);
    }
  }
)));
}
