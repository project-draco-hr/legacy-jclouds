{
  PortApi api=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,authenticatedGET().endpoint(endpoint + "/tenants/3456/networks/16dba3bc-f3fa-4775-afdc-237e12c72f6a/ports/646c123b-871a-4124-9fa2-a94f04a582df").build(),HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/port.json",APPLICATION_JSON)).build()).getPortApiForRegionAndNetwork("region-a.geo-1","16dba3bc-f3fa-4775-afdc-237e12c72f6a");
  Port port=api.get("646c123b-871a-4124-9fa2-a94f04a582df");
  assertEquals(port,new ParsePortTest().expected());
}
