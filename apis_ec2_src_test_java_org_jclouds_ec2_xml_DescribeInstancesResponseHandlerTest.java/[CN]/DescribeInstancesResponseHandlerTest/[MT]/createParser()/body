{
  Injector injector=Guice.createInjector(new SaxParserModule(),new AbstractModule(){
    @Override protected void configure(){
      bind(new TypeLiteral<Supplier<String>>(){
      }
).annotatedWith(Region.class).toInstance(Suppliers.ofInstance("us-east-1"));
    }
  }
);
  ParseSax<Set<Reservation<? extends RunningInstance>>> parser=injector.getInstance(ParseSax.Factory.class).create(injector.getInstance(DescribeInstancesResponseHandler.class));
  return parser;
}
