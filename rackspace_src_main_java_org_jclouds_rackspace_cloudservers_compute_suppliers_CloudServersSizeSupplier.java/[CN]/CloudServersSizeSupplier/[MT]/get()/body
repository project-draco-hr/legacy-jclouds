{
  final Set<Size> sizes=Sets.newHashSet();
  logger.debug(">> providing sizes");
  for (  final Flavor from : sync.listFlavors(ListOptions.Builder.withDetails())) {
    sizes.add(new SizeImpl(from.getId() + "",from.getName(),from.getId() + "",location.get(),null,ImmutableMap.<String,String>of(),from.getDisk() / 10,from.getRam(),from.getDisk(),ImagePredicates.any()));
  }
  logger.debug("<< sizes(%d)",sizes.size());
  return sizes;
}
