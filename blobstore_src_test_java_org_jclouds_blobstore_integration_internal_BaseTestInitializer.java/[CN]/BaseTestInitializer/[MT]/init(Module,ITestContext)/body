{
  String endpoint=System.getProperty("jclouds.test.endpoint");
  String app=System.getProperty("jclouds.test.app");
  String identity=System.getProperty("jclouds.test.identity");
  String credential=System.getProperty("jclouds.test.credential");
  if (endpoint != null)   testContext.setAttribute("jclouds.test.endpoint",endpoint);
  if (app != null)   testContext.setAttribute("jclouds.test.app",app);
  if (identity != null)   testContext.setAttribute("jclouds.test.identity",identity);
  if (credential != null)   testContext.setAttribute("jclouds.test.credential",credential);
  if (identity != null) {
    return createLiveContext(configurationModule,endpoint,app,identity,credential);
  }
 else {
    return createStubContext();
  }
}
