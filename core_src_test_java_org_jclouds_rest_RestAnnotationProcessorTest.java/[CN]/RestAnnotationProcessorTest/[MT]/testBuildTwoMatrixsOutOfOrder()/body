{
  Method twoMatrixsOutOfOrder=TestMatrixReplace.class.getMethod("twoMatrixsOutOfOrder",String.class,String.class);
  String path=factory(TestMatrixReplace.class).createRequest(twoMatrixsOutOfOrder,new Object[]{"robot","eggs"}).getEndpoint().getPath();
  assertEquals(path,"/;x-amz-copy-source=/eggs/robot");
}
