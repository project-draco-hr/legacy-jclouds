{
  Method oneMatrix=TestMatrixReplace.class.getMethod("matrixInOptions",String.class,TestReplaceMatrixOptions.class);
  String path=factory(TestMatrixReplace.class).createRequest(oneMatrix,new Object[]{"robot",new TestReplaceMatrixOptions()}).getEndpoint().getPath();
  assertEquals(path,"/;x-amz-copy-source=/robot");
}
