{
  MediaInsertOrEjectParams params=MediaInsertOrEjectParams.builder().media(Reference.builder().href(lazyGetMedia().getHref()).type(MEDIA).build()).build();
  Task insertMediaTask=vmApi.insertMedia(vmUrn,params);
  assertTrue(retryTaskSuccess.apply(insertMediaTask),String.format(TASK_COMPLETE_TIMELY,"insertMedia"));
}
