{
  key=name("key-");
  metadataValue=name("value-");
  vmApi.getMetadataApi(vmUrn).put(key,metadataValue);
  String newMetadataValue=vmApi.getMetadataApi(vmUrn).get(key);
  assertEquals(newMetadataValue,metadataValue,String.format(CORRECT_VALUE_OBJECT_FMT,"Value","MetadataValue",metadataValue,newMetadataValue));
}
