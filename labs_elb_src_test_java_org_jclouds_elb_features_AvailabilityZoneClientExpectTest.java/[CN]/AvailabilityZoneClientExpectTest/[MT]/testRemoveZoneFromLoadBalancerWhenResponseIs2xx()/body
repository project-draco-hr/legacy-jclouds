{
  HttpResponse getResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/zones.xml","text/xml")).build();
  ELBClient clientWhenExist=requestSendsResponse(removeZoneFromLoadBalancer,getResponse);
  assertEquals(clientWhenExist.getAvailabilityZoneClient().removeAvailabilityZoneFromLoadBalancer("us-east-1a","name").toString(),new AvailabilityZonesResultHandlerTest().expected().toString());
}
