{
  HttpResponse getResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/zones.xml","text/xml")).build();
  ELBClient clientWhenExist=requestSendsResponse(addZoneToLoadBalancer,getResponse);
  assertEquals(clientWhenExist.getAvailabilityZoneClient().addAvailabilityZoneToLoadBalancer("us-east-1a","name").toString(),new AvailabilityZonesResultHandlerTest().expected().toString());
}
