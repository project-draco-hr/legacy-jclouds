{
  NioHttpCommandConnectionPool pool=new NioHttpCommandConnectionPool(null,null,null,null,createNiceMock(AsyncNHttpClientHandler.class),null,createNiceMock(HttpParams.class),URI.create("http://adriancole.blobstore1138eu.s3-external-3.amazonaws.com"),1,1);
  assertEquals(pool.getTarget(),new InetSocketAddress("adriancole.blobstore1138eu.s3-external-3.amazonaws.com",80));
}
