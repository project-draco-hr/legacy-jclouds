{
  if (testContext.getAttribute("jclouds.test.identity") != null) {
    AWSCredentials credentials=new AWSCredentials((String)testContext.getAttribute("jclouds.test.identity"),(String)testContext.getAttribute("jclouds.test.credential"));
    jetClient=new RestS3Service(credentials);
  }
 else {
    throw new RuntimeException("not configured properly");
  }
}
