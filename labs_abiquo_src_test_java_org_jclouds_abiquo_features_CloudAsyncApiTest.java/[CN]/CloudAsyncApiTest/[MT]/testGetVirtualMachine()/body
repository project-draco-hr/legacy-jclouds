{
  Method method=CloudAsyncApi.class.getMethod("getVirtualMachine",VirtualApplianceDto.class,Integer.class);
  GeneratedHttpRequest request=processor.createRequest(method,CloudResources.virtualAppliancePut(),1);
  assertRequestLineEquals(request,"GET http://localhost/api/cloud/virtualdatacenters/1/virtualappliances/1/virtualmachines/1 HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"Accept: " + VirtualMachineWithNodeExtendedDto.BASE_MEDIA_TYPE + "\n");
  assertPayloadEquals(request,null,null,false);
  assertResponseParserClassEquals(method,request,ParseXMLWithJAXB.class);
  assertSaxResponseParserClassEquals(method,null);
  assertExceptionParserClassEquals(method,ReturnNullOnNotFoundOr404.class);
  checkFilters(request);
}
