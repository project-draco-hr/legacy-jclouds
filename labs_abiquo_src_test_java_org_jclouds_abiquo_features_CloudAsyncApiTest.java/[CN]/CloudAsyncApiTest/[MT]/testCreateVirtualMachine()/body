{
  Method method=CloudAsyncApi.class.getMethod("createVirtualMachine",VirtualApplianceDto.class,VirtualMachineWithNodeExtendedDto.class);
  GeneratedHttpRequest request=processor.createRequest(method,CloudResources.virtualAppliancePut(),CloudResources.virtualMachinePost());
  assertRequestLineEquals(request,"POST http://localhost/api/cloud/virtualdatacenters/1/virtualappliances/1/virtualmachines HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"Accept: " + VirtualMachineWithNodeExtendedDto.BASE_MEDIA_TYPE + "\n");
  assertPayloadEquals(request,withHeader(CloudResources.virtualMachinePostPayload()),VirtualMachineWithNodeExtendedDto.class,VirtualMachineWithNodeExtendedDto.BASE_MEDIA_TYPE,false);
  assertResponseParserClassEquals(method,request,ParseXMLWithJAXB.class);
  assertSaxResponseParserClassEquals(method,null);
  assertFallbackClassEquals(method,null);
  checkFilters(request);
}
