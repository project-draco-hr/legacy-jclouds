{
  final String path=container + "/" + blob.getMetadata().getName();
  final AtmosObject object=blob2Object.apply(blob);
  try {
    sync.createFile(container,object,options);
  }
 catch (  IllegalStateException e) {
    deleteAndEnsureGone(sync,path);
    sync.createFile(container,object,options);
  }
  return path;
}
