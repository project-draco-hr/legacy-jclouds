{
  HttpRequest request=buildGETWithQuery("Action=GetEFMStatus" + "&efmId=CONTRACT-VSYS00001-S-0001" + "&vsysId=CONTRACT-VSYS00001");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/GetEFMStatus-response.xml")).build();
  BuiltinServerApi api=requestSendsResponse(request,response).getFirewallApi();
  assertEquals(api.getStatus("CONTRACT-VSYS00001-S-0001"),BuiltinServerStatus.RUNNING);
}
