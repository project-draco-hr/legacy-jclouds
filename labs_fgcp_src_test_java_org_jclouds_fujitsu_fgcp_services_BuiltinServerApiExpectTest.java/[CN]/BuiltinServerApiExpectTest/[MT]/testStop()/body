{
  HttpRequest request=buildGETWithQuery("Action=StopEFM" + "&efmId=CONTRACT-VSYS00001-S-0001" + "&vsysId=CONTRACT-VSYS00001");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/StopEFM-response.xml")).build();
  BuiltinServerApi api=requestSendsResponse(request,response).getFirewallApi();
  api.stop("CONTRACT-VSYS00001-S-0001");
}
