{
  HttpRequest request=buildGETWithQuery("Action=DestroyEFM" + "&efmId=CONTRACT-VSYS00001-S-0001" + "&vsysId=CONTRACT-VSYS00001");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/DestroyEFM-response.xml")).build();
  BuiltinServerApi api=requestSendsResponse(request,response).getFirewallApi();
  api.destroy("CONTRACT-VSYS00001-S-0001");
}
