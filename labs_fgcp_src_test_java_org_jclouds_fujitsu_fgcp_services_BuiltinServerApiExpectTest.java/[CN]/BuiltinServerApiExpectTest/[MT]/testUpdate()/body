{
  HttpRequest request=buildGETWithQuery("Action=UpdateEFMAttribute" + "&efmId=CONTRACT-VSYS00001-S-0001" + "&attributeValue=new%20name"+ "&attributeName=vserverName"+ "&vsysId=CONTRACT-VSYS00001");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/UpdateEFMAttribute-response.xml")).build();
  BuiltinServerApi api=requestSendsResponse(request,response).getFirewallApi();
  api.update("CONTRACT-VSYS00001-S-0001","vserverName","new name");
}
