{
  Invokable<?,?> method=Invokable.from(BrowsingAsyncApi.class.getMethod("getVMInVDC",String.class,String.class,String.class,GetVMOptions[].class));
  HttpRequest request=processor.createRequest(method,Lists.<Object>newArrayList((String)null,"22","VM-Tier01"));
  assertRequestLineEquals(request,"GET https://api.savvis.net/vpdc/v1.0/org/1/vdc/22/vApp/VM-Tier01 HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"");
  assertPayloadEquals(request,null,null,false);
  assertResponseParserClassEquals(method,request,ParseSax.class);
  assertSaxResponseParserClassEquals(method,VMHandler.class);
  assertFallbackClassEquals(method,NullOnNotFoundOr404.class);
  checkFilters(request);
}
