{
  Map<String,String> attributes=Utils.cleanseAttributes(attrs);
  if (qName.equals("state")) {
    state=InstanceState.valueOf(attributes.get("name").toUpperCase());
  }
 else   if (qName.equals("transition")) {
    if (attributes.containsKey("auto"))     states.put(state,new TransitionAutomatically(InstanceState.valueOf(attributes.get("to").toUpperCase())));
 else     states.put(state,new TransitionOnAction(attributes.get("action"),InstanceState.valueOf(attributes.get("to").toUpperCase())));
  }
}
