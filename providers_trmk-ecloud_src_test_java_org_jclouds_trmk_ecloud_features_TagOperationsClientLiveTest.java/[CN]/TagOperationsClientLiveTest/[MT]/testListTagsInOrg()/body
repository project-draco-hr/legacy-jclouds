{
  for (  ReferenceType response : connection.listOrgs().values()) {
    ECloudOrg org=connection.getOrg(response.getHref());
    assertNotNull(response);
    assertNotNull(response.getName());
    assertNotNull(response.getHref());
    assertEquals(connection.getTagOperationsClient().getTagNameToUsageCountInOrg(org.getHref()),connection.getTagOperationsClient().getTagNameToUsageCount(org.getTags().getHref()));
  }
}
