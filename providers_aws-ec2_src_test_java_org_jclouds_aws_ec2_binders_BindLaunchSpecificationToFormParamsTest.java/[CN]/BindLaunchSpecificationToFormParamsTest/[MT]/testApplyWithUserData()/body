{
  LaunchSpecification spec=LaunchSpecification.builder().instanceType(InstanceType.T1_MICRO).imageId("ami-123").userData("hello".getBytes()).build();
  assertEquals(binder.apply(spec),ImmutableMap.of("LaunchSpecification.InstanceType","t1.micro","LaunchSpecification.ImageId","ami-123","LaunchSpecification.UserData",CryptoStreams.base64("hello".getBytes())));
}
