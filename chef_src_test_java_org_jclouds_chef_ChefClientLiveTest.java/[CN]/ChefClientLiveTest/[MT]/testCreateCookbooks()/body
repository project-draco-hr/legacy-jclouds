{
  InputStream in=null;
  try {
    in=URI.create("https://s3.amazonaws.com/opscode-community/cookbook_versions/tarballs/194/original/java.tar.gz").toURL().openStream();
    byte[] content=ByteStreams.toByteArray(in);
    System.err.println(clientConnection.getApi().createCookbook("java-bytearray",content));
    File file=File.createTempFile("foo","bar");
    Files.write(content,file);
    file.deleteOnExit();
    System.err.println(clientConnection.getApi().createCookbook("java-file",file));
  }
  finally {
    if (in != null)     in.close();
  }
}
