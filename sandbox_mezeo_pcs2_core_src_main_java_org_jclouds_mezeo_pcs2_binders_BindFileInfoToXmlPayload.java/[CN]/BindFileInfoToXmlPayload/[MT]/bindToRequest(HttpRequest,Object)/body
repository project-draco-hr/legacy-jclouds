{
  PCSFile blob=(PCSFile)toBind;
  String file=String.format("<file><name>%s</name><mime_type>%s</mime_type><public>false</public></file>",new File(blob.getMetadata().getName()).getName(),blob.getMetadata().getMimeType());
  request.setPayload(file);
  request.getHeaders().replaceValues(HttpHeaders.CONTENT_LENGTH,Collections.singletonList(file.getBytes().length + ""));
  request.getHeaders().replaceValues(HttpHeaders.CONTENT_TYPE,Collections.singletonList("application/vnd.csp.file-info+xml"));
}
