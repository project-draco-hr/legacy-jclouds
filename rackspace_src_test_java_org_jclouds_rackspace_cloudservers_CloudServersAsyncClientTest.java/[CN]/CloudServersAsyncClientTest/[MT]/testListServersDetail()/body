{
  Method method=CloudServersAsyncClient.class.getMethod("listServers",listOptionsVarargsClass);
  HttpRequest request=processor.createRequest(method,new Object[]{withDetails()});
  assertEquals(request.getEndpoint().getHost(),"serverManagementUrl");
  assertEquals(request.getEndpoint().getPath(),"/servers/detail");
  assertEquals(request.getEndpoint().getQuery(),"format=json");
  assertEquals(request.getMethod(),HttpMethod.GET);
  assertEquals(request.getHeaders().size(),0);
  assertEquals(processor.createResponseParser(method,request).getClass(),ParseServerListFromJsonResponse.class);
  assertEquals(processor.createExceptionParserOrThrowResourceNotFoundOn404IfNoAnnotation(method).getClass(),MapHttp4xxCodesToExceptions.class);
}
