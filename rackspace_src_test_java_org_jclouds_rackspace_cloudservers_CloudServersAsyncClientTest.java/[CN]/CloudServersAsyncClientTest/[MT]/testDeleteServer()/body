{
  Method method=CloudServersAsyncClient.class.getMethod("deleteServer",int.class);
  GeneratedHttpRequest<CloudServersAsyncClient> httpMethod=processor.createRequest(method,new Object[]{2});
  assertEquals(httpMethod.getEndpoint().getHost(),"serverManagementUrl");
  assertEquals(httpMethod.getEndpoint().getPath(),"/servers/2");
  assertEquals(httpMethod.getMethod(),HttpMethod.DELETE);
  assertEquals(httpMethod.getHeaders().size(),0);
  assertEquals(processor.createExceptionParserOrThrowResourceNotFoundOn404IfNoAnnotation(method).getClass(),ReturnFalseOnNotFoundOr404.class);
  assertEquals(processor.createResponseParser(method,httpMethod).getClass(),ReturnTrueIf2xx.class);
}
