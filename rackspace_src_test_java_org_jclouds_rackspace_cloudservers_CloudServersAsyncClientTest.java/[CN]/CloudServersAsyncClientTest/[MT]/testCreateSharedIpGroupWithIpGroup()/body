{
  Method method=CloudServersAsyncClient.class.getMethod("createSharedIpGroup",String.class,createSharedIpGroupOptionsVarargsClass);
  HttpRequest request=processor.createRequest(method,new Object[]{"ralphie",withServer(2)});
  assertEquals("{\"sharedIpGroup\":{\"name\":\"ralphie\",\"server\":2}}",request.getPayload().getRawContent());
  validateCreateSharedIpGroup(method,request);
}
