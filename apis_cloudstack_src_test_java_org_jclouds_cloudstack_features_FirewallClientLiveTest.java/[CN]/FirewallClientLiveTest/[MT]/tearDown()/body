{
  if (rule != null) {
    client.getFirewallClient().deletePortForwardingRule(rule.getId());
  }
  if (vm != null) {
    jobComplete.apply(client.getVirtualMachineClient().destroyVirtualMachine(vm.getId()));
  }
  if (ip != null) {
    client.getAddressClient().disassociateIPAddress(ip.getId());
  }
  super.tearDown();
}
