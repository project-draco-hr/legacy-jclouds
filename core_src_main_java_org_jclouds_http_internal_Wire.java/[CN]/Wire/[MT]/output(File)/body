{
  checkNotNull(out,"output");
  exec.submit(new Runnable(){
    public void run(){
      InputStream in=null;
      try {
        in=new FileInputStream(out);
        wire(">> ",in);
      }
 catch (      FileNotFoundException e) {
        logger.error(e,"Error tapping file: %s",out);
      }
 finally {
        IOUtils.closeQuietly(in);
      }
    }
  }
);
}
