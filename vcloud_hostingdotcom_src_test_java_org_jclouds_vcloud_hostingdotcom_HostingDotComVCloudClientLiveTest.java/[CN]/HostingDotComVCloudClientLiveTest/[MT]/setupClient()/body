{
  account=checkNotNull(System.getProperty("jclouds.test.user"),"jclouds.test.user");
  String key=checkNotNull(System.getProperty("jclouds.test.key"),"jclouds.test.key");
  context=new RestContextFactory().createContext("hostingdotcom",account,key,ImmutableSet.<Module>of(new Log4JLoggingModule()),new Properties());
  connection=context.getApi();
}
