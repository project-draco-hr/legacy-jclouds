{
  return CacheBuilder.newBuilder().expireAfterWrite(30,TimeUnit.SECONDS).build(new CacheLoader<String,AccessControlList>(){
    @Override public AccessControlList load(    String bucketName){
      return client.getBucketACL(bucketName);
    }
    @Override public String toString(){
      return "getBucketAcl()";
    }
  }
);
}
