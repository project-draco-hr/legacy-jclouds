{
  checkArgument(input instanceof GetVMOptions[],"this binder is only valid for GetVAppOptions!");
  GetVMOptions[] options=GetVMOptions[].class.cast(input);
  if (options.length > 0 && options[0].isWithPowerState())   return (R)request.toBuilder().endpoint(uriBuilder.get().uri(request.getEndpoint()).path("withpowerstate").build()).build();
 else   return request;
}
