{
  InputStream is=getClass().getResourceAsStream("/network.xml");
  Injector injector=Guice.createInjector(new SaxParserModule());
  Factory factory=injector.getInstance(ParseSax.Factory.class);
  Network result=factory.create(injector.getInstance(NetworkHandler.class)).parse(is);
  assertEquals(result.toString(),Network.builder().type(VCloudMediaType.NETWORK_XML).name("VM-Tier01").gateway("1.1.1.1").netmask("255.255.255.240").build().toString());
}
