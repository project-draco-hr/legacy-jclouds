{
  return new HardwareBuilder().id(in.getId()).providerId(in.getProviderId()).location(in.getLocation()).name(in.getName()).uri(in.getUri()).userMetadata(in.getUserMetadata()).tags(in.getTags()).processors(List.class.cast(in.getProcessors())).ram(in.getRam()).volumes(List.class.cast(in.getVolumes())).supportsImage(in.supportsImage()).hypervisor(in.getHypervisor());
}
