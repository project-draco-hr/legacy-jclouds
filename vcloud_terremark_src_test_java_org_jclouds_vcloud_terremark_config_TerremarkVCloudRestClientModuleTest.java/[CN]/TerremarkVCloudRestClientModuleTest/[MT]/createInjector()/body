{
  return Guice.createInjector(new TerremarkVCloudRestClientModule(),new RestModule(),new AbstractModule(){
    @Override protected void configure(){
      Names.bindProperties(binder(),checkNotNull(new TerremarkVCloudPropertiesBuilder("user","pass").build(),"properties"));
      bind(TransformingHttpCommandExecutorService.class).toInstance(createMock(TransformingHttpCommandExecutorService.class));
    }
  }
);
}
