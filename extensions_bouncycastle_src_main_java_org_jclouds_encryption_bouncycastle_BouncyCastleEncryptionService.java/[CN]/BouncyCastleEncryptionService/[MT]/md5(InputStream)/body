{
  MD5Digest eTag=new MD5Digest();
  byte[] resBuf=new byte[eTag.getDigestSize()];
  byte[] buffer=new byte[BUF_SIZE];
  int numRead=-1;
  try {
    do {
      numRead=toEncode.read(buffer);
      if (numRead > 0) {
        eTag.update(buffer,0,numRead);
      }
    }
 while (numRead != -1);
  }
 catch (  IOException e) {
    propagate(e);
  }
 finally {
    closeQuietly(toEncode);
  }
  eTag.doFinal(resBuf,0);
  return resBuf;
}
