{
  byte[] resBuf=new byte[digest.getDigestSize()];
  byte[] buffer=new byte[BUF_SIZE];
  long length=0;
  int numRead=-1;
  try {
    do {
      numRead=plainBytes.read(buffer);
      if (numRead > 0) {
        length+=numRead;
        digest.update(buffer,0,numRead);
      }
    }
 while (numRead != -1);
  }
 catch (  IOException e) {
    propagate(e);
  }
 finally {
    closeQuietly(plainBytes);
  }
  digest.doFinal(resBuf,0);
  return resBuf;
}
