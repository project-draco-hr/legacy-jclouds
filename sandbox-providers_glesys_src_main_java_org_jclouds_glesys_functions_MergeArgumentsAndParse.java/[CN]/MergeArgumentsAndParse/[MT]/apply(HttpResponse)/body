{
  try {
    if (arg0.getPayload() == null)     return null;
    arg0=makePayloadReplayable(arg0);
    Map<String,JsonBall> server=parseServer.apply(arg0);
    if (server == null)     return null;
    Map<String,JsonBall> arguments=parseArguments.apply(arg0);
