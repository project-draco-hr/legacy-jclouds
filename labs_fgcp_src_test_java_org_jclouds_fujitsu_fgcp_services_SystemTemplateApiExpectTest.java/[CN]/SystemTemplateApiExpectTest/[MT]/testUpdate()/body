{
  HttpRequest request=buildGETWithQuery("Action=UpdateVSYSDescriptorAttribute" + "&vsysDescriptorId=3-tier%20Skeleton" + "&updateLcId=en"+ "&attributeName=updateName"+ "&attributeValue=new-name");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/UpdateVSYSDescriptorAttribute-response.xml")).build();
  SystemTemplateApi api=requestSendsResponse(request,response).getSystemTemplateApi();
  api.update("3-tier Skeleton","en","updateName","new-name");
}
