{
  OsFamily family=osFamily().apply(from);
  Integer bits=osBits().apply(from);
  OperatingSystem os=OperatingSystem.builder().description(from.getDescription()).family(family).version(osVersion().apply(from)).is64Bit(bits.equals(64)).build();
  return new ImageBuilder().id("" + from.getId()).description(from.getDescription()).operatingSystem(os).build();
}
