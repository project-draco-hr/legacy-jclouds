{
  if (allocationUnits != null)   virtualQuantityUnits=allocationUnits;
  ResourceAllocation allocation=newResourceAllocation();
  address=null;
  addressOnParent=null;
  allocationUnits=null;
  automaticAllocation=null;
  connected=null;
  description=null;
  elementName=null;
  instanceID=-1;
  parent=null;
  resourceSubType=null;
  resourceType=null;
  virtualQuantity=1;
  virtualQuantityUnits=null;
  hostResource=null;
  return allocation;
}
