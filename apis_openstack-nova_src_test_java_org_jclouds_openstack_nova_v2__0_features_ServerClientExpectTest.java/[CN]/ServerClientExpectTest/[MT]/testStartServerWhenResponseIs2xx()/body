{
  String serverId="123";
  HttpRequest startServer=HttpRequest.builder().method("POST").endpoint(URI.create("https://az-1.region-a.geo-1.compute.hpcloudsvc.com/v1.1/3456/servers/" + serverId + "/action")).headers(ImmutableMultimap.<String,String>builder().put("Accept","*/*").put("X-Auth-Token",authToken).build()).payload(payloadFromStringWithContentType("{\"os-start\":null}","application/json")).build();
  HttpResponse startServerResponse=HttpResponse.builder().statusCode(202).build();
  NovaClient clientWhenServerExists=requestsSendResponses(keystoneAuthWithUsernameAndPasswordAndTenantName,responseWithKeystoneAccess,startServer,startServerResponse);
  clientWhenServerExists.getServerClientForZone("az-1.region-a.geo-1").startServer(serverId);
}
