{
  IMachine vm=manager.get().getVBox().findMachine(vmName);
  ensureMachineIsLaunched(vmName);
  NodeMetadata vmMetadata=new IMachineToNodeMetadata().apply(vm);
  ListenableFuture<ExecResponse> execFuture=context.getComputeService().submitScriptOnNode(vmMetadata.getId(),new InstallGuestAdditions(vboxVersion),RunScriptOptions.NONE);
  Futures.getUnchecked(execFuture);
  return vm;
}
