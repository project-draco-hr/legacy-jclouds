{
  Role role=Role.builder().name("foo").jsonDefaultAttributes("{\"foo\":\"bar\"}").build();
  assertEquals(role.toJsonString(),"{\"name\": \"foo\",\"description\":\"\",\"default_attributes\":{\"foo\":\"bar\"}," + "\"override_attributes\":{},\"json_class\":\"Chef::Role\",\"chef_type\":\"role\"," + "\"run_list\":[]}");
}
