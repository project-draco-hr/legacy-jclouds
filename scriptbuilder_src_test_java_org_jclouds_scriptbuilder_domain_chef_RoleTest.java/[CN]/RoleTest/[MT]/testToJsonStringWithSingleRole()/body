{
  Role role=Role.builder().name("foo").installRole("webserver").build();
  assertEquals(role.toJsonString(),"{\"name\": \"foo\",\"description\":\"\",\"default_attributes\":{},\"override_attributes\":{}," + "\"json_class\":\"Chef::Role\",\"chef_type\":\"role\",\"run_list\":[\"role[webserver]\"]}");
}
