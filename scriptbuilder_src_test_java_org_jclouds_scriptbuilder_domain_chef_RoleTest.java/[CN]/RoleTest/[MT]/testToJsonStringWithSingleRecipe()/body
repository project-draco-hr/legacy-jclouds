{
  RunList runlist=RunList.builder().recipe("apache2").build();
  Role role=Role.builder().name("foo").runlist(runlist).build();
  assertEquals(role.toJsonString(),"{\"name\": \"foo\",\"description\":\"\",\"default_attributes\":{},\"override_attributes\":{}," + "\"json_class\":\"Chef::Role\",\"chef_type\":\"role\",\"run_list\":[\"recipe[apache2]\"]}");
}
