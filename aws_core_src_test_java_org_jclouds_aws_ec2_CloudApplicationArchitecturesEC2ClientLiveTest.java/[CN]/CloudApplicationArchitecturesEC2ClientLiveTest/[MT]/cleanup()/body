{
  if (address != null)   client.getElasticIPAddressServices().releaseAddressInRegion(null,address);
  if (instanceId != null)   client.getInstanceServices().terminateInstancesInRegion(null,instanceId);
  if (keyPair != null)   client.getKeyPairServices().deleteKeyPairInRegion(null,keyPair.getKeyName());
  if (securityGroupName != null)   client.getSecurityGroupServices().deleteSecurityGroupInRegion(null,securityGroupName);
}
