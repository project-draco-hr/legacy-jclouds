{
  super(String.format("error runScript on node tag(%s) options(%s)%n%s%n%s",tag,options,ComputeUtils.createExecutionErrorMessage(executionExceptions),ComputeUtils.createNodeErrorMessage(failedNodes)));
  this.tag=tag;
  this.runScript=runScript;
  this.options=options;
  this.successfulNodes=successfulNodes;
  this.failedNodes=failedNodes;
  this.executionExceptions=executionExceptions;
}
