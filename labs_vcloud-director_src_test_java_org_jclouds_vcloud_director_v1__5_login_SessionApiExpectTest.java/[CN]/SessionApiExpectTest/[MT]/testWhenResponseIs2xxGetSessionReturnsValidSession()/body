{
  URI sessionUrl=URI.create("https://vcloudbeta.bluelock.com/api/session");
  String token="mIaR3/6Lna8DWImd7/JPR5rK8FcUHabt+G/UCJV5pJQ=";
  SessionApi api=requestSendsResponse(HttpRequest.builder().method("GET").endpoint(sessionUrl).headers(ImmutableMultimap.<String,String>builder().put("x-vcloud-authorization",token).put("Accept","*/*").build()).build(),HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/session.xml",VCloudDirectorMediaType.SESSION + ";version=1.5")).build());
  assertEquals(api.getSessionWithToken(sessionUrl,token),SESSION);
}
