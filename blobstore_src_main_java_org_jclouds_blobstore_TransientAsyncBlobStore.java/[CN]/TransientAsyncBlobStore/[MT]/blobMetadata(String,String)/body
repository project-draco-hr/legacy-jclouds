{
  try {
    Blob blob=getBlob(container,key).get();
    return immediateFuture(blob != null ? (BlobMetadata)copy(blob.getMetadata()) : null);
  }
 catch (  Exception e) {
    if (Iterables.size(Iterables.filter(Throwables.getCausalChain(e),KeyNotFoundException.class)) >= 1)     return immediateFuture(null);
    return immediateFailedFuture(e);
  }
}
