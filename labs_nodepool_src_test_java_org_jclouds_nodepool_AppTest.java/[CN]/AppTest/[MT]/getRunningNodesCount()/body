{
  int nodeCount=0;
  for (  ComputeMetadata cm : pooledCompute.listNodes()) {
    if (NodeMetadata.class.isInstance(cm)) {
      String nodeGroup=((NodeMetadata)cm).getGroup();
      if (!((NodeMetadata)cm).getState().equals(NodeState.SUSPENDED) && !((NodeMetadata)cm).getState().equals(NodeState.TERMINATED)) {
        nodeCount++;
      }
    }
  }
  return nodeCount;
}
