{
  createCompute();
  assertNotNull(compute);
  createAndStartPool();
  assertNotNull(pooledCompute);
  for (int i=0; i < 3; i++) {
    startCounter();
    provision("pool-1");
    stopCounter();
  }
  for (int i=0; i < 3; i++) {
    startCounter();
    provision("pool-2");
    stopCounter();
  }
  for (int i=0; i < 3; i++) {
    startCounter();
    provision("pool-3");
    stopCounter();
  }
  assertEquals(9,getRunningNodesCount());
  for (  NodeMetadata slave : nodes) {
    assertNotNull(slave);
    LOGGER.info(slave.getId() + "-" + slave.getGroup());
    terminate(slave);
  }
  assertEquals(0,getRunningNodesCount());
}
