{
  AbstractFuture<ExecResponse> future=new AbstractFuture<ExecResponse>(){
    @Override public boolean isCancelled(){
      return true;
    }
  }
;
  LoopUntilTrueOrThrowCancellationException pred=new LoopUntilTrueOrThrowCancellationException(Predicates.<String>alwaysFalse(),1,1,future);
  assertEquals(pred.apply("foo"),false);
}
