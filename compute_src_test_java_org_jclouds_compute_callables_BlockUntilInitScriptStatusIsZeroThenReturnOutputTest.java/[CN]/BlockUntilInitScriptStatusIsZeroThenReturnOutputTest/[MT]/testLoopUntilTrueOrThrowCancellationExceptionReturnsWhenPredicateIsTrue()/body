{
  AbstractFuture<ExecResponse> future=new AbstractFuture<ExecResponse>(){
    @Override public boolean isCancelled(){
      return false;
    }
  }
;
  LoopUntilTrueOrThrowCancellationException pred=new LoopUntilTrueOrThrowCancellationException(Predicates.<String>alwaysTrue(),1,1,future);
  assertEquals(pred.apply("foo"),true);
}
