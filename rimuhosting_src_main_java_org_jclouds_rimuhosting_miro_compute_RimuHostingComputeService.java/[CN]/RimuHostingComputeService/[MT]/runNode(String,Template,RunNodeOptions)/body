{
  NewServerResponse serverResponse=client.createServer(name,checkNotNull(template.getImage().getId(),"imageId"),checkNotNull(template.getSize().getId(),"sizeId"));
  return new CreateNodeResponseImpl(null,name,location,null,ImmutableMap.<String,String>of(),NodeState.UNKNOWN,ImmutableList.<InetAddress>of(),ImmutableList.<InetAddress>of(),new Credentials("root",serverResponse.getNewInstanceRequest().getCreateOptions().getPassword()),ImmutableMap.<String,String>of());
}
