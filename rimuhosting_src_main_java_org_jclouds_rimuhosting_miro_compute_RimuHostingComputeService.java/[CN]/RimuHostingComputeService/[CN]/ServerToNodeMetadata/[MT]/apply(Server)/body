{
  String locationId="//TODO";
  String tag=from.getName().replaceAll("-[0-9]+","");
  Credentials creds=null;
  NodeState state=NodeState.UNKNOWN;
  return new NodeMetadataImpl(from.getId() + "",from.getName(),locationId,null,ImmutableMap.<String,String>of(),tag,state,getPublicAddresses(from),ImmutableList.<InetAddress>of(),ImmutableMap.<String,String>of("state",from.getState()),creds);
}
