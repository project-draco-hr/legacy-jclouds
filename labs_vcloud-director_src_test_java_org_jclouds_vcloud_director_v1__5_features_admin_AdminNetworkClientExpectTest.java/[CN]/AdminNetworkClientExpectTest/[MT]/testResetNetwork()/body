{
  VCloudDirectorAdminClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST","/admin/network/b466c0c5-8a5c-4335-b703-a2e2e6b5f3e1/action/reset").acceptMedia(VCloudDirectorMediaType.TASK).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/network/admin/resetNetworkTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=resetNetworkTask();
  assertEquals(client.getNetworkClient().resetNetwork(networkRef.getHref()),expected);
}
