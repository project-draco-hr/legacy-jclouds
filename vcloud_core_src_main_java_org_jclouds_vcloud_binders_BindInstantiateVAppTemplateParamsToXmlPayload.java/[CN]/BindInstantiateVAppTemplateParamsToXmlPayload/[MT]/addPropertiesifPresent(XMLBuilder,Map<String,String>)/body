{
  if (properties.size() > 0) {
    XMLBuilder productSectionBuilder=instantiationParamsBuilder.e("ProductSection").a("xmlns:q1","http://www.vmware.com/vcloud/v1").a("xmlns:ovf","http://schemas.dmtf.org/ovf/envelope/1");
    for (    Entry<String,String> entry : properties.entrySet()) {
      productSectionBuilder.e("Property").a("xmlns","http://schemas.dmtf.org/ovf/envelope/1").a("ovf:key",entry.getKey()).a("ovf:value",entry.getValue());
    }
  }
}
