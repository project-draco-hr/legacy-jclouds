{
  NovaClient clientWhenExtensionNotInList=requestsSendResponses(keystoneAuthWithAccessKeyAndSecretKey,responseWithKeystoneAccess,extensionsOfNovaRequest,unmatchedExtensionsOfNovaResponse);
  assertEquals(clientWhenExtensionNotInList.getConfiguredRegions(),ImmutableSet.of("North"));
  assertFalse(clientWhenExtensionNotInList.getFloatingIPExtensionForRegion("North").isPresent());
}
