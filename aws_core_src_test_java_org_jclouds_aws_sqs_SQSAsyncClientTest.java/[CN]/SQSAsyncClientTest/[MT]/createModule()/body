{
  return new SQSRestClientModule(){
    @Override protected void configure(){
      Names.bindProperties(binder(),new SQSPropertiesBuilder(new Properties()).withCredentials("user","key").build());
      install(new NullLoggingModule());
      super.configure();
    }
    @Override protected String provideTimeStamp(    final DateService dateService,    @Named(SQSConstants.PROPERTY_AWS_EXPIREINTERVAL) final int expiration){
      return "2009-11-08T15:54:08.897Z";
    }
  }
;
}
