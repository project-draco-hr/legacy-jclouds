{
  AzureBlob object=(AzureBlob)entity;
  checkArgument(object.getProperties().getSize() >= 0,"size must be set");
  checkArgument(checkNotNull(object.getContentLength(),"object.getContentLength()") <= 64 * 1024 * 1024,"maximum size for put Blob is 64MB");
  super.bindToRequest(request,azureBlob2Blob.apply(object));
  if (object.getProperties().getContentLanguage() != null) {
    request.getHeaders().put(HttpHeaders.CONTENT_LANGUAGE,object.getProperties().getContentLanguage());
  }
  if (object.getProperties().getContentEncoding() != null) {
    request.getHeaders().put(HttpHeaders.CONTENT_ENCODING,object.getProperties().getContentEncoding());
  }
}
