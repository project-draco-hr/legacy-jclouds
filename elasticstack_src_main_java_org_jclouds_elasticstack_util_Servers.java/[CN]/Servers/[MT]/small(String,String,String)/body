{
  return new Server.Builder().name(name).cpu(1000).mem(512).persistent(true).devices(ImmutableMap.of("ide:0:0",new IDEDevice.Builder(0,0).uuid(driveUuuid).build())).bootDeviceIds(ImmutableSet.of("ide:0:0")).nics(ImmutableSet.of(new NIC.Builder().model(Model.E1000).dhcp("auto").build())).vnc(new VNC(null,vncPassword,false));
}
