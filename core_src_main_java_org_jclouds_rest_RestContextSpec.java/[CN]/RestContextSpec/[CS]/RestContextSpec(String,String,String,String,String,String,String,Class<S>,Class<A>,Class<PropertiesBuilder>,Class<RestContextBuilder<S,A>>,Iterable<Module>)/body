{
  this.provider=checkNotNull(provider,"provider");
  this.endpoint=endpoint;
  this.apiVersion=apiVersion;
  this.buildVersion=buildVersion;
  this.identity=identity;
  this.credential=credential;
  this.iso3166Codes=iso3166Codes;
  this.sync=sync;
  this.async=async;
  checkArgument(RestContextBuilder.class.isAssignableFrom(contextBuilderClass),contextBuilderClass.getName() + " is not a subclass of " + RestContextBuilder.class.getName());
  checkArgument(PropertiesBuilder.class.isAssignableFrom(propertiesBuilderClass),propertiesBuilderClass.getName() + " is not a subclass of " + PropertiesBuilder.class.getName());
  this.propertiesBuilderClass=propertiesBuilderClass;
  this.contextBuilderClass=contextBuilderClass;
  this.modules=ImmutableList.copyOf(modules);
}
