{
  assertEquals(SshStatements.sshdConfig(ImmutableMap.of("AddressFamily","inet6")).render(OsFamily.UNIX),new StringBuilder().append("exec 3<> /etc/ssh/sshd_config && awk -v TEXT=\"").append("AddressFamily inet6").append("\n").append("\" 'BEGIN {print TEXT}{print}' /etc/ssh/sshd_config >&3").append("\n").append("/etc/init.d/sshd reload||/etc/init.d/ssh reload").append("\n").toString());
}
