{
  if (validatorConnection.getApi().clientExists(PREFIX))   validatorConnection.getApi().deleteClient(PREFIX);
  if (adminConnection.getApi().nodeExists(PREFIX))   adminConnection.getApi().deleteNode(PREFIX);
  if (adminConnection.getApi().roleExists(PREFIX))   adminConnection.getApi().deleteRole(PREFIX);
  if (clientConnection != null)   clientConnection.close();
  if (validatorConnection != null)   validatorConnection.close();
  if (adminConnection != null)   adminConnection.close();
}
