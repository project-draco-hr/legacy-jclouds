{
  List<Module> modules=new ArrayList<Module>();
  CredentialStoreModule module=new CredentialStoreModule();
  modules.add(module);
  new RestContextBuilder<String,String>(String.class,String.class,new Properties()).addCredentialStoreIfNotPresent(modules);
  assertEquals(modules.size(),1);
  assertEquals(modules.remove(0),module);
}
