{
  String statement=String.format(">> running [%s] as %s@%s",command.replace(node.getAdminPassword() != null ? node.getAdminPassword() : "XXXXX","XXXXX"),ssh.getUsername(),ssh.getHostAddress());
  if (command.endsWith("status"))   logger.trace(statement);
 else   computeLogger.debug(statement);
  return ssh.exec(command);
}
