{
  URI endpoint=URI.create("https://compute.north.host/v1.1/3456/servers/1/action");
  AdminActionsClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,extensionsOfNovaRequest,extensionsOfNovaResponse,standardActionRequestBuilderVoidResponse(endpoint,"unpause").build(),standardResponseBuilder(404).build()).getAdminActionsExtensionForZone("az-1.region-a.geo-1").get();
  assertFalse(client.unpauseServer("1"));
}
