{
  URI endpoint=URI.create("https://compute.north.host/v1.1/3456/servers/1/action");
  AdminActionsClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,extensionsOfNovaRequest,extensionsOfNovaResponse,standardActionRequestBuilderVoidResponse(endpoint,"lock").build(),standardResponseBuilder(202).build()).getAdminActionsExtensionForZone("az-1.region-a.geo-1").get();
  assertTrue(client.lockServer("1"));
}
