{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,get.toBuilder().endpoint(endpoint + "/network/9e08c2f6-077a-42ce-bece-d5332e2ebb5c").build(),HttpResponse.builder().statusCode(403).payload(payloadFromResourceWithContentType("/network/error403-catalog.xml",ERROR + ";version=1.5")).build());
  assertNull(api.getNetworkApi().get(URI.create(endpoint + "/network/9e08c2f6-077a-42ce-bece-d5332e2ebb5c")));
}
