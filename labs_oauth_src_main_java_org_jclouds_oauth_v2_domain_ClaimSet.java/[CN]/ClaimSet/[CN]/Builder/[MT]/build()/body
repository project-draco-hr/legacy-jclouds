{
  Map<String,String> claimsMap=claims.build();
  checkState(Sets.intersection(claimsMap.keySet(),requiredClaims).size() == requiredClaims.size(),"not all required claims were present");
  if (expirationTime == 0) {
    expirationTime=emissionTime + 3600;
  }
  return new ClaimSet(claimsMap,emissionTime,expirationTime);
}
