{
  Exception e=new TestException();
  assertEquals(returnFirstExceptionIfInListOrThrowStandardExceptionOrCause(ImmutableSet.<TypeToken<? extends Throwable>>of(TypeToken.of(TestException.class)),e),e);
  assertEquals(returnFirstExceptionIfInListOrThrowStandardExceptionOrCause(ImmutableSet.<TypeToken<? extends Throwable>>of(TypeToken.of(TestException.class)),new RuntimeException(e)),e);
}
