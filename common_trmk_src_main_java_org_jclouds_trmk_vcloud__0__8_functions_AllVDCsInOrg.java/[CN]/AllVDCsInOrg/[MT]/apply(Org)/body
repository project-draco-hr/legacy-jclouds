{
  Iterable<? extends org.jclouds.trmk.vcloud_0_8.domain.VDC> catalogItems=transformParallel(org.getVDCs().values(),new Function<ReferenceType,Future<? extends org.jclouds.trmk.vcloud_0_8.domain.VDC>>(){
    @Override public Future<? extends org.jclouds.trmk.vcloud_0_8.domain.VDC> apply(    ReferenceType from){
