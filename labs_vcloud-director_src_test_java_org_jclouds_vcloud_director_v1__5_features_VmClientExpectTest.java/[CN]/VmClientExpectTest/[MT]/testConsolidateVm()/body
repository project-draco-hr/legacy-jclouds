{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",vmId + "/action/consolidate").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vm/consolidateVmTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=consolidateVmTask();
  assertEquals(client.getVmClient().consolidateVm(vmURI),expected);
}
