{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",vmId + "/power/action/suspend").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/suspend.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=suspendTask();
  assertEquals(client.getVmClient().suspend(vmURI),expected);
}
