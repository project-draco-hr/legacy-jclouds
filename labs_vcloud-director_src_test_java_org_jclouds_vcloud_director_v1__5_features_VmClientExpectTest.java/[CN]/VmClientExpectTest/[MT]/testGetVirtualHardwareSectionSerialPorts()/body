{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vmId + "/virtualHardwareSection/serialPorts").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getVirtualHardwareSectionSerialPorts.xml",VCloudDirectorMediaType.VAPP).httpResponseBuilder().build());
  RasdItemsList expected=getVirtualHardwareSectionSerialPorts();
  assertEquals(client.getVmClient().getVirtualHardwareSectionSerialPorts(vmURI),expected);
}
