{
  checkArgument(checkNotNull(request,"input") instanceof GeneratedHttpRequest,"this binder is only valid for GeneratedHttpRequests!");
  checkArgument(checkNotNull(input,"input") instanceof InetAddress[],"this binder is only valid for InetAddress[] : " + input.getClass());
  InetAddress[] addresses=(InetAddress[])input;
  String[] addressStrings=new String[addresses.length];
  for (int i=0; i < addresses.length; i++) {
    addressStrings[i]=addresses[i].getHostAddress();
  }
  EC2Utils.indexFormValuesWithPrefix((GeneratedHttpRequest<?>)request,"PublicIp",addressStrings);
}
