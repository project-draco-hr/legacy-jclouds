{
  EmailAccountApi api=requestSendsResponse(HttpRequest.builder().method("POST").endpoint("https://api.glesys.com/email/editaccount/format/json").addHeader("Accept","application/json").addHeader("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").addFormParam("emailaccount","test@jclouds.org").addFormParam("password","anotherpass").build(),HttpResponse.builder().statusCode(404).build()).getEmailAccountApi();
  assertEquals(api.update("test@jclouds.org",UpdateAccountOptions.Builder.password("anotherpass")).toString(),getEmailAccountInDetails().toString());
}
