{
  HttpResponse response=new HttpResponse(200,"OK",Payloads.newInputStreamPayload(getClass().getResourceAsStream("/orglist.xml")));
  response.getPayload().setContentType("Content-Type: application/xml; charset=utf-8");
  response.getPayload().setContentLength(307l);
  response.getHeaders().put(HttpHeaders.SET_COOKIE,"vcloud-token=c9f232506df9b65d7b7d97b7499eddd7; Domain=.bluelock.com; Path=/");
  VCloudSession reply=parser.apply(response);
  assertEquals(reply.getVCloudToken(),"c9f232506df9b65d7b7d97b7499eddd7");
  assertEquals(reply.getOrgs(),ImmutableMap.of("adrian@jclouds.org",new NamedResourceImpl("adrian@jclouds.org",VCloudExpressMediaType.ORG_XML,URI.create("https://services.vcloudexpress.terremark.com/api/v0.8/org/48"))));
}
