{
  properties.setProperty(PROPERTY_S3_REGIONS,Joiner.on(',').join(Region.US_STANDARD,Region.US_WEST_1,Region.EU_WEST_1,Region.AP_SOUTHEAST_1));
  properties.setProperty(PROPERTY_S3_ENDPOINT,"https://s3.amazonaws.com");
  properties.setProperty(PROPERTY_S3_ENDPOINT + "." + Region.US_STANDARD,"https://s3.amazonaws.com");
  properties.setProperty(PROPERTY_S3_ENDPOINT + "." + Region.US_WEST_1,"https://s3-us-west-1.amazonaws.com");
  properties.setProperty(PROPERTY_S3_ENDPOINT + "." + Region.EU_WEST_1,"https://s3-eu-west-1.amazonaws.com");
  properties.setProperty(PROPERTY_S3_ENDPOINT + "." + Region.AP_SOUTHEAST_1,"https://s3-ap-southeast-1.amazonaws.com");
  return properties;
}
