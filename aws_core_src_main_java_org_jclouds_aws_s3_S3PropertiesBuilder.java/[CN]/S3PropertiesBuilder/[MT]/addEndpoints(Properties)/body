{
  properties.setProperty(PROPERTY_S3_REGIONS,Joiner.on(',').join(Region.US_STANDARD,Region.US_EAST_1,Region.US_WEST_1,"EU",Region.AP_SOUTHEAST_1));
  properties.setProperty(PROPERTY_S3_DEFAULT_REGIONS,Joiner.on(',').join(Region.US_STANDARD,Region.US_EAST_1));
  properties.setProperty(PROPERTY_S3_ENDPOINT,"https://s3.amazonaws.com");
  properties.setProperty(PROPERTY_S3_ENDPOINT + "." + Region.US_STANDARD,"https://s3.amazonaws.com");
  properties.setProperty(PROPERTY_S3_ENDPOINT + "." + Region.US_EAST_1,"https://s3.amazonaws.com");
  properties.setProperty(PROPERTY_S3_ENDPOINT + "." + Region.US_WEST_1,"https://s3-us-west-1.amazonaws.com");
  properties.setProperty(PROPERTY_S3_ENDPOINT + "." + "EU","https://s3-eu-west-1.amazonaws.com");
  properties.setProperty(PROPERTY_S3_ENDPOINT + "." + Region.AP_SOUTHEAST_1,"https://s3-ap-southeast-1.amazonaws.com");
  return properties;
}
