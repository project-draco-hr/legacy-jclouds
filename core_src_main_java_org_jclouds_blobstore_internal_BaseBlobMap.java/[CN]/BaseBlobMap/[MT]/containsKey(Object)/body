{
  String realKey=prefixer.apply(key.toString());
  try {
    return connection.blobMetadata(containerName,realKey).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS) != null;
  }
 catch (  KeyNotFoundException e) {
    return false;
  }
catch (  Exception e) {
    Utils.<BlobRuntimeException>rethrowIfRuntimeOrSameType(e);
    throw new BlobRuntimeException(String.format("Error searching for %1$s:%2$s",containerName,realKey),e);
  }
}
