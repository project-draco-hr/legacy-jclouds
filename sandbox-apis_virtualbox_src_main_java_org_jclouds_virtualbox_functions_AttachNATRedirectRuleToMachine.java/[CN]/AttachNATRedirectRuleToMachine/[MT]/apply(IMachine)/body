{
  INetworkAdapter networkAdapter=machine.getNetworkAdapter(adapterIndex);
  networkAdapter.setAttachmentType(NAT);
  networkAdapter.getNatDriver().addRedirect("guestssh",redirectRule.getProtocol(),redirectRule.getHost(),redirectRule.getHostPort(),redirectRule.getGuest(),redirectRule.getGuestPort());
  networkAdapter.setEnabled(true);
  machine.saveSettings();
  return null;
}
