{
  buffer.append(utils.valueOrEmpty(request.getPayload() == null ? null : request.getPayload().getContentMetadata().getContentMD5())).append("\n");
  buffer.append(utils.valueOrEmpty(request.getPayload() == null ? request.getFirstHeaderOrNull(HttpHeaders.CONTENT_TYPE) : request.getPayload().getContentMetadata().getContentType())).append("\n");
  for (  String header : firstHeadersToSign)   buffer.append(valueOrEmpty(request.getHeaders().get(header))).append("\n");
}
