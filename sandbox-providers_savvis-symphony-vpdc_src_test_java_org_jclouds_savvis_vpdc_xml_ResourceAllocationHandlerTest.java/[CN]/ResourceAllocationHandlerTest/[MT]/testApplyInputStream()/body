{
  InputStream is=getClass().getResourceAsStream("/resourceallocation.xml");
  ResourceAllocation result=factory.create(injector.getInstance(ResourceAllocationHandler.class)).parse(is);
  ResourceAllocation expects=ResourceAllocation.builder().allocationUnits("Gigabytes").caption("1234568").description("Hard Disk").name("D:\\").hostResource("data").id(6).type(ResourceAllocation.Type.DATA_DISK).virtualQuantity(50).build();
  assertEquals(result,expects);
}
