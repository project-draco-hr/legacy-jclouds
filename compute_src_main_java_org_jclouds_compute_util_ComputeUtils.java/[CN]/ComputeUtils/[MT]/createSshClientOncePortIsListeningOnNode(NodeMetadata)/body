{
  IPSocket socket=new IPSocket(Iterables.get(node.getPublicAddresses(),0),22);
  socketTester.apply(socket);
  SshClient ssh=isKeyAuth(node) ? sshFactory.create(socket,node.getCredentials().identity,node.getCredentials().credential.getBytes()) : sshFactory.create(socket,node.getCredentials().identity,node.getCredentials().credential);
  return ssh;
}
