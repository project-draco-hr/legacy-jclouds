{
  Map<NodeMetadata,Future<Void>> responses=newLinkedHashMap();
  for (  NodeMetadata node : runningNodes) {
    responses.put(node,executor.submit(customizeNodeAndAddToGoodMapOrPutExceptionIntoBadMapFactory.create(options,new AtomicReference<NodeMetadata>(node),goodNodes,badNodes,customizationResponses)));
  }
  return responses;
}
