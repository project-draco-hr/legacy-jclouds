{
  Map<SshCallable<?>,Object> responses=newHashMap();
  if (size(parallel) > 0) {
    responses.putAll(runCallablesUsingSshClient(parallel,ssh));
  }
  if (last != null) {
    last.setConnection(ssh,logger);
    try {
      responses.put(last,last.call());
    }
 catch (    Exception e) {
      propagate(e);
    }
  }
  return responses;
}
