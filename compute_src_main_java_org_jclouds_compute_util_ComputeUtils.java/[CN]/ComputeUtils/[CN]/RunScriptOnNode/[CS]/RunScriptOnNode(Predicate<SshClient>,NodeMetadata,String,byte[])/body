{
  this.runScriptNotRunning=runScriptNotRunning;
  this.node=checkNotNull(node,"node");
  this.scriptName=checkNotNull(scriptName,"scriptName");
  this.script=new InitBuilder("runscript","/tmp","/tmp",ImmutableMap.<String,String>of(),Iterables.toArray(Splitter.on("\n").split(new String(checkNotNull(script,"script"))),String.class)).build(OsFamily.UNIX).getBytes();
}
