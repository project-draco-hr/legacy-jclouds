{
  String bucketName=getContainerName();
  try {
    Map<String,V> map=createMap(context,bucketName);
    assertConsistencyAwareMapSize(map,0);
    putString(map,"one","apple");
    assertConsistencyAwareMapSize(map,1);
    map.clear();
    assertConsistencyAwareMapSize(map,0);
  }
  finally {
    returnContainer(bucketName);
  }
}
