{
  Set<ListenableFuture<Void>> responses=Sets.newHashSet();
  for (  StorageMetadata md : getAllBlobMetadata.execute(containerName,options)) {
    if (md.getType() == StorageType.BLOB)     responses.add(connection.removeBlob(containerName,md.getName()));
  }
  awaitCompletion(responses,userExecutor,maxTime,logger,String.format("deleting from containerName: %s",containerName));
}
