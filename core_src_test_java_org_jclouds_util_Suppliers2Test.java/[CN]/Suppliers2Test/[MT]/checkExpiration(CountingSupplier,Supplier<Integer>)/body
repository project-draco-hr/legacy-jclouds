{
  assertEquals(0,countingSupplier.calls);
  assertEquals(10,(int)memoizedSupplier.get());
  assertEquals(1,countingSupplier.calls);
  assertEquals(10,(int)memoizedSupplier.get());
  assertEquals(1,countingSupplier.calls);
  Thread.sleep(150);
  assertEquals(20,(int)memoizedSupplier.get());
  assertEquals(2,countingSupplier.calls);
  assertEquals(20,(int)memoizedSupplier.get());
  assertEquals(2,countingSupplier.calls);
}
