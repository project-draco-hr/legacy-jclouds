{
  checkState(getIfMatch() == null,"ifMd5Matches() is not compatible with ifModifiedSince()");
  checkState(getIfUnmodifiedSince() == null,"ifUnmodifiedSince() is not compatible with ifModifiedSince()");
  replaceHeader("x-amz-copy-source-if-modified-since",dateService.toHeaderString(checkNotNull(ifModifiedSince,"ifModifiedSince")));
  return this;
}
