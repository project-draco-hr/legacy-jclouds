{
  checkState(getIfNoneMatch() == null,"ifMd5DoesntMatch() is not compatible with ifUnmodifiedSince()");
  checkState(getIfModifiedSince() == null,"ifModifiedSince() is not compatible with ifUnmodifiedSince()");
  replaceHeader("x-amz-copy-source-if-unmodified-since",dateService.rfc822DateFormat(checkNotNull(ifUnmodifiedSince,"ifUnmodifiedSince")));
  return this;
}
