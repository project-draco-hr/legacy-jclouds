{
  Invokable<?,?> method=method(SecurityGroupAsyncClient.class,"authorizeSecurityGroupIngressInRegion",String.class,String.class,UserIdGroupPair.class);
  GeneratedHttpRequest request=processor.createRequest(method,Lists.<Object>newArrayList(null,"group",new UserIdGroupPair("sourceUser","sourceGroup")));
  assertRequestLineEquals(request,"POST https://ec2.us-east-1.amazonaws.com/ HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"Host: ec2.us-east-1.amazonaws.com\n");
  assertPayloadEquals(request,"Action=AuthorizeSecurityGroupIngress&GroupName=group&SourceSecurityGroupOwnerId=sourceUser&SourceSecurityGroupName=sourceGroup","application/x-www-form-urlencoded",false);
  assertResponseParserClassEquals(method,request,ReleasePayloadAndReturn.class);
  assertSaxResponseParserClassEquals(method,null);
  assertFallbackClassEquals(method,null);
  checkFilters(request);
}
