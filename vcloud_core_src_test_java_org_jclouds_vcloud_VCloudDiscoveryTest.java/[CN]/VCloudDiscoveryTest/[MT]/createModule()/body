{
  return new AbstractModule(){
    @Override protected void configure(){
      bind(URI.class).annotatedWith(Org.class).toInstance(URI.create("http://org"));
      bind(SetVCloudTokenCookie.class).toInstance(new SetVCloudTokenCookie(new Provider<String>(){
        public String get(){
          return "token";
        }
      }
));
      bind(Logger.LoggerFactory.class).toInstance(new LoggerFactory(){
        public Logger getLogger(        String category){
          return Logger.NULL;
        }
      }
);
    }
    @SuppressWarnings("unused") @Provides @VCloudApi URI provide(){
      return URI.create("https://services.vcloudexpress.terremark.com/api/v0.8");
    }
  }
;
}
