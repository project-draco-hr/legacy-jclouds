{
  boolean found=false;
  for (  RASD item : items.getItems()) {
    String itemInstanceId=item.getInstanceID().getValue();
    if (itemInstanceId.equals(instanceId)) {
      assertEquals(item.getElementName().getValue(),elementName,String.format(OBJ_FIELD_EQ,VAPP,context + "/" + instanceId+ "/elementName",elementName,"" + item.getElementName().getValue()));
      found=true;
      break;
    }
  }
  assertTrue(found,"no " + context + " item found with id "+ instanceId+ "; only found "+ items);
}
