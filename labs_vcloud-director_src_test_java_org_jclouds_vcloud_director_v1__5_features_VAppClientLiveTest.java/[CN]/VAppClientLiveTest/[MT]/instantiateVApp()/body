{
  InstantiateVAppTemplateParams instantiate=InstantiateVAppTemplateParams.builder().name("test-vapp").notDeploy().notPowerOn().description("Test VApp").instantiationParams(instantiationParams()).source(ReferenceType.builder().href(vAppTemplateURI).build()).build();
  VApp vAppInstantiated=vdcClient.instantiateVApp(vdcURI,instantiate);
  assertNotNull(vAppInstantiated,String.format(ENTITY_NON_NULL,VAPP));
  return vAppInstantiated;
}
