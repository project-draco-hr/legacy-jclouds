{
  String schemafileName="master.xsd";
  HttpResponse response=context.getUtils().getHttpClient().invoke(HttpRequest.builder().method("GET").endpoint(URI.create(endpoint + "/v1.5/schema/" + schemafileName)).headers(ImmutableMultimap.of("x-vcloud-authorization",sessionWithToken.getToken(),"Accept","*/*")).build());
  String schema=Strings2.toStringAndClose(response.getPayload().getInput());
}
