{
  return fromResourceType(in).isDefaultSmtpServer(in.isDefaultSmtpServer()).isDefaultOrgEmail(in.isDefaultOrgEmail()).fromEmailAddress(in.getFromEmailAddress()).defaultSubjectPrefix(in.getDefaultSubjectPrefix()).isAlertEmailToAllAdmins(in.isAlertEmailToAllAdmins()).alertEmailTo(in.getAlertEmailTo()).smtpServerSettings(in.getSmtpServerSettings());
}
