{
  if (serverId > 0) {
    connection.deleteServer(serverId);
    Server server=connection.getServer(serverId);
    assertEquals(server,Server.NOT_FOUND);
  }
  if (serverId2 > 0) {
    connection.deleteServer(serverId2);
    Server server=connection.getServer(serverId2);
    assertEquals(server,Server.NOT_FOUND);
  }
}
