{
  String account=System.getProperty("jclouds.test.user");
  String key=System.getProperty("jclouds.test.key");
  connection=new CloudServersContextBuilder(account,key).withModules(new Log4JLoggingModule()).buildContext().getApi();
  Injector injector=Guice.createInjector(new Log4JLoggingModule(),new JschSshConnectionModule(),new ExecutorServiceModule(new WithinThreadExecutorService()));
  sshFactory=injector.getInstance(SshConnection.Factory.class);
}
