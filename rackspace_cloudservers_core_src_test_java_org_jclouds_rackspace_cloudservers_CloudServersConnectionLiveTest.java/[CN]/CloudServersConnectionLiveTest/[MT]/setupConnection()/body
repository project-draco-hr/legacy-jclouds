{
  Injector injector=CloudServersContextBuilder.newBuilder(sysRackspaceUser,sysRackspaceKey).withModules(new Log4JLoggingModule(),new JschSshConnectionModule()).withJsonDebug().buildInjector();
  connection=injector.getInstance(CloudServersConnection.class);
  sshFactory=injector.getInstance(SshConnection.Factory.class);
}
