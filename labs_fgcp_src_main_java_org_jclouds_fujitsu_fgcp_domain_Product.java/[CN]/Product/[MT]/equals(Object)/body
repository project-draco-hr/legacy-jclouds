{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Product that=Product.class.cast(obj);
  return Objects.equal(this.name,that.name) && Objects.equal(this.unitName,that.unitName) && Objects.equal(this.usedPoints,that.usedPoints);
}
