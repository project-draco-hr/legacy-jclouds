{
  String identity=checkNotNull(System.getProperty("jclouds.test.identity"),"jclouds.test.identity");
  String credential=checkNotNull(System.getProperty("jclouds.test.credential"),"jclouds.test.credential");
  String endpoint=System.getProperty("jclouds.test.endpoint");
  Properties props=new Properties();
  if (endpoint != null && !"".equals(endpoint))   props.setProperty("terremark.endpoint",endpoint);
  context=new RestContextFactory().createContext("terremark",identity,credential,ImmutableSet.<Module>of(new Log4JLoggingModule(),new JschSshClientModule()),props);
  tmClient=context.getApi();
}
