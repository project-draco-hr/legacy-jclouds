{
  InternetService is=tmClient.addInternetServiceToVDC(tmClient.getDefaultVDC().getId(),"test-" + 22,Protocol.TCP,22);
  is=tmClient.configureInternetService(is.getId(),new InternetServiceConfiguration().changeNameTo("test-33"));
  assertEquals(is.getName(),"test-33");
  services.add(is);
  PublicIpAddress ip=is.getPublicIpAddress();
  for (  int port : new int[]{80,8080}) {
    services.add(tmClient.addInternetServiceToExistingIp(ip.getId(),"test-" + port,Protocol.HTTP,port));
  }
}
