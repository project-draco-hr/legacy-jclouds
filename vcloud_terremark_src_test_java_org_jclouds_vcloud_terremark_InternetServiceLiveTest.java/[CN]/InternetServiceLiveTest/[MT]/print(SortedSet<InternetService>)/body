{
  for (  InternetService service : set) {
    System.out.printf("%d (%s:%d%n)",service.getId(),service.getPublicIpAddress().getAddress().getHostAddress(),service.getPort());
    for (    Node node : tmClient.getNodes(service.getId())) {
      System.out.printf("   %d (%s:%d%n)",node.getId(),node.getIpAddress().getHostAddress(),node.getPort());
    }
  }
}
