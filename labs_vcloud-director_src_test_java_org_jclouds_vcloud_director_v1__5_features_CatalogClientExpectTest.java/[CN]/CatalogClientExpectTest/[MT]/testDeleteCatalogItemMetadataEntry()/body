{
  HttpRequest catalogItemRequest=HttpRequest.builder().method("DELETE").endpoint(URI.create(endpoint + "/catalogItem/a36fdac9-b8c2-43e2-9a4c-2ffaf3ee13df/metadata/KEY")).headers(ImmutableMultimap.<String,String>builder().put("Accept","application/vnd.vmware.vcloud.task+xml").put("x-vcloud-authorization",token).build()).build();
  HttpResponse catalogItemResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/catalog/deleteMetadataEntryTask.xml",VCloudDirectorMediaType.TASK)).build();
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,catalogItemRequest,catalogItemResponse);
  URI catalogItemURI=URI.create("https://vcloudbeta.bluelock.com/api/catalogItem/a36fdac9-b8c2-43e2-9a4c-2ffaf3ee13df");
  Task expected=deleteMetadataEntryTask();
  assertEquals(client.getCatalogClient().getMetadataClient().deleteMetadataEntry(catalogItemURI,"KEY"),expected);
}
