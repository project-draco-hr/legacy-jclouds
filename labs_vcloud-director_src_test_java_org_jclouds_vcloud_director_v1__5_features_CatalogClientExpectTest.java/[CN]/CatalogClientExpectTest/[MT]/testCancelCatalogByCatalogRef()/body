{
  HttpRequest catalogRequest=HttpRequest.builder().method("POST").endpoint(URI.create(endpoint + "/catalog/5fcd2af3-d0ec-45ce-9451-8c585a2c766b/action/cancel")).headers(ImmutableMultimap.<String,String>builder().put("Accept","*/*").put("x-vcloud-authorization",token).build()).build();
  HttpResponse catalogResponse=HttpResponse.builder().statusCode(200).build();
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,catalogRequest,catalogResponse);
  Reference catalogRef=Reference.builder().type("application/vnd.vmware.vcloud.catalog+xml").name("catalog").href(URI.create(endpoint + "/catalog/5fcd2af3-d0ec-45ce-9451-8c585a2c766b")).build();
  client.getCatalogClient().cancelCatalog(catalogRef);
}
