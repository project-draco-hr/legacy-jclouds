{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,getStandardRequest("GET","/catalogItem/a36fdac9-b8c2-43e2-9a4c-2ffaf3ee13df/metadata/KEY"),getStandardPayloadResponse("/catalog/catalogItemMetadataEntry.xml",VCloudDirectorMediaType.METADATA_ENTRY));
  Reference catalogItemReference=Reference.builder().type("application/vnd.vmware.vcloud.catalogItem+xml").name("ubuntu10").href(URI.create("https://vcloudbeta.bluelock.com/api/catalogItem/a36fdac9-b8c2-43e2-9a4c-2ffaf3ee13df")).build();
  MetadataEntry expected=itemMetadataEntry();
  assertEquals(client.getCatalogClient().getCatalogItemMetadataEntry(catalogItemReference,"KEY"),expected);
}
