{
  Iterable<? extends LoadBalancer> loadBalancers;
  if (regions != null)   loadBalancers=concat(transformParallel(regions,new Function<String,Future<Set<? extends LoadBalancer>>>(){
    @Override public ListenableFuture<Set<? extends LoadBalancer>> apply(    String from){
      return aclient.describeLoadBalancersInRegion(from);
    }
  }
,executor,null,logger,"loadbalancers"));
 else   loadBalancers=client.describeLoadBalancersInRegion(null);
  return transform(loadBalancers,converter);
}
