{
  Location location=locationSupplier.get();
  boolean returnVal=true;
  if (location != null && input.getLocation() != null)   returnVal=location.equals(input.getLocation()) || location.getParent() != null && location.getParent().equals(input.getLocation()) || location.getParent().getParent() != null && location.getParent().getParent().equals(input.getLocation());
  return returnVal;
}
