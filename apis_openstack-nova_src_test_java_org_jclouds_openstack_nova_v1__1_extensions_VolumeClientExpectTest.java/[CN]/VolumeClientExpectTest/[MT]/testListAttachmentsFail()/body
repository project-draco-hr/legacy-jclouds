{
  URI endpoint=URI.create("https://compute.north.host/v1.1/3456/servers/instance-2/os-volume_attachments");
  VolumeClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,extensionsOfNovaRequest,extensionsOfNovaResponse,standardRequestBuilder(endpoint).build(),standardResponseBuilder(401).build()).getVolumeExtensionForZone("az-1.region-a.geo-1").get();
  client.listAttachmentsOnServer("instance-2");
}
