{
  if (this == obj)   return true;
  if (obj == null || getClass() != obj.getClass())   return false;
  DirectionalRecord that=DirectionalRecord.class.cast(obj);
  return equal(this.type,that.type) && equal(this.ttl,that.ttl) && equal(this.noResponseRecord,that.noResponseRecord)&& equal(this.infoValues,that.infoValues);
}
