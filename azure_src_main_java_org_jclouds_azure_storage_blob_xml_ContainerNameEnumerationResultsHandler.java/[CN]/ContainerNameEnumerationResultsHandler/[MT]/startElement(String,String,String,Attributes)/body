{
  if (qName.equals("Blob")) {
    inBlob=true;
    inBlobPrefix=false;
  }
 else   if (qName.equals("BlobPrefix")) {
    inBlob=false;
    inBlobPrefix=true;
  }
 else   if (qName.equals("Metadata")) {
    inBlob=true;
    inBlobMetadata=true;
  }
 else   if (qName.equals("EnumerationResults")) {
    containerUrl=URI.create(attributes.getValue("ContainerName").toString().trim());
  }
}
