{
  return Guice.createInjector(new RimuHostingRestClientModule(),new RimuHostingContextModule(),new RestModule(),new ExecutorServiceModule(sameThreadExecutor(),sameThreadExecutor()),new AbstractModule(){
    @Override protected void configure(){
      Names.bindProperties(binder(),checkNotNull(new RimuHostingPropertiesBuilder("apikey").build(),"properties"));
      bind(TransformingHttpCommandExecutorService.class).toInstance(createMock(TransformingHttpCommandExecutorService.class));
    }
  }
);
}
