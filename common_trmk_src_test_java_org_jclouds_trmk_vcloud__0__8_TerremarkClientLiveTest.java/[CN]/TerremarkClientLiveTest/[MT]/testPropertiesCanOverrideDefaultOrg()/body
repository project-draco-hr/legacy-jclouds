{
  for (  Org org : orgs) {
    RestContext<S,A> newContext=null;
    try {
      newContext=createContext(overrideDefaults(ImmutableMap.of(VCloudConstants.PROPERTY_VCLOUD_DEFAULT_ORG,org.getName())),setupModules()).getProviderSpecificContext();
      assertEquals(newContext.getApi().findOrgNamed(null),org);
    }
  finally {
      newContext.close();
    }
  }
}
