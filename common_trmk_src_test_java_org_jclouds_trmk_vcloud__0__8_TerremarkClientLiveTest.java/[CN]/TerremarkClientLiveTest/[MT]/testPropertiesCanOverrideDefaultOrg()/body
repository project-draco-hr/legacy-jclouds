{
  for (  Org org : orgs) {
    RestContext<S,A> newContext=null;
    try {
      newContext=createView(overrideDefaults(ImmutableMap.of(VCloudConstants.PROPERTY_VCLOUD_DEFAULT_ORG,org.getName())),setupModules()).unwrap();
      assertEquals(newContext.getApi().findOrgNamed(null),org);
    }
  finally {
      newContext.close();
    }
  }
}
