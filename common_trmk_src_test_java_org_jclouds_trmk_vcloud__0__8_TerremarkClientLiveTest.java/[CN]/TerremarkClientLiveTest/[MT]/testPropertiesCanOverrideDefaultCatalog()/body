{
  for (  Org org : orgs) {
    for (    ReferenceType cat : org.getCatalogs().values()) {
      RestContext<S,A> newContext=null;
      try {
        newContext=createContext(overrideDefaults(ImmutableMap.of(VCloudConstants.PROPERTY_VCLOUD_DEFAULT_ORG,org.getName(),VCloudConstants.PROPERTY_VCLOUD_DEFAULT_CATALOG,cat.getName())),setupModules()).getProviderSpecificContext();
        assertEquals(newContext.getApi().findCatalogInOrgNamed(null,null),connection.getCatalog(cat.getHref()));
      }
  finally {
        newContext.close();
      }
    }
  }
}
