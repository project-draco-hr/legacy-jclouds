{
  for (  Org org : orgs) {
    for (    ReferenceType tasksList : org.getTasksLists().values()) {
      RestContext<S,A> newContext=null;
      try {
        newContext=createView(overrideDefaults(ImmutableMap.of(VCloudConstants.PROPERTY_VCLOUD_DEFAULT_ORG,org.getName(),VCloudConstants.PROPERTY_VCLOUD_DEFAULT_TASKSLIST,tasksList.getName())),setupModules()).unwrap();
        assertEquals(newContext.getApi().findTasksListInOrgNamed(null,null),connection.getTasksList(tasksList.getHref()));
      }
  finally {
        newContext.close();
      }
    }
  }
}
