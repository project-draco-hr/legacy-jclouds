{
  VCloudDirectorAdminClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/admin/org/6f312e42-cd2b-488d-a2bb-97519cd57ed0/settings/passwordPolicy").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/org/admin/passwordPolicy.xml",VCloudDirectorMediaType.ORG_PASSWORD_POLICY_SETTINGS).httpResponseBuilder().build());
  OrgPasswordPolicySettings expected=passwordPolicy();
  assertEquals(client.getOrgClient().getPasswordPolicy(orgRef.getHref()),expected);
}
