{
  allConnections.release();
  FutureCommand<URI,?,?,?> frequest=(FutureCommand<URI,?,?,?>)request.getAttachment();
  if (frequest != null) {
    logger.error("%1$s->%2$s[%3$s] - Cancelling FutureCommand",request.getLocalAddress(),request.getRemoteAddress(),frequest);
    frequest.cancel(true);
  }
}
