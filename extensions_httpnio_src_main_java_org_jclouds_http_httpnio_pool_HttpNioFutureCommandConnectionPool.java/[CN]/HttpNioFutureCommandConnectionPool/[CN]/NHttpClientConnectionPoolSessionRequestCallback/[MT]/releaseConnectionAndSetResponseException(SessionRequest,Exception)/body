{
  allConnections.release();
  HttpFutureCommand<?> frequest=(HttpFutureCommand<?>)request.getAttachment();
  if (frequest != null) {
    logger.error(e,"%1s->%2s[%3s] - Setting Exception on FutureCommand",request.getLocalAddress(),request.getRemoteAddress(),frequest);
    frequest.setException(e);
  }
}
