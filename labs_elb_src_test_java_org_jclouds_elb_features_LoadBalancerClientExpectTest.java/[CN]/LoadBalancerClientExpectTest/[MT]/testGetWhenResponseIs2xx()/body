{
  HttpResponse getResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/describe_loadbalancers.xml","text/xml")).build();
  ELBClient clientWhenExist=requestSendsResponse(get,getResponse);
  assertEquals(clientWhenExist.getLoadBalancerClient().get("name").toString(),new GetLoadBalancerResponseTest().expected().toString());
