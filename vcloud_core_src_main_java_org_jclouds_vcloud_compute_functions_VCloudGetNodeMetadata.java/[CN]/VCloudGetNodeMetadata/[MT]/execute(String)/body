{
  URI id=URI.create(in);
  VApp from=client.getVApp(id);
  if (from == null)   return null;
  String tag=parseTagFromName(from.getName());
  Location location=findLocationForResourceInVDC.apply(from.getVDC());
  return new NodeMetadataImpl(in,from.getName(),in,location,from.getId(),ImmutableMap.<String,String>of(),tag,null,vAppStatusToNodeState.get(from.getStatus()),computeClient.getPublicAddresses(id),computeClient.getPrivateAddresses(id),getExtra.apply(from),null);
}
