{
  assertEquals(createMock("details","POST",200,"/archive_details.json",userName).getArchiveDetails("xxxxxx_test1"),ArchiveDetails.builder().username("xxxxxx_test1").freeSize("30 GB").totalSize("30 GB").locked(false).build());
  assertNull(createMock("details","POST",404,"/archive_details.json",userName).getArchiveDetails("xxxxxx_test1"));
}
