{
  VirtualMachineState[] states={VirtualMachineState.ALLOCATED,VirtualMachineState.CONFIGURED,VirtualMachineState.LOCKED,VirtualMachineState.OFF,VirtualMachineState.PAUSED};
  checkStatesReturn(new MockVirtualMachine(),new VirtualMachineDeployMonitor(),states,MonitorStatus.CONTINUE);
  checkStatesReturn(new MockVirtualMachineFailing(),new VirtualMachineDeployMonitor(),states,MonitorStatus.CONTINUE);
}
