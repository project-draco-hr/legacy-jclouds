{
  return Guice.createInjector(new ExecutorServiceModule(new WithinThreadExecutorService()),new StubCloudFilesClientModule(),new StubRackspaceAuthenticationModule(),new CloudFilesContextModule(){
    @Override protected void configure(){
      bindConstant().annotatedWith(Jsr330.named(CloudFilesConstants.PROPERTY_RACKSPACE_USER)).to("user");
      bindConstant().annotatedWith(Jsr330.named(CloudFilesConstants.PROPERTY_RACKSPACE_KEY)).to("key");
      super.configure();
    }
  }
);
}
