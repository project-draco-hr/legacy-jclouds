{
  return blobUtils.directoryExists(containerName,directory) ? Futures.immediateFuture((Void)null) : org.jclouds.concurrent.Futures.makeListenable(service.submit(new Callable<Void>(){
    public Void call() throws Exception {
      blobUtils.createDirectory(containerName,directory);
      return null;
    }
  }
),service);
}
