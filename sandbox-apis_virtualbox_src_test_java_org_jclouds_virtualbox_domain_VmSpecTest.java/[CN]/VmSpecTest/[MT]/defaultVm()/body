{
  return VmSpec.builder().id("MyVmId").name("My VM").osTypeId("Ubuntu").memoryMB(1024).natNetworkAdapter(0,NatAdapter.builder().tcpRedirectRule("localhost",2222,"",22).build()).forceOverwrite(true).controller(StorageController.builder().name("Controller").bus(StorageBus.IDE).attachHardDisk(0,0,"/tmp/tempdisk.vdi","tempdisk").build());
}
