{
  return VmSpec.builder().id("MyVmId").name("My VM").osTypeId("Ubuntu").memoryMB(1024).cleanUpMode(CleanupMode.Full).natNetworkAdapter(0,NatAdapter.builder().tcpRedirectRule("localhost",2222,"",22).build()).forceOverwrite(true).controller(StorageController.builder().name("Controller").bus(StorageBus.IDE).attachHardDisk(HardDisk.builder().diskpath("/tmp/tempdisk.vdi").controllerPort(0).deviceSlot(0).build()).build());
}
