{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Device other=(Device)obj;
  if (driveUuid == null) {
    if (other.driveUuid != null)     return false;
  }
 else   if (!driveUuid.equals(other.driveUuid))   return false;
  if (mediaType != other.mediaType)   return false;
  if (readBytes != other.readBytes)   return false;
  if (readRequests != other.readRequests)   return false;
  if (writeBytes != other.writeBytes)   return false;
  if (writeRequests != other.writeRequests)   return false;
  return true;
}
