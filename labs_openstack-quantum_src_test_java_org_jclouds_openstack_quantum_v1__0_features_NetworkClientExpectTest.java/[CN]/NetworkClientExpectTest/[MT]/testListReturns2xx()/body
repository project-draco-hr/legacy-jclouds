{
  NetworkClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,standardRequestBuilder(endpoint + "/tenants/3456/networks/detail").build(),standardResponseBuilder(200).payload(payloadFromResourceWithContentType("/list_networks.json",APPLICATION_JSON)).build()).getNetworkClientForRegion("region-a.geo-1");
  Set<Network> nets=client.list();
  assertEquals(nets,listOfNetworks());
}
