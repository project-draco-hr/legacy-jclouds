{
  NetworkClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,standardRequestBuilder(endpoint + "/tenants/3456/networks").method("POST").payload(payloadFromStringWithContentType("{\"network\":{\"name\":\"another-test\"}}",MediaType.APPLICATION_JSON)).build(),standardResponseBuilder(200).payload(payloadFromStringWithContentType("{\"network\":{\"id\":\"12345\"}}",APPLICATION_JSON)).build()).getNetworkClientForRegion("region-a.geo-1");
  Reference net=client.create("another-test");
  assertEquals(net,Reference.builder().id("12345").build());
}
