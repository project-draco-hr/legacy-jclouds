{
  NetworkClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,standardRequestBuilder(endpoint + "/tenants/3456/networks/12345").method("PUT").payload(payloadFromStringWithContentType("{\"network\":{\"name\":\"another-test\"}}",MediaType.APPLICATION_JSON)).build(),standardResponseBuilder(200).build()).getNetworkClientForRegion("region-a.geo-1");
  assertTrue(client.update("12345","another-test"));
}
