{
  NetworkClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,standardRequestBuilder(endpoint + "/tenants/3456/networks/16dba3bc-f3fa-4775-afdc-237e12c72f6a/detail").build(),standardResponseBuilder(200).payload(payloadFromResourceWithContentType("/network_details.json",APPLICATION_JSON)).build()).getNetworkClientForRegion("region-a.geo-1");
  NetworkDetails net=client.showDetails("16dba3bc-f3fa-4775-afdc-237e12c72f6a");
  assertEquals(net,new ParseNetworkDetailsTest().expected());
}
