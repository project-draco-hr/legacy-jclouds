{
  vdc=vdcClient.getVdc(vdcURI);
  Iterable<Reference> vApps=Iterables.filter(vdc.getResourceEntities(),Predicates.and(ReferencePredicates.<Reference>typeEquals(VCloudDirectorMediaType.VAPP),ReferencePredicates.<Reference>nameIn(vAppNames)));
  if (!Iterables.isEmpty(vApps)) {
    for (    Reference ref : vApps) {
      cleanUpVApp(ref.getHref());
    }
  }
 else {
    logger.warn("No VApps in list found in Vdc %s (%s)",vdc.getName(),Iterables.toString(vAppNames));
  }
}
