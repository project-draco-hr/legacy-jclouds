{
  return api.getImageApiForZone(zoneAndId.getZone()).listInDetail().concat().firstMatch(new Predicate<org.jclouds.openstack.nova.v2_0.domain.Image>(){
    @Override public boolean apply(    org.jclouds.openstack.nova.v2_0.domain.Image input){
      return input.getId().equals(zoneAndId.getId());
    }
  }
).orNull();
}
