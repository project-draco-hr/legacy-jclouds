{
  Map<String,String> attributes=SaxUtils.cleanseAttributes(attrs);
  if (qName.equals("state")) {
    state=instanceStateWarningOnUnrecognized(attributes.get("name"));
  }
 else   if (qName.equals("transition")) {
    if (attributes.containsKey("auto"))     states.put(state,new TransitionAutomatically(instanceStateWarningOnUnrecognized(attributes.get("to"))));
 else     states.put(state,new TransitionOnAction(instanceActionWarningOnUnrecognized(attributes.get("action")),instanceStateWarningOnUnrecognized(attributes.get("to"))));
  }
}
