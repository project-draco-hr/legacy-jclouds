{
  if (this == object) {
    return true;
  }
  if (object instanceof Computer) {
    final Computer other=Computer.class.cast(object);
    return equal(getDisplayName(),other.getDisplayName()) && equal(isIdle(),other.isIdle()) && equal(isOffline(),other.isOffline());
  }
 else {
    return false;
  }
}
