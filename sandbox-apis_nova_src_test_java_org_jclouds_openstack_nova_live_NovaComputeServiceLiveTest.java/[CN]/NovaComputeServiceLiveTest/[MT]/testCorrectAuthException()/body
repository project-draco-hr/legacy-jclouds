{
  ComputeServiceContext context=null;
  try {
    context=new ComputeServiceContextFactory(setupRestProperties()).createContext(provider,"MOMMA","MIA",ImmutableSet.<Module>of(new SLF4JLoggingModule()),overrides);
    context.getComputeService().listNodes();
  }
  finally {
    if (context != null)     context.close();
  }
}
