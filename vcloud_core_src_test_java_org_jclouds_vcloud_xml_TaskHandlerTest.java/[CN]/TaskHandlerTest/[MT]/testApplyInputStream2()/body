{
  InputStream is=getClass().getResourceAsStream("/task-hosting.xml");
  Task result=factory.create(injector.getInstance(TaskHandler.class)).parse(is);
  Task expects=new TaskImpl(VCloudMediaType.TASK_XML,URI.create("https://vcloud.safesecureweb.com/api/v0.8/task/d188849-34"),TaskStatus.QUEUED,null,null,null,null);
  assertEquals(result,expects);
}
