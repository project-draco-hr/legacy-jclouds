{
  InputStream is=getClass().getResourceAsStream("/express/task-self.xml");
  Task result=factory.create(injector.getInstance(TaskHandler.class)).parse(is);
  Task expects=new TaskImpl(URI.create("https://vcloud.safesecureweb.com/api/v0.8/task/d188849-78"),TaskStatus.QUEUED,null,null,null,null,null);
  assertEquals(result,expects);
}
