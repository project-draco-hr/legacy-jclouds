{
  ControlAccessParams params=ControlAccessParams.builder().notSharedToEveryone().accessSetting(AccessSetting.builder().subject(Reference.builder().href(user.getHref()).type(ADMIN_USER).build()).accessLevel("ReadOnly").build()).build();
  ControlAccessParams modified=vAppApi.editControlAccess(vApp.getHref(),params);
  checkControlAccessParams(modified);
  assertEquals(modified,params,String.format(ENTITY_EQUAL,"ControlAccessParams"));
}
