{
  key=name("key-");
  String value=name("value-");
  context.getApi().getMetadataApi(vAppUrn).put(key,value);
  Metadata metadata=context.getApi().getMetadataApi(vAppUrn).get();
  checkMetadata(metadata);
  assertFalse(Iterables.isEmpty(metadata.getMetadataEntries()),String.format(NOT_EMPTY_OBJECT_FMT,"MetadataEntry","vApp"));
}
