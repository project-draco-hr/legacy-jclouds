{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  if (!super.equals(o))   return false;
  VirtualMachine that=(VirtualMachine)o;
  if (customizationPending != that.customizationPending)   return false;
  if (poweredOn != that.poweredOn)   return false;
  if (description != null ? !description.equals(that.description) : that.description != null)   return false;
  if (hardwareConfiguation != null ? !hardwareConfiguation.equals(that.hardwareConfiguation) : that.hardwareConfiguation != null)   return false;
  if (!ipAddresses.equals(that.ipAddresses))   return false;
  if (layout != null ? !layout.equals(that.layout) : that.layout != null)   return false;
  if (mediaStatus != that.mediaStatus)   return false;
  if (operatingSystem != null ? !operatingSystem.equals(that.operatingSystem) : that.operatingSystem != null)   return false;
  if (status != that.status)   return false;
  if (!tasks.equals(that.tasks))   return false;
  if (toolsStatus != that.toolsStatus)   return false;
  return true;
}
