{
  String realKey=prefixer.apply(o.toString());
  try {
    return (InputStream)(connection.getBlob(containerName,realKey).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS)).getContent();
  }
 catch (  KeyNotFoundException e) {
    return null;
  }
catch (  Exception e) {
    Throwable cause=Throwables.getRootCause(e);
    if (cause instanceof KeyNotFoundException)     return null;
    Throwables.propagateIfInstanceOf(e,BlobRuntimeException.class);
    throw new BlobRuntimeException(String.format("Error geting object %1$s:%2$s",containerName,realKey),e);
  }
}
