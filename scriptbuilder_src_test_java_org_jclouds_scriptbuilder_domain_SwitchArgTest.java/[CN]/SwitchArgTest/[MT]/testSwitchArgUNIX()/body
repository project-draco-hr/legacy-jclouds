{
  assertEquals(new SwitchArg(1,ImmutableMap.of("0",newStatementList(appendFile("{tmp}{fs}{uid}{fs}scripttest{fs}temp.txt",Collections.singleton("hello world")),interpret("echo hello zero{lf}")),"1",interpret("echo hello one{lf}"))).render(OsFamily.UNIX),"case $1 in\n0)\n   cat >> /tmp/$USER/scripttest/temp.txt <<'END_OF_FILE'\nhello world\nEND_OF_FILE\n   echo hello zero\n   ;;\n1)\n   echo hello one\n   ;;\nesac\n");
}
