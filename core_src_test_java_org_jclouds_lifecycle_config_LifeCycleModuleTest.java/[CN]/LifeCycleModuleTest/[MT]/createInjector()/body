{
  Injector i=Guice.createInjector(new LifeCycleModule(){
    @SuppressWarnings("unused") @Provides @Named(Constants.PROPERTY_USER_THREADS) int p(){
      return 1;
    }
    @SuppressWarnings("unused") @Provides @Named(Constants.PROPERTY_MAX_CONNECTIONS_PER_CONTEXT) int p2(){
      return 1;
    }
