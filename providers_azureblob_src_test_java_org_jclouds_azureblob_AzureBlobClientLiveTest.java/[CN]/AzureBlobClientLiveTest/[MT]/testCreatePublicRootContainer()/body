{
  try {
    getApi().deleteRootContainer();
  }
 catch (  ContainerNotFoundException e) {
    Thread.sleep(5000);
  }
catch (  AzureStorageResponseException htpe) {
    if (htpe.getResponse().getStatusCode() == 409) {
      Thread.sleep(5000);
    }
 else {
      throw htpe;
    }
  }
  boolean created=false;
  while (!created) {
    try {
      created=getApi().createRootContainer();
    }
 catch (    AzureStorageResponseException htpe) {
      if (htpe.getResponse().getStatusCode() == 409) {
        Thread.sleep(5000);
        continue;
      }
 else {
        throw htpe;
      }
    }
  }
  ListBlobsResponse list=getApi().listBlobs();
  assertEquals(list.getUrl(),URI.create(String.format("https://%s.blob.core.windows.net/$root",wrapper.unwrap(AzureBlobApiMetadata.CONTEXT_TOKEN).getIdentity())));
}
