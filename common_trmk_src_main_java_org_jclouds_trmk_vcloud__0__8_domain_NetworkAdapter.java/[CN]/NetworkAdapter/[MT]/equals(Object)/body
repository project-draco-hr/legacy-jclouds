{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  NetworkAdapter other=(NetworkAdapter)obj;
  if (macAddress == null) {
    if (other.macAddress != null)     return false;
  }
 else   if (!macAddress.equals(other.macAddress))   return false;
  if (name == null) {
    if (other.name != null)     return false;
  }
 else   if (!name.equals(other.name))   return false;
  if (subnet == null) {
    if (other.subnet != null)     return false;
  }
 else   if (!subnet.equals(other.subnet))   return false;
  return true;
}
