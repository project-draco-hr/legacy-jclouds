{
  String endpoint=checkNotNull(System.getProperty("test." + provider + ".endpoint"),"test." + provider + ".endpoint");
  String identity=checkNotNull(System.getProperty("test." + provider + ".identity"),"test." + provider + ".identity");
  String credential=checkNotNull(System.getProperty("test." + provider + ".credential"),"test." + provider + ".credential");
  ContextSpec<VCloudVersionsClient,VCloudVersionsAsyncClient> contextSpec=contextSpec("test",endpoint,"1",identity,credential,VCloudVersionsClient.class,VCloudVersionsAsyncClient.class);
  context=createContextBuilder(contextSpec,ImmutableSet.<Module>of(new Log4JLoggingModule(),new ExecutorServiceModule(MoreExecutors.sameThreadExecutor(),MoreExecutors.sameThreadExecutor()))).buildContext();
}
