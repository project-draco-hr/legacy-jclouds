{
  deviceBuilder.uuid(from.get(key));
  if (from.containsKey(key + ":read:bytes"))   deviceBuilder.readBytes(new Long(from.get(key + ":read:bytes")));
  if (from.containsKey(key + ":read:requests"))   deviceBuilder.readRequests(new Long(from.get(key + ":read:requests")));
  if (from.containsKey(key + ":write:bytes"))   deviceBuilder.writeBytes(new Long(from.get(key + ":write:bytes")));
  if (from.containsKey(key + ":write:requests"))   deviceBuilder.writeRequests(new Long(from.get(key + ":write:requests")));
  if (from.containsKey(key + ":media"))   deviceBuilder.mediaType(MediaType.fromValue(from.get(key + ":media")));
  return deviceBuilder;
}
