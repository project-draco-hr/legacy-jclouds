{
  try {
    CopyObjectOptions options=Util.convertCopyObjectOptions(acl,destinationMetadata,ifModifiedSince,ifUnmodifiedSince,ifMatchTags,ifNoneMatchTags);
    ObjectMetadata jcObjectMetadata=connection.copyObject(sourceBucketName,sourceObjectKey,destinationBucketName,destinationObjectKey,options);
    Map map=new HashMap();
    map.put("Last-Modified",jcObjectMetadata.getLastModified());
    map.put("ETag",jcObjectMetadata.getETag());
    return map;
  }
 catch (  Exception e) {
    Throwables.propagateIfPossible(e,S3ServiceException.class);
    throw new S3ServiceException("error copying object",e);
  }
}
