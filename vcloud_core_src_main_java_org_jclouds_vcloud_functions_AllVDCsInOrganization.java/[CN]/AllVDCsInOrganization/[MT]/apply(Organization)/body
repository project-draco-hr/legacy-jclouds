{
  Iterable<VDC> catalogItems=transformParallel(from.getVDCs().values(),new Function<NamedResource,Future<VDC>>(){
    @SuppressWarnings("unchecked") @Override public Future<VDC> apply(    NamedResource from){
      return (Future<VDC>)aclient.getVDC(from.getId());
    }
  }
,executor,null,logger,"vdcs in " + from.getId());
  return catalogItems;
}
