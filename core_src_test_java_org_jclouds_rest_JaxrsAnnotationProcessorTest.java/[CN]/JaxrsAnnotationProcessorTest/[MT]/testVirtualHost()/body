{
  Method method=TestVirtualHost.class.getMethod("get",String.class,String.class);
  HttpRequest httpMethod=factory.create(TestVirtualHost.class).createRequest(method,new Object[]{"1","localhost"});
  assertEquals(httpMethod.getEndpoint().getHost(),"localhost");
  assertEquals(httpMethod.getEndpoint().getPath(),"/1");
  assertEquals(httpMethod.getMethod(),HttpMethod.GET);
  assertEquals(httpMethod.getHeaders().size(),1);
  assertEquals(httpMethod.getHeaders().get(HttpHeaders.HOST),Collections.singletonList("localhost"));
}
