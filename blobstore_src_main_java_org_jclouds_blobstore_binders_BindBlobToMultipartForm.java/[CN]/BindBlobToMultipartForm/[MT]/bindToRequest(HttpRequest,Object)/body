{
  Blob object=(Blob)payload;
  Part part=Part.create(object.getMetadata().getName(),object.getPayload(),new PartOptions().contentType(object.getMetadata().getContentType()));
  MultipartForm form=new MultipartForm(BOUNDARY,part);
  request.setPayload(form.getInput());
  request.getHeaders().put(HttpHeaders.CONTENT_TYPE,"multipart/form-data; boundary=" + BOUNDARY);
  request.getHeaders().put(HttpHeaders.CONTENT_LENGTH,form.calculateSize() + "");
}
