{
  checkArgument(checkNotNull(input,"input") instanceof Region,"this binder is only valid for Region!");
  Region constraint=(Region)input;
  String value=null;
switch (constraint) {
case US_STANDARD:
case US_EAST_1:
case DEFAULT:
    return;
case EU_WEST_1:
  value="EU";
break;
case US_WEST_1:
value="us-west-1";
break;
default :
throw new IllegalStateException("unimplemented location: " + this);
}
super.bindToRequest(request,String.format("<CreateBucketConfiguration><LocationConstraint>%s</LocationConstraint></CreateBucketConfiguration>",value));
}
