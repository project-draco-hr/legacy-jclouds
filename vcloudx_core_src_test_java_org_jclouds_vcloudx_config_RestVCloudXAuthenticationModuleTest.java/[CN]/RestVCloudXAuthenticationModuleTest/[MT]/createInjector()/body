{
  return Guice.createInjector(new RestVCloudXAuthenticationModule(),new ParserModule(),new AbstractModule(){
    @Override protected void configure(){
      bindConstant().annotatedWith(Jsr330.named(PROPERTY_VCLOUDX_USER)).to("user");
      bindConstant().annotatedWith(Jsr330.named(PROPERTY_VCLOUDX_KEY)).to("secret");
      bindConstant().annotatedWith(Jsr330.named(PROPERTY_VCLOUDX_ENDPOINT)).to("http://localhost");
      bindConstant().annotatedWith(Jsr330.named(PROPERTY_VCLOUDX_SESSIONINTERVAL)).to("2");
    }
  }
);
}
