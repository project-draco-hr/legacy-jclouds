{
  NodeMetadata metadata=node instanceof NodeMetadata ? NodeMetadata.class.cast(node) : computeService.getNodeMetadata(node.getId());
  log(String.format("   node id=%s, name=%s, group=%s, location=%s, state=%s, publicIp=%s, privateIp=%s, hardware=%s",metadata.getProviderId(),metadata.getName(),metadata.getGroup(),metadata.getLocation(),formatStatus(metadata),ComputeTaskUtils.ipOrEmptyString(metadata.getPublicAddresses()),ipOrEmptyString(metadata.getPrivateAddresses()),metadata.getHardware()));
}
