{
  return new InitBuilder(scriptName,"/tmp/" + scriptName,"/tmp/" + scriptName,ImmutableMap.<String,String>of(),ImmutableList.<Statement>of(Statements.interpret(Iterables.toArray(Splitter.on("\n").split(new String(checkNotNull(script,"script"))),String.class)))).build(org.jclouds.scriptbuilder.domain.OsFamily.UNIX);
}
