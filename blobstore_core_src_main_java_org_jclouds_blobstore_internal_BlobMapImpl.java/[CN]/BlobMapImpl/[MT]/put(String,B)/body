{
  B returnVal=getLastValue(key);
  try {
    connection.putBlob(containerName,value).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS);
  }
 catch (  Exception e) {
    Utils.<BlobRuntimeException>rethrowIfRuntimeOrSameType(e);
    throw new BlobRuntimeException(String.format("Error putting object %1$s:%2$s%n%3$s",containerName,key,value),e);
  }
  return returnVal;
}
