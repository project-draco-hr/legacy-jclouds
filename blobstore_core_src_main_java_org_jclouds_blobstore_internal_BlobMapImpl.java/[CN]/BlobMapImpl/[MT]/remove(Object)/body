{
  B old=getLastValue(key);
  try {
    connection.removeBlob(containerName,key.toString()).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS);
  }
 catch (  Exception e) {
    Utils.<BlobRuntimeException>rethrowIfRuntimeOrSameType(e);
    throw new BlobRuntimeException(String.format("Error removing object %1$s:%2$s",containerName,key),e);
  }
  return old;
}
