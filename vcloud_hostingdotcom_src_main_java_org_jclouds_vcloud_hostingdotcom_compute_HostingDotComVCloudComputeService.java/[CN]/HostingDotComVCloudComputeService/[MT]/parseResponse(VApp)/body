{
  checkState(vAppResponse instanceof HostingDotComVApp,"bad configuration, vApp should be an instance of " + HostingDotComVApp.class.getName());
  HostingDotComVApp hVApp=HostingDotComVApp.class.cast(vAppResponse);
  return ImmutableMap.<String,String>of("id",vAppResponse.getId(),"username",hVApp.getUsername(),"password",hVApp.getPassword());
}
