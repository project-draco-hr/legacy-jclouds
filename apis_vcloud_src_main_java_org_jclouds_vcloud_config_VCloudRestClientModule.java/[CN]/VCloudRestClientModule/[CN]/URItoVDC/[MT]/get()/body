{
  return uniqueIndex(concat(transform(orgVDCMap.get().values(),new Function<Map<String,org.jclouds.vcloud.domain.VDC>,Iterable<org.jclouds.vcloud.domain.VDC>>(){
    @Override public Iterable<org.jclouds.vcloud.domain.VDC> apply(    Map<String,org.jclouds.vcloud.domain.VDC> from){
      return from.values();
    }
  }
)),new Function<org.jclouds.vcloud.domain.VDC,URI>(){
    @Override public URI apply(    org.jclouds.vcloud.domain.VDC from){
      return from.getHref();
    }
  }
);
}
