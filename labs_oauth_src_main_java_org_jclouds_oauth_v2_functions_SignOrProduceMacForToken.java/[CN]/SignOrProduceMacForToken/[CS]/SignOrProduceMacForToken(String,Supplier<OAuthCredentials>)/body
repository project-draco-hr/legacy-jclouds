{
  checkState(OAUTH_ALGORITHM_NAMES_TO_SIGNATURE_ALGORITHM_NAMES.containsKey(signatureOrMacAlgorithm),format("the signature algorithm %s is not supported",signatureOrMacAlgorithm));
  this.signatureOrMacAlgorithm=OAUTH_ALGORITHM_NAMES_TO_SIGNATURE_ALGORITHM_NAMES.get(signatureOrMacAlgorithm);
  this.credentials=credentials;
}
