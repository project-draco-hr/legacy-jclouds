{
  Map<String,String> attributes=cleanseAttributes(attrs);
  if (equalsOrSuffix(qName,"PoolRecordData")) {
    poolRecord=PoolRecord.builder().poolId(attributes.get("poolId")).id(attributes.get("poolRecordID")).description(attributes.get("description")).type(attributes.get("recordType")).pointsTo(attributes.get("pointsTo")).build();
  }
}
