{
  RegisterImageBackedByEbsOptions options=new RegisterImageBackedByEbsOptions();
  options.addNewBlockDevice("deviceName","virtualName",1);
  assertEquals(options.buildFormParameters().entries(),ImmutableMultimap.of("BlockDeviceMapping.1.Ebs.DeleteOnTermination","false","BlockDeviceMapping.1.DeviceName","deviceName","BlockDeviceMapping.1.VirtualName","virtualName","BlockDeviceMapping.1.Ebs.VolumeSize","1").entries());
}
