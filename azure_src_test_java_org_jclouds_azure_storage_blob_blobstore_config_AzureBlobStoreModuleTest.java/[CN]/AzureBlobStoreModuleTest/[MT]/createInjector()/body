{
  return Guice.createInjector(new AzureBlobStubClientModule(),new AzureBlobStoreContextModule(),new ExecutorServiceModule(sameThreadExecutor(),sameThreadExecutor()),new AbstractModule(){
    @Override protected void configure(){
      Names.bindProperties(binder(),checkNotNull(new AzureBlobPropertiesBuilder("user","secret").build(),"properties"));
    }
  }
);
