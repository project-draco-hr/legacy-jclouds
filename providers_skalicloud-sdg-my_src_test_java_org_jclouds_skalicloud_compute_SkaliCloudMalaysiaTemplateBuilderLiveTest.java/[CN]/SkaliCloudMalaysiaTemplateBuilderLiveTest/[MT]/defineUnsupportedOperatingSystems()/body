{
  return new Predicate<OsFamilyVersion64Bit>(){
    @Override public boolean apply(    OsFamilyVersion64Bit input){
switch (input.family) {
case UBUNTU:
        return !input.version.equals("") && !input.version.equals("10.10");
case CENTOS:
      return !input.version.equals("") && !input.version.equals("5.5");
case WINDOWS:
    return !(input.version.equals("") && input.is64Bit) && !(input.version.equals("2008 R2") && input.is64Bit);
default :
  return true;
}
}
}
;
}
