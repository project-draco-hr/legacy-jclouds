{
  properties.setProperty(PROPERTY_REGIONS,"Walrus");
  properties.setProperty(PROPERTY_DEFAULT_REGIONS,"Walrus");
  properties.setProperty(PROPERTY_API_VERSION,"Walrus-1.6");
