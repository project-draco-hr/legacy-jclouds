{
  S3ContextBuilder builder=new S3ContextBuilder(new S3PropertiesBuilder("id","secret").build());
  assertEquals(builder.getProperties().getProperty(PROPERTY_USER_METADATA_PREFIX),"x-amz-meta-");
  assertEquals(builder.getProperties().getProperty(PROPERTY_AWS_ACCESSKEYID),"id");
  assertEquals(builder.getProperties().getProperty(PROPERTY_AWS_SECRETACCESSKEY),"secret");
}
