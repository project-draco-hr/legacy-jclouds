{
  HttpCommandRendezvous<?> rendezvous=commandQueue.poll(1,TimeUnit.SECONDS);
  if (rendezvous != null) {
    try {
      invoke(rendezvous);
    }
 catch (    Exception e) {
      Throwables.propagateIfPossible(e,InterruptedException.class);
      logger.error(e,"Error processing command %s",rendezvous.getCommand());
    }
  }
}
