{
  DriveMetrics.Builder metricsBuilder=new DriveMetrics.Builder();
  if (from.containsKey("read:bytes"))   metricsBuilder.readBytes(new Long(from.get("read:bytes")));
  if (from.containsKey("read:requests"))   metricsBuilder.readRequests(new Long(from.get("read:requests")));
  if (from.containsKey("write:bytes"))   metricsBuilder.writeBytes(new Long(from.get("write:bytes")));
  if (from.containsKey("write:requests"))   metricsBuilder.writeRequests(new Long(from.get("write:requests")));
  return metricsBuilder.build();
}
