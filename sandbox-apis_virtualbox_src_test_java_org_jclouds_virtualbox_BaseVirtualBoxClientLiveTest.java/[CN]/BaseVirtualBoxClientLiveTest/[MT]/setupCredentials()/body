{
  Properties defaultVBoxProperties=new VirtualBoxPropertiesBuilder().build();
  if (!System.getProperties().containsKey("test." + provider + ".identity"))   System.setProperty("test." + provider + ".identity",defaultVBoxProperties.getProperty(Constants.PROPERTY_IDENTITY));
  super.setupCredentials();
  if (endpoint == null)   endpoint=defaultVBoxProperties.getProperty(Constants.PROPERTY_ENDPOINT);
  if (buildVersion == null)   buildVersion=defaultVBoxProperties.getProperty(Constants.PROPERTY_BUILD_VERSION);
}
