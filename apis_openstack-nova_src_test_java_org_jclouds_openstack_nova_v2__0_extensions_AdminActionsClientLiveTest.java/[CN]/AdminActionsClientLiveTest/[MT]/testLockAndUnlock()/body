{
  if (clientOption.isPresent()) {
    AdminActionsClient client=clientOption.get();
    assertTrue(client.unlockServer(testServerId));
    assertTrue(client.unlockServer(testServerId));
    assertTrue(client.lockServer(testServerId));
    assertTrue(client.lockServer(testServerId));
    assertTrue(client.unlockServer(testServerId));
    assertTrue(client.unlockServer(testServerId));
  }
}
