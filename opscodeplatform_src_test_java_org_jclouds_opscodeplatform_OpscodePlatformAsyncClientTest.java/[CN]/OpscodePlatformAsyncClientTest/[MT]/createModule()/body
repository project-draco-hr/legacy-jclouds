{
  return new OpscodePlatformRestClientModule(){
    @Override protected void configure(){
      Jsr330.bindProperties(binder(),new OpscodePlatformPropertiesBuilder("user",SignedHeaderAuthTest.PRIVATE_KEY).build());
      install(new NullLoggingModule());
      super.configure();
    }
    @Override protected String provideTimeStamp(    @TimeStamp Supplier<String> cache){
      return "timestamp";
    }
  }
;
}
