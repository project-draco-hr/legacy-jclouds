{
  GetIPAddressFromMAC statement=new GetIPAddressFromMAC(macAddressWith00);
  assertEquals(statement.render(OsFamily.UNIX),"MAC=" + unixMacAddressWith00 + " && [[ `uname -s` = \"Darwin\" ]] && MAC="+ bsdMacAddressWith00+ "\n arp -an | grep $MAC\n");
  statement=new GetIPAddressFromMAC(macAddressWith0c);
  assertEquals(statement.render(OsFamily.UNIX),"MAC=" + unixMacAddressWith0c + " && [[ `uname -s` = \"Darwin\" ]] && MAC="+ bsdMacAddressWith0c+ "\n arp -an | grep $MAC\n");
}
