{
  String name=getContentMetadata().getName() != null ? getContentMetadata().getName() : getSystemMetadata().getObjectName();
  if (name == null)   return -1;
  String otherName=o.getContentMetadata().getName() != null ? o.getContentMetadata().getName() : o.getSystemMetadata().getObjectName();
  return (this == o) ? 0 : name.compareTo(otherName);
}
