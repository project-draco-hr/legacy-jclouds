{
  URI template=tmClient.getCatalog().get(10,TimeUnit.SECONDS).get("Ubuntu Server 9.04 (32-bit)").getLocation();
  URI network=tmClient.getDefaultVDC().get(10,TimeUnit.SECONDS).getAvailableNetworks().values().iterator().next().getLocation();
  String response=tmClient.instantiateVAppTemplate("adriantest",template,1,512,network);
  System.out.println(response);
}
