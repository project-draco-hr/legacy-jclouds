{
  assertEquals(whenExtensionsInZoneInclude("zone",floatingIps).apply(getFloatingIPExtension().toBuilder().args(new Object[]{"differentzone"}).build()),Optional.absent());
  assertEquals(whenExtensionsInZoneInclude("zone",keypairs).apply(getKeyPairExtension().toBuilder().args(new Object[]{"differentzone"}).build()),Optional.absent());
}
