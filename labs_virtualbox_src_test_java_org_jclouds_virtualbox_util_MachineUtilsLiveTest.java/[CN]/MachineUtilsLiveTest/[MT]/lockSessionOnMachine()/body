{
  IMachine machine=cloneFromMaster();
  ISession session=machineUtils.lockSessionOnMachineAndApply(instanceName,LockType.Shared,new Function<ISession,ISession>(){
    @Override public ISession apply(    ISession session){
      return session;
    }
  }
);
  checkState(session.getState().equals(SessionState.Unlocked));
  machine=manager.get().getVBox().findMachine(instanceName);
  undoVm(instanceName);
}
