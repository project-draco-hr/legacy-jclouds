{
  super.configure();
  install(new BlobStoreObjectModule());
  install(new BlobStoreMapModule());
  bind(ConsistencyModel.class).toInstance(ConsistencyModel.STRICT);
  bind(BlobStoreContext.class).to(new TypeLiteral<BlobStoreContextImpl<TransientBlobStore,AsyncBlobStore>>(){
  }
).in(Scopes.SINGLETON);
}
