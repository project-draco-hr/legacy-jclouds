{
  if (location != null)   throw new UnsupportedOperationException("Bucket location is not yet supported");
  if (acl != null)   throw new UnsupportedOperationException("Bucket ACL is not yet supported");
  try {
    if (connection.createContainer(bucketName).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS)) {
    }
  }
 catch (  Exception e) {
    Utils.<S3ServiceException>rethrowIfRuntimeOrSameType(e);
    throw new S3ServiceException("error creating bucket: " + bucketName,e);
  }
  return new S3Bucket(bucketName);
}
