{
  try {
    GetOptions options=Util.convertGetObjectOptions(ifModifiedSince,ifUnmodifiedSince,ifMatchTags,ifNoneMatchTags);
    return Util.convertObject(connection.getBlob(bucketName,objectKey,options).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS));
  }
 catch (  Exception e) {
    Utils.<S3ServiceException>rethrowIfRuntimeOrSameType(e);
    throw new S3ServiceException(String.format("error retrieving object: %1$s:%2$s",bucketName,objectKey),e);
  }
}
