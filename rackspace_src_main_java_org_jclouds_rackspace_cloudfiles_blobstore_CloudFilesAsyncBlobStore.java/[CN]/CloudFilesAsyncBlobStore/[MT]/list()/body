{
  return compose(async.listContainers(),new Function<SortedSet<ContainerMetadata>,org.jclouds.blobstore.domain.ListResponse<? extends ResourceMetadata>>(){
    public org.jclouds.blobstore.domain.ListResponse<? extends ResourceMetadata> apply(    SortedSet<ContainerMetadata> from){
      return new ListResponseImpl<ResourceMetadata>(Iterables.transform(from,container2ResourceMd),null,null,false);
    }
  }
,service);
}
