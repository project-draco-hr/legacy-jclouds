{
  return compose(async.listContainers(),new Function<SortedSet<ContainerMetadata>,org.jclouds.blobstore.domain.ListResponse<? extends StorageMetadata>>(){
    public org.jclouds.blobstore.domain.ListResponse<? extends StorageMetadata> apply(    SortedSet<ContainerMetadata> from){
      return new ListResponseImpl<StorageMetadata>(Iterables.transform(from,container2ResourceMd),null,null,false);
    }
  }
,service);
}
