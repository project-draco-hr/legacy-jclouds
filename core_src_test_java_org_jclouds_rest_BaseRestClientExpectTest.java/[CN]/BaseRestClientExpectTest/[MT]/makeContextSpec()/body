{
  if (getClass().isAnnotationPresent(RegisterContext.class))   return (RestContextSpec<S,?>)contextSpec(provider,"http://mock","1","","","userfoo",null,getClass().getAnnotation(RegisterContext.class).sync(),getClass().getAnnotation(RegisterContext.class).async(),ImmutableSet.<Module>of());
 else   return new RestContextFactory(setupRestProperties()).createContextSpec(provider,"identity","credential",new Properties());
}
