{
  return Suppliers.memoize(new Supplier<ComputeService>(){
    @Override public ComputeService get(){
      ComputeServiceContext ctx=ContextBuilder.newBuilder(provider).credentials(creds.identity,creds.credential).overrides(overrides.get()).modules(modules).buildView(ComputeServiceContext.class);
      closer.addToClose(ctx);
      return ctx.getComputeService();
    }
  }
);
}
