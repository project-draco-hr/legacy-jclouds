{
  String key=checkNotNull(System.getProperty("jclouds.test.key"),"jclouds.test.key");
  context=new ComputeServiceContextFactory().createContext("rimuhosting",key,key,ImmutableSet.of(new Log4JLoggingModule()),new Properties());
  Injector injector=Guice.createInjector(new JschSshClientModule());
  sshFactory=injector.getInstance(SshClient.Factory.class);
  SocketOpen socketOpen=injector.getInstance(SocketOpen.class);
  socketTester=new RetryablePredicate<InetSocketAddress>(socketOpen,60,1,TimeUnit.SECONDS);
  injector.injectMembers(socketOpen);
}
