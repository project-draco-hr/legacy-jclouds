{
  try {
    chefContext.getApi().createDatabag(databag);
  }
 catch (  IllegalStateException e) {
  }
  chefContext.getApi().updateDatabagItem(databag,new DatabagItem(tag,chefContext.utils().json().toJson(ImmutableMap.<String,List<String>>of("run_list",Lists.newArrayList(runList)),RunListForTag.RUN_LIST_TYPE)));
