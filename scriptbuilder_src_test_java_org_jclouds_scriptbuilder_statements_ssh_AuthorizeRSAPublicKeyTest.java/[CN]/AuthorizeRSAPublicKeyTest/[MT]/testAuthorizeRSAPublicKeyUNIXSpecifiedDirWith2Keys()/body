{
  assertEquals(new AuthorizeRSAPublicKeys("/home/me/.ssh",ImmutableSet.of("ssh-dss AAAAB","ssh-dss CCCCD")).render(OsFamily.UNIX),"mkdir -p /home/me/.ssh\ncat >> /home/me/.ssh/authorized_keys <<'END_OF_FILE'\nssh-dss AAAAB\n\nssh-dss CCCCD\nEND_OF_FILE\nchmod 600 /home/me/.ssh/authorized_keys\n");
}
