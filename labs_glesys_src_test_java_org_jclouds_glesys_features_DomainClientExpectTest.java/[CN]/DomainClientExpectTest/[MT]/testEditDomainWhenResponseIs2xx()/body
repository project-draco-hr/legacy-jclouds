{
  DomainClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint(URI.create("https://api.glesys.com/domain/edit/format/json")).headers(ImmutableMultimap.<String,String>builder().put("Accept","application/json").put("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").build()).payload(newUrlEncodedFormPayload(ImmutableMultimap.<String,String>builder().put("domainname","x").build())).build(),HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/domain_details.json",MediaType.APPLICATION_JSON)).build()).getDomainClient();
  assertEquals(client.editDomain("x"),domainInDomainDetails());
}
