{
  DomainClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint(URI.create("https://api.glesys.com/domain/details/format/json")).headers(ImmutableMultimap.<String,String>builder().put("Accept","application/json").put("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").build()).payload(newUrlEncodedFormPayload(ImmutableMultimap.<String,String>builder().put("domainname","cl66666_x").build())).build(),HttpResponse.builder().statusCode(404).build()).getDomainClient();
  assertNull(client.getDomain("cl66666_x"));
}
