{
  DomainClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint("https://api.glesys.com/domain/list/format/json").addHeader("Accept","application/json").addHeader("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").build(),HttpResponse.builder().statusCode(404).build()).getDomainClient();
  assertTrue(client.listDomains().isEmpty());
}
