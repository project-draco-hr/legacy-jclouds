{
  Blob<?> object=(Blob<?>)entity;
  if (object.getMetadata().getContentMD5() == null) {
    try {
      object.generateMD5();
    }
 catch (    IOException e) {
      throw new RuntimeException("Could not generate MD5 for " + object.getName(),e);
    }
  }
  super.bindToRequest(request,entity);
}
