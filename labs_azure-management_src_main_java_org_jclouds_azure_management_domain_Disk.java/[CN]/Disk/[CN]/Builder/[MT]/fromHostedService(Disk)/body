{
  return this.attachedTo(in.attachedTo.orNull()).os(in.getOS()).name(in.getName()).logicalSizeInGB(in.getLogicalSizeInGB().orNull()).description(in.getDescription().orNull()).location(in.getLocation().orNull()).affinityGroup(in.getAffinityGroup().orNull()).mediaLink(in.getMediaLink().orNull()).sourceImage(in.getSourceImage().orNull()).label(in.getLabel().orNull()).hasOperatingSystem(in.hasOperatingSystem).isCorrupted(in.isCorrupted);
}
