{
  String initializerClass=checkNotNull(System.getProperty("jclouds.test.initializer"),"jclouds.test.initializer");
  Class<BaseTestInitializer<S,C,M,B>> clazz=(Class<BaseTestInitializer<S,C,M,B>>)Class.forName(initializerClass);
  BaseTestInitializer<S,C,M,B> initializer=clazz.newInstance();
  return initializer.init(createHttpModule(),testContext);
}
