{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  InputEndpoint other=(InputEndpoint)obj;
  if (externalPort == null) {
    if (other.externalPort != null)     return false;
  }
 else   if (!externalPort.equals(other.externalPort))   return false;
  if (localPort == null) {
    if (other.localPort != null)     return false;
  }
 else   if (!localPort.equals(other.localPort))   return false;
  if (name == null) {
    if (other.name != null)     return false;
  }
 else   if (!name.equals(other.name))   return false;
  if (protocol != other.protocol)   return false;
  return true;
}
