{
  InputStream is=getClass().getResourceAsStream("/location.xml");
  Set<? extends Location> result=factory.create(injector.getInstance(LocationsHandler.class)).parse(is);
  Map<String,Map<String,String>> capabilites=ImmutableMap.<String,Map<String,String>>of(IBMDeveloperCloudConstants.CAPABILITY_CAPACITY,ImmutableMap.<String,String>of("SMALL","50","MEDIUM","100","LARGE","200"),IBMDeveloperCloudConstants.CAPABILITY_FORMAT,ImmutableMap.<String,String>of("EXT3","ext3"),IBMDeveloperCloudConstants.CAPABILITY_I386,ImmutableMap.<String,String>of("SMALL","SMALL","MEDIUM","MEDIUM","LARGE","LARGE"),IBMDeveloperCloudConstants.CAPABILITY_x86_64,ImmutableMap.<String,String>of());
  Location location1=new Location("1","US North East: Poughkeepsie, NY",null,"POK",capabilites);
  assertEquals(result,ImmutableSet.of(location1));
}
