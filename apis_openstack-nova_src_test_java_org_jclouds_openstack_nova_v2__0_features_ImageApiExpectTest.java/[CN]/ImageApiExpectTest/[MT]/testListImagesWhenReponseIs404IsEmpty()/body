{
  HttpRequest listImages=HttpRequest.builder().method("GET").endpoint("https://az-1.region-a.geo-1.compute.hpcloudsvc.com/v1.1/3456/images").addHeader("Accept","application/json").addHeader("X-Auth-Token",authToken).build();
  HttpResponse listImagesResponse=HttpResponse.builder().statusCode(404).build();
  NovaApi apiWhenNoServersExist=requestsSendResponses(keystoneAuthWithUsernameAndPasswordAndTenantName,responseWithKeystoneAccess,listImages,listImagesResponse);
  assertTrue(apiWhenNoServersExist.getImageApiForZone("az-1.region-a.geo-1").listImages().isEmpty());
}
