{
  super(new Function<Credentials,AuthenticationResponse>(){
    @Override public AuthenticationResponse apply(    Credentials input){
      try {
        Future<AuthenticationResponse> response=factory.create(OpenStackAuthAsyncClient.class).authenticate(input.identity,input.credential);
        return response.get(30,TimeUnit.SECONDS);
      }
 catch (      Exception e) {
        throw Throwables.propagate(e);
      }
    }
  }
);
}
