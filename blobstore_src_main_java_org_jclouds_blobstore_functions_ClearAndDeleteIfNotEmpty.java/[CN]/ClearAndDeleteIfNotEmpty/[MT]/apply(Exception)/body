{
  if (from instanceof HttpResponseException) {
    HttpResponseException responseException=(HttpResponseException)from;
    if (responseException.getResponse().getStatusCode() == 404) {
      return null;
    }
 else     if (responseException.getResponse().getStatusCode() == 409) {
      clear.execute(request.getArgs()[0].toString());
      try {
        connection.deleteContainer(request.getArgs()[0].toString()).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS);
        return null;
      }
 catch (      Exception e) {
        Throwables.propagateIfPossible(e,BlobRuntimeException.class);
        throw new BlobRuntimeException("Error deleting container: " + request.getArgs()[0].toString(),e);
      }
    }
  }
  return Void.class.cast(propagateOrNull(from));
}
