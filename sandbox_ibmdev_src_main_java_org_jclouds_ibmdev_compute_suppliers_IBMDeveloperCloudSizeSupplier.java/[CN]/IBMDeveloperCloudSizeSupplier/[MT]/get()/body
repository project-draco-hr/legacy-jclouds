{
  final Set<Size> sizes=Sets.newHashSet();
  logger.debug(">> providing sizes");
  for (  Image in : images.get()) {
    IBMImage image=IBMImage.class.cast(in);
    for (    InstanceType instanceType : image.getRawImage().getSupportedInstanceTypes())     sizes.add(new IBMSize(image,instanceType));
  }
  logger.debug("<< sizes(%d)",sizes.size());
  return sizes;
}
