{
  Method method=InfrastructureAsyncApi.class.getMethod("getRemoteService",DatacenterDto.class,RemoteServiceType.class);
  GeneratedHttpRequest request=processor.createRequest(method,InfrastructureResources.datacenterPut(),RemoteServiceType.STORAGE_SYSTEM_MONITOR);
  assertRequestLineEquals(request,"GET http://localhost/api/admin/datacenters/1/remoteservices/storagesystemmonitor HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"Accept: " + RemoteServiceDto.BASE_MEDIA_TYPE + "\n");
  assertPayloadEquals(request,null,null,false);
  assertResponseParserClassEquals(method,request,ParseXMLWithJAXB.class);
  assertSaxResponseParserClassEquals(method,null);
  assertFallbackClassEquals(method,NullOnNotFoundOr404.class);
  checkFilters(request);
}
