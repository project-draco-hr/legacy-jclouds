{
  Method method=InfrastructureAsyncApi.class.getMethod("createRemoteService",DatacenterDto.class,RemoteServiceDto.class);
  GeneratedHttpRequest request=processor.createRequest(method,InfrastructureResources.datacenterPut(),InfrastructureResources.remoteServicePost());
  assertRequestLineEquals(request,"POST http://localhost/api/admin/datacenters/1/remoteservices HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"Accept: " + RemoteServiceDto.BASE_MEDIA_TYPE + "\n");
  assertPayloadEquals(request,withHeader(InfrastructureResources.remoteServicePostPayload()),RemoteServiceDto.class,RemoteServiceDto.BASE_MEDIA_TYPE,false);
  assertResponseParserClassEquals(method,request,ParseXMLWithJAXB.class);
  assertSaxResponseParserClassEquals(method,null);
  assertFallbackClassEquals(method,null);
  checkFilters(request);
}
