{
  Invokable<?,?> method=method(InfrastructureAsyncApi.class,"updateTier",TierDto.class);
  GeneratedHttpRequest request=processor.apply(Invocation.create(method,ImmutableList.<Object>of(InfrastructureResources.tierPut())));
  assertRequestLineEquals(request,"PUT http://localhost/api/admin/datacenters/1/storage/tiers/1 HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"Accept: " + TierDto.BASE_MEDIA_TYPE + "\n");
  assertPayloadEquals(request,withHeader(InfrastructureResources.tierPutPayload()),TierDto.class,TierDto.BASE_MEDIA_TYPE,false);
  assertResponseParserClassEquals(method,request,ParseXMLWithJAXB.class);
  assertSaxResponseParserClassEquals(method,null);
  assertFallbackClassEquals(method,null);
  checkFilters(request);
}
