{
  Method method=InfrastructureAsyncApi.class.getMethod("checkTagAvailability",DatacenterDto.class,Integer.class);
  GeneratedHttpRequest request=processor.createRequest(method,InfrastructureResources.datacenterPut(),2);
  assertRequestLineEquals(request,"GET http://localhost/api/admin/datacenters/1/network/action/checkavailability?tag=2 HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"Accept: " + VlanTagAvailabilityDto.BASE_MEDIA_TYPE + "\n");
  assertPayloadEquals(request,null,null,false);
  assertResponseParserClassEquals(method,request,ParseXMLWithJAXB.class);
  assertSaxResponseParserClassEquals(method,null);
  assertExceptionParserClassEquals(method,MapHttp4xxCodesToExceptions.class);
  checkFilters(request);
}
