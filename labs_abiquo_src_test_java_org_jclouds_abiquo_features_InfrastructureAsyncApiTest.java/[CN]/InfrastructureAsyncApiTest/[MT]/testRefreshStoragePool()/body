{
  Method method=InfrastructureAsyncApi.class.getMethod("refreshStoragePool",StoragePoolDto.class,StoragePoolOptions.class);
  GeneratedHttpRequest request=processor.createRequest(method,InfrastructureResources.storagePoolPut(),StoragePoolOptions.builder().sync(true).build());
  assertRequestLineEquals(request,"GET http://localhost/api/admin/datacenters/1/storage/devices/1/pools/tururututu?sync=true HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"Accept: " + StoragePoolDto.BASE_MEDIA_TYPE + "\n");
  assertPayloadEquals(request,null,null,false);
  assertResponseParserClassEquals(method,request,ParseXMLWithJAXB.class);
  assertSaxResponseParserClassEquals(method,null);
  assertFallbackClassEquals(method,MapHttp4xxCodesToExceptions.class);
  checkFilters(request);
}
