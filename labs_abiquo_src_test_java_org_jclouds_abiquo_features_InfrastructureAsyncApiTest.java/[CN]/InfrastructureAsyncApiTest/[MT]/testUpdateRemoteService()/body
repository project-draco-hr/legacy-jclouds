{
  Invokable<?,?> method=method(InfrastructureAsyncApi.class,"updateRemoteService",RemoteServiceDto.class);
  GeneratedHttpRequest request=processor.apply(Invocation.create(method,ImmutableList.<Object>of(InfrastructureResources.remoteServicePut())));
  assertRequestLineEquals(request,"PUT http://localhost/api/admin/datacenters/1/remoteservices/nodecollector HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"Accept: " + RemoteServiceDto.BASE_MEDIA_TYPE + "\n");
  assertPayloadEquals(request,withHeader(InfrastructureResources.remoteServicePutPayload()),RemoteServiceDto.class,RemoteServiceDto.BASE_MEDIA_TYPE,false);
  assertResponseParserClassEquals(method,request,ParseXMLWithJAXB.class);
  assertSaxResponseParserClassEquals(method,null);
  assertFallbackClassEquals(method,null);
  checkFilters(request);
}
