{
  Method method=InfrastructureAsyncApi.class.getMethod("getStoragePool",StorageDeviceDto.class,String.class);
  GeneratedHttpRequest request=processor.createRequest(method,InfrastructureResources.storageDevicePut(),InfrastructureResources.storagePoolPut().getIdStorage());
  assertRequestLineEquals(request,"GET http://localhost/api/admin/datacenters/1/storage/devices/1/pools/tururututu HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"Accept: " + StoragePoolDto.BASE_MEDIA_TYPE + "\n");
  assertPayloadEquals(request,null,null,false);
  assertResponseParserClassEquals(method,request,ParseXMLWithJAXB.class);
  assertSaxResponseParserClassEquals(method,null);
  assertFallbackClassEquals(method,NullOnNotFoundOr404.class);
  checkFilters(request);
}
