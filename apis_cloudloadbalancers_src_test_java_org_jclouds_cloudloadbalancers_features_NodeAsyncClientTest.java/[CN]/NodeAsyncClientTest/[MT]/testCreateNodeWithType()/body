{
  Method method=NodeAsyncClient.class.getMethod("createNode",int.class,NodeRequest.class);
  HttpRequest httpRequest=processor.createRequest(method,3,NodeRequest.builder().address("192.168.1.1").port(8080).build());
  assertRequestLineEquals(httpRequest,"POST https://dfw.loadbalancers.api.rackspacecloud.com/v1.0/1234/loadbalancers/3/nodes HTTP/1.1");
  assertNonPayloadHeadersEqual(httpRequest,"Accept: application/json\n");
  assertPayloadEquals(httpRequest,"{\"node\":{\"address\":\"192.168.1.1\",\"port\":8080,\"condition\":\"ENABLED\"}}","application/json",false);
  assertResponseParserClassEquals(method,httpRequest,UnwrapNode.class);
  assertSaxResponseParserClassEquals(method,null);
  assertExceptionParserClassEquals(method,ReturnNullOnNotFoundOr404.class);
  checkFilters(httpRequest);
}
