{
  Builder<String,String> builder=ImmutableMultimap.builder();
  if (md.getContentType() != null)   builder.put(HttpHeaders.CONTENT_TYPE,md.getContentType());
  if (md.getContentDisposition() != null)   builder.put("Content-Disposition",md.getContentDisposition());
  if (md.getContentEncoding() != null)   builder.put(HttpHeaders.CONTENT_ENCODING,md.getContentEncoding());
  if (md.getContentLanguage() != null)   builder.put(HttpHeaders.CONTENT_LANGUAGE,md.getContentLanguage());
  if (md.getContentLength() != null)   builder.put(HttpHeaders.CONTENT_LENGTH,md.getContentLength() + "");
  if (md.getContentMD5() != null)   builder.put("Content-MD5",CryptoStreams.base64(md.getContentMD5()));
  return builder.build();
}
