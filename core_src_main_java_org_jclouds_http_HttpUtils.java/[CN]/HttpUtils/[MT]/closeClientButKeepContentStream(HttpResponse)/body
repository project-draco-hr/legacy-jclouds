{
  if (response.getContent() != null) {
    try {
      byte[] data=toByteArray(response.getContent());
      response.setContent(new ByteArrayInputStream(data));
      return data;
    }
 catch (    IOException e) {
      propagate(e);
    }
 finally {
      closeQuietly(response.getContent());
    }
  }
  return null;
}
