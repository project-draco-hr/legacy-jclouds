{
  VirtualAppliance repeated=VirtualAppliance.Builder.fromVirtualAppliance(env.virtualAppliance).build();
  repeated.save();
  List<VirtualApplianceDto> virtualAppliances=env.cloudApi.listVirtualAppliances(env.virtualDatacenter.unwrap()).getCollection();
  assertEquals(virtualAppliances.size(),2);
  repeated.delete();
}
