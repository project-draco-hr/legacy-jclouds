{
  for (  String region : ec2Client.getConfiguredRegions()) {
    SortedSet<SecurityGroup> allResults=ImmutableSortedSet.<SecurityGroup>copyOf(client.describeSecurityGroupsInRegion(region));
    assertNotNull(allResults);
    if (allResults.size() >= 1) {
      SecurityGroup group=allResults.last();
      SortedSet<SecurityGroup> result=ImmutableSortedSet.<SecurityGroup>copyOf(client.describeSecurityGroupsInRegion(region,group.getName()));
      assertNotNull(result);
      SecurityGroup compare=result.last();
      assertEquals(compare,group);
    }
  }
}
