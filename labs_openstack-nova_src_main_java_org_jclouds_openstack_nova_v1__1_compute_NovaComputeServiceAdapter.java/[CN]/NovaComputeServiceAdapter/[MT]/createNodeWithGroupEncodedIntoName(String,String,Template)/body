{
  String zoneId=template.getLocation().getId();
  Server server=novaClient.getServerClientForZone(zoneId).createServer(name,template.getImage().getId(),template.getHardware().getId());
  return new NodeAndInitialCredentials<ServerInZone>(new ServerInZone(server,zoneId),server.getId() + "",LoginCredentials.builder().password(server.getAdminPass()).build());
}
