{
  VCloudDirectorAdminApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT","/admin/group/???").xmlFilePayload("/group/editGroupSource.xml",VCloudDirectorMediaType.GROUP).acceptMedia(VCloudDirectorMediaType.GROUP).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/group/editGroup.xml",VCloudDirectorMediaType.GROUP).httpResponseBuilder().build());
  Group expected=editGroup();
  assertEquals(api.getGroupApi().edit(groupRef.getHref(),expected),expected);
}
