{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Server other=(Server)obj;
  if (datacenter == null) {
    if (other.datacenter != null)     return false;
  }
 else   if (!datacenter.equals(other.datacenter))   return false;
  if (description == null) {
    if (other.description != null)     return false;
  }
 else   if (!description.equals(other.description))   return false;
  if (id != other.id)   return false;
  if (image == null) {
    if (other.image != null)     return false;
  }
 else   if (!image.equals(other.image))   return false;
  if (ip == null) {
    if (other.ip != null)     return false;
  }
 else   if (!ip.equals(other.ip))   return false;
  if (isSandbox != other.isSandbox)   return false;
  if (name == null) {
    if (other.name != null)     return false;
  }
 else   if (!name.equals(other.name))   return false;
  if (os == null) {
    if (other.os != null)     return false;
  }
 else   if (!os.equals(other.os))   return false;
  if (ram == null) {
    if (other.ram != null)     return false;
  }
 else   if (!ram.equals(other.ram))   return false;
  if (state == null) {
    if (other.state != null)     return false;
  }
 else   if (!state.equals(other.state))   return false;
  if (type == null) {
    if (other.type != null)     return false;
  }
 else   if (!type.equals(other.type))   return false;
  return true;
}
