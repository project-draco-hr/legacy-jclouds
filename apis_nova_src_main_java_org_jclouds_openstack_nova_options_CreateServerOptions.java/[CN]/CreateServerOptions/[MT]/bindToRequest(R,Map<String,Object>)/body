{
  Set<SecurityGroup> groups=Sets.newLinkedHashSet();
  for (  String groupName : securityGroups) {
    groups.add(SecurityGroup.builder().name(groupName).build());
  }
  ServerRequest server=new ServerRequest(checkNotNull(postParams.get("name"),"name parameter not present").toString(),checkNotNull(postParams.get("imageRef"),"imageRef parameter not present").toString(),checkNotNull(postParams.get("flavorRef"),"flavorRef parameter not present").toString(),adminPass,metadata,files,keyName,groups);
  return bindToRequest(request,ImmutableMap.of("server",server));
}
