{
  return new Predicate<OsFamilyVersion64Bit>(){
    @Override public boolean apply(    OsFamilyVersion64Bit input){
switch (input.family) {
case UBUNTU:
        return !(input.version.equals("") || input.version.startsWith("10."));
case CENTOS:
      return !input.version.equals("");
default :
    return true;
}
}
}
;
}
