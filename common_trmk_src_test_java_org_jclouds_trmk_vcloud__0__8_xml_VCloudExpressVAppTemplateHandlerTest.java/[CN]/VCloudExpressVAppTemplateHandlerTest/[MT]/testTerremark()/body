{
  InputStream is=getClass().getResourceAsStream("/vAppTemplate-trmk.xml");
  injector=Guice.createInjector(new SaxParserModule());
  factory=injector.getInstance(ParseSax.Factory.class);
  VCloudExpressVAppTemplate result=factory.create(injector.getInstance(VCloudExpressVAppTemplateHandler.class)).parse(is);
  assertEquals(result,new VCloudExpressVAppTemplateImpl("CentOS 5.3 (32-bit)",URI.create("https://services.vcloudexpress.terremark.com/api/v0.8/vAppTemplate/5"),"description of CentOS 5.3 (32-bit)",null));
}
