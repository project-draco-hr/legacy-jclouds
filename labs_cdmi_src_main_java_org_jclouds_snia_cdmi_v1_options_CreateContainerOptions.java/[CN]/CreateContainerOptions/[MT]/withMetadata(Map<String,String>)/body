{
  String s="{ \"metadata\" : {\"key1\" : \"value1\",\"key2\" : \"value2\"} }";
  this.payload=s;
  String payload="{ \"metadata\" : {";
  String separator=" ";
  for (  Entry<String,String> entry : metadata.entrySet()) {
    payload=payload + separator + "\""+ entry.getKey()+ "\" : \""+ entry.getValue()+ "\"";
    separator=",";
  }
  this.payload=payload + "} }";
  return this;
}
