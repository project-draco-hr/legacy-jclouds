{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  VirtualHardwareSection other=(VirtualHardwareSection)obj;
  if (info == null) {
    if (other.info != null)     return false;
  }
 else   if (!info.equals(other.info))   return false;
  if (resourceAllocations == null) {
    if (other.resourceAllocations != null)     return false;
  }
 else   if (!resourceAllocations.equals(other.resourceAllocations))   return false;
  if (virtualSystem == null) {
    if (other.virtualSystem != null)     return false;
  }
 else   if (!virtualSystem.equals(other.virtualSystem))   return false;
  return true;
}
