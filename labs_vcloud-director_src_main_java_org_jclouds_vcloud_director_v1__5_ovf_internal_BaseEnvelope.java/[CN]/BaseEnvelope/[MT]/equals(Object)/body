{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  BaseEnvelope<?,?> other=(BaseEnvelope<?,?>)obj;
  return Objects.equal(additionalSections,other.additionalSections) && Objects.equal(diskSections,other.diskSections) && Objects.equal(networkSections,other.networkSections)&& Objects.equal(virtualSystem,other.virtualSystem);
}
