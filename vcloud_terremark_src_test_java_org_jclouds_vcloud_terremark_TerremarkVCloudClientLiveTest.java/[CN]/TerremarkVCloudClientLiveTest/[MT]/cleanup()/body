{
  if (node != null)   tmClient.deleteNode(node.getId());
  if (is != null)   tmClient.deleteInternetService(is.getId());
  if (vApp != null) {
    try {
      successTester.apply(tmClient.powerOffVApp(vApp.getId()).getLocation());
    }
 catch (    Exception e) {
    }
    tmClient.deleteVApp(vApp.getId());
  }
}
