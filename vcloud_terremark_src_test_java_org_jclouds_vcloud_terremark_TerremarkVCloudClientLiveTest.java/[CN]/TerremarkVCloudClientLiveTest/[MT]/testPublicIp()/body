{
  node=tmClient.addNode(is.getId(),vApp.getNetworkToAddresses().values().iterator().next(),vApp.getName() + "-SSH",22).get(45,TimeUnit.SECONDS);
  publicIp=is.getPublicIpAddress().getAddress();
  try {
    doCheckPass(publicIp);
  }
 catch (  Exception e) {
    System.err.printf("%s:22 -> %s:22%n%s%n",vApp.getNetworkToAddresses().values().iterator().next(),publicIp,e.getMessage());
  }
}
