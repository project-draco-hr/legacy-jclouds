{
  HttpRequest request=signer.signGetBlob("container","name");
  assertRequestLineEquals(request,"GET https://container.s3.amazonaws.com/name HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"Authorization: AWS identity:0uvBv1wEskuhFHYJF/L6kEV9A7o=\nDate: Thu, 05 Jun 2008 16:38:19 GMT\nHost: container.s3.amazonaws.com\n");
  assertPayloadEquals(request,null,null,false);
  assertEquals(request.getFilters().size(),0);
}
