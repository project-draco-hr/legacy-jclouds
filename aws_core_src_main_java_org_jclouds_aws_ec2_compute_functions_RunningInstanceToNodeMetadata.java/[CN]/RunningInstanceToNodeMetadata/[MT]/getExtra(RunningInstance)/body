{
  Map<String,String> extra=Maps.newHashMap();
  extra.put("virtualizationType",instance.getVirtualizationType());
  if (instance.getPlacementGroup() != null)   extra.put("placementGroup",instance.getPlacementGroup());
  if (instance.getSubnetId() != null)   extra.put("subnetId",instance.getSubnetId());
  extra.putAll(instanceToStorageMapping.apply(instance));
  return extra;
}
