{
  org.jclouds.aws.ec2.domain.Image image=Iterables.getOnlyElement(client.getAMIServices().describeImagesInRegion(from.getRegion(),DescribeImagesOptions.Builder.imageIds(from.getImageId())));
  return checkNotNull(credentialProvider.execute(image),"login from image: " + from.getImageId()).identity;
}
