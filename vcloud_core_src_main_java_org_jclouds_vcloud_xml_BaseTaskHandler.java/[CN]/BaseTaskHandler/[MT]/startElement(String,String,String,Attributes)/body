{
  if (qName.equals("Task")) {
    taskLink=Utils.newLink(attributes);
    status=TaskStatus.fromValue(attributes.getValue(attributes.getIndex("status")));
    startTime=dateService.iso8601DateParse(attributes.getValue(attributes.getIndex("startTime")));
    if (attributes.getIndex("endTime") != -1) {
      try {
        endTime=dateService.iso8601DateParse(attributes.getValue(attributes.getIndex("endTime")));
      }
 catch (      RuntimeException e) {
        if (!(e.getCause() instanceof ParseException))         throw e;
      }
    }
  }
 else   if (qName.equals("Owner")) {
    owner=Utils.newNamedLink(attributes);
  }
 else   if (qName.equals("Result")) {
    result=Utils.newNamedLink(attributes);
  }
}
