{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (getClass() != obj.getClass())   return false;
  HardwareImpl other=(HardwareImpl)obj;
  if (processors == null) {
    if (other.processors != null)     return false;
  }
 else   if (!processors.equals(other.processors))   return false;
  if (ram != other.ram)   return false;
  if (supportsImage == null) {
    if (other.supportsImage != null)     return false;
  }
 else   if (!supportsImage.equals(other.supportsImage))   return false;
  if (volumes == null) {
    if (other.volumes != null)     return false;
  }
 else   if (!volumes.equals(other.volumes))   return false;
  return true;
}
