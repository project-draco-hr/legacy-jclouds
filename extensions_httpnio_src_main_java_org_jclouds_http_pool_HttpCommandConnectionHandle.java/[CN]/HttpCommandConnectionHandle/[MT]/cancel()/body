{
  if (isCompleted()) {
    return;
  }
  if (conn != null) {
    logger.trace("%1$s - %2$d - cancelled; shutting down connection",conn,conn.hashCode());
    try {
      shutdownConnection();
    }
  finally {
      conn=null;
      command=null;
      maxConnections.release();
    }
  }
  completed.release();
}
