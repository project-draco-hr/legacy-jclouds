{
  String input="{ \"queryasyncjobresultresponse\" : {\"jobid\":860,\"jobstatus\":0,\"jobprocstatus\":0,\"jobresultcode\":0} }";
  AsyncJob<PublicIPAddress> expects=AsyncJob.<PublicIPAddress>builder().id(860).status(0).progress(0).resultCode(0).build();
  ParseAsyncJob parser=i.getInstance(ParseAsyncJob.class);
  @SuppressWarnings("unchecked") AsyncJob<PublicIPAddress> response=(AsyncJob<PublicIPAddress>)parser.apply(new HttpResponse(200,"ok",Payloads.newStringPayload(input)));
  assertEquals(response,expects);
}
