{
  Method method=CloudServersAsyncClient.class.getMethod("getSharedIpGroup",int.class);
  GeneratedHttpRequest<CloudServersAsyncClient> httpMethod=processor.createRequest(method,new Object[]{2});
  assertEquals(httpMethod.getEndpoint().getHost(),"localhost");
  assertEquals(httpMethod.getEndpoint().getPath(),"/shared_ip_groups/2");
  assertEquals(httpMethod.getEndpoint().getQuery(),"format=json");
  assertEquals(httpMethod.getMethod(),HttpMethod.GET);
  assertEquals(httpMethod.getHeaders().size(),0);
  assertEquals(processor.createResponseParser(method,httpMethod).getClass(),ParseSharedIpGroupFromJsonResponse.class);
  assertEquals(processor.createExceptionParserOrThrowResourceNotFoundOn404IfNoAnnotation(method).getClass(),ReturnNullOnNotFoundOr404.class);
}
