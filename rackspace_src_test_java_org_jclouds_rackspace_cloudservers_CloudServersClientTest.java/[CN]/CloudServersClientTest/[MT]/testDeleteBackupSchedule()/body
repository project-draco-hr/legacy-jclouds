{
  Method method=CloudServersAsyncClient.class.getMethod("deleteBackupSchedule",int.class);
  GeneratedHttpRequest<CloudServersAsyncClient> httpMethod=processor.createRequest(method,new Object[]{2});
  assertEquals(httpMethod.getEndpoint().getHost(),"localhost");
  assertEquals(httpMethod.getEndpoint().getPath(),"/servers/2/backup_schedule");
  assertEquals(httpMethod.getMethod(),HttpMethod.DELETE);
  assertEquals(httpMethod.getHeaders().size(),0);
  assertEquals(processor.createExceptionParserOrThrowResourceNotFoundOn404IfNoAnnotation(method).getClass(),ReturnFalseOn404.class);
  assertEquals(processor.createResponseParser(method,httpMethod).getClass(),ReturnTrueIf2xx.class);
}
