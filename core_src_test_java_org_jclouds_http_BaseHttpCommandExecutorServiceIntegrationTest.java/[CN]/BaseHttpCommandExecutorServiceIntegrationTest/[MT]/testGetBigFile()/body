{
  InputStream input=getConsitution();
  try {
    assertEquals(md5Base64(InputSuppliers.of(input)),md5);
  }
 catch (  RuntimeException e) {
    closeQuietly(input);
    if (getFirstThrowableOfType(e,IOException.class) != null) {
      input=getConsitution();
      assertEquals(md5Base64(InputSuppliers.of(input)),md5);
    }
  }
 finally {
    closeQuietly(input);
  }
}
