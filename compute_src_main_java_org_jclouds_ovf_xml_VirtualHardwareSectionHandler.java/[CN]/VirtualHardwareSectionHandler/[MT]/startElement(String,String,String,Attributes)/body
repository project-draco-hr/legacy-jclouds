{
  if (qName.endsWith("System")) {
    inSystem=true;
  }
 else   if (!inSystem && qName.endsWith("Item")) {
    inItem=true;
  }
  if (inSystem) {
    systemHandler.startElement(uri,localName,qName,attrs);
  }
 else   if (inItem) {
    allocationHandler.startElement(uri,localName,qName,attrs);
  }
}
