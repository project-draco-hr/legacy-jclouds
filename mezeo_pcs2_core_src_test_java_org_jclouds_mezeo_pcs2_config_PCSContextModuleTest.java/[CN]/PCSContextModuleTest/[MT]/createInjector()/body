{
  return Guice.createInjector(new PCSRestClientModule(),new PCSContextModule("mezeo"){
    @Override protected void configure(){
      Jsr330.bindProperties(this.binder(),new PCSPropertiesBuilder(URI.create("http://localhost:8080"),"user","key").build());
      bind(Logger.LoggerFactory.class).toInstance(new LoggerFactory(){
        public Logger getLogger(        String category){
          return Logger.NULL;
        }
      }
);
      bind(UriBuilder.class).to(UriBuilderImpl.class);
      super.configure();
    }
  }
,new ParserModule(),new JavaUrlHttpCommandExecutorServiceModule(),new ExecutorServiceModule(sameThreadExecutor(),sameThreadExecutor()));
}
