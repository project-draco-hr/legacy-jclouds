{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  ListContainerOptions other=(ListContainerOptions)obj;
  return (detailed == other.detailed) && recursive == other.recursive && Objects.equal(dir,other.dir) && Objects.equal(getMarker(),other.getMarker()) && Objects.equal(getMaxResults(),other.getMaxResults());
}
