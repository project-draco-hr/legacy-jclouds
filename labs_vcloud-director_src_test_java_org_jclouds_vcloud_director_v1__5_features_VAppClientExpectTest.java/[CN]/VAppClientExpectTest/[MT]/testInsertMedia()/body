{
  VCloudDirectorUserClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT",vAppId + "/media/action/insertMedia").xmlFilePayload("/vApp/insertMediaParams.xml",VCloudDirectorMediaType.MEDIA_PARAMS).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/insertMediaTask.xml",VCloudDirectorMediaType.VAPP).httpResponseBuilder().build());
  MediaInsertOrEjectParams params=MediaInsertOrEjectParams.builder().build();
  Task expected=insertMediaTask();
  assertEquals(client.getVAppClient().insertMedia(vAppURI,params),expected);
}
