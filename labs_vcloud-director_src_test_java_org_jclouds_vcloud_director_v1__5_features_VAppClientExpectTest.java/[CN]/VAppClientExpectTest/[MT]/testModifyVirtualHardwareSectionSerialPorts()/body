{
  VCloudDirectorUserClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT",vAppId + "/virtualHardwareSection/serialPorts").xmlFilePayload("/vApp/modifyVirtualHardwareSectionSerialPorts.xml",VCloudDirectorMediaType.OVF_RASD_ITEMS_LIST).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/modifyVirtualHardwareSectionSerialPortsTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  RasdItemsList serialPorts=getVirtualHardwareSectionSerialPorts().toBuilder().build();
  Task expected=modifyVirtualHardwareSectionSerialPortsTask();
  assertEquals(client.getVAppClient().modifyVirtualHardwareSectionSerialPorts(vAppURI,serialPorts),expected);
}
