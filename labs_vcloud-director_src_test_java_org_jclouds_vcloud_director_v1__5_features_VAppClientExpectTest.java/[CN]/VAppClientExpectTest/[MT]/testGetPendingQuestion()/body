{
  VCloudDirectorUserClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vAppId + "/question").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getPendingQuestion.xml",VCloudDirectorMediaType.VM_PENDING_QUESTION).httpResponseBuilder().build());
  VmPendingQuestion expected=getPendingQuestion();
  assertEquals(client.getVAppClient().getPendingQuestion(vAppURI),expected);
}
