{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT",vAppId + "/operatingSystemSection").xmlFilePayload("/vApp/modifyOperatingSystemSection.xml",VCloudDirectorMediaType.OPERATING_SYSTEM_SECTION).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/modifyOperatingSystemSectionTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  OperatingSystemSection section=getOperatingSystemSection().toBuilder().build();
  Task expected=modifyOperatingSystemSectionTask();
  assertEquals(client.getVAppClient().modifyOperatingSystemSection(vAppURI,section),expected);
}
