{
  Task task=Task.builder().error(Error.builder().build()).org(Reference.builder().build()).owner(Reference.builder().build()).user(Reference.builder().build()).params(null).progress(0).status(status).operation(operation).operationName(operationName).startTime(dateService.iso8601DateParse(startTime)).endTime(null).expiryTime(null).tasks(Sets.<Task>newLinkedHashSet()).description(description).name(name).id("urn:vcloud:" + taskId).href(URI.create(endpoint + "/task/" + taskId)).links(Sets.<Link>newLinkedHashSet()).build();
  return task;
}
