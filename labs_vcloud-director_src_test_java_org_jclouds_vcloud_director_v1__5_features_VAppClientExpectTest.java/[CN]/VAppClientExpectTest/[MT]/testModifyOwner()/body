{
  VCloudDirectorUserClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT",vAppId + "/owner").xmlFilePayload("/vApp/modifyOwner.xml",VCloudDirectorMediaType.OWNER).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().httpResponseBuilder().statusCode(204).build());
  Owner owner=Owner.builder().build();
  client.getVAppClient().modifyOwner(vAppURI,owner);
}
