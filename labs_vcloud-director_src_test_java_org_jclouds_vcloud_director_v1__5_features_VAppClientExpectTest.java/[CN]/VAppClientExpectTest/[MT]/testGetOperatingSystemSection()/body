{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vAppId + "/operatingSystemSection").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getOperatingSystemSection.xml",VCloudDirectorMediaType.OPERATING_SYSTEM_SECTION).httpResponseBuilder().build());
  OperatingSystemSection expected=getOperatingSystemSection();
  assertEquals(client.getVAppClient().getOperatingSystemSection(vAppURI),expected);
}
