{
  VCloudDirectorUserClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT",vAppId + "/question/action/answer").xmlFilePayload("/vApp/answerQuestion.xml",VCloudDirectorMediaType.VM_PENDING_ANSWER).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().httpResponseBuilder().statusCode(204).build());
  VmQuestionAnswer answer=null;
  client.getVAppClient().answerQuestion(vAppURI,answer);
}
