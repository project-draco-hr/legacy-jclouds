{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("DELETE",vAppId).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/deleteVAppTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=deleteVAppTask();
  assertEquals(client.getVAppClient().deleteVApp(vAppURI),expected);
}
