{
  VCloudDirectorUserClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",vAppId + "/action/installVMwareTools").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/installVMwareToolsTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=installVMwareToolsTask();
  assertEquals(client.getVAppClient().installVMwareTools(vAppURI),expected);
}
