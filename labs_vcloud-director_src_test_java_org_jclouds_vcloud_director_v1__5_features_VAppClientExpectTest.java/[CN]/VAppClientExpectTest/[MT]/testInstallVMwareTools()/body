{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("","/vapp/e9cd3387-ac57-4d27-a481-9bee75e0690f").xmlFilePayload("/vapp/installVMwareTools.xml",VCloudDirectorMediaType.VAPP).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vdc/installVMwareTools.xml",VCloudDirectorMediaType.VAPP).httpResponseBuilder().build());
  VApp expected=installVMwareTools();
  assertEquals(client.getVAppClient().installVMwareTools(vAppURI),expected);
}
