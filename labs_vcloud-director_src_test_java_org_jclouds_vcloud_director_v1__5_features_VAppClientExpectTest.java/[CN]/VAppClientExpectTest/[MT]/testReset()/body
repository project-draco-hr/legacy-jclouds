{
  VCloudDirectorUserClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",vAppId + "/power/action/reset").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/resetTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=resetTask();
  assertEquals(client.getVAppClient().reset(vAppURI),expected);
}
