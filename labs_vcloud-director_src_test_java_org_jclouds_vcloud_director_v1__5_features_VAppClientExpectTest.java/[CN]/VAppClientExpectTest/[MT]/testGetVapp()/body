{
  VCloudDirectorClient client=orderedRequestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vAppId).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/vApp.xml",VCloudDirectorMediaType.VAPP).httpResponseBuilder().build());
  VApp expected=getVApp();
  assertEquals(client.getVAppClient().getVApp(vAppURI),expected);
}
