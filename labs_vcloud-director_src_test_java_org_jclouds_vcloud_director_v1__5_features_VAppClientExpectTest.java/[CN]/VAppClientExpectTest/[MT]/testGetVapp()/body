{
  VCloudDirectorClient client=orderedRequestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/vApp/e9cd3387-ac57-4d27-a481-9bee75e0690f").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vapp/vapp.xml",VCloudDirectorMediaType.VAPP).httpResponseBuilder().build());
  VApp expected=getVApp();
  assertEquals(client.getVAppClient().getVApp(vAppURI),expected);
}
