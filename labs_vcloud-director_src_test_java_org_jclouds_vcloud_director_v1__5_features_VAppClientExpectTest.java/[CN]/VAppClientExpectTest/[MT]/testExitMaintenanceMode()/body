{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",vAppId + "/action/exitMaintenanceMode").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().httpResponseBuilder().build());
  client.getVAppClient().exitMaintenanceMode(vAppURI);
}
