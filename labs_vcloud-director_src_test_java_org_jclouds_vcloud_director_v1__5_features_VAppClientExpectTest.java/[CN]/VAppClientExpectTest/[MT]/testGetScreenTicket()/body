{
  VCloudDirectorUserClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",vAppId + "/screen/action/acquireTicket").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getScreenTicket.xml",VCloudDirectorMediaType.SCREEN_TICKET).httpResponseBuilder().build());
  ScreenTicket expected=getScreenTicket();
  assertEquals(client.getVAppClient().getScreenTicket(vAppURI),expected);
}
