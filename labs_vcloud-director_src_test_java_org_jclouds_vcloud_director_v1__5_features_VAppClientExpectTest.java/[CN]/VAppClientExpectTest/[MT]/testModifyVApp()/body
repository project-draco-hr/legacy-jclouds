{
  VCloudDirectorUserClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT",vAppId).xmlFilePayload("/vApp/modifyVApp.xml",VCloudDirectorMediaType.VAPP).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/modifiedVapp.xml",VCloudDirectorMediaType.VAPP).httpResponseBuilder().build());
  VApp modified=getVApp();
  modified.setName("new-name");
  modified.setDescription("New Description");
  Task expected=modifyVAppTask();
  assertEquals(client.getVAppClient().modifyVApp(vAppURI,modified),expected);
}
