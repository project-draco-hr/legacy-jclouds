{
  VCloudDirectorUserClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vAppId + "/productSections").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getProductSections.xml",VCloudDirectorMediaType.PRODUCT_SECTION_LIST).httpResponseBuilder().build());
  ProductSectionList expected=getProductSections();
  assertEquals(client.getVAppClient().getProductSections(vAppURI),expected);
}
