{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vAppId + "/virtualHardwareSection/networkCards").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getVirtualHardwareSectionNetworkCards.xml",VCloudDirectorMediaType.OVF_RASD_ITEMS_LIST).httpResponseBuilder().build());
  RasdItemsList expected=getVirtualHardwareSectionNetworkCards();
  assertEquals(client.getVAppClient().getVirtualHardwareSectionNetworkCards(vAppURI),expected);
}
