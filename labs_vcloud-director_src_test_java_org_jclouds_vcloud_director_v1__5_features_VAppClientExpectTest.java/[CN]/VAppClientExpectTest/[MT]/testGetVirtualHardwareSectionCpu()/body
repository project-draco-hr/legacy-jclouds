{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vAppId + "/virtualHardwareSection/cpu").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getVirtualHardwareSectionCpu.xml",VCloudDirectorMediaType.OVF_RASD_ITEM).httpResponseBuilder().build());
  RASD expected=getVirtualHardwareSectionCpu();
  assertEquals(client.getVAppClient().getVirtualHardwareSectionCpu(vAppURI),expected);
}
