{
  VCloudDirectorUserClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT",vAppId + "/virtualHardwareSection/networkCards").xmlFilePayload("/vApp/modifyVirtualHardwareSectionNetworkCards.xml",VCloudDirectorMediaType.OVF_RASD_ITEMS_LIST).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/modifyVirtualHardwareSectionNetworkCardsTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  RasdItemsList networkCards=getVirtualHardwareSectionNetworkCards().toBuilder().build();
  Task expected=modifyVirtualHardwareSectionNetworkCardsTask();
  assertEquals(client.getVAppClient().modifyVirtualHardwareSectionNetworkCards(vAppURI,networkCards),expected);
}
