{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("","/vapp/e9cd3387-ac57-4d27-a481-9bee75e0690f").xmlFilePayload("/vapp/getLeaseSettingsSection.xml",VCloudDirectorMediaType.VAPP).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vdc/getLeaseSettingsSection.xml",VCloudDirectorMediaType.VAPP).httpResponseBuilder().build());
  VApp expected=getLeaseSettingsSection();
  assertEquals(client.getVAppClient().getLeaseSettingsSection(vAppURI),expected);
}
