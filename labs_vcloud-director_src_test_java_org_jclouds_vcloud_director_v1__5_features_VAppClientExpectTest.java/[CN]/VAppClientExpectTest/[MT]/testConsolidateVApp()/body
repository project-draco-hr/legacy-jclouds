{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",vAppId + "/action/consolidate").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/consolidateVAppTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=consolidateVAppTask();
  assertEquals(client.getVAppClient().consolidateVm(vAppURI),expected);
}
