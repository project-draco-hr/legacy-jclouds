{
  VCloudDirectorUserClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",vAppId + "/action/enterMaintenanceMode").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().httpResponseBuilder().statusCode(204).build());
  client.getVAppClient().enterMaintenanceMode(vAppURI);
}
