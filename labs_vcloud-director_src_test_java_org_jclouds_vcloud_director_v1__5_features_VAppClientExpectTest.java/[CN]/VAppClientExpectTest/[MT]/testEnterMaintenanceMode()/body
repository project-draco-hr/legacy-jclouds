{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",vAppId + "/action/enterMaintenanceMode").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().httpResponseBuilder().build());
  client.getVAppClient().enterMaintenanceMode(vAppURI);
}
