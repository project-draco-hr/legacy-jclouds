{
  return from(from).filter(LocationPredicates.isZone()).transform(new Function<Location,String>(){
    @Override public String apply(    Location from){
      return from.getParent().getId();
    }
  }
).transform(new Function<String,Org>(){
    @Override public Org apply(    String from){
      return api.getOrgApi().get(from);
    }
  }
);
}
