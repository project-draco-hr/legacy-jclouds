{
  try {
    Set<Image> list=gson.fromJson(new InputStreamReader(stream,"UTF-8"),ImageListResponse.class).images;
    for (    Image image : list)     if (emptyString.equals(image.getProductCodes()))     image.getProductCodes().clear();
    return list;
  }
 catch (  UnsupportedEncodingException e) {
    throw new RuntimeException("jclouds requires UTF-8 encoding",e);
  }
}
