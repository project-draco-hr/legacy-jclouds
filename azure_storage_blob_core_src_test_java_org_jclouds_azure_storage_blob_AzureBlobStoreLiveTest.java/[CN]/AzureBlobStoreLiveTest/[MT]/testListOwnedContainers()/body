{
  List<ContainerMetadata> response=connection.listContainers();
  response=connection.listContainers(ListOptions.Builder.prefix(privateContainer.substring(0,privateContainer.length() - 1)).maxResults(1));
  assertEquals(response.size(),1);
  assertEquals(response.get(0).getName(),privateContainer);
  response=connection.listContainers(ListOptions.Builder.prefix(publicContainer).maxResults(1));
  assertEquals(response.size(),1);
  assertEquals(response.get(0).getName(),publicContainer);
}
