{
  account=System.getProperty("jclouds.test.user");
  String key=System.getProperty("jclouds.test.key");
  Injector injector=AzureBlobContextBuilder.newBuilder(account,key).withModules(new Log4JLoggingModule()).withSaxDebug().buildInjector();
  connection=injector.getInstance(AzureBlobStore.class);
}
