{
  Map<String,String> expected=new ImmutableMap.Builder<String,String>().put("fs","/").put("ps",":").put("lf","\n").put("sh","bash").put("source",".").put("rem","#").put("args","$@").put("varstart","$").put("varend","").put("shebang","#!/bin/bash\n").put("zeroPath","export PATH=/usr/ucb/bin:/bin:/usr/bin:/usr/sbin\n").put("exe","").build();
  assertEquals(ShellToken.tokenValueMap(OsFamily.UNIX),expected);
}
