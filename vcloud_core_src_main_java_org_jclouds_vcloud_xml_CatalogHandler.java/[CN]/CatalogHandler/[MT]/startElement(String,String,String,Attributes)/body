{
  if (qName.equals("Catalog")) {
    Catalog=new NamedLinkImpl(attributes.getValue(attributes.getIndex("name")),attributes.getValue(attributes.getIndex("type")),URI.create(attributes.getValue(attributes.getIndex("href"))));
  }
 else   if (qName.equals("CatalogItem")) {
    putNamedLink(contents,attributes);
  }
}
