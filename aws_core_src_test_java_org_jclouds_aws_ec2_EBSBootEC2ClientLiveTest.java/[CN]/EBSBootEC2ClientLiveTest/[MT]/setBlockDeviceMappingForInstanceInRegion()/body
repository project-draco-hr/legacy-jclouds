{
  try {
    client.getInstanceServices().setBlockDeviceMappingForInstanceInRegion(Region.DEFAULT,ebsInstance.getId(),"whoopie");
    assertEquals(ImmutableMap.<String,EbsBlockDevice>of("whoopie",null),client.getInstanceServices().getBlockDeviceMappingForInstanceInRegion(Region.DEFAULT,ebsInstance.getId()));
    System.out.println("OK: setBlockDeviceMappingForInstanceInRegion");
  }
 catch (  Exception e) {
    System.err.println("setBlockDeviceMappingForInstanceInRegion");
    e.printStackTrace();
  }
}
