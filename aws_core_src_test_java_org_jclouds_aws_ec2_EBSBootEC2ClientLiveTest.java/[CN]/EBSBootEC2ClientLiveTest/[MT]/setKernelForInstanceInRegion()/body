{
  String oldKernel=client.getInstanceServices().getKernelForInstanceInRegion(Region.DEFAULT,ebsInstance.getId());
  client.getInstanceServices().setKernelForInstanceInRegion(Region.DEFAULT,ebsInstance.getId(),oldKernel);
  assertEquals(oldKernel,client.getInstanceServices().getKernelForInstanceInRegion(Region.DEFAULT,ebsInstance.getId()));
}
