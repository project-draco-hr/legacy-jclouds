{
  String ramdisk=client.getInstanceServices().getRamdiskForInstanceInRegion(Region.DEFAULT,ebsInstance.getId());
  client.getInstanceServices().setRamdiskForInstanceInRegion(Region.DEFAULT,ebsInstance.getId(),ramdisk);
  assertEquals(ramdisk,client.getInstanceServices().getRamdiskForInstanceInRegion(Region.DEFAULT,ebsInstance.getId()));
}
