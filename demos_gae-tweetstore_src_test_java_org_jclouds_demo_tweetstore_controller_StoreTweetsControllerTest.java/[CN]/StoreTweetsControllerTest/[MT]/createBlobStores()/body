{
  Map<String,BlobStoreContext<?>> contexts=ImmutableMap.<String,BlobStoreContext<?>>of("test1",new StubBlobStoreContextBuilder().buildContext(),"test2",new StubBlobStoreContextBuilder().buildContext());
  for (  BlobStoreContext<?> blobstore : contexts.values()) {
    blobstore.getBlobStore().createContainer("favo").get();
  }
  return contexts;
}
