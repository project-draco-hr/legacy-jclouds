{
  Map<String,BlobStoreContext> contexts=ImmutableMap.<String,BlobStoreContext>of("test1",new StubBlobStoreContextBuilder().buildBlobStoreContext(),"test2",new StubBlobStoreContextBuilder().buildBlobStoreContext());
