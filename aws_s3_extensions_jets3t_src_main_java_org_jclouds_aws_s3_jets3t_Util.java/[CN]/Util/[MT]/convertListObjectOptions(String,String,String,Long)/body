{
  ListBucketOptions options=new ListBucketOptions();
  if (prefix != null) {
    options.withPrefix(prefix);
  }
  if (marker != null) {
    options.afterMarker(marker);
  }
  if (maxKeys != null) {
    options.maxResults(maxKeys);
  }
  if (delimiter != null) {
    options.delimiter(delimiter);
  }
  return options;
}
