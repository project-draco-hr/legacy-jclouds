{
  MutableBlobMetadata to=new MutableBlobMetadataImpl();
  if (from.getUrl() != null) {
    to.setId(from.getUrl().getPath().replaceAll(".*objects/",""));
  }
  to.setLocation(from.getUrl());
  to.setName(from.getName());
  if (from.getMimeType() != null)   to.setContentType(from.getMimeType());
  if (from.getModified() != null)   to.setLastModified(from.getModified());
  if (from.getBytes() != null)   to.setSize(from.getBytes());
  to.setType(ResourceType.BLOB);
  return to;
}
