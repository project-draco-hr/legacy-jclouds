{
  Map<String,String> attributes=cleanseAttributes(attrs);
  if (equalsOrSuffix(qName,"Org")) {
    if (!attributes.containsKey("href") && getRequest() != null)     attributes=ImmutableMap.<String,String>builder().putAll(attributes).put("href",getRequest().getEndpoint().toASCIIString()).build();
    Resource org=newResource(attributes);
    builder.name(org.getName()).type(org.getType()).id(org.getId()).href(org.getHref());
  }
 else   if (equalsOrSuffix(qName,"Link")) {
    Link link=Link.class.cast(newResource(attributes));
    if ("down".equals(link.getRel()))     builder.vDC(link);
 else     builder.image(link);
  }
}
