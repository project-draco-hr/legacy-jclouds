{
  URI mediaUri=URI.create(endpoint + "/media/794eb334-754e-4917-b5a0-5df85cbd61d1");
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,getStandardPayloadRequest("PUT",mediaUri,"/media/media.xml",VCloudDirectorMediaType.MEDIA),getStandardPayloadResponse("/media/media.xml",VCloudDirectorMediaType.MEDIA));
  Media expected=media();
  Reference mediaRef=Reference.builder().href(mediaUri).build();
  assertEquals(client.getMediaClient().updateMedia(mediaRef,expected),expected);
}
