{
  URI mediaUri=URI.create(endpoint + "/media/794eb334-754e-4917-b5a0-5df85cbd61d1");
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT","/media/794eb334-754e-4917-b5a0-5df85cbd61d1").xmlFilePayload("/media/updateMedia.xml",VCloudDirectorMediaType.MEDIA).acceptMedia(VCloudDirectorMediaType.TASK).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/media/updateMediaTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Media update=updateMedia();
  Task expected=updateMediaTask();
  assertEquals(client.getMediaClient().updateMedia(mediaUri,update),expected);
}
