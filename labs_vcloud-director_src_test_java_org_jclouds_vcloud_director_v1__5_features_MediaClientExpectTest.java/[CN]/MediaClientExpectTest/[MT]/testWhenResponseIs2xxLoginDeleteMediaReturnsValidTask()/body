{
  URI mediaUri=URI.create(endpoint + "/media/794eb334-754e-4917-b5a0-5df85cbd61d1");
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("DELETE","/media/794eb334-754e-4917-b5a0-5df85cbd61d1").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/media/deleteMediaTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=Task.builder().build();
  Reference mediaRef=Reference.builder().href(mediaUri).build();
  assertEquals(client.getMediaClient().deleteMedia(mediaRef),expected);
}
