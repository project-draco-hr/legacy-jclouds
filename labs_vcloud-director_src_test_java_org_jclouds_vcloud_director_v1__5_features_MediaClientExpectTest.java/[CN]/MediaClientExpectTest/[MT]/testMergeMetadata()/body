{
  URI mediaUri=URI.create("https://vcloudbeta.bluelock.com/api/media/794eb334-754e-4917-b5a0-5df85cbd61d1");
  VCloudDirectorUserClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST","/media/794eb334-754e-4917-b5a0-5df85cbd61d1/metadata").xmlFilePayload("/media/mergeMetadata.xml",VCloudDirectorMediaType.METADATA).acceptMedia(VCloudDirectorMediaType.TASK).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/media/mergeMetadataTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Metadata inputMetadata=metadata();
  Task expectedTask=mergeMetadataTask();
  assertEquals(client.getMediaClient().getMetadataClient().mergeMetadata(mediaUri,inputMetadata),expectedTask);
}
