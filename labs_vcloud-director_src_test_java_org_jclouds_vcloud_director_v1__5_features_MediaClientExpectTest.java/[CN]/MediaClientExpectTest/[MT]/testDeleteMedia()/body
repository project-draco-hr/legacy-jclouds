{
  URI mediaUri=URI.create(endpoint + "/media/794eb334-754e-4917-b5a0-5df85cbd61d1");
  VCloudDirectorUserClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("DELETE","/media/794eb334-754e-4917-b5a0-5df85cbd61d1").acceptMedia(VCloudDirectorMediaType.TASK).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/media/deleteMediaTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=deleteMediaTask();
  assertEquals(client.getMediaClient().deleteMedia(mediaUri),expected);
}
