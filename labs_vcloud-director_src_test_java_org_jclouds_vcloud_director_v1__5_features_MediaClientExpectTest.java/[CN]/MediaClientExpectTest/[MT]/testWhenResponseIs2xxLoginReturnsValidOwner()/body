{
  URI mediaUri=URI.create(endpoint + "/media/794eb334-754e-4917-b5a0-5df85cbd61d1");
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/media/794eb334-754e-4917-b5a0-5df85cbd61d1/owner").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/media/owner.xml",VCloudDirectorMediaType.OWNER).httpResponseBuilder().build());
  Owner expected=owner();
  Reference mediaRef=Reference.builder().href(mediaUri).build();
  assertEquals(client.getMediaClient().getOwner(mediaRef),expected);
}
