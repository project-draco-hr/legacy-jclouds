{
  Method allParamsValidatedMethod=InputParamValidatorForm.class.getMethod("allParamsValidated",String.class,String.class);
  Method oneParamValidatedMethod=InputParamValidatorForm.class.getMethod("oneParamValidated",String.class,String.class);
  RestAnnotationProcessor restAnnotationProcessor=factory(InputParamValidatorForm.class);
  restAnnotationProcessor.createRequest(allParamsValidatedMethod,"blah","blah");
  restAnnotationProcessor.createRequest(oneParamValidatedMethod,"blah","blah");
  try {
    restAnnotationProcessor.createRequest(allParamsValidatedMethod,"BLAH","blah");
    throw new TestException("AllLowerCaseValidator shouldn't have passed 'BLAH' as a parameter because it's uppercase.");
  }
 catch (  IllegalArgumentException e) {
  }
  restAnnotationProcessor.createRequest(oneParamValidatedMethod,"BLAH","blah");
  try {
    restAnnotationProcessor.createRequest(oneParamValidatedMethod,"blah","BLAH");
    throw new TestException("AllLowerCaseValidator shouldn't have passed 'BLAH' as the second parameter because it's uppercase.");
  }
 catch (  IllegalArgumentException e) {
  }
}
