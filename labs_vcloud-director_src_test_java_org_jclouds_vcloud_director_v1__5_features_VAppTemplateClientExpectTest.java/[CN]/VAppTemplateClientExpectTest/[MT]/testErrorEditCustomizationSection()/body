{
  final String templateId="/vAppTemplate/vappTemplate-ef4415e6-d413-4cbb-9262-f9bbec5f2ea9";
  URI uri=URI.create(endpoint + templateId);
  VAppTemplateClient client=orderedRequestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT",templateId + "/customizationSection").xmlFilePayload("/vapptemplate/customizationSection.xml",CUSTOMIZATION_SECTION).acceptMedia(TASK).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vapptemplate/error403.xml",ERROR).httpResponseBuilder().statusCode(403).build()).getVAppTemplateClient();
  client.modifyCustomizationSection(uri,exampleCustomizationSection());
}
