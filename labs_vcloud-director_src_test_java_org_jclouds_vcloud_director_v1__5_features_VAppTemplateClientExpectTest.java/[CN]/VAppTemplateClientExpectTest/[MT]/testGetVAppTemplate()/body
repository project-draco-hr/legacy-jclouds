{
  final String templateId="/vAppTemplate/vAppTemplate/vappTemplate-xxxx-xxxx-xxxx-xxx";
  Reference vappTemplateRef=Reference.builder().href(URI.create(endpoint + templateId)).build();
  VAppTemplateClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",templateId).acceptMedia(VAPP_TEMPLATE).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vapptemplate/vAppTemplate.xml",VAPP_TEMPLATE).httpResponseBuilder().build()).getVAppTemplateClient();
  assertNotNull(client);
  VAppTemplate template=client.getVAppTemplate(vappTemplateRef);
  assertEquals(template,exampleTemplate());
}
