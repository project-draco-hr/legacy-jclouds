{
  String realKey=prefixer.apply(key.toString());
  try {
    return stripPrefix(connection.getBlob(containerName,realKey).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS));
  }
 catch (  Exception e) {
    Throwable cause=Throwables.getRootCause(e);
    if (cause instanceof KeyNotFoundException) {
      return null;
    }
 else {
      throw new BlobRuntimeException(String.format("Error geting object %1$s:%2$s",containerName,realKey),cause);
    }
  }
}
