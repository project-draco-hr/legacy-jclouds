{
  InputStream is=getClass().getResourceAsStream("/networkconfigsection.xml");
  Injector injector=Guice.createInjector(new SaxParserModule());
  Factory factory=injector.getInstance(ParseSax.Factory.class);
  NetworkConfigSection result=factory.create(injector.getInstance(NetworkConfigSectionHandler.class)).parse(is);
  assertEquals(result.toString(),NetworkConfigSection.builder().network("VM Tier01").gateway("0.0.0.0").netmask("0.0.0.0").info("MAC=00:00:00:00:00:00").fenceMode("allowInOut").dhcp(true).internalToExternalNATRule("10.76.2.4","206.24.124.1").build().toString());
}
