{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  if (!super.equals(o))   return false;
  Location location=(Location)o;
  if (friendlyName != null ? !friendlyName.equals(location.friendlyName) : location.friendlyName != null)   return false;
  if (iso3166 != null ? !iso3166.equals(location.iso3166) : location.iso3166 != null)   return false;
  if (locode != null ? !locode.equals(location.locode) : location.locode != null)   return false;
  return true;
}
