{
  try {
    return (bridgedInterface.getName().startsWith(networkInterface.getDisplayName()) && bridgedInterface.getStatus().equals("Up") && networkInterface.isUp()&& !networkInterface.isLoopback());
  }
 catch (  SocketException e) {
    logger.error(e,"Problem in listing network interfaces.");
    throw Throwables.propagate(e);
  }
}
