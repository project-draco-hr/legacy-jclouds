{
  String launchGroup=PREFIX + "1";
  for (  String region : Region.DEFAULT_REGIONS)   for (  SpotInstanceRequest request : client.getSpotInstanceServices().describeSpotInstanceRequestsInRegion(region))   if (launchGroup.equals(request.getLaunchGroup()))   client.getSpotInstanceServices().cancelSpotInstanceRequestsInRegion(region,request.getId());
  start=System.currentTimeMillis();
  requests=client.getSpotInstanceServices().requestSpotInstancesInRegion("us-west-1",0.09f,1,LaunchSpecification.builder().imageId("ami-951945d0").instanceType(InstanceType.M1_SMALL).build(),launchGroup(launchGroup).availabilityZoneGroup(launchGroup).validFrom(new Date(System.currentTimeMillis() + TimeUnit.SECONDS.toMillis(2))).validUntil(new Date(System.currentTimeMillis() + TimeUnit.SECONDS.toMillis(SPOT_DELAY_SECONDS))));
  assertNotNull(requests);
  for (  SpotInstanceRequest request : requests)   verifySpotInstance(request);
}
