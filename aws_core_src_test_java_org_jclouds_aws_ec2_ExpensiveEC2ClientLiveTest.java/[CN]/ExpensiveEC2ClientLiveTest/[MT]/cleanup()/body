{
  if (address != null)   client.getElasticIPAddressServices().releaseAddress(address);
  if (serverId != null)   client.getInstanceServices().terminateInstances(serverId);
  if (keyPair != null)   client.getKeyPairServices().deleteKeyPair(keyPair.getKeyName());
  if (securityGroupName != null)   client.getSecurityGroupServices().deleteSecurityGroup(securityGroupName);
}
