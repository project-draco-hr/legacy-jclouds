{
  if (address != null)   client.releaseAddress(address).get(30,TimeUnit.SECONDS);
  if (serverId != null)   client.terminateInstances(serverId).get(30,TimeUnit.SECONDS);
  if (keyPair != null)   client.deleteKeyPair(keyPair.getKeyName()).get(30,TimeUnit.SECONDS);
  if (securityGroupName != null)   client.deleteSecurityGroup(securityGroupName).get(30,TimeUnit.SECONDS);
}
