{
  SshClient connection=sshFactory.create(new InetSocketAddress(newDetails.getDnsName(),22),"root",keyPair.getKeyMaterial().getBytes());
  try {
    connection.connect();
    InputStream etcPasswd=connection.get("/etc/passwd");
    Utils.toStringAndClose(etcPasswd);
  }
  finally {
    if (connection != null)     connection.disconnect();
  }
}
