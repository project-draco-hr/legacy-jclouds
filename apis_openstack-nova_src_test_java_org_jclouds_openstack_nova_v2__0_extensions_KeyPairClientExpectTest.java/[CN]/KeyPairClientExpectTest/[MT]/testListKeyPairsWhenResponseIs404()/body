{
  HttpRequest listKeyPairs=HttpRequest.builder().method("GET").endpoint("https://az-1.region-a.geo-1.compute.hpcloudsvc.com/v1.1/3456/os-keypairs").addHeader("Accept","application/json").addHeader("X-Auth-Token",authToken).build();
  HttpResponse listKeyPairsResponse=HttpResponse.builder().statusCode(404).build();
  NovaClient clientWhenNoServersExist=requestsSendResponses(keystoneAuthWithUsernameAndPasswordAndTenantName,responseWithKeystoneAccess,extensionsOfNovaRequest,extensionsOfNovaResponse,listKeyPairs,listKeyPairsResponse);
  assertTrue(clientWhenNoServersExist.getKeyPairExtensionForZone("az-1.region-a.geo-1").get().listKeyPairs().isEmpty());
}
