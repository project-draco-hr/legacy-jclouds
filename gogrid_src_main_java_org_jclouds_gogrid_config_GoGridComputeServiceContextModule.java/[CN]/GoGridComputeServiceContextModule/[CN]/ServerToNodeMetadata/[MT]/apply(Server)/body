{
  String locationId="Unavailable";
  String tag=CharMatcher.JAVA_LETTER.retainFrom(from.getName());
  Credentials creds=client.getServerServices().getServerCredentialsList().get(from.getName());
  Set<InetAddress> ipSet=ImmutableSet.of(stringIpToInetAddress.apply(from.getIp().getIp()));
  NodeState state=serverStateToNodeState.get(from.getState().getName());
  return new NodeMetadataImpl(from.getId() + "",from.getName(),locationId,null,ImmutableMap.<String,String>of(),tag,state,ipSet,ImmutableList.<InetAddress>of(),ImmutableMap.<String,String>of(),creds);
}
