{
  IMachine source=getVmWithGuestAdditionsInstalled();
  CloneSpec cloneSpec=CloneSpec.builder().vm(machineSpec.getVmSpec()).network(machineSpec.getNetworkSpec()).master(source).linked(true).build();
  IMachine clone=new CloneAndRegisterMachineFromIMachineIfNotAlreadyExists(manager,workingDir,machineUtils).apply(cloneSpec);
  assertEquals(clone.getName(),cloneSpec.getVmSpec().getVmName());
}
