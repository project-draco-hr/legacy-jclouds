{
  StringBuffer buffer=new StringBuffer();
  if (this.isSecure) {
    buffer.append("https://");
  }
 else {
    buffer.append("http://");
  }
  buffer.append(this.server).append(":").append(this.port).append("/").append(bucket);
  buffer.append("/").append(Utils.urlencode(key));
  return buffer.toString();
}
