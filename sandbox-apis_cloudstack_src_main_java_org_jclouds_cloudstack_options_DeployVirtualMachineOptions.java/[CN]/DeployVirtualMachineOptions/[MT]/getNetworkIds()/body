{
  if (queryParameters.get("networkids").size() == 1) {
    return Iterables.transform(Splitter.on(",").split(Iterables.getOnlyElement(queryParameters.get("networkids"))),new Function<String,Long>(){
      @Override public Long apply(      String arg0){
        return Long.parseLong(arg0);
      }
    }
);
  }
 else {
    return ImmutableSet.<Long>of();
  }
}
