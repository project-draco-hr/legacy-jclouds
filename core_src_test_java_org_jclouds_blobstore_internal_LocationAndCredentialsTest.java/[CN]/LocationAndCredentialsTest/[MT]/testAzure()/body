{
  LocationAndCredentials creds=LocationAndCredentials.parse("blobstore://account:Base64==@azureblob/container-hyphen/prefix");
  assertEquals(creds.acccount,"account");
  assertEquals(creds.key,"Base64==");
  assertEquals(creds.uri,URI.create("blobstore://account:Base64==@azureblob/container-hyphen/prefix"));
}
