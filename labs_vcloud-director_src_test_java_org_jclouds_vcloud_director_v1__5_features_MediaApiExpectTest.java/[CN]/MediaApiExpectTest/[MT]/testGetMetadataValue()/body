{
  URI mediaUri=URI.create("https://vcloudbeta.bluelock.com/api/media/794eb334-754e-4917-b5a0-5df85cbd61d1");
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/media/794eb334-754e-4917-b5a0-5df85cbd61d1/metadata/key").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/media/metadataValue.xml",VCloudDirectorMediaType.METADATA_VALUE).httpResponseBuilder().build());
  MetadataValue expected=metadataValue();
  assertEquals(api.getMediaApi().getMetadataApi().getMetadataValue(mediaUri,"key"),expected);
}
