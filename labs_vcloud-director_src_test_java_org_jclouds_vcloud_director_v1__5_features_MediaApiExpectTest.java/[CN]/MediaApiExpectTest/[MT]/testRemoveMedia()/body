{
  URI mediaUri=URI.create(endpoint + "/media/794eb334-754e-4917-b5a0-5df85cbd61d1");
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("DELETE","/media/794eb334-754e-4917-b5a0-5df85cbd61d1").acceptMedia(VCloudDirectorMediaType.TASK).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/media/removeMediaTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=removeMediaTask();
  assertEquals(api.getMediaApi().removeMedia(mediaUri),expected);
}
