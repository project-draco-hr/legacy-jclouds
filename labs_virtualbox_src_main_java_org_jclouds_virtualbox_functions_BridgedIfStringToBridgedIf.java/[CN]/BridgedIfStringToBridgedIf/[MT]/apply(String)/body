{
  checkNotNull(rawBridgedIf,"bridged interface can't be null");
  String transformedBridgedIf=transformRawBridgedIf(rawBridgedIf);
  Map<String,String> bridgedIfMap=Splitter.on("\n").omitEmptyStrings().withKeyValueSeparator("=").split(transformedBridgedIf);
  return BridgedIf.builder().name(getValueFromMap(bridgedIfMap,BRIDGED_IF_NAME)).guid(getValueFromMap(bridgedIfMap,BRIDGED_IF_GUID)).ip(getValueFromMap(bridgedIfMap,BRIDGED_IF_IP_ADDRESS)).networkMask(getValueFromMap(bridgedIfMap,BRIDGED_IF_NETWORK_MASK)).mediumType(getValueFromMap(bridgedIfMap,BRIDGED_IF_MEDIUM_TYPE)).status(getValueFromMap(bridgedIfMap,BRIDGED_IF_STATUS)).build();
}
