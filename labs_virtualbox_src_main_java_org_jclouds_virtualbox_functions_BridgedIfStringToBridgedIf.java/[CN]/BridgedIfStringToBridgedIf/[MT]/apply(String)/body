{
  checkNotNull(rawBridgedIf,"rawBridgedIf");
  String transformedBridgedIf=transformRawBridgedIf(rawBridgedIf);
  Map<String,String> bridegedIfMap=Splitter.on("\n").omitEmptyStrings().withKeyValueSeparator("=").split(transformedBridgedIf);
  return BridgedIf.builder().name(getValueFromMap(bridegedIfMap,BRIDGED_IF_NAME)).guid(getValueFromMap(bridegedIfMap,BRIDGED_IF_GUID)).ip(getValueFromMap(bridegedIfMap,BRIDGED_IF_IP_ADDRESS)).networkMask(getValueFromMap(bridegedIfMap,BRIDGED_IF_NETWORK_MASK)).mediumType(getValueFromMap(bridegedIfMap,BRIDGED_IF_MEDIUM_TYPE)).status(getValueFromMap(bridegedIfMap,BRIDGED_IF_STATUS)).build();
}
