{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  VirtualSystem other=(VirtualSystem)obj;
  return super.equals(other) && equal(operatingSystem,other.operatingSystem) && equal(virtualHardwareSections,other.virtualHardwareSections);
}
