{
  NovaTemplateOptions templateOptions=NovaTemplateOptions.class.cast(template.getOptions());
  String zone=template.getLocation().getId();
  if (templateOptions.isAutoAssignFloatingIp()) {
    checkArgument(novaClient.getFloatingIPExtensionForZone(zone).isPresent(),"Floating IP settings are required by configuration, but the extension is not available!");
  }
  return super.execute(group,count,template,goodNodes,badNodes,customizationResponses);
}
