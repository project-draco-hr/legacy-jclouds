{
  Blob<?> blob=(Blob<?>)toBind;
  String bareKey=BlobStoreUtils.parseKey(new Key("trash",blob.getKey())).getKey();
  String file=String.format("<file><name>%s</name><mime_type>%s</mime_type><public>false</public></file>",bareKey,blob.getMetadata().getContentType());
  request.setEntity(file);
  request.getHeaders().replaceValues(HttpHeaders.CONTENT_LENGTH,Collections.singletonList(file.getBytes().length + ""));
  request.getHeaders().replaceValues(HttpHeaders.CONTENT_TYPE,Collections.singletonList("application/vnd.csp.file-info+xml"));
}
