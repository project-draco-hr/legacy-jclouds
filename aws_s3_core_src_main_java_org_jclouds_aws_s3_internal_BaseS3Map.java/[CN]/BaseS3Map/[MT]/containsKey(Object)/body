{
  try {
    return connection.headObject(bucket,key.toString()).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS) != S3Object.Metadata.NOT_FOUND;
  }
 catch (  Exception e) {
    Utils.<S3RuntimeException>rethrowIfRuntimeOrSameType(e);
    throw new S3RuntimeException(String.format("Error searching for %1$s:%2$s",bucket,key),e);
  }
}
