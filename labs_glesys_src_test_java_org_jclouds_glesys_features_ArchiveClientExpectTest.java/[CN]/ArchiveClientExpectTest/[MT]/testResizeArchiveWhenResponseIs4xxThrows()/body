{
  ArchiveClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint(URI.create("https://api.glesys.com/archive/resize/format/json")).headers(ImmutableMultimap.<String,String>builder().put("Accept","application/json").put("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").build()).payload(newUrlEncodedFormPayload(ImmutableMultimap.<String,String>builder().put("username","username1").put("size","5").build())).build(),HttpResponse.builder().statusCode(404).build()).getArchiveClient();
  client.resizeArchive("username1",5);
}
