{
  ArchiveClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint(URI.create("https://api.glesys.com/archive/create/format/json")).headers(ImmutableMultimap.<String,String>builder().put("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").build()).payload(newUrlEncodedFormPayload(ImmutableMultimap.<String,String>builder().put("username","xxxxxx_test1").put("size","5").put("password","somepass").build())).build(),HttpResponse.builder().statusCode(200).build()).getArchiveClient();
  client.createArchive("xxxxxx_test1","somepass",5);
}
