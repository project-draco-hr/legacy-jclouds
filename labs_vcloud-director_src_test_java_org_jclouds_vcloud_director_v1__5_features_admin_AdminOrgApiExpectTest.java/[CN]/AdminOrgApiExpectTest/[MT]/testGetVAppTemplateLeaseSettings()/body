{
  VCloudDirectorAdminApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/admin/org/6f312e42-cd2b-488d-a2bb-97519cd57ed0/settings/vAppTemplateLeaseSettings").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/org/admin/vAppTemplateLeaseSettings.xml",VCloudDirectorMediaType.ORG_VAPP_TEMPLATE_LEASE_SETTINGS).httpResponseBuilder().build());
  OrgVAppTemplateLeaseSettings expected=vAppTemplateLeaseSettings();
  assertEquals(api.getOrgApi().getVAppTemplateLeaseSettings(orgRef.getHref()),expected);
}
