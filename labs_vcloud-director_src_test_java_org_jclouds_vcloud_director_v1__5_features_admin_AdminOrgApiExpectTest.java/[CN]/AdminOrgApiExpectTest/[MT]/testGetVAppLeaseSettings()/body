{
  VCloudDirectorAdminApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/admin/org/6f312e42-cd2b-488d-a2bb-97519cd57ed0/settings/vAppLeaseSettings").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/org/admin/vAppLeaseSettings.xml",VCloudDirectorMediaType.ORG_LEASE_SETTINGS).httpResponseBuilder().build());
  OrgLeaseSettings expected=vAppLeaseSettings();
  assertEquals(api.getOrgApi().getVAppLeaseSettings(orgRef.getHref()),expected);
}
