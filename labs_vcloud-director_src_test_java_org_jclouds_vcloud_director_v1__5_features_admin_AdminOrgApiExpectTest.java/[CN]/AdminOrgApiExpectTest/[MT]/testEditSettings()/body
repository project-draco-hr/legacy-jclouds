{
  VCloudDirectorAdminApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT","/admin/org/6f312e42-cd2b-488d-a2bb-97519cd57ed0/settings/").xmlFilePayload("/org/admin/editSettingsSource.xml",VCloudDirectorMediaType.ORG_SETTINGS).acceptMedia(VCloudDirectorMediaType.ORG_SETTINGS).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/org/admin/editSettings.xml",VCloudDirectorMediaType.ORG_SETTINGS).httpResponseBuilder().build());
  OrgSettings expected=editSettings();
  assertEquals(api.getOrgApi().editSettings(orgRef.getHref(),expected),expected);
}
