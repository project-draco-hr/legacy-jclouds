{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Section<?> other=(Section<?>)obj;
  if (info == null) {
    if (other.info != null)     return false;
  }
 else   if (!info.equals(other.info))   return false;
  return true;
}
