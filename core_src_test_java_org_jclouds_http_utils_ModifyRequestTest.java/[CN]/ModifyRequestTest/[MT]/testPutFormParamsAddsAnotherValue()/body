{
  HttpRequest request=HttpRequest.builder().method("GET").endpoint(URI.create("http://foo")).payload(Payloads.newStringPayload("foo=bar")).build();
  Payload payload=Payloads.newStringPayload("foo=bar&foo=baz");
  payload.getContentMetadata().setContentType(MediaType.APPLICATION_FORM_URLENCODED);
  assertEquals(ModifyRequest.putFormParams(request,ImmutableMultimap.of("foo","baz")),HttpRequest.builder().method("GET").endpoint(URI.create("http://foo")).payload(payload).build());
}
