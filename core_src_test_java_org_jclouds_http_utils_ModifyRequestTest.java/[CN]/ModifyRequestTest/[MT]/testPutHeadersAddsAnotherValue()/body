{
  HttpRequest request=HttpRequest.builder().method("GET").endpoint(URI.create("http://foo")).headers(ImmutableMultimap.of("foo","bar")).build();
  assertEquals(ModifyRequest.putHeaders(request,ImmutableMultimap.of("foo","baz")),HttpRequest.builder().method("GET").endpoint(URI.create("http://foo")).headers(ImmutableMultimap.<String,String>builder().put("foo","bar").put("foo","baz").build()).build());
}
