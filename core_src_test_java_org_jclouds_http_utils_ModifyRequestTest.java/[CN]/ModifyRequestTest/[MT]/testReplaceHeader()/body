{
  HttpRequest request=HttpRequest.builder().method("GET").endpoint(URI.create("http://foo")).headers(ImmutableMultimap.of("foo","bar")).build();
  assertEquals(ModifyRequest.replaceHeader(request,"foo","baz"),HttpRequest.builder().method("GET").endpoint(URI.create("http://foo")).headers(ImmutableMultimap.of("foo","baz")).build());
}
