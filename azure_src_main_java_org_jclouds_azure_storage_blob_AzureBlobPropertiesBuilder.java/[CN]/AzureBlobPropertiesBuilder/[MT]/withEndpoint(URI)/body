{
  String account=checkNotNull(properties.getProperty(PROPERTY_AZURESTORAGE_ACCOUNT),"azureStorageAccount");
  properties.setProperty(PROPERTY_AZUREBLOB_ENDPOINT,checkNotNull(endpoint,"endpoint").toString());
  properties.setProperty(PROPERTY_AZUREBLOB_ENDPOINT,Utils.replaceAll(endpoint.toASCIIString(),ACCOUNT_PATTERN,account));
  return this;
}
