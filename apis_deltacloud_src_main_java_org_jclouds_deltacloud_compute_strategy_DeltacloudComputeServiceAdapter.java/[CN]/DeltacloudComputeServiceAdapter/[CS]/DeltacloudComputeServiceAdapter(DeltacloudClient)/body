{
  this.client=checkNotNull(client,"client");
  stateChanges=ImmutableMap.<Instance.State,Predicate<Instance>>of(Instance.State.RUNNING,new RetryablePredicate<Instance>(new InstanceRunning(client),600,1,TimeUnit.SECONDS),Instance.State.FINISH,new RetryablePredicate<Instance>(new InstanceFinished(client),30,1,TimeUnit.SECONDS));
}
