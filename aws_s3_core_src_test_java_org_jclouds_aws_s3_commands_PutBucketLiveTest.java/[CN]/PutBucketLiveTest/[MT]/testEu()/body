{
  String bucketName=getScratchBucketName();
  try {
    deleteBucket(bucketName);
    client.putBucketIfNotExists(bucketName,createIn(LocationConstraint.EU).withBucketAcl(CannedAccessPolicy.PUBLIC_READ)).get(10,TimeUnit.SECONDS);
    URL url=new URL(String.format("http://%s.s3.amazonaws.com",bucketName));
    Utils.toStringAndClose(url.openStream());
  }
  finally {
    returnScratchBucket(bucketName);
  }
}
