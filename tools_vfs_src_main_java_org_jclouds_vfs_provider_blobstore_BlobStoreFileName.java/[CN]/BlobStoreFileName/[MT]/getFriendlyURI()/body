{
  if (uriWithoutAuth != null) {
    return uriWithoutAuth;
  }
  StringBuffer sb=new StringBuffer(120);
  sb.append(getScheme());
  sb.append("://");
  sb.append(getService());
  sb.append("/");
  sb.append(getContainer());
  sb.append(getPath());
  uriWithoutAuth=sb.toString();
  return uriWithoutAuth;
}
