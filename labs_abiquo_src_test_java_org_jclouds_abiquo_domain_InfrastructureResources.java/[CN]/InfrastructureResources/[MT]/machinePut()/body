  machine.addLink(new RESTLink("ledoff","http://localhost/api/admin/datacenters/1/racks/1/machines/1/action/ledoff"));
  machine.addLink(new RESTLink("ledon","http://localhost/api/admin/datacenters/1/racks/1/machines/1/action/ledon"));
