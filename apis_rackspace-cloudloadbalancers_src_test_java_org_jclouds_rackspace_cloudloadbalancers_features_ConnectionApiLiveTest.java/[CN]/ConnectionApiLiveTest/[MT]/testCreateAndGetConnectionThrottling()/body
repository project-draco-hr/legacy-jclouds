{
  clbApi.getConnectionApiForZoneAndLoadBalancer(zone,lb.getId()).createOrUpdateConnectionThrottle(ConnectionApiExpectTest.getConnectionThrottle());
  assertTrue(awaitAvailable(clbApi.getLoadBalancerApiForZone(zone)).apply(lb));
  ConnectionThrottle connectionThrottle=clbApi.getConnectionApiForZoneAndLoadBalancer(zone,lb.getId()).getConnectionThrottle();
  assertEquals(connectionThrottle,ConnectionApiExpectTest.getConnectionThrottle());
}
