{
  return Maps.transformValues(sessionSupplier.get().getOrgs(),new Function<NamedResource,NamedResource>(){
    @SuppressWarnings("deprecation") @Override public NamedResource apply(    NamedResource from){
      return new NamedResourceImpl(from.getId(),from.getName(),TerremarkVCloudExpressMediaType.KEYSLIST_XML,URI.create(from.getLocation().toASCIIString() + "/keysList"));
    }
  }
);
}
