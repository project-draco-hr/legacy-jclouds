{
  String value=metadataValue.getValue();
  assertNotNull(value,String.format(OBJ_FIELD_ATTRB_REQ,api,"MetadataEntry",metadataValue.toString(),"value"));
  assertEquals(value,expectedValue,String.format(OBJ_FIELD_EQ,api,"metadataEntry.value",expectedValue,value));
  checkResourceType(metadataValue);
}
