{
  String value=metadataValue.getValue();
  assertNotNull(value,String.format(OBJ_FIELD_ATTRB_REQ,client,"MetadataEntry",metadataValue.toString(),"value"));
  assertEquals(value,expectedValue,String.format(OBJ_FIELD_EQ,client,"metadataEntry.value",expectedValue,value));
  checkResourceType(metadataValue);
}
