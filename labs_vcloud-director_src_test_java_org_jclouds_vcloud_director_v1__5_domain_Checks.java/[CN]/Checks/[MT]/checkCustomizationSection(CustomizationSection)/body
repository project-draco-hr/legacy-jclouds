{
  assertNotNull(val,String.format(NOT_NULL_OBJECT_FMT,"CustomizationSection",""));
  if (val.getLinks() != null) {
    for (    Link link : val.getLinks()) {
      checkLink(link);
    }
  }
  if (val.getType() != null) {
    checkType(val.getType());
  }
  if (val.getHref() != null) {
    checkHref(val.getHref());
  }
  checkOvfSectionType(val);
}
