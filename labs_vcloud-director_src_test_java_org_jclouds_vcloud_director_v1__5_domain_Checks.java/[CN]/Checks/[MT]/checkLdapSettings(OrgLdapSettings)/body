{
  if (settings.getOrgLdapMode() != null) {
    assertTrue(LdapMode.ALL.contains(settings.getOrgLdapMode()),String.format(REQUIRED_VALUE_OBJECT_FMT,"LdapMode","OrdLdapSettings",settings.getOrgLdapMode(),Iterables.toString(OrgLdapSettings.LdapMode.ALL)));
  }
  if (settings.getCustomOrgLdapSettings() != null) {
    checkCustomOrgLdapSettings(settings.getCustomOrgLdapSettings());
  }
  checkResourceType(settings);
}
