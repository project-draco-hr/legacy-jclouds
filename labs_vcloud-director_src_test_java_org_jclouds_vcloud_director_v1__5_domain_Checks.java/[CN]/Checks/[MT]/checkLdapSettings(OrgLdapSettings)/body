{
  if (settings.getLdapMode() != null) {
    assertTrue(LdapMode.ALL.contains(settings.getLdapMode()),String.format(REQUIRED_VALUE_OBJECT_FMT,"LdapMode","OrdLdapSettings",settings.getLdapMode(),Iterables.toString(OrgLdapSettings.LdapMode.ALL)));
  }
  if (settings.getCustomOrgLdapSettings() != null) {
    checkCustomOrgLdapSettings(settings.getCustomOrgLdapSettings());
  }
  checkResourceType(settings);
}
