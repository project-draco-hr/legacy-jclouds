{
  if (settings.getStorageLeaseSeconds() != null) {
    assertTrue(settings.getStorageLeaseSeconds() >= 0,String.format(OBJ_FIELD_GTE_0,"OrgLeaseSettings","storageLeaseSeconds",settings.getStorageLeaseSeconds()));
  }
  if (settings.getDeploymentLeaseSeconds() != null) {
    assertTrue(settings.getDeploymentLeaseSeconds() >= 0,String.format(OBJ_FIELD_GTE_0,"OrgLeaseSettings","deploymentLeaseSeconds",settings.getDeploymentLeaseSeconds()));
  }
  checkResource(settings);
}
