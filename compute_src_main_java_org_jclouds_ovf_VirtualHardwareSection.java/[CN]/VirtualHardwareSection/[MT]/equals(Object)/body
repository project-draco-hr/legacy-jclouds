{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (getClass() != obj.getClass())   return false;
  VirtualHardwareSection other=(VirtualHardwareSection)obj;
  if (items == null) {
    if (other.items != null)     return false;
  }
 else   if (!items.equals(other.items))   return false;
  if (transports == null) {
    if (other.transports != null)     return false;
  }
 else   if (!transports.equals(other.transports))   return false;
  if (virtualSystem == null) {
    if (other.virtualSystem != null)     return false;
  }
 else   if (!virtualSystem.equals(other.virtualSystem))   return false;
  return true;
}
