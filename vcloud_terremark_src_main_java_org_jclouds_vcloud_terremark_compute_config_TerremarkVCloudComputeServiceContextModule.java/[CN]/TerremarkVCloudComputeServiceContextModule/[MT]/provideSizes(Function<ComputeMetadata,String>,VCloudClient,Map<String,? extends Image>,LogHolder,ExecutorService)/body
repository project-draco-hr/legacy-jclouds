{
  Image anyImage=Iterables.get(images.values(),0);
  holder.logger.debug(">> providing sizes");
  SortedSet<Size> sizes=Sets.newTreeSet(Iterables.transform(TerremarkVCloudClient.class.cast(client).getComputeOptionsOfCatalogItem(anyImage.getId()),sizeConverter));
  holder.logger.debug("<< sizes(%d)",sizes.size());
  return Maps.uniqueIndex(sizes,indexer);
}
