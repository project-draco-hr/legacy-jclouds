{
  Image anyImage=Iterables.get(images,0);
  holder.logger.debug(">> providing sizes");
  LinkedHashSet<Size> sizes=Sets.newLinkedHashSet(Iterables.transform(client.getComputeOptionsOfCatalogItem(anyImage.getId()),sizeConverter));
  holder.logger.debug("<< sizes(%d)",sizes.size());
  return sizes;
}
