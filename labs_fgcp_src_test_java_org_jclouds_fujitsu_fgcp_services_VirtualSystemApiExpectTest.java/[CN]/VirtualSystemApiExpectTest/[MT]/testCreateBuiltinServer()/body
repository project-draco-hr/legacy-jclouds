{
  HttpRequest request=buildGETWithQuery("Action=CreateEFM" + "&efmType=SLB" + "&efmName=web%20load%20balancer"+ "&networkId=ABCDEFGH-A123B456CE-N-DMZ"+ "&vsysId=ABCDEFGH-A123B456CE");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/CreateEFM-response.xml")).build();
  VirtualSystemApi api=requestSendsResponse(request,response).getVirtualSystemApi();
  String id=api.createBuiltinServer("web load balancer","ABCDEFGH-A123B456CE-N-DMZ");
  assertEquals(id,"CONTRACT-EFM00001");
}
