{
  HttpRequest request=buildGETWithQuery("Action=ListVDisk" + "&vsysId=ABCDEFGH-A123B456CE");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/ListVDisk-response.xml")).build();
  VirtualSystemApi api=requestSendsResponse(request,response).getVirtualSystemApi();
  Set<VDisk> disks=api.listDisks("ABCDEFGH-A123B456CE");
  assertNotNull(disks,"disks");
  assertEquals(disks.size(),1);
}
