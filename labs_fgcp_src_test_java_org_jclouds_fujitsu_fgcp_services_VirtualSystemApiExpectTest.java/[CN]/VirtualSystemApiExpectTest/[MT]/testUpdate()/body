{
  HttpRequest request=buildGETWithQuery("Action=UpdateVSYSAttribute" + "&vsysId=ABCDEFGH-A123B456CE" + "&attributeName=updateName"+ "&attributeValue=new-name");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/UpdateVDiskAttribute-response.xml")).build();
  VirtualSystemApi api=requestSendsResponse(request,response).getVirtualSystemApi();
  api.update("ABCDEFGH-A123B456CE","updateName","new-name");
}
