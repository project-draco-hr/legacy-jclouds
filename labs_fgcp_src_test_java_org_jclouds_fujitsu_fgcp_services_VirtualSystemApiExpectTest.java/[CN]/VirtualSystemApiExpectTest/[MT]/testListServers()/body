{
  HttpRequest request=buildGETWithQuery("Action=ListVServer" + "&vsysId=ABCDEFGH-A123B456CE");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/ListVServer-response.xml")).build();
  VirtualSystemApi api=requestSendsResponse(request,response).getVirtualSystemApi();
  Set<VServer> servers=api.listServers("ABCDEFGH-A123B456CE");
  assertNotNull(servers,"servers");
  assertEquals(servers.size(),2);
}
