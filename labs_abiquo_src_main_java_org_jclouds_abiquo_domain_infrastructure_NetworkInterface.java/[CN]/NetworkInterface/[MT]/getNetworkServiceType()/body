{
  RESTLink link=target.getNetworkServiceTypeLink();
  ExtendedUtils utils=(ExtendedUtils)context.getUtils();
  HttpResponse response=utils.getAbiquoHttpClient().get(link);
  ParseXMLWithJAXB<NetworkServiceTypeDto> parser=new ParseXMLWithJAXB<NetworkServiceTypeDto>(utils.getXml(),TypeLiteral.get(NetworkServiceTypeDto.class));
  return wrap(context,NetworkServiceType.class,parser.apply(response));
}
