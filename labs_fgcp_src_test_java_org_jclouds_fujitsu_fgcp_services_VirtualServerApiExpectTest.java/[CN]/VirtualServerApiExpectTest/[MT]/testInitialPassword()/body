{
  HttpRequest request=buildGETWithQuery("Action=GetVServerInitialPassword" + "&vserverId=CONTRACT-VSYS00001-S-0005" + "&vsysId=CONTRACT-VSYS00001");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/GetVServerInitialPassword-response.xml")).build();
  VirtualServerApi api=requestSendsResponse(request,response).getVirtualServerApi();
  assertEquals(api.getInitialPassword("CONTRACT-VSYS00001-S-0005"),"mySecretpwd1");
}
