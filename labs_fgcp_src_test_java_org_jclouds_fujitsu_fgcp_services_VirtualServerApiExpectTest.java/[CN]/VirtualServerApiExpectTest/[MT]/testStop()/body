{
  HttpRequest request=buildGETWithQuery("Action=StopVServer" + "&vserverId=CONTRACT-VSYS00001-S-0005" + "&vsysId=CONTRACT-VSYS00001");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/StopVServer-response.xml")).build();
  VirtualServerApi api=requestSendsResponse(request,response).getVirtualServerApi();
  api.stop("CONTRACT-VSYS00001-S-0005");
}
