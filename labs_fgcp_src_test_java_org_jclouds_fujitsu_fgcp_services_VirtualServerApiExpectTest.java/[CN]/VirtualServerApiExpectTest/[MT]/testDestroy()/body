{
  HttpRequest request=buildGETWithQuery("Action=DestroyVServer" + "&vserverId=CONTRACT-VSYS00001-S-0005" + "&vsysId=CONTRACT-VSYS00001");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/DestroyVServer-response.xml")).build();
  VirtualServerApi api=requestSendsResponse(request,response).getVirtualServerApi();
  api.destroy("CONTRACT-VSYS00001-S-0005");
}
