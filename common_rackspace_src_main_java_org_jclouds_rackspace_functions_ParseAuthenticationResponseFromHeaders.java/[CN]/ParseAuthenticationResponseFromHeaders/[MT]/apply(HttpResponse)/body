{
  releasePayload(from);
  AuthenticationResponse response=new AuthenticationResponseImpl(checkNotNull(from.getFirstHeaderOrNull(AUTH_TOKEN),AUTH_TOKEN),getURI(from,CDN_MANAGEMENT_URL),getURI(from,SERVER_MANAGEMENT_URL),getURI(from,STORAGE_URL));
  logger.debug("will connect to: ",response);
  return response;
}
