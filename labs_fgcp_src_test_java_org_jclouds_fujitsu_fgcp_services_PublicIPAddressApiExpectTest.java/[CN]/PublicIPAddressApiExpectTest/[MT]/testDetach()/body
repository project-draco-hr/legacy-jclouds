{
  HttpRequest request=buildGETWithQuery("Action=DetachPublicIP" + "&vsysId=CONTRACT-VSYS00001" + "&publicIp=123.45.67.89");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/DetachPublicIP-response.xml")).build();
  PublicIPAddressApi api=requestSendsResponse(request,response).getPublicIPAddressApi();
  api.detach("CONTRACT-VSYS00001","123.45.67.89");
}
