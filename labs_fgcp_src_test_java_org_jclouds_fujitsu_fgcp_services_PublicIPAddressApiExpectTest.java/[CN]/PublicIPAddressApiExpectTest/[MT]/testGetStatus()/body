{
  HttpRequest request=buildGETWithQuery("Action=GetPublicIPStatus" + "&publicIp=123.45.67.89");
  HttpResponse response=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/GetPublicIPStatus-response.xml")).build();
  PublicIPAddressApi api=requestSendsResponse(request,response).getPublicIPAddressApi();
  PublicIPStatus status=api.getStatus("123.45.67.89");
  assertEquals(status,PublicIPStatus.ATTACHED);
}
