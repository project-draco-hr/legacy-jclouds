{
  HttpRequest get=HttpRequest.builder().method("GET").endpoint("https://glance.jclouds.org:9292/v1.0/images/fcc451d0-f6e4-4824-ad8f-70ec12326d07").addHeader("X-Auth-Token",authToken).build();
  HttpResponse getResponse=HttpResponse.builder().statusCode(404).build();
  GlanceApi apiWhenNoExist=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,get,getResponse);
  assertNull(apiWhenNoExist.getImageApiForZone("az-1.region-a.geo-1").getAsStream("fcc451d0-f6e4-4824-ad8f-70ec12326d07"));
}
