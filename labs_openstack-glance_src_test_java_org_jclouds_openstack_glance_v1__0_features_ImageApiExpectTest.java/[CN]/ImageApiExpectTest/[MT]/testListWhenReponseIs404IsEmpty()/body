{
  HttpRequest list=HttpRequest.builder().method("GET").endpoint("https://glance.jclouds.org:9292/v1.0/images").addHeader("Accept","application/json").addHeader("X-Auth-Token",authToken).build();
  HttpResponse listResponse=HttpResponse.builder().statusCode(404).build();
  GlanceApi apiWhenNoExist=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,list,listResponse);
  assertTrue(apiWhenNoExist.getImageApiForZone("az-1.region-a.geo-1").list().concat().isEmpty());
}
