{
  HttpRequest list=HttpRequest.builder().method("GET").endpoint("https://glance.jclouds.org:9292/v1.0/images").addHeader("Accept","application/json").addHeader("X-Auth-Token",authToken).build();
  HttpResponse listResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/images.json")).build();
  GlanceApi apiWhenExist=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,list,listResponse);
  assertEquals(apiWhenExist.getConfiguredZones(),ImmutableSet.of("az-1.region-a.geo-1"));
  assertEquals(apiWhenExist.getImageApiForZone("az-1.region-a.geo-1").list().concat().toString(),new ParseImagesTest().expected().toString());
}
