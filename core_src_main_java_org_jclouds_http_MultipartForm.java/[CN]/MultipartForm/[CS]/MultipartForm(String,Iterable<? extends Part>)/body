{
  this.parts=parts;
  String boundaryrn=boundary + rn;
  isRepeatable=true;
  InputSupplier<? extends InputStream> chain=ByteStreams.join();
  for (  Part part : parts) {
    if (!part.isRepeatable())     isRepeatable=false;
    size+=part.calculateSize();
    chain=ByteStreams.join(chain,addLengthAndReturnHeaders(boundaryrn,part),part,addLengthAndReturnRn());
  }
  chain=ByteStreams.join(chain,addLengthAndReturnFooter(boundary));
  this.chain=chain;
}
