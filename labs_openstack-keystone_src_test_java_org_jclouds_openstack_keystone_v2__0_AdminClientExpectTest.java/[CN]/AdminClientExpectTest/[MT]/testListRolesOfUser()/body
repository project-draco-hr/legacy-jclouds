{
  AdminClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,standardRequestBuilder(endpoint + "/v2.0/users/3f6c1c9ba993495ead7d2eb2192e284f/roles").build(),standardResponseBuilder(200).payload(payloadFromResourceWithContentType("/user_role_list.json",APPLICATION_JSON)).build()).getAdminClientForRegion("region-a.geo-1");
  Set<Role> roles=client.listRolesOfUser("3f6c1c9ba993495ead7d2eb2192e284f");
  assertNotNull(roles);
  assertFalse(roles.isEmpty());
  assertEquals(roles,ImmutableSet.of(Role.builder().id("79cada5c02814b57a52e0eed4dd388cb").name("admin").build()));
}
