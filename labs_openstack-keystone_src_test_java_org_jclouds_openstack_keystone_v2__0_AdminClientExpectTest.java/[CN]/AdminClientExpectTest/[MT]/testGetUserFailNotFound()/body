{
  AdminClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,standardRequestBuilder(endpoint + "/v2.0/users/f021dfd758eb44a89f1c57c8ef3be8e2").build(),standardResponseBuilder(404).build()).getAdminClientForRegion("region-a.geo-1");
  assertNull(client.getUser("f021dfd758eb44a89f1c57c8ef3be8e2"));
}
