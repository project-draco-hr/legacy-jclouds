{
  String account=checkNotNull(properties.getProperty(AzureStorageConstants.PROPERTY_AZURESTORAGE_ACCOUNT),"azureStorageAccount");
  properties.setProperty(AzureQueueConstants.PROPERTY_AZUREQUEUE_ENDPOINT,checkNotNull(endpoint,"endpoint").toString());
  properties.setProperty(AzureQueueConstants.PROPERTY_AZUREQUEUE_ENDPOINT,Utils.replaceAll(endpoint.toASCIIString(),ACCOUNT_PATTERN,account));
  return this;
}
