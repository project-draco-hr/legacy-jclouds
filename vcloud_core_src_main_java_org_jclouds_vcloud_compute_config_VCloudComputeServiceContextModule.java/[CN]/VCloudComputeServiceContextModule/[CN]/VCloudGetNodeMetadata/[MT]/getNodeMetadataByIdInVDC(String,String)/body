{
  VApp vApp=client.getVApp(id);
  String tag=vApp.getName().replaceAll("-[0-9]+","");
  return new NodeMetadataImpl(vApp.getId(),vApp.getName(),vDCId,vApp.getLocation(),ImmutableMap.<String,String>of(),tag,vAppStatusToNodeState.get(vApp.getStatus()),computeClient.getPublicAddresses(id),computeClient.getPrivateAddresses(id),ImmutableMap.<String,String>of(),null);
}
