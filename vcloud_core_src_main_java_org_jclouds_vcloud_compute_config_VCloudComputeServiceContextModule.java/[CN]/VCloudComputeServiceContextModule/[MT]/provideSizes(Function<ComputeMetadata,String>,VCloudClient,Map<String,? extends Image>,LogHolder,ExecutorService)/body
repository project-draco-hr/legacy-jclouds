{
  Set<Size> sizes=Sets.newHashSet();
  for (  int cpus : new int[]{1,2,4})   for (  int ram : new int[]{512,1024,2048,4096,8192,16384})   sizes.add(new SizeImpl(String.format("cpu=%d,ram=%s,disk=%d",cpus,ram,10),null,null,null,ImmutableMap.<String,String>of(),cpus,ram,10,ImmutableSet.<Architecture>of(Architecture.X86_32,Architecture.X86_64)));
  return Maps.uniqueIndex(sizes,indexer);
}
