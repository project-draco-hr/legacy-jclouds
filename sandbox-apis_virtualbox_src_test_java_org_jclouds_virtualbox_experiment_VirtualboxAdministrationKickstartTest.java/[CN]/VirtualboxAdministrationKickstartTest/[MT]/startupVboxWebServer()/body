{
  logger().debug("disabling password access");
  runScriptOnNode(hostId,"VBoxManage setproperty websrvauthlibrary null");
  logger().debug("starting vboxwebsrv");
  String vboxwebsrv="vboxwebsrv -t 5 -v";
  if (isOSX(hostId))   vboxwebsrv="cd /Applications/VirtualBox.app/Contents/MacOS/&&" + vboxwebsrv;
  runScriptOnNode(hostId,vboxwebsrv,blockOnPort(endpoint.getPort(),10).blockOnComplete(false).nameTask("vboxwebsrv"));
}
