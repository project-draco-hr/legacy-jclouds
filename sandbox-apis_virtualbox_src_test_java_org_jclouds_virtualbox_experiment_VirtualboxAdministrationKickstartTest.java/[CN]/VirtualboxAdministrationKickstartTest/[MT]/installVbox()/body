{
  SshClient client=setupSshClient();
  try {
    client.exec("echo " + sshPass + " | "+ installVboxOse);
  }
 catch (  Exception e) {
    System.out.println("It is impossible to install virtualbox with this command " + installVboxOse);
  }
 finally {
    if (client != null)     client.disconnect();
  }
}
