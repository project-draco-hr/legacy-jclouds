{
  Exception exception=null;
  try {
    vdcApi.disableVdc(adminVdcUri);
  }
 catch (  Exception e) {
    exception=e;
  }
 finally {
    try {
      vdcApi.enableVdc(adminVdcUri);
    }
 catch (    Exception e) {
      if (exception != null) {
        logger.warn(e,"Error resetting adminVdc.name; rethrowing original test exception...");
        throw exception;
      }
 else {
        throw e;
      }
    }
  }
}
