{
  if (metadataKey != null) {
    try {
      Task task=metadataApi.deleteEntry(lazyGetVdc().getHref(),metadataKey);
      taskDoneEventually(task);
    }
 catch (    VCloudDirectorException e) {
      logger.warn(e,"Error deleting metadata-value (perhaps it doesn't exist?); continuing...");
    }
  }
}
