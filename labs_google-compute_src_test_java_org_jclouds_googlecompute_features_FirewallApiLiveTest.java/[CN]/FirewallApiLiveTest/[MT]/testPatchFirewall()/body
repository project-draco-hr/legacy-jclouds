{
  FirewallOptions firewall=new FirewallOptions().name(FIREWALL_NAME).network(getNetworkUrl(getUserProject(),FIREWALL_NETWORK_NAME)).allowedRules(ImmutableSet.of(Firewall.Rule.builder().IPProtocol(IPProtocol.TCP).addPort(22).build(),Firewall.Rule.builder().IPProtocol(IPProtocol.TCP).addPort(23).build())).addSourceRange("10.0.0.0/8").addSourceTag("tag1").addTargetTag("tag2");
  assertOperationDoneSucessfully(api().update(FIREWALL_NAME,firewall),TIME_WAIT);
}
