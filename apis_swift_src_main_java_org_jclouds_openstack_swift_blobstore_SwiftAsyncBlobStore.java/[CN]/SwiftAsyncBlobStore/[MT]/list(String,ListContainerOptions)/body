{
  org.jclouds.openstack.swift.options.ListContainerOptions httpOptions=container2ContainerListOptions.apply(options);
  ListenableFuture<PageSet<ObjectInfo>> returnVal=async.listObjects(container,httpOptions);
  ListenableFuture<PageSet<? extends StorageMetadata>> list=Futures.compose(returnVal,container2ResourceList,service);
  return options.isDetailed() ? Futures.compose(list,fetchBlobMetadataProvider.get().setContainerName(container),service) : list;
}
