{
  S3Object returnVal=get(key);
  try {
    connection.putObject(bucket,value).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS);
  }
 catch (  Exception e) {
    Utils.<S3RuntimeException>rethrowIfRuntimeOrSameType(e);
    throw new S3RuntimeException(String.format("Error putting object %1s:%2s%n%1s",bucket,key,value),e);
  }
  return returnVal;
}
