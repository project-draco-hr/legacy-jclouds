{
  try {
    return connection.getObject(bucket,key.toString()).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS);
  }
 catch (  Exception e) {
    Utils.<S3RuntimeException>rethrowIfRuntimeOrSameType(e);
    throw new S3RuntimeException(String.format("Error geting object %1$s:%2$s",bucket,key),e);
  }
}
