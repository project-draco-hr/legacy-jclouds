{
  ImmutableMap.Builder<String,String> builder=ImmutableMap.builder();
  if (input.getPublicKey() != null) {
    builder.put("sshKeys",format("%s:%s %s@localhost",checkNotNull(input.getLoginUser(),"loginUser cannot be null"),input.getPublicKey(),input.getLoginUser()));
  }
  builder.putAll(input.getUserMetadata());
  return builder;
}
