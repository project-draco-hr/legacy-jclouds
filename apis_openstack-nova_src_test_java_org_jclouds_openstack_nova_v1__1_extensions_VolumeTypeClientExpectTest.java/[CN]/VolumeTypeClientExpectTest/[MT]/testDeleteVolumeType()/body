{
  URI endpoint=URI.create("https://az-1.region-a.geo-1.compute.hpcloudsvc.com/v1.1/3456/os-volume-types/8");
  VolumeTypeClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,extensionsOfNovaRequest,extensionsOfNovaResponse,standardRequestBuilder(endpoint).method("DELETE").build(),standardResponseBuilder(200).build()).getVolumeTypeExtensionForZone("az-1.region-a.geo-1").get();
  assertTrue(client.deleteVolumeType("8"));
}
