{
  Iterable<? extends Resource> wildcardExtendsResources=iterable.fromJson("[{\"id\":\"i-foo\",\"name\":\"foo\"},{\"id\":\"i-bar\",\"name\":\"bar\"}]",iterableWildcardExtendsResourceType);
  assertEquals(wildcardExtendsResources,ImmutableList.of(new Resource("i-foo","foo"),new Resource("i-bar","bar")));
}
