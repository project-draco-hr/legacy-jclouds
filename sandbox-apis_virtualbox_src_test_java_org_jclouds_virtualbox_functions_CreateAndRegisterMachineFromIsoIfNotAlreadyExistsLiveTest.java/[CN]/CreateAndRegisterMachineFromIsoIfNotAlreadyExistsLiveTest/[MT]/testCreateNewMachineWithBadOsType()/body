{
  String vmName="jclouds-test-create-2-node";
  VmSpec launchSpecification=VmSpec.builder().id(vmName).name(vmName).memoryMB(512).controller(ideController).cleanUpMode(mode).osTypeId("SomeWeirdUnknownOs").forceOverwrite(true).build();
  undoVm(launchSpecification);
  try {
    context.utils().injector().getInstance(CreateAndRegisterMachineFromIsoIfNotAlreadyExists.class).apply(launchSpecification);
    fail();
  }
 catch (  VBoxException e) {
    ErrorCode errorCode=ErrorCode.valueOf(e);
    assertEquals(errorCode,ErrorCode.VBOX_E_OBJECT_NOT_FOUND);
  }
 finally {
    undoVm(launchSpecification);
  }
}
