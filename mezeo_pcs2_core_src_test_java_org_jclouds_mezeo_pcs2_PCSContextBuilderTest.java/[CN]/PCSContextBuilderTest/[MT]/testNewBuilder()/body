{
  PCSContextBuilder builder=new PCSContextBuilder(URI.create("https://localhost:8080"),"id","secret");
  assertEquals(builder.getProperties().getProperty(PCSConstants.PROPERTY_PCS2_ENDPOINT),"https://localhost:8080");
  assertEquals(builder.getProperties().getProperty(PCSConstants.PROPERTY_PCS2_USER),"id");
  assertEquals(builder.getProperties().getProperty(PCSConstants.PROPERTY_PCS2_PASSWORD),"secret");
}
