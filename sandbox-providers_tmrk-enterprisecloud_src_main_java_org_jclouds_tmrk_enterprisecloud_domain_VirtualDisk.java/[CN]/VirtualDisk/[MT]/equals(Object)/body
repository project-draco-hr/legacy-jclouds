{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  VirtualDisk disk=(VirtualDisk)o;
  if (index != disk.index)   return false;
  if (name != null ? !name.equals(disk.name) : disk.name != null)   return false;
  if (size != null ? !size.equals(disk.size) : disk.size != null)   return false;
  return true;
}
