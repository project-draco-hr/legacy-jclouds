{
  InputStream input=null;
  try {
    input=resourceLoader.getResource("/WEB-INF/jclouds.properties").getInputStream();
    props.load(input);
    if (!initializing)     initialize();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
 finally {
    Closeables.closeQuietly(input);
  }
}
