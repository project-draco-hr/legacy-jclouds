{
  Properties props=loadJCloudsProperties(servletContextEvent);
  ImmutableSet<Module> modules=ImmutableSet.<Module>of(new GoogleAppEngineConfigurationModule());
  try {
    blobsStoreContexts=ImmutableMap.<String,BlobStoreContext>of("s3",new BlobStoreContextFactory().createContext("s3",modules,props));
    computeServiceContexts=ImmutableMap.<String,ComputeServiceContext>of("ec2",new ComputeServiceContextFactory().createContext("ec2",modules,props));
  }
 catch (  IOException e) {
    Throwables.propagate(e);
  }
  super.contextInitialized(servletContextEvent);
}
