{
  String[] sequenceSplited=keyboardSequence.split(" ");
  StringBuilder sb=new StringBuilder();
  for (  String word : sequenceSplited) {
    String converted=stringToKeycode(word);
    for (    String string : converted.split("  ")) {
      sb.append("vboxmanage controlvm " + vmName + " keyboardputscancode "+ string+ "; ");
      if (string.contains(KeyboardScancodes.SPECIAL_KEYBOARD_BUTTON_MAP.get("<Return>"))) {
        runScriptOnNode(hostId,sb.toString(),runAsRoot(false).wrapInInitScript(false));
        sb=new StringBuilder();
      }
    }
  }
  runScriptOnNode(hostId,sb.toString(),runAsRoot(false).wrapInInitScript(false));
}
