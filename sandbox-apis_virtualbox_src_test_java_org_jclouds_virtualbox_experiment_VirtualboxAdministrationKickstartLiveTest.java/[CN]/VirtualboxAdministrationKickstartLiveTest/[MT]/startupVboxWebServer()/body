{
  logger().debug("disabling password access");
  runScriptOnNode(hostId,"VBoxManage setproperty websrvauthlibrary null",runAsRoot(false).wrapInInitScript(false));
  logger().debug("starting vboxwebsrv");
  String vboxwebsrv="vboxwebsrv -t 10000 -v -b";
  if (isOSX(hostId))   vboxwebsrv="cd /Applications/VirtualBox.app/Contents/MacOS/ && " + vboxwebsrv;
  runScriptOnNode(hostId,vboxwebsrv,runAsRoot(false).wrapInInitScript(false).blockOnPort(endpoint.getPort(),10).blockOnComplete(false).nameTask("vboxwebsrv"));
}
