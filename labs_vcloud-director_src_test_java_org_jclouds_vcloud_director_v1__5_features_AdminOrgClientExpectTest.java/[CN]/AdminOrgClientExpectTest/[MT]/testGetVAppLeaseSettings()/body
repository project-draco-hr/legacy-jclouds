{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/admin/org/???/settings/vAppLeaseSettings").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/org/admin/vAppLeaseSettings.xml",VCloudDirectorMediaType.ORG_LEASE_SETTINGS).httpResponseBuilder().build());
  OrgLeaseSettings expected=orgVAppLeaseSettings();
  assertEquals(client.getAdminOrgClient().getVAppLeaseSettings(orgRef.getURI()),expected);
}
