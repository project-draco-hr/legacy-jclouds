{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/admin/org/6f312e42-cd2b-488d-a2bb-97519cd57ed0/settings/ldap").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/org/admin/ldapSettings.xml",VCloudDirectorMediaType.ORG_LDAP_SETTINGS).httpResponseBuilder().build());
  OrgLdapSettings expected=ldapSettings();
  assertEquals(client.getAdminOrgClient().getLdapSettings(orgRef.getURI()),expected);
}
