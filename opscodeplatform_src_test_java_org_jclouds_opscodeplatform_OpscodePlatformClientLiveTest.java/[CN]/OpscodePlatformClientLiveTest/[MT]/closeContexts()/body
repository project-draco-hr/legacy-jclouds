{
  if (orgUser != null)   adminConnection.getApi().deleteUser(PREFIX);
  if (createdOrgname != null)   adminConnection.getApi().deleteOrganization(createdOrgname);
  if (clientConnection != null)   clientConnection.close();
  if (validatorConnection != null)   validatorConnection.close();
  if (adminConnection != null)   adminConnection.close();
}
