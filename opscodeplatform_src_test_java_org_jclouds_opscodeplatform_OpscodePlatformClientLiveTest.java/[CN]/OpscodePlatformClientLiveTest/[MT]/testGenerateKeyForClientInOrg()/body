{
  clientKey=validatorConnection.utils().encryption().toPem(validatorConnection.getApi().getChefClientForOrg(orgname).createClient(PREFIX).getPrivateKey());
  assertNotNull(clientKey);
  clientConnection.close();
  clientConnection=createConnection(PREFIX,clientKey);
  clientConnection.getApi().getChefClientForOrg(orgname).clientExists(PREFIX);
}
