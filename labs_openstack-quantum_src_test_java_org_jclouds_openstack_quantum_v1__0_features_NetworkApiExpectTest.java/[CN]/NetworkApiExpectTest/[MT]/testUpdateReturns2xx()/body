{
  NetworkApi api=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,authenticatedGET().endpoint(endpoint + "/tenants/3456/networks/12345").method("PUT").payload(payloadFromStringWithContentType("{\"network\":{\"name\":\"another-test\"}}",MediaType.APPLICATION_JSON)).build(),HttpResponse.builder().statusCode(200).build()).getNetworkApiForRegion("region-a.geo-1");
  assertTrue(api.rename("12345","another-test"));
}
