{
  NetworkApi api=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,authenticatedGET().endpoint(endpoint + "/tenants/3456/networks").build(),HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/list_network_refs.json",APPLICATION_JSON)).build()).getNetworkApiForRegion("region-a.geo-1");
  Set<? extends Reference> nets=api.listReferences();
  assertEquals(nets,listOfNetworkRefs());
}
