{
  InputStream is=getClass().getResourceAsStream("/task-error.xml");
  Task result=factory.create(injector.getInstance(TaskHandler.class)).parse(is);
  Task expects=Task.builder().startTime(dateService.iso8601DateParse("2010-05-26T08:08:08.000+08:00")).endTime(dateService.iso8601DateParse("2010-05-26T08:09:09.000+08:00")).status(Task.Status.ERROR).error(TaskError.builder().vendorSpecificErrorCode("0").minorErrorCode(0).majorErrorCode(0).message("There is an internal exception occured in System, please kindly contact savvis support team to get solution.").build()).build();
  assertEquals(result.toString(),expects.toString());
}
