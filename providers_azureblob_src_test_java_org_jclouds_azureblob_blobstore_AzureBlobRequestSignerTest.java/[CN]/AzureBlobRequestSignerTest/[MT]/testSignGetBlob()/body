{
  HttpRequest request=signer.signGetBlob("container","name");
  assertRequestLineEquals(request,"GET https://identity.blob.core.windows.net/container/name HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"Authorization: SharedKeyLite identity:nI6ca9CdLWhPoMuSzk4perqx5pzi2hx7YBJ92FCqeXM=\nDate: Thu, 05 Jun 2008 16:38:19 GMT\nx-ms-version: 2009-09-19\n");
  assertPayloadEquals(request,null,null,false);
  assertEquals(request.getFilters().size(),0);
}
