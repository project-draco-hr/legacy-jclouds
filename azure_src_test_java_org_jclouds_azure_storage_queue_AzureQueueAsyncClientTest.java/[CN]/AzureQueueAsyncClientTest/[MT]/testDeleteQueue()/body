{
  Method method=AzureQueueAsyncClient.class.getMethod("deleteQueue",String.class);
  GeneratedHttpRequest<AzureQueueAsyncClient> httpMethod=processor.createRequest(method,new Object[]{"queue"});
  assertEquals(httpMethod.getEndpoint().getHost(),"localhost");
  assertEquals(httpMethod.getEndpoint().getPath(),"/queue");
  assertEquals(httpMethod.getEndpoint().getQuery(),null);
  assertEquals(httpMethod.getMethod(),HttpMethod.DELETE);
  assertEquals(httpMethod.getHeaders().size(),1);
  assertEquals(httpMethod.getHeaders().get("x-ms-version"),Collections.singletonList("2009-09-19"));
  assertEquals(processor.createResponseParser(method,httpMethod).getClass(),CloseContentAndReturn.class);
  assertEquals(processor.createExceptionParserOrThrowResourceNotFoundOn404IfNoAnnotation(method).getClass(),MapHttp4xxCodesToExceptions.class);
}
