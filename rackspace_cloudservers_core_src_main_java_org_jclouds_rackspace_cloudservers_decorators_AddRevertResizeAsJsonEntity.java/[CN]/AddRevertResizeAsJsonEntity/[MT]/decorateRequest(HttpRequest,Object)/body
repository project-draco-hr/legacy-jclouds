{
  request.setEntity("{\"revertResize\":null}");
  request.getHeaders().replaceValues(HttpHeaders.CONTENT_LENGTH,Collections.singletonList("{\"revertResize\":null}".getBytes().length + ""));
  request.getHeaders().replaceValues(HttpHeaders.CONTENT_TYPE,Collections.singletonList(MediaType.APPLICATION_JSON));
