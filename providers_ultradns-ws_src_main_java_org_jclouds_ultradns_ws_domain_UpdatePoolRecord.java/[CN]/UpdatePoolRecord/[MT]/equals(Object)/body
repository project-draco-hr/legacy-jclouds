{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  UpdatePoolRecord that=UpdatePoolRecord.class.cast(obj);
  return equal(this.pointsTo,that.pointsTo) && equal(this.mode,that.mode) && equal(this.priority,that.priority)&& equal(this.weight,that.weight)&& equal(this.failOverDelay,that.failOverDelay)&& equal(this.threshold,that.threshold)&& equal(this.ttl,that.ttl);
}
