{
  TestConfiguration.INSTANCE.reset();
  try {
    assertEquals(AdminAccess.builder().grantSudoToAdminUser(false).authorizeAdminPublicKey(true).installAdminPrivateKey(true).lockSsh(false).resetLoginPassword(false).build().apply(TestConfiguration.INSTANCE).render(OsFamily.UNIX),CharStreams.toString(Resources.newReaderSupplier(Resources.getResource("test_adminaccess_plainuser.sh"),Charsets.UTF_8)));
  }
  finally {
    TestConfiguration.INSTANCE.reset();
  }
}
