{
  String cookieHeader=checkNotNull(from.getFirstHeaderOrNull(HttpHeaders.SET_COOKIE),HttpHeaders.SET_COOKIE);
  final Matcher matcher=pattern.matcher(cookieHeader);
  boolean matchFound=matcher.find();
  try {
    if (matchFound) {
      final Map<String,NamedResource> org=factory.create(orgHandlerProvider.get()).parse(from.getPayload().getInput());
      return new VCloudSession(){
        @VCloudToken public String getVCloudToken(){
          return matcher.group(1);
        }
        @Org public Map<String,NamedResource> getOrgs(){
          return org;
        }
      }
;
    }
  }
  finally {
    releasePayload(from);
  }
  throw new HttpResponseException("not found ",null,from);
}
