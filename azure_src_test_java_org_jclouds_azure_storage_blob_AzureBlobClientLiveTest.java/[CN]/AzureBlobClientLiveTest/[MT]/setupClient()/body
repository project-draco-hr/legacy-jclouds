{
  identity=System.getProperty("jclouds.test.identity");
  String credential=System.getProperty("jclouds.test.credential");
  context=new BlobStoreContextFactory().createContext("azureblob",identity,credential,ImmutableSet.<Module>of(new Log4JLoggingModule()));
