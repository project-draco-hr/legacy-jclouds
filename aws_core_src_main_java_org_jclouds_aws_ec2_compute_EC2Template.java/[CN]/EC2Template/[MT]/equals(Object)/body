{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  EC2Template other=(EC2Template)obj;
  if (image == null) {
    if (other.image != null)     return false;
  }
 else   if (!image.equals(other.image))   return false;
  if (operatingSystem == null) {
    if (other.operatingSystem != null)     return false;
  }
 else   if (!operatingSystem.equals(other.operatingSystem))   return false;
  if (region == null) {
    if (other.region != null)     return false;
  }
 else   if (!region.equals(other.region))   return false;
  if (size == null) {
    if (other.size != null)     return false;
  }
 else   if (!size.equals(other.size))   return false;
  return true;
}
