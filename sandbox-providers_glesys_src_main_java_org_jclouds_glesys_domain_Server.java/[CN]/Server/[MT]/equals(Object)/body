{
  if (this == object) {
    return true;
  }
  if (object instanceof Server) {
    final Server other=(Server)object;
    return Objects.equal(datacenter,other.datacenter) && Objects.equal(hostname,other.hostname) && Objects.equal(id,other.id)&& Objects.equal(platform,other.platform);
  }
 else {
    return false;
  }
}
