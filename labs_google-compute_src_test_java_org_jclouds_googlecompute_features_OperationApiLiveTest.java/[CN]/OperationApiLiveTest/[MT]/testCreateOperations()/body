{
  addOperation=assertOperationDoneSucessfully(addItemToMetadata(context.getApi().getProjectApi(),getUserProject(),METADATA_ITEM_KEY,METADATA_ITEM_VALUE),20);
  deleteOperation=assertOperationDoneSucessfully(deleteItemFromMetadata(context.getApi().getProjectApi(),getUserProject(),METADATA_ITEM_KEY),20);
  assertNotNull(addOperation);
  assertNotNull(deleteOperation);
}
