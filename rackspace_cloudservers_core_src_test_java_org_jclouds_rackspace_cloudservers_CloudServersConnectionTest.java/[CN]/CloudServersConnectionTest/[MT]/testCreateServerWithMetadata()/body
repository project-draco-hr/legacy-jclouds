{
  Method method=CloudServersConnection.class.getMethod("createServer",String.class,int.class,int.class,createServerOptionsVarargsClass);
  HttpRequest httpMethod=processor.createRequest(method,new Object[]{"ralphie",2,1,withMetadata(ImmutableMap.of("foo","bar"))});
  assertEquals("{\"server\":{\"name\":\"ralphie\",\"imageId\":2,\"flavorId\":1,\"metadata\":{\"foo\":\"bar\"}}}",httpMethod.getEntity());
  validateCreateServer(method,httpMethod,null);
}
