{
  Method method=CloudServersConnection.class.getMethod("createSharedIpGroup",String.class,createSharedIpGroupOptionsVarargsClass);
  HttpRequest httpMethod=processor.createRequest(method,new Object[]{"ralphie",withServer(2)});
  assertEquals("{\"sharedIpGroup\":{\"name\":\"ralphie\",\"server\":2}}",httpMethod.getEntity());
  validateCreateSharedIpGroup(method,httpMethod);
}
