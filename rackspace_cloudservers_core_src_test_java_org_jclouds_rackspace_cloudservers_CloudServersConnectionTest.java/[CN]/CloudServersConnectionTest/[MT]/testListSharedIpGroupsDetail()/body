{
  Method method=CloudServersConnection.class.getMethod("listSharedIpGroups",listOptionsVarargsClass);
  HttpRequest httpMethod=processor.createRequest(method,new Object[]{withDetails()});
  assertEquals(httpMethod.getEndpoint().getHost(),"localhost");
  assertEquals(httpMethod.getEndpoint().getPath(),"/shared_ip_groups/detail");
  assertEquals(httpMethod.getEndpoint().getQuery(),"format=json");
  assertEquals(httpMethod.getMethod(),HttpMethod.GET);
  assertEquals(httpMethod.getHeaders().size(),0);
  assertEquals(processor.createResponseParser(method,httpMethod,null).getClass(),ParseSharedIpGroupListFromJsonResponse.class);
  assertEquals(processor.createExceptionParserOrNullIfNotFound(method),null);
}
