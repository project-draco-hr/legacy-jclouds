{
  Method method=CloudServersConnection.class.getMethod("createServer",String.class,int.class,int.class,createServerOptionsVarargsClass);
  GeneratedHttpRequest<CloudServersConnection> httpMethod=processor.createRequest(method,new Object[]{"ralphie",2,1,new CreateServerOptions[]{withFile("/etc/jclouds","foo".getBytes())}});
  assertEquals("{\"server\":{\"name\":\"ralphie\",\"imageId\":2,\"flavorId\":1,\"personality\":[{\"path\":\"/etc/jclouds\",\"contents\":\"Zm9v\"}]}}",httpMethod.getEntity());
  validateCreateServer(method,httpMethod,null);
}
