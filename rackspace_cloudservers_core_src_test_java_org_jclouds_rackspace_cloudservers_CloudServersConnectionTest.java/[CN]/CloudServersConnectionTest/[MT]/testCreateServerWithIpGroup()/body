{
  Method method=CloudServersConnection.class.getMethod("createServer",String.class,int.class,int.class,createServerOptionsVarargsClass);
  GeneratedHttpRequest<CloudServersConnection> httpMethod=processor.createRequest(method,new Object[]{"ralphie",2,1,withSharedIpGroup(2)});
  assertEquals("{\"server\":{\"name\":\"ralphie\",\"imageId\":2,\"flavorId\":1,\"sharedIpGroupId\":2}}",httpMethod.getEntity());
  validateCreateServer(method,httpMethod,null);
}
