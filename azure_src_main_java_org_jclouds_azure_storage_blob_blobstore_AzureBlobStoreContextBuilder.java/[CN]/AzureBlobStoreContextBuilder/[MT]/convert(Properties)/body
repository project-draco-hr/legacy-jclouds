{
  for (  Entry<String,String> entry : ImmutableMap.of(PROPERTY_AZUREBLOB_METADATA_PREFIX,PROPERTY_USER_METADATA_PREFIX,PROPERTY_AZUREBLOB_RETRY,PROPERTY_BLOBSTORE_RETRY,PROPERTY_AZUREBLOB_TIMEOUT,PROPERTY_USER_METADATA_PREFIX).entrySet()) {
    if (props.containsKey(entry.getKey()))     props.setProperty(entry.getValue(),props.getProperty(entry.getKey()));
  }
  return props;
}
