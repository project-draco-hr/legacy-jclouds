{
  StorageDevice storageDevice=env.datacenter.findStorageDevice(StorageDevicePredicates.name(env.storageDevice.getName()));
  assertNotNull(storageDevice);
  storageDevice=env.datacenter.findStorageDevice(StorageDevicePredicates.name(env.storageDevice.getName() + "FAIL"));
  assertNull(storageDevice);
}
