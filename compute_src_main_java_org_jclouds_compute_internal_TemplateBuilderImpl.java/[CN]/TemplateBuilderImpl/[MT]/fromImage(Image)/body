{
  if (image.getLocation() != null)   this.locationId=image.getLocation().getId();
  if (image.getOsFamily() != null)   this.os=image.getOsFamily();
  if (image.getName() != null)   this.imageName=image.getName();
  if (image.getDescription() != null)   this.imageDescription=image.getDescription();
  if (image.getOsDescription() != null)   this.osDescription=image.getOsDescription();
  if (image.getVersion() != null)   this.imageVersion=image.getVersion();
  if (image.getArchitecture() != null)   this.arch=image.getArchitecture();
  return this;
}
