{
  HttpResponse operationResponse=HttpResponse.builder().statusCode(404).build();
  MachineTypeApi machineTypeApi=requestsSendResponses(requestForScopes(COMPUTE_READONLY_SCOPE),TOKEN_RESPONSE,LIST_MACHINE_TYPES_REQUEST,operationResponse).getMachineTypeApiForProject("myproject");
  assertTrue(machineTypeApi.list().concat().isEmpty());
}
