{
  if (testContext.getAttribute("jclouds.test.user") != null) {
    AWSCredentials credentials=new AWSCredentials((String)testContext.getAttribute("jclouds.test.user"),(String)testContext.getAttribute("jclouds.test.key"));
    jetClient=new RestS3Service(credentials);
  }
 else {
    throw new RuntimeException("not configured properly");
  }
}
