{
  if (this == obj)   return true;
  if (obj == null || getClass() != obj.getClass())   return false;
  ServerExtendedStatus that=ServerExtendedStatus.class.cast(obj);
  return Objects.equal(this.taskState,that.taskState) && Objects.equal(this.vmState,that.vmState) && Objects.equal(this.powerState,that.powerState);
}
