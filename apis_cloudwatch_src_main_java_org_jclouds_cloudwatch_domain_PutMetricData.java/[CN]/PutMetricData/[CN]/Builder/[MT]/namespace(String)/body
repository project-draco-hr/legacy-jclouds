{
  Preconditions.checkNotNull(namespace,"namespace cannot be null.");
  Preconditions.checkArgument(namespace.length() > 1,"namespace must not be empty.");
  Preconditions.checkArgument(!namespace.startsWith("AWS/"),"namespace cannot start with 'AWS/' as it's " + "reserved for AWS CloudWatch metrics.");
  this.namespace=namespace;
  return this;
}
