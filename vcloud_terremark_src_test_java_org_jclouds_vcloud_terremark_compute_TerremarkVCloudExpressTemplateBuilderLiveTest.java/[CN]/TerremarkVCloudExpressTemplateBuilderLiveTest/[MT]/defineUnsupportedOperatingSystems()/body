{
  return new Predicate<OsFamilyVersion64Bit>(){
    @Override public boolean apply(    OsFamilyVersion64Bit input){
      return ((input.family == OsFamily.RHEL || input.family == OsFamily.CENTOS) || (input.family == OsFamily.UBUNTU && !input.version.equals("9.10")) || (input.family == OsFamily.WINDOWS && (input.version.equals("2008 SP2") || input.version.equals("2008 R2"))));
    }
  }
;
}
