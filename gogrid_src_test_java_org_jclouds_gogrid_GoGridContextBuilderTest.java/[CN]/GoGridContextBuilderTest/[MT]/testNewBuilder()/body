{
  GoGridContextBuilder builder=newBuilder();
  assertEquals(builder.getProperties().getProperty(GoGridConstants.PROPERTY_GOGRID_ENDPOINT),"https://api.gogrid.com/api");
  assertEquals(builder.getProperties().getProperty(GoGridConstants.PROPERTY_GOGRID_USER),"user");
  assertEquals(builder.getProperties().getProperty(GoGridConstants.PROPERTY_GOGRID_PASSWORD),"password");
}
