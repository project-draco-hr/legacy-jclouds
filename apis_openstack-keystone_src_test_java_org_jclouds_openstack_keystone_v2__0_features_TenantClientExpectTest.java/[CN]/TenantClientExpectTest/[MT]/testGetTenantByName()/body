{
  TenantClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,authenticatedGET().endpoint(endpoint + "/v2.0/tenants?name=admin").build(),HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/tenant_details.json",APPLICATION_JSON)).build()).getTenantClient().get();
  Tenant tenant=client.getByName("admin");
  assertNotNull(tenant);
  assertEquals(tenant,Tenant.builder().id("013ba41150a14830bec85ffe93353bcc").name("admin").build());
}
