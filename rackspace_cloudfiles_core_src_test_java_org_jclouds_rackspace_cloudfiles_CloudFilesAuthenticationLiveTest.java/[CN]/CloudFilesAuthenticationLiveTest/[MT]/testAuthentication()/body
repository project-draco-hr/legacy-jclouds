{
  CloudFilesAuthentication authentication=injector.getInstance(CloudFilesAuthentication.class);
  AuthenticationResponse response=authentication.authenticate(sysRackspaceUser,sysRackspaceKey);
  assertNotNull(response);
  assertNotNull(response.getStorageUrl());
  assertNotNull(response.getCDNManagementUrl());
  assertNotNull(response.getAuthToken());
}
