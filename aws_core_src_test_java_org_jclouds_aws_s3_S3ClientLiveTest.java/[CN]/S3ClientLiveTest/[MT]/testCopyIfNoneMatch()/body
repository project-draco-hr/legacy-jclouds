{
  String containerName=getContainerName();
  String destinationContainer=getContainerName();
  try {
    String goodETag=addToContainerAndValidate(containerName,sourceKey);
    context.getApi().copyObject(containerName,sourceKey,destinationContainer,destinationKey,ifSourceETagDoesntMatch("asfasdf"));
    validateContent(destinationContainer,destinationKey);
    try {
      context.getApi().copyObject(containerName,sourceKey,destinationContainer,destinationKey,ifSourceETagDoesntMatch(goodETag));
    }
 catch (    HttpResponseException ex) {
      assertEquals(ex.getResponse().getStatusCode(),412);
    }
  }
  finally {
    returnContainer(containerName);
    returnContainer(destinationContainer);
  }
}
