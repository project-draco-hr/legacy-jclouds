{
  Iterable<? extends Vdc> catalogItems=transformParallel(org.getVdcs(),new Function<Reference,Future<? extends Vdc>>(){
    @Override public Future<? extends Vdc> apply(    Reference from){
      return aapi.getVdcApi().get(from.getHref());
    }
  }
,executor,null,logger,"vdcs in org " + org.getName());
  return catalogItems;
}
