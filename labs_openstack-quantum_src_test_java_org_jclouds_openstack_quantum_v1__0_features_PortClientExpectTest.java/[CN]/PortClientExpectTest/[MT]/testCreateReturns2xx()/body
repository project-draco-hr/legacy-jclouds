{
  PortClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,standardRequestBuilder(endpoint + "/tenants/3456/networks/16dba3bc-f3fa-4775-afdc-237e12c72f6a/ports").method("POST").build(),standardResponseBuilder(200).payload(payloadFromStringWithContentType("{\"port\":{\"id\":\"12345\"}}",APPLICATION_JSON)).build()).getPortClientForRegion("region-a.geo-1");
  Reference port=client.create("16dba3bc-f3fa-4775-afdc-237e12c72f6a");
  assertEquals(port,Reference.builder().id("12345").build());
}
