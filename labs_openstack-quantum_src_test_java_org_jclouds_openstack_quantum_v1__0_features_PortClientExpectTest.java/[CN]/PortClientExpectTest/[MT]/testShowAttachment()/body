{
  PortClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,authenticatedGET().endpoint(endpoint + "/tenants/3456/networks/16dba3bc-f3fa-4775-afdc-237e12c72f6a/ports/77777/attachment").build(),HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/attachment.json",APPLICATION_JSON)).build()).getPortClientForRegionAndNetwork("region-a.geo-1","16dba3bc-f3fa-4775-afdc-237e12c72f6a");
  Attachment attachment=client.showAttachment("77777");
  assertEquals(attachment,Attachment.builder().id("jclouds-live-test").build());
}
