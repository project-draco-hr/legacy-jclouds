{
  PortClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,standardRequestBuilder(endpoint + "/tenants/3456/networks/16dba3bc-f3fa-4775-afdc-237e12c72f6a/ports/77777/attachment").build(),standardResponseBuilder(200).payload(payloadFromResourceWithContentType("/attachment.json",APPLICATION_JSON)).build()).getPortClientForRegion("region-a.geo-1");
  Attachment attachment=client.showAttachment("16dba3bc-f3fa-4775-afdc-237e12c72f6a","77777");
  assertEquals(attachment,Attachment.builder().id("jclouds-live-test").build());
}
