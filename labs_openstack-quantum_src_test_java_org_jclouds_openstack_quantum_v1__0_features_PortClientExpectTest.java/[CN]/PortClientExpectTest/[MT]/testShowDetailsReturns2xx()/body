{
  PortClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,standardRequestBuilder(endpoint + "/tenants/3456/networks/16dba3bc-f3fa-4775-afdc-237e12c72f6a/ports/646c123b-871a-4124-9fa2-a94f04a582df/detail").build(),standardResponseBuilder(200).payload(payloadFromResourceWithContentType("/port_details.json",APPLICATION_JSON)).build()).getPortClientForRegionAndNetwork("region-a.geo-1","16dba3bc-f3fa-4775-afdc-237e12c72f6a");
  PortDetails net=client.getDetails("646c123b-871a-4124-9fa2-a94f04a582df");
  assertEquals(net,new ParsePortDetailsTest().expected());
}
