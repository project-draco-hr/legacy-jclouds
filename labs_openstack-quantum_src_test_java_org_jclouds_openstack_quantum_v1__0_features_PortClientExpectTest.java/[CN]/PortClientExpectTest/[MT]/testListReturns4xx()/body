{
  PortClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,authenticatedGET().endpoint(endpoint + "/tenants/3456/networks/1a104cf5-cb18-4d35-9407-2fd2646d9d0b/ports/detail").build(),HttpResponse.builder().statusCode(404).build()).getPortClientForRegionAndNetwork("region-a.geo-1","1a104cf5-cb18-4d35-9407-2fd2646d9d0b");
  assertTrue(client.list().isEmpty());
}
