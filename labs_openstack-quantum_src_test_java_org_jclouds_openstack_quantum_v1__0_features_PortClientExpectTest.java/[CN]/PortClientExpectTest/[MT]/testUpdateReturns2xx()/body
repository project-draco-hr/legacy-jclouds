{
  PortClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,standardRequestBuilder(endpoint + "/tenants/3456/networks/16dba3bc-f3fa-4775-afdc-237e12c72f6a/ports/77777").method("PUT").payload(payloadFromStringWithContentType("{\"port\":{\"state\":\"ACTIVE\"}}",MediaType.APPLICATION_JSON)).build(),standardResponseBuilder(200).build()).getPortClientForRegionAndNetwork("region-a.geo-1","16dba3bc-f3fa-4775-afdc-237e12c72f6a");
  assertTrue(client.updateState("77777",Port.State.ACTIVE));
}
