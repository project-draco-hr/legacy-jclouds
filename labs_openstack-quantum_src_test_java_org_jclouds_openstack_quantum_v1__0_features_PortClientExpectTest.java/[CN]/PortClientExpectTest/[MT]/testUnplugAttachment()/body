{
  PortClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,standardRequestBuilder(endpoint + "/tenants/3456/networks/16dba3bc-f3fa-4775-afdc-237e12c72f6a/ports/77777/attachment").method("DELETE").build(),standardResponseBuilder(200).build()).getPortClientForRegionAndNetwork("region-a.geo-1","16dba3bc-f3fa-4775-afdc-237e12c72f6a");
  assertTrue(client.unplugAttachment("77777"));
}
