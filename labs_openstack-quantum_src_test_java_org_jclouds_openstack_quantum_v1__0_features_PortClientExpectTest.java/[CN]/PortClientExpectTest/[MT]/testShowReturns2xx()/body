{
  PortClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,standardRequestBuilder(endpoint + "/tenants/3456/networks/16dba3bc-f3fa-4775-afdc-237e12c72f6a/ports/646c123b-871a-4124-9fa2-a94f04a582df").build(),standardResponseBuilder(200).payload(payloadFromResourceWithContentType("/port.json",APPLICATION_JSON)).build()).getPortClientForRegion("region-a.geo-1");
  Port port=client.show("16dba3bc-f3fa-4775-afdc-237e12c72f6a","646c123b-871a-4124-9fa2-a94f04a582df");
  assertEquals(port,new ParsePortTest().expected());
}
