{
  int count=0;
  for (  String ip : ipsToNetworks.keySet()) {
    this.queryParameters.replaceValues(String.format("ipnetworklist[%d].ip",count),ImmutableSet.of(ip));
    this.queryParameters.replaceValues(String.format("ipnetworklist[%d].networkid",count),ImmutableSet.of("" + ipsToNetworks.get(ip)));
    count+=1;
  }
  return this;
}
