{
  return "EC2TemplateOptions [groupIds=" + groupIds + ", keyPair="+ keyPair+ ", noKeyPair="+ noKeyPair+ ", inboundPorts="+ Arrays.toString(inboundPorts)+ ", privateKey="+ (privateKey != null)+ ", publicKey="+ (publicKey != null)+ ", runScript="+ (script != null)+ ", port:seconds="+ port+ ":"+ seconds+ ", metadata/details: "+ includeMetadata+ "]";
