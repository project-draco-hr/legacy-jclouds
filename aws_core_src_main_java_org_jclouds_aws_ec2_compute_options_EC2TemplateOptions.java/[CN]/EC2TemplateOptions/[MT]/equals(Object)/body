{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (getClass() != obj.getClass())   return false;
  EC2TemplateOptions other=(EC2TemplateOptions)obj;
  if (groupIds == null) {
    if (other.groupIds != null)     return false;
  }
 else   if (!groupIds.equals(other.groupIds))   return false;
  if (keyPair == null) {
    if (other.keyPair != null)     return false;
  }
 else   if (!keyPair.equals(other.keyPair))   return false;
  if (noKeyPair != other.noKeyPair)   return false;
  return true;
}
