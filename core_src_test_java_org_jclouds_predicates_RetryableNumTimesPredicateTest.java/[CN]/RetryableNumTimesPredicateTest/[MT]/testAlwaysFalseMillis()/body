{
  RetryableNumTimesPredicate<String> predicate=new RetryableNumTimesPredicate<String>(Predicates.<String>alwaysFalse(),3,1L,TimeUnit.SECONDS);
  stopwatch.start();
  predicate.apply("");
  long duration=stopwatch.elapsedMillis();
  assertOrdered(2500 - EARLY_RETURN_GRACE,duration,2500 + SLOW_BUILD_SERVER_GRACE);
}
