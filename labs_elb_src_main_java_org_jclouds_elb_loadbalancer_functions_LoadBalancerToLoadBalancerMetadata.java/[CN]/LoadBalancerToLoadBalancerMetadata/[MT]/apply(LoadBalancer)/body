{
  Location location=input.getRegion() != null ? findLocationWithId(input.getRegion()) : defaultLocationSupplier.get();
  String id=input.getRegion() != null ? input.getRegion() + "/" + input.getName() : input.getName();
  return new LoadBalancerMetadataImpl(LoadBalancerType.LB,input.getName(),input.getName(),id,location,null,ImmutableMap.<String,String>of(),ImmutableSet.of(input.getDnsName()));
}
