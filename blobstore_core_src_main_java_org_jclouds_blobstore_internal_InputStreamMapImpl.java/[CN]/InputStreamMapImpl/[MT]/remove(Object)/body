{
  InputStream old=getLastValue(o);
  try {
    connection.removeBlob(containerName,o.toString()).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS);
  }
 catch (  Exception e) {
    Utils.<BlobRuntimeException>rethrowIfRuntimeOrSameType(e);
    throw new BlobRuntimeException(String.format("Error removing object %1$s:%2$s",containerName,o),e);
  }
  return old;
}
