{
  if (inInstances && !inProductCodes) {
    instances.add(new RunningInstance(amiLaunchIndex,dnsName,imageId,instanceId,instanceState,instanceType,ipAddress,kernelId,keyName,launchTime,monitoring,availabilityZone,platform,privateDnsName,privateIpAddress,productCodes,ramdiskId,reason,subnetId,vpcId));
    this.amiLaunchIndex=null;
    this.dnsName=null;
    this.imageId=null;
    this.instanceId=null;
    this.instanceState=null;
    this.instanceType=null;
    this.ipAddress=null;
    this.kernelId=null;
    this.keyName=null;
    this.launchTime=null;
    this.monitoring=false;
    this.availabilityZone=null;
    this.platform=null;
    this.privateDnsName=null;
    this.privateIpAddress=null;
    this.productCodes=Sets.newHashSet();
    this.ramdiskId=null;
    this.reason=null;
    this.subnetId=null;
    this.vpcId=null;
  }
}
