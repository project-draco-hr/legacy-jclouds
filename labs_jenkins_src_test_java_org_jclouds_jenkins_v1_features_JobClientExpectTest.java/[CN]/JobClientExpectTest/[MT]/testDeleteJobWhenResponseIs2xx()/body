{
  HttpRequest deleteJob=HttpRequest.builder().method("POST").endpoint(URI.create("http://localhost:8080/job/blagoo/doDelete")).headers(ImmutableMultimap.<String,String>builder().put("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").build()).build();
  HttpResponse deleteJobResponse=HttpResponse.builder().statusCode(200).build();
  JenkinsClient deleteJobWhenDeleted=requestSendsResponse(deleteJob,deleteJobResponse);
  deleteJobWhenDeleted.getJobClient().delete("blagoo");
}
