{
  String name="TestLoadBalancer";
  for (  Entry<String,String> regionZone : ImmutableMap.<String,String>of(Region.US_EAST_1,AvailabilityZone.US_EAST_1A,Region.US_WEST_1,AvailabilityZone.US_WEST_1A,Region.EU_WEST_1,AvailabilityZone.EU_WEST_1A,Region.AP_SOUTHEAST_1,AvailabilityZone.AP_SOUTHEAST_1A).entrySet()) {
    String dnsName=client.createLoadBalancer(regionZone.getKey(),name,"http",80,80,regionZone.getValue());
    assertNotNull(dnsName);
    assert(dnsName.startsWith(name));
  }
}
