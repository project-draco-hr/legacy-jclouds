{
  final Set<Size> sizes=Sets.newHashSet();
  holder.logger.debug(">> providing sizes");
  for (  final Flavor from : sync.listFlavors(ListOptions.Builder.withDetails())) {
    sizes.add(new CloudServersSize(from,from.getId() + "",from.getDisk() / 10,from.getRam(),from.getDisk(),ImmutableSet.<Architecture>of(Architecture.X86_32,Architecture.X86_64)));
  }
  holder.logger.debug("<< sizes(%d)",sizes.size());
  return sizes;
}
