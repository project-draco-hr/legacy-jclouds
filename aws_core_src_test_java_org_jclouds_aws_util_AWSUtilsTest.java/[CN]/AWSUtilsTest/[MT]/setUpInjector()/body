{
  Injector injector=new RestContextFactory().createContextBuilder("ec2","foo","bar",ImmutableSet.of(new MockModule(),new NullLoggingModule()),new Properties()).buildInjector();
  utils=injector.getInstance(AWSUtils.class);
  command=createMock(HttpCommand.class);
  expect(command.getCurrentRequest()).andReturn(createMock(HttpRequest.class)).atLeastOnce();
  replay(command);
}
