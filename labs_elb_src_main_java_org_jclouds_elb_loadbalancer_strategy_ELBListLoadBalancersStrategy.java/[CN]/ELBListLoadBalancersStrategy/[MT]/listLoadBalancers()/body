{
  Iterable<? extends CrappyLoadBalancer> loadBalancers;
  Set<String> regions=this.regions.get();
  if (regions.size() > 0)   loadBalancers=concat(transformParallel(regions,new Function<String,Future<? extends Set<? extends CrappyLoadBalancer>>>(){
    @Override public ListenableFuture<Set<? extends CrappyLoadBalancer>> apply(    String from){
      return aclient.describeLoadBalancersInRegion(from);
    }
  }
,executor,null,logger,"loadbalancers"));
 else   loadBalancers=client.describeLoadBalancersInRegion(null);
  return transform(loadBalancers,converter);
}
