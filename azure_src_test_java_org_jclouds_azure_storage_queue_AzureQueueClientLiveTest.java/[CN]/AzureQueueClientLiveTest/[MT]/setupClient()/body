{
  account=System.getProperty("jclouds.test.user");
  String key=System.getProperty("jclouds.test.key");
  Injector injector=new AzureQueueContextBuilder("azurequeue",new AzureQueuePropertiesBuilder(account,key).build()).withModules(new Log4JLoggingModule()).buildInjector();
  connection=injector.getInstance(AzureQueueClient.class);
}
