{
  HttpRequest request=HttpRequest.builder().method("GET").endpoint("http://localhost:8080/client/api?response=json&command=prepareHostForMaintenance&id=1&apiKey=identity&signature=9tDwdox%2FxAKmZr9kVrR6Ttnxf3U%3D").addHeader("Accept","application/json").build();
