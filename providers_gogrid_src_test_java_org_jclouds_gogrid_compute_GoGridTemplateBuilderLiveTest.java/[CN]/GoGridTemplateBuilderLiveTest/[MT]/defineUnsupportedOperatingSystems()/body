{
  return new Predicate<OsFamilyVersion64Bit>(){
    @Override public boolean apply(    OsFamilyVersion64Bit input){
switch (input.family) {
case RHEL:
        return !input.version.equals("") && !input.version.equals("5.4");
case UBUNTU:
      return !input.version.equals("") && !input.version.equals("10.04");
case CENTOS:
    return !input.version.equals("") && !input.version.equals("5.3");
case WINDOWS:
  return !input.version.equals("") && (input.is64Bit && !input.version.matches("200[38]")) || (input.version.matches("200[38] [RS]P?2") && !input.is64Bit);
default :
return true;
}
}
}
;
}
