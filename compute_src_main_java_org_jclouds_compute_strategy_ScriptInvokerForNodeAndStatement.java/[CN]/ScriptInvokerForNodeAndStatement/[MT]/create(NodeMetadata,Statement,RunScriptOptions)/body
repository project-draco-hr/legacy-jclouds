{
  checkNotNull(node,"node");
  checkNotNull(runScript,"runScript");
  checkNotNull(options,"options");
  return options.shouldBlockOnComplete() ? initAndStartScriptOnNodeFactory.blockOnComplete(node,options.getTaskName(),runScript,options.shouldRunAsRoot()) : initAndStartScriptOnNodeFactory.dontBlockOnComplete(node,options.getTaskName(),runScript,options.shouldRunAsRoot());
}
