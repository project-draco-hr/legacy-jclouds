{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/admin/catalog/7212e451-76e1-4631-b2de-ba1dfd8080e4/owner").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/catalog/admin/owner.xml",VCloudDirectorMediaType.OWNER).httpResponseBuilder().build());
  Owner expected=owner();
  assertEquals(client.getAdminCatalogClient().getOwner(catalogRef.getHref()),expected);
}
