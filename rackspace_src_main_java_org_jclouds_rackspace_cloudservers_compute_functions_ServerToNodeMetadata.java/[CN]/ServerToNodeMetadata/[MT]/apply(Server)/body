{
  String tag=parseTagFromName(from.getName());
  Location host=new LocationImpl(LocationScope.HOST,from.getHostId(),from.getHostId(),location);
  Image image=null;
  try {
    image=Iterables.find(images,new FindImageForServer(host,from));
  }
 catch (  NoSuchElementException e) {
    logger.warn("could not find a matching image for server %s in location %s",from,location);
  }
  return new NodeMetadataImpl(from.getId() + "",from.getName(),from.getId() + "",host,null,from.getMetadata(),tag,image,serverToNodeState.get(from.getStatus()),from.getAddresses().getPublicAddresses(),from.getAddresses().getPrivateAddresses(),ImmutableMap.<String,String>of(),null);
}
