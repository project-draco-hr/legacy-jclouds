{
  if (media != null) {
    if (catalogItem.getEntity().getHref().equals(media.getHref())) {
      try {
        Task remove=context.getApi().getMediaApi().remove(media.getId());
        taskDoneEventually(remove);
        media=null;
      }
 catch (      Exception e) {
        logger.warn(e,"Error when deleting media '%s'",media.getName());
      }
    }
  }
}
