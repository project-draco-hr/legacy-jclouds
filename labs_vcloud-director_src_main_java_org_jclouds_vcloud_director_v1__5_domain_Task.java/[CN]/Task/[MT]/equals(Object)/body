{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  Task that=Task.class.cast(o);
  return super.equals(that) && equal(this.error,that.error) && equal(this.org,that.org)&& equal(this.progress,that.progress)&& equal(this.status,that.status)&& equal(this.operation,that.operation)&& equal(this.operationName,that.operationName)&& datesEqual(this.startTime,that.startTime)&& datesEqual(this.endTime,that.endTime)&& datesEqual(this.expiryTime,that.expiryTime);
}
