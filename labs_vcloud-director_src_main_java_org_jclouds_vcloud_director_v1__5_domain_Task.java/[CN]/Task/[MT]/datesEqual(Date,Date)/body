{
  Date cDate1=null;
  if (date1 != null) {
    Calendar c1=Calendar.getInstance();
    c1.setTime(date1);
    c1.clear(Calendar.MILLISECOND);
    cDate1=c1.getTime();
  }
  Date cDate2=null;
  if (date2 != null) {
    Calendar c2=Calendar.getInstance();
    c2.setTime(date2);
    c2.clear(Calendar.MILLISECOND);
    cDate2=c2.getTime();
  }
  return equal(cDate1,cDate2);
}
