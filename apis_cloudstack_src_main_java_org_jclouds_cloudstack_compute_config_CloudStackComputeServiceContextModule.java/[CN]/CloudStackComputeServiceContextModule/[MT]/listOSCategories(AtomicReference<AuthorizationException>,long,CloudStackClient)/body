{
  return new MemoizedRetryOnTimeOutButNotOnAuthorizationExceptionSupplier<Map<String,String>>(authException,seconds,new Supplier<Map<String,String>>(){
    @Override public Map<String,String> get(){
      GuestOSClient guestOSClient=client.getGuestOSClient();
      return guestOSClient.listOSCategories();
    }
  }
