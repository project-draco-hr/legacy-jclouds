{
  if (this == obj)   return true;
  if (obj == null || getClass() != obj.getClass())   return false;
  Record that=Record.class.cast(obj);
  return equal(this.name,that.name) && equal(this.type,that.type) && equal(this.ttl,that.ttl)&& equal(this.data,that.data)&& equal(this.priority,that.priority)&& equal(this.comment,that.comment);
}
