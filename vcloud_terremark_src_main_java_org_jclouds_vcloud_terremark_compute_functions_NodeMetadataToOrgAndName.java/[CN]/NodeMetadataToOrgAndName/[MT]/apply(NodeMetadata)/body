{
  if (from.getTag() != null) {
    String org=vdcToOrg.get().get(from.getLocation().getId());
    if (org == null) {
      logger.warn("did not find an association for vdc %s in %s",from.getLocation().getId(),vdcToOrg);
    }
 else {
      return new OrgAndName(org,from.getTag());
    }
  }
  return null;
}
