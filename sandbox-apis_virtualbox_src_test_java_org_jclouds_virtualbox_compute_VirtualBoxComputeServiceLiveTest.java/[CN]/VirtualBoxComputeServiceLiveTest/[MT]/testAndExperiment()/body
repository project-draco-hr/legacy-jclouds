{
  ComputeServiceContext context=null;
  try {
    context=new ComputeServiceContextFactory().createContext(new StandaloneComputeServiceContextSpec<VirtualBoxManager,IMachine,IMachine,IMachine,Host>("virtualbox",endpoint,apiversion,"",identity,credential,VirtualBoxManager.class,VirtualBoxContextBuilder.class,ImmutableSet.<Module>of()));
    context.getComputeService().listNodes();
  }
  finally {
    if (context != null)     context.close();
  }
}
