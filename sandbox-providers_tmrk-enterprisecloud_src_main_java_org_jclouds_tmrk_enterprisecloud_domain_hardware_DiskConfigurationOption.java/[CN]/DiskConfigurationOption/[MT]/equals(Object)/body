{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  DiskConfigurationOption that=(DiskConfigurationOption)o;
  if (maximum != that.maximum)   return false;
  if (minimum != that.minimum)   return false;
  if (dataDisk != null ? !dataDisk.equals(that.dataDisk) : that.dataDisk != null)   return false;
  if (systemDisk != null ? !systemDisk.equals(that.systemDisk) : that.systemDisk != null)   return false;
  return true;
}
