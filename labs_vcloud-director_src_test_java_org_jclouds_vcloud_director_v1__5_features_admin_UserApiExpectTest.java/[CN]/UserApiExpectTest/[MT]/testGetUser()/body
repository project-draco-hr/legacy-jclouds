{
  VCloudDirectorAdminApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/admin/user/b37223f3-8792-477a-820f-334998f61cd6").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/user/user.xml",VCloudDirectorMediaType.USER).httpResponseBuilder().build());
