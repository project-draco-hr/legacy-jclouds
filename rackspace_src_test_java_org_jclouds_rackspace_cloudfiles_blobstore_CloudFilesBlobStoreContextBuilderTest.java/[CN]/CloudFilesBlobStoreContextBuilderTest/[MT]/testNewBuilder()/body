{
  CloudFilesBlobStoreContextBuilder builder=newBuilder();
  assertEquals(builder.getProperties().getProperty(PROPERTY_USER_METADATA_PREFIX),"X-Object-Meta-");
  assertEquals(builder.getProperties().getProperty(RackspaceConstants.PROPERTY_RACKSPACE_USER),"id");
  assertEquals(builder.getProperties().getProperty(RackspaceConstants.PROPERTY_RACKSPACE_KEY),"secret");
}
