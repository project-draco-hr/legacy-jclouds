{
  Properties properties=new Properties();
  properties.putAll(Region.regionProperties());
  properties.setProperty(PROPERTY_REGION + "." + US_EAST_1+ ".endpoint","https://sqs.us-east-1.amazonaws.com");
  properties.setProperty(PROPERTY_REGION + "." + US_WEST_1+ ".endpoint","https://sqs.us-west-1.amazonaws.com");
  properties.setProperty(PROPERTY_REGION + "." + US_WEST_2+ ".endpoint","https://sqs.us-west-2.amazonaws.com");
  properties.setProperty(PROPERTY_REGION + "." + SA_EAST_1+ ".endpoint","https://sqs.sa-east-1.amazonaws.com");
  properties.setProperty(PROPERTY_REGION + "." + EU_WEST_1+ ".endpoint","https://sqs.eu-west-1.amazonaws.com");
  properties.setProperty(PROPERTY_REGION + "." + AP_SOUTHEAST_1+ ".endpoint","https://sqs.ap-southeast-1.amazonaws.com");
  properties.setProperty(PROPERTY_REGION + "." + AP_NORTHEAST_1+ ".endpoint","https://sqs.ap-northeast-1.amazonaws.com");
  return properties;
}
