{
  URI uri=URI.create("http://goo.com:443?header=value1");
  HttpRequest request=HttpRequest.builder().method("GET").endpoint(uri).build();
  assertEquals(request.toBuilder().replaceQueryParam("header","1.1.1.1/24,1.1.1.2/24").build(),HttpRequest.builder().method("GET").endpoint("http://goo.com:443?header=1.1.1.1%2F24%2C1.1.1.2%2F24").build());
}
