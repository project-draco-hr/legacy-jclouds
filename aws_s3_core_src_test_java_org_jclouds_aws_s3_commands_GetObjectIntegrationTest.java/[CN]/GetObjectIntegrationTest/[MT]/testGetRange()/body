{
  String bucket=bucketPrefix + "testGetRange".toLowerCase();
  String key="apples";
  setUpBucket(bucket,key);
  S3Object object1=client.getObject(bucket,key,range(0,5)).get(10,TimeUnit.SECONDS);
  assertEquals(S3Utils.getContentAsStringAndClose(object1),TEST_STRING.substring(0,6));
  S3Object object2=client.getObject(bucket,key,range(5,TEST_STRING.length())).get(10,TimeUnit.SECONDS);
  assertEquals(S3Utils.getContentAsStringAndClose(object2),TEST_STRING.substring(5,TEST_STRING.length()));
}
