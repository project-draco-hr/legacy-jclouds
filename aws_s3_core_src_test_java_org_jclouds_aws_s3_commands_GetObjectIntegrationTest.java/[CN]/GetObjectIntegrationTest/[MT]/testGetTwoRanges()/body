{
  String bucketName=getBucketName();
  try {
    String key="apples";
    addObjectAndValidateContent(bucketName,key);
    S3Object object=client.getObject(bucketName,key,range(0,5).range(6,TEST_STRING.length())).get(10,TimeUnit.SECONDS);
    assertEquals(S3Utils.getContentAsStringAndClose(object),TEST_STRING);
  }
  finally {
    returnBucket(bucketName);
  }
}
