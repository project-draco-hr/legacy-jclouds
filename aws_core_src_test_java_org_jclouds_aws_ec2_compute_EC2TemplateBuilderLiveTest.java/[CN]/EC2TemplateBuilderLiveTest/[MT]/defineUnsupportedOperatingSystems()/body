{
  return new Predicate<OsFamilyVersion64Bit>(){
    @Override public boolean apply(    OsFamilyVersion64Bit input){
      return input.family == OsFamily.RHEL || (input.family == OsFamily.CENTOS && !input.version.matches("5.[42]")) || (input.family == OsFamily.WINDOWS && !input.version.matches("200[38]"));
    }
  }
;
}
