{
  VirtualMachineState[] states={VirtualMachineState.ALLOCATED,VirtualMachineState.CONFIGURED,VirtualMachineState.LOCKED,VirtualMachineState.OFF,VirtualMachineState.PAUSED,VirtualMachineState.NOT_ALLOCATED,VirtualMachineState.UNKNOWN};
  checkStatesReturn(new MockVirtualMachine(),new VirtualMachineStateMonitor(VirtualMachineState.ON),states,MonitorStatus.CONTINUE);
  checkStatesReturn(new MockVirtualMachineFailing(),new VirtualMachineStateMonitor(VirtualMachineState.ON),states,MonitorStatus.CONTINUE);
}
