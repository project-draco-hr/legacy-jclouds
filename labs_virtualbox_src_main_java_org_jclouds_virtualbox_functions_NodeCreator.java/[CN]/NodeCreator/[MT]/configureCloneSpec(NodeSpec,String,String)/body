{
  String cloneName=generateCloneName(nodeSpec);
  VmSpec cloneVmSpec=VmSpec.builder().id(cloneName).name(cloneName).memoryMB(ram).osTypeId(nodeSpec.getMaster().getMachine().getOSTypeId()).guestUser(guestOsUser).guestPassword(guestOsPassword).cleanUpMode(CleanupMode.Full).forceOverwrite(true).build();
  NetworkSpec networkSpec=networkUtils.createNetworkSpecWhenVboxIsLocalhost();
  return CloneSpec.builder().linked(true).master(nodeSpec.getMaster().getMachine()).network(networkSpec).vm(cloneVmSpec).build();
}
