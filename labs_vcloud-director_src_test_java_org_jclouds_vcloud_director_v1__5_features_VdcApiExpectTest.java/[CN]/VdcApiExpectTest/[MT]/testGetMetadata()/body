{
  URI vdcUri=URI.create(endpoint + "/vdc/e9cd3387-ac57-4d27-a481-9bee75e0690f");
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/vdc/e9cd3387-ac57-4d27-a481-9bee75e0690f/metadata").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vdc/metadata.xml",VCloudDirectorMediaType.METADATA).httpResponseBuilder().build());
  Metadata expected=metadata();
  assertEquals(api.getVdcApi().getMetadataApi().get(vdcUri),expected);
}
