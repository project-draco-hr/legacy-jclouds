{
  String containerName=getContainerName();
  try {
    String key="apples";
    addObjectAndValidateContent(containerName,key);
    B object=context.getApi().getBlob(containerName,key,range(0,5).range(6,TEST_STRING.length())).get(10,TimeUnit.SECONDS);
    assertEquals(BlobStoreUtils.getContentAsStringAndClose(object),TEST_STRING);
  }
  finally {
    returnContainer(containerName);
  }
}
