{
  HttpResponse listResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/list_roles_marker.xml","text/xml")).build();
  HttpRequest listPathPrefix2=HttpRequest.builder().method("POST").endpoint("https://iam.amazonaws.com/").addHeader("Host","iam.amazonaws.com").addFormParam("Action","ListRoles").addFormParam("Marker","MARKER").addFormParam("PathPrefix","/subdivision").addFormParam("Signature","Y05M4vbhJpd35erXuhECszxjtx56cdIULGHnRaVr13s%3D").addFormParam("SignatureMethod","HmacSHA256").addFormParam("SignatureVersion","2").addFormParam("Timestamp","2009-11-08T15%3A54%3A08.897Z").addFormParam("Version","2010-05-08").addFormParam("AWSAccessKeyId","identity").build();
  HttpResponse list2Response=HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/list_roles.xml","text/xml")).build();
  IAMApi apiWhenExist=requestsSendResponses(listPathPrefix,listResponse,listPathPrefix2,list2Response);
  assertEquals(apiWhenExist.getRoleApi().listPathPrefix("/subdivision").concat().toList(),ImmutableList.copyOf(Iterables.concat(new ListRolesResponseTest().expected(),new ListRolesResponseTest().expected())));
