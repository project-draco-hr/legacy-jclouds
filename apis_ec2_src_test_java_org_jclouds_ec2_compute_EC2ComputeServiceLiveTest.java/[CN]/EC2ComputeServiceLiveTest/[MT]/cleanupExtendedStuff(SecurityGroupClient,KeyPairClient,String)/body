{
  try {
    for (    SecurityGroup group : securityGroupClient.describeSecurityGroupsInRegion(null))     if (group.getName().startsWith("jclouds#" + tag) || group.getName().equals(tag)) {
      securityGroupClient.deleteSecurityGroupInRegion(null,group.getName());
    }
  }
 catch (  Exception e) {
  }
  try {
    for (    KeyPair pair : keyPairClient.describeKeyPairsInRegion(null))     if (pair.getKeyName().startsWith("jclouds#" + tag) || pair.getKeyName().equals(tag)) {
      keyPairClient.deleteKeyPairInRegion(null,pair.getKeyName());
    }
  }
 catch (  Exception e) {
  }
  Thread.sleep(2000);
}
