{
  String containerName=getScratchContainerName();
  try {
    context.getApi().putBucketIfNotExists(containerName,withBucketAcl(CannedAccessPolicy.PUBLIC_READ)).get(10,TimeUnit.SECONDS);
    AccessControlList acl=context.getApi().getBucketACL(containerName).get(10,TimeUnit.SECONDS);
    assertTrue(acl.hasPermission(GroupGranteeURI.ALL_USERS,Permission.READ),acl.toString());
  }
  finally {
    destroyContainer(containerName);
  }
}
