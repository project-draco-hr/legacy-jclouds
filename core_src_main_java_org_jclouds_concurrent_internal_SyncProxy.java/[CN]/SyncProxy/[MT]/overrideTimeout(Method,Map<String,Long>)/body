{
  if (timeouts == null) {
    return null;
  }
  final String className=declaring.getSimpleName();
  Long timeout=timeouts.get(className + "." + method.getName());
  if (timeout == null) {
    timeout=timeouts.get(className);
  }
  return timeout != null ? TimeUnit.MILLISECONDS.toNanos(timeout) : null;
}
