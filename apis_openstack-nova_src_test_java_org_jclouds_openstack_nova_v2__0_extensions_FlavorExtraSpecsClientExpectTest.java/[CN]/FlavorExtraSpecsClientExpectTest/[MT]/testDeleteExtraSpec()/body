{
  URI endpoint=URI.create("https://az-1.region-a.geo-1.compute.hpcloudsvc.com/v1.1/3456/flavors/5/os-extra_specs/test1");
  FlavorExtraSpecsClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,extensionsOfNovaRequest,extensionsOfNovaResponse,standardRequestBuilder(endpoint).method("DELETE").build(),standardResponseBuilder(200).build()).getFlavorExtraSpecsExtensionForZone("az-1.region-a.geo-1").get();
  assertTrue(client.deleteExtraSpec("5","test1"));
}
