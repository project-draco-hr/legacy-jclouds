{
  return Predicates.not(new Predicate<OsFamilyVersion64Bit>(){
    @Override public boolean apply(    OsFamilyVersion64Bit input){
switch (input.family) {
case UBUNTU:
        return input.version.equals("");
case DEBIAN:
      return input.version.equals("") && !input.is64Bit;
case RHEL:
    return input.version.equals("") && input.is64Bit;
case WINDOWS:
  return input.version.equals("") && input.is64Bit;
default :
return false;
}
}
}
);
}
