{
  if (account == null)   checkArgument(securityGroupName == null && CIDR != null,"if you do not specify an account and security group, you must specify a CIDR range");
  if (CIDR == null)   checkArgument(account != null && securityGroupName != null,"if you do not specify an account and security group, you must specify a CIDR range");
  this.account=account;
  this.CIDR=CIDR;
  this.endPort=endPort;
  this.ICMPCode=iCMPCode;
  this.ICMPType=iCMPType;
  this.protocol=protocol;
  this.id=id;
  this.securityGroupName=securityGroupName;
  this.startPort=startPort;
}
