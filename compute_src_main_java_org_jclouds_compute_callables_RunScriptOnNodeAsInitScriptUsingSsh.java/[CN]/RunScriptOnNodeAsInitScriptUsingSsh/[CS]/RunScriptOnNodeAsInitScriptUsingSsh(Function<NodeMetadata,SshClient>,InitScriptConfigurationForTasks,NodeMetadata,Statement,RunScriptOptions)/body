{
  super(sshFactory,options.shouldRunAsRoot(),checkNotNull(node,"node"),checkNotNull(script,"script") instanceof InitBuilder ? InitBuilder.class.cast(script) : createInitScript(checkNotNull(initScriptConfiguration,"initScriptConfiguration"),options.getTaskName(),script));
  this.initFile=String.format(initScriptConfiguration.getInitScriptPattern(),init.getInstanceName());
}
