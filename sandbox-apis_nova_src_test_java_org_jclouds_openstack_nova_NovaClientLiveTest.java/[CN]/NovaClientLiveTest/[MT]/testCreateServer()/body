{
  String imageRef="14362";
  String flavorRef="1";
  String serverName=serverPrefix + "createserver" + new SecureRandom().nextInt();
  Server server=client.createServer(serverName,imageRef,flavorRef,withFile("/etc/jclouds.txt","rackspace".getBytes()).withMetadata(metadata));
  assertNotNull(server.getAdminPass());
  serverId=server.getId();
  adminPass=server.getAdminPass();
  ip=server.getAddresses().getPublicAddresses().iterator().next().getAddress();
  assertEquals(server.getStatus(),ServerStatus.BUILD);
  blockUntilServerActive(serverId);
}
