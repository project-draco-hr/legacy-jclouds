{
  MessageDigest eTag=getDigest();
  byte[] buffer=new byte[BUF_SIZE];
  int numRead=-1;
  try {
    do {
      numRead=toEncode.read(buffer);
      if (numRead > 0) {
        eTag.update(buffer,0,numRead);
      }
    }
 while (numRead != -1);
  }
 catch (  IOException e) {
    propagate(e);
  }
 finally {
    closeQuietly(toEncode);
  }
  return eTag.digest();
}
