{
  VApp vApp=client.getVApp(id);
  String tag=vApp.getName().replaceAll("-[0-9]+","");
  return new NodeMetadataImpl(vApp.getId(),vApp.getName(),vDCId,vApp.getLocation(),ImmutableMap.<String,String>of(),tag,vAppStatusToNodeState.get(vApp.getStatus()),vApp.getNetworkToAddresses().values(),ImmutableSet.<InetAddress>of(),ImmutableMap.<String,String>of(),null);
}
