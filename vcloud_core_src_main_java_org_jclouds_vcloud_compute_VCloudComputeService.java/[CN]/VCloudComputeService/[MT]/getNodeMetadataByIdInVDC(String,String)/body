{
  VApp vApp=client.getVApp(id);
  return new NodeMetadataImpl(vApp.getId(),vApp.getName(),vDCId,vApp.getLocation(),ImmutableMap.<String,String>of(),vAppStatusToNodeState.get(vApp.getStatus()),vApp.getNetworkToAddresses().values(),ImmutableSet.<InetAddress>of(),ImmutableMap.<String,String>of());
}
