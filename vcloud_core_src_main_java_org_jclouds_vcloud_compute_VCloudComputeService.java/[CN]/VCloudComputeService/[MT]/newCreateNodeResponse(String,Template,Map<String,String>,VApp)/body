{
  return new NodeMetadataImpl(vApp.getId(),vApp.getName(),template.getLocation().getId(),vApp.getLocation(),ImmutableMap.<String,String>of(),tag,vAppStatusToNodeState.get(vApp.getStatus()),getPublicAddresses(vApp.getId()),vApp.getNetworkToAddresses().values(),ImmutableMap.<String,String>of(),new Credentials(metaMap.get("username"),metaMap.get("password")));
}
