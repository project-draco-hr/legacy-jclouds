{
  Map<String,String> metaMap=start(template.getLocation().getId(),name,template.getImage().getId(),template.getSize().getCores(),template.getSize().getRam(),template.getSize().getDisk() * 1024 * 1024l,ImmutableMap.<String,String>of(),template.getOptions().getInboundPorts());
  VApp vApp=client.getVApp(metaMap.get("id"));
  NodeMetadata node=newCreateNodeResponse(tag,template,metaMap,vApp);
  nodes.add(node);
  utils.runOptionsOnNode(node,template.getOptions());
  logger.debug("<< options applied vApp(%s)",node.getId());
}
