{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  DNS other=(DNS)obj;
  if (dnsServers == null) {
    if (other.dnsServers != null)     return false;
  }
 else   if (!dnsServers.equals(other.dnsServers))   return false;
  return true;
}
