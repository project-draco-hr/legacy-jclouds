{
  Method method=GridImageAsyncClient.class.getMethod("saveImageFromServer",String.class,String.class,SaveImageOptions[].class);
  HttpRequest httpRequest=processor.createRequest(method,"friendly","serverName");
  assertRequestLineEquals(httpRequest,"GET https://api.gogrid.com/api/grid/image/save?v=1.5&friendlyName=friendly&server=serverName HTTP/1.1");
  assertNonPayloadHeadersEqual(httpRequest,"");
  assertPayloadEquals(httpRequest,null,null,false);
  assertResponseParserClassEquals(method,httpRequest,ParseImageFromJsonResponse.class);
  assertSaxResponseParserClassEquals(method,null);
  assertFallbackClassEquals(method,null);
}
