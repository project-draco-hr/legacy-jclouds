{
  Owner directOwner=mediaApi.getOwner(media.getId());
  assertEquals(owner.toBuilder().user(owner.getUser()).build(),directOwner.toBuilder().links(Collections.<Link>emptySet()).build(),String.format(GETTER_RETURNS_SAME_OBJ,"getOwner()","owner","media.getOwner()",owner.toString(),directOwner.toString()));
  Checks.checkResource(directOwner);
  assertNotNull(directOwner.getUser(),String.format(OBJ_FIELD_REQ,"Owner","user"));
  Checks.checkReferenceType(directOwner.getUser());
}
