{
  int failures=0;
  while (true) {
    try {
      checkSystemMetadataAndPutIfPresentReplaceStrategy(object);
      break;
    }
 catch (    ExecutionException e1) {
      if (!(e1.getCause() instanceof KeyAlreadyExistsException))       throw e1;
 else       failures++;
    }
  }
  if (failures > 0)   System.err.printf("%d failures create/replacing %s%n",failures,object.getData() instanceof InputStream ? "stream" : "string");
}
