{
  Multimap<String,String> params=super.buildFormParameters();
  if (marker != null)   params.put("Marker",marker);
  if (names.size() > 0) {
    int nameIndex=1;
    for (    String name : names) {
      params.put("LoadBalancerNames.member." + nameIndex,name);
      nameIndex++;
    }
  }
  return params;
}
