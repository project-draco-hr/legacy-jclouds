{
  try {
    return Iterables.find(Lists.newArrayList(overrides.getProperty(prov + "." + key),overrides.getProperty("jclouds." + key),defaultVal),Predicates.notNull());
  }
 catch (  NoSuchElementException e) {
    throw new NoSuchElementException("no " + key + " configured for provider: "+ prov);
  }
}
