{
  injector=Guice.createInjector(new AbstractModule(){
    @Override protected void configure(){
      bind(URI.class).annotatedWith(PCS.class).toInstance(URI.create("https://pcsbeta.mezeo.net/v2"));
    }
    @SuppressWarnings("unused") @Provides @Singleton public BasicAuthentication provideBasicAuthentication() throws UnsupportedEncodingException {
      return new BasicAuthentication(user,password);
    }
    @SuppressWarnings("unused") @Provides @Singleton protected PCSCloud provideCloud(    RestClientFactory factory){
      return factory.create(PCSCloud.class);
    }
  }
,new JaxrsModule(),new ExecutorServiceModule(new WithinThreadExecutorService()),new JavaUrlHttpCommandExecutorServiceModule());
}
