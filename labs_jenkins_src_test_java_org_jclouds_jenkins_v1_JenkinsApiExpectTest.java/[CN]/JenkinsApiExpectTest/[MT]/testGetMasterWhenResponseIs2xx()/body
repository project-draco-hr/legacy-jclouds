{
  HttpRequest getMaster=HttpRequest.builder().method("GET").endpoint("http://localhost:8080/api/json").addHeader("Accept","application/json").addHeader("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").build();
  HttpResponse getMasterResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/master.json")).build();
  JenkinsApi apiWhenMasterExists=requestSendsResponse(getMaster,getMasterResponse);
  assertEquals(apiWhenMasterExists.getMaster().toString(),new ParseNodeTest().expected().toString());
}
