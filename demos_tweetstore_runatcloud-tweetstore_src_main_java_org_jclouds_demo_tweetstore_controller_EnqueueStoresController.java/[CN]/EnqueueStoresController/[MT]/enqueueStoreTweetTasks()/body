{
  for (  String contextName : contextNames) {
    logger.debug("enqueuing task to store tweets in blobstore '%s'",contextName);
    taskQueue.add(taskQueue.getHttpRequestFactory().create(HttpRequest.builder().endpoint(URI.create(baseUrl + "/store/do")).headers(ImmutableMultimap.of("context",contextName)).method("GET").build()));
  }
}
