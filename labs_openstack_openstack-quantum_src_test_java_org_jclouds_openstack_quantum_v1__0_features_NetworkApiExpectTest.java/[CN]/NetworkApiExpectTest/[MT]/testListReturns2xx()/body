{
  NetworkApi api=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,authenticatedGET().endpoint(endpoint + "/tenants/3456/networks/detail").build(),HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/list_networks.json",APPLICATION_JSON)).build()).getNetworkApiForZone("region-a.geo-1");
  Set<? extends Network> nets=api.list().toSet();
  assertEquals(nets,listOfNetworks());
}
