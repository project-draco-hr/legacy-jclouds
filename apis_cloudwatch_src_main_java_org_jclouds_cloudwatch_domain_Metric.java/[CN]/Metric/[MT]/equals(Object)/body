{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Metric other=(Metric)obj;
  return Objects.equal(this.dimensions,other.dimensions) && Objects.equal(this.metricName,other.metricName) && Objects.equal(this.namespace,other.namespace);
}
