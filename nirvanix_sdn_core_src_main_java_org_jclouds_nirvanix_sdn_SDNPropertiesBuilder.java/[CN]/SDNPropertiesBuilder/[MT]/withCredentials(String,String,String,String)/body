{
  properties.setProperty(SDNConstants.PROPERTY_SDN_APPKEY,checkNotNull(appkey,"appkey"));
  properties.setProperty(SDNConstants.PROPERTY_SDN_APPNAME,checkNotNull(appname,"appname"));
  properties.setProperty(SDNConstants.PROPERTY_SDN_USERNAME,checkNotNull(username,"username"));
  properties.setProperty(SDNConstants.PROPERTY_SDN_PASSWORD,checkNotNull(password,"password"));
  return this;
}
