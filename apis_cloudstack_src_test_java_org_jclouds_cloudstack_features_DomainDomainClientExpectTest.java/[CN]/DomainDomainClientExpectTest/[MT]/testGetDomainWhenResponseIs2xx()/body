{
  DomainDomainClient client=requestSendsResponse(HttpRequest.builder().method("GET").endpoint(URI.create("http://localhost:8080/client/api?response=json&" + "command=listDomains&listAll=true&id=1&apiKey=identity&signature=M16YxHWKST%2FcIRUHvWhfWovJugU%3D")).addHeader("Accept","application/json").build(),HttpResponse.builder().statusCode(200).payload(payloadFromResource("/getdomainresponse.json")).build());
  assertEquals(client.getDomainById("1"),Domain.builder().id("1").name("ROOT").level(0).hasChild(true).build());
}
