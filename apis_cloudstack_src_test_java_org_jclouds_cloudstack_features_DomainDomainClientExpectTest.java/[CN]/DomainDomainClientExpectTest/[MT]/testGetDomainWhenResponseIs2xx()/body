{
  DomainDomainClient client=requestSendsResponse(HttpRequest.builder().method("GET").endpoint(URI.create("http://localhost:8080/client/api?response=json&" + "command=listDomains&id=1&apiKey=identity&signature=emQKWkVhospRkaUzjKljME2rW0k%3D")).headers(ImmutableMultimap.<String,String>builder().put("Accept","application/json").build()).build(),HttpResponse.builder().statusCode(200).payload(payloadFromResource("/getdomainresponse.json")).build());
  assertEquals(client.getDomainById(1),Domain.builder().id(1L).name("ROOT").level(0).hasChild(true).build());
}
