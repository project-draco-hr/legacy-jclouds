{
  if (inHardware)   virtualHardwareHandler.characters(ch,start,length);
 else   if (inOs)   operatingSystemHandler.characters(ch,start,length);
 else   if (inGuestCustomization)   guestCustomizationHandler.characters(ch,start,length);
 else   if (inNetworkConnectionSection)   networkConnectionSectionHandler.characters(ch,start,length);
 else   if (inTasks)   taskHandler.characters(ch,start,length);
 else   currentText.append(ch,start,length);
}
