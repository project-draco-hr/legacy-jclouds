{
  Multimap<String,String> map=request.getPayload() != null ? parseQueryToMap(request.getPayload().getRawContent().toString()) : LinkedHashMultimap.<String,String>create();
  for (  Object o : values)   map.put(key,o.toString());
  return (R)request.toBuilder().payload(newUrlEncodedFormPayload(map)).build();
}
