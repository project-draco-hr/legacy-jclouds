{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  SmtpServerSettings that=SmtpServerSettings.class.cast(o);
  return equal(useAuthentication,that.useAuthentication) && equal(host,that.host) && equal(username,that.username)&& equal(password,that.password);
}
