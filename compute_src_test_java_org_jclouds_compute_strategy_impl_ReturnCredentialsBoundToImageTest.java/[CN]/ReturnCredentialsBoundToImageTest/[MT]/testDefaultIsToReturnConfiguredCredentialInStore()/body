{
  Image image=createMock(Image.class);
  expect(image.getId()).andReturn("1").times(2);
  replay(image);
  LoginCredentials creds=new LoginCredentials("ubuntu","foo",null,false);
  assertEquals(new ReturnCredentialsBoundToImage(null,ImmutableMap.<String,Credentials>of("image#1",creds)).execute(image),creds);
  verify(image);
}
