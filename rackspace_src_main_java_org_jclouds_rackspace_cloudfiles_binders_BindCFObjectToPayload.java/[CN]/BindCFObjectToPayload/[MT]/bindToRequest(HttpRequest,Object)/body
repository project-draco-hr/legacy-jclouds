{
  CFObject object=(CFObject)payload;
  if (object.getPayload().getContentLength() != null && object.getPayload().getContentLength() >= 0) {
    checkArgument(object.getPayload().getContentLength() <= 5l * 1024 * 1024* 1024,"maximum size for put object is 5GB");
  }
 else {
    request.getHeaders().put("Transfer-Encoding","chunked");
  }
  mdBinder.bindToRequest(request,object2Blob.apply(object));
}
