{
  Builder<Hardware> sizes=ImmutableSet.builder();
  sizes.add(cc1_4xlarge().supportsImageIds(ccAmis).build());
  sizes.addAll(ImmutableSet.<Hardware>of(t1_micro().build(),c1_medium().build(),c1_xlarge().build(),m1_large().build(),m1_small32().build(),m1_xlarge().build(),m2_xlarge().build(),m2_2xlarge().build(),m2_4xlarge().build()));
  return sizes.build();
}
