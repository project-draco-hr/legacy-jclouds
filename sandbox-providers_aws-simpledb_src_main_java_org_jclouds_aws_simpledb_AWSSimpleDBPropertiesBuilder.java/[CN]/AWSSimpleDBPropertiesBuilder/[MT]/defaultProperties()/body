{
  Properties properties=super.defaultProperties();
  properties.setProperty(PROPERTY_AUTH_TAG,"AWS");
  properties.putAll(Region.regionProperties());
  properties.setProperty(PROPERTY_ENDPOINT,"https://sdb.amazonaws.com");
  properties.setProperty(PROPERTY_REGION + "." + US_EAST_1+ ".endpoint","https://sdb.amazonaws.com");
  properties.setProperty(PROPERTY_REGION + "." + US_WEST_1+ ".endpoint","https://sdb.us-west-1.amazonaws.com");
  properties.setProperty(PROPERTY_REGION + "." + US_WEST_2+ ".endpoint","https://sdb.us-west-2.amazonaws.com");
  properties.setProperty(PROPERTY_REGION + "." + SA_EAST_1+ ".endpoint","https://sdb.sa-east-1.amazonaws.com");
  properties.setProperty(PROPERTY_REGION + "." + EU_WEST_1+ ".endpoint","https://sdb.eu-west-1.amazonaws.com");
  properties.setProperty(PROPERTY_REGION + "." + AP_SOUTHEAST_1+ ".endpoint","https://sdb.ap-southeast-1.amazonaws.com");
  properties.setProperty(PROPERTY_REGION + "." + AP_NORTHEAST_1+ ".endpoint","https://sdb.ap-northeast-1.amazonaws.com");
  return properties;
}
