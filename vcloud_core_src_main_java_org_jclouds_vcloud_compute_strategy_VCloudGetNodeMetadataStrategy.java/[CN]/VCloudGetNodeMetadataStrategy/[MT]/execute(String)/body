{
  URI id=URI.create(in);
  VApp from=client.getVApp(id);
  if (from == null)   return null;
  String tag=parseTagFromName(from.getName());
  Location location=findLocationForResourceInVDC.apply(from.getVDC());
  Hardware hardware=hardwareForVApp.apply(from);
  return new NodeMetadataImpl(in,from.getName(),in,location,from.getHref(),ImmutableMap.<String,String>of(),tag,hardware,null,toComputeOs(from,null),vAppStatusToNodeState.get(from.getStatus()),getPublicIpsFromVApp(from),getPrivateIpsFromVApp(from),getCredentialsFrom(from));
}
