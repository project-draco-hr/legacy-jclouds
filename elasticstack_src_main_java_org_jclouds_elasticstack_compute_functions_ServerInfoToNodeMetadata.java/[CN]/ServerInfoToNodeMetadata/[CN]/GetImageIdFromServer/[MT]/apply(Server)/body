{
  String imageId=null;
  String bootDeviceId=Iterables.get(from.getBootDeviceIds(),0);
  Device bootDevice=from.getDevices().get(bootDeviceId);
  if (bootDevice != null) {
    try {
      imageId=cache.get(bootDevice.getDriveUuid()).getUuid();
    }
 catch (    NullPointerException e) {
    }
  }
  return imageId;
}
