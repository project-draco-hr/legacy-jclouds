{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Blob<M> other=(Blob<M>)obj;
  if (contentLength != other.contentLength)   return false;
  if (contentRange == null) {
    if (other.contentRange != null)     return false;
  }
 else   if (!contentRange.equals(other.contentRange))   return false;
  if (data == null) {
    if (other.data != null)     return false;
  }
 else   if (!data.equals(other.data))   return false;
  if (getMetadata() == null) {
    if (other.getMetadata() != null)     return false;
  }
 else   if (!getMetadata().equals(other.getMetadata()))   return false;
  return true;
}
