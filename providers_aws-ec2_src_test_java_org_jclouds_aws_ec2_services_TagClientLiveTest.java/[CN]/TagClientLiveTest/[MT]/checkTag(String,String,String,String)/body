{
  Set<Tag> results=client.describeTagsInRegion(null,ImmutableList.<TagFilter>builder().add(new TagFilter(FilterName.RESOURCE_ID,ImmutableList.<String>builder().add(resourceId).build())).add(new TagFilter(FilterName.RESOURCE_TYPE,ImmutableList.<String>builder().add(resourceType).build())).add(new TagFilter(FilterName.KEY,ImmutableList.<String>builder().add(key).build())).add(new TagFilter(FilterName.VALUE,ImmutableList.<String>builder().add(value).build())).build());
  assertNotNull(results);
  assertEquals(results.size(),1);
  Tag tag=Iterables.getOnlyElement(results);
  assertEquals(tag.getResourceId(),resourceId);
  assertEquals(tag.getResourceType(),resourceType);
  assertEquals(tag.getKey(),key);
  assertEquals(tag.getValue(),value);
}
