{
  VApp vApp=tmClient.getVApp(id);
  Set<InetAddress> publicAddresses=computeClient.getPublicAddresses(id);
  return new NodeMetadataImpl(vApp.getId(),vApp.getName(),vAppStatusToNodeState.get(vApp.getStatus()),publicAddresses,vApp.getNetworkToAddresses().values(),22,LoginType.SSH,ImmutableMap.<String,String>of());
}
