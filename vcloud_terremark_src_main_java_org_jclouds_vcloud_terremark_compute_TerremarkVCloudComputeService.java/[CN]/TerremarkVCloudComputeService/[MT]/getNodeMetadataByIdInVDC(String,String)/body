{
  VApp vApp=client.getVApp(id);
  Set<InetAddress> publicAddresses=computeClient.getPublicAddresses(vApp.getId());
  return new NodeMetadataImpl(vApp.getId(),vApp.getName(),vDCId,vApp.getLocation(),ImmutableMap.<String,String>of(),vAppStatusToNodeState.get(vApp.getStatus()),publicAddresses,vApp.getNetworkToAddresses().values(),22,LoginType.SSH,ImmutableMap.<String,String>of());
}
