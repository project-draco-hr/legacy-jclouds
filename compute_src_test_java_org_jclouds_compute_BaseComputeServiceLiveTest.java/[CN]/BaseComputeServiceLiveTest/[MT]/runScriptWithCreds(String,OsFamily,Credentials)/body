{
  Predicate<NodeMetadata> filter=new Predicate<NodeMetadata>(){
    @Override public boolean apply(    @Nullable NodeMetadata nodeMetadata){
      return true;
    }
  }
;
  try {
    return client.runScriptOnNodesMatching(filter,buildScript(osFamily).getBytes(),RunScriptOptions.Builder.overrideCredentialsWith(creds));
  }
 catch (  SshException e) {
    if (Throwables.getRootCause(e).getMessage().contains("Auth fail")) {
      System.err.printf("bad credentials: %s:%s for %s%n",creds.account,creds.key,client.listNodesWithTag(tag));
    }
    throw e;
  }
}
