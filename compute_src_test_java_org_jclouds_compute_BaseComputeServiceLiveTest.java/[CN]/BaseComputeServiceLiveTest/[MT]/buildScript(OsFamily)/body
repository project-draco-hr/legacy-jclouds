{
switch (osFamily) {
case JEOS:
case UBUNTU:
    return new StringBuilder().append("echo nameserver 208.67.222.222 >> /etc/resolv.conf\n").append("cp /etc/apt/sources.list /etc/apt/sources.list.old\n").append("sed 's~us.archive.ubuntu.com~mirror.anl.gov/pub~g' /etc/apt/sources.list.old >/etc/apt/sources.list\n").append("apt-get update\n").append("apt-get install -f -y --force-yes openjdk-6-jdk\n").append("wget -qO/usr/bin/runurl run.alestic.com/runurl\n").append("chmod 755 /usr/bin/runurl\n").toString();
case CENTOS:
case RHEL:
  return new StringBuilder().append("echo nameserver 208.67.222.222 >> /etc/resolv.conf\n").append("echo \"[jdkrepo]\" >> /etc/yum.repos.d/CentOS-Base.repo\n").append("echo \"name=jdkrepository\" >> /etc/yum.repos.d/CentOS-Base.repo\n").append("echo \"baseurl=http://ec2-us-east-mirror.rightscale.com/epel/5/i386/\" >> /etc/yum.repos.d/CentOS-Base.repo\n").append("echo \"enabled=1\" >> /etc/yum.repos.d/CentOS-Base.repo\n").append("yum --nogpgcheck -y install java-1.6.0-openjdk\n").append("echo \"export PATH=\\\"/usr/lib/jvm/jre-1.6.0-openjdk/bin/:\\$PATH\\\"\" >> /root/.bashrc\n").toString();
default :
throw new IllegalArgumentException(osFamily.toString());
}
}
