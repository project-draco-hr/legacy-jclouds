{
  ComputeServiceContext context=null;
  try {
    context=new ComputeServiceContextFactory(setupRestProperties()).createContext(provider,"MOMMA","MIA",ImmutableSet.<Module>of(new Log4JLoggingModule()));
    context.getComputeService().listNodes();
  }
 catch (  AuthorizationException e) {
    throw e;
  }
catch (  RuntimeException e) {
    e.printStackTrace();
    throw e;
  }
 finally {
    if (context != null)     context.close();
  }
}
