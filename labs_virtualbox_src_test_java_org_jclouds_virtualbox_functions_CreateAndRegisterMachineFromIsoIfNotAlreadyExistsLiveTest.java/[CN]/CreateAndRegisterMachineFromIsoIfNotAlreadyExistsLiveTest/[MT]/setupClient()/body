{
  super.setupClient();
  ideControllerName="IDE Controller";
  mode=CleanupMode.Full;
  ideController=StorageController.builder().name(ideControllerName).bus(StorageBus.IDE).attachISO(0,0,operatingSystemIso).attachHardDisk(HardDisk.builder().diskpath(adminDisk).controllerPort(0).deviceSlot(1).build()).attachISO(1,1,guestAdditionsIso).build();
}
