{
  vmName="jclouds-test-create-2-node";
  String vmId=UUID.randomUUID().toString();
  StorageController ideController=StorageController.builder().name(ideControllerName).bus(StorageBus.IDE).attachISO(0,0,operatingSystemIso).attachHardDisk(HardDisk.builder().diskpath(adminDisk(vmName)).controllerPort(0).deviceSlot(1).build()).attachISO(1,1,guestAdditionsIso).build();
  VmSpec vmSpec=VmSpec.builder().id(vmId).name(vmName).memoryMB(512).controller(ideController).cleanUpMode(mode).osTypeId("SomeWeirdUnknownOs").forceOverwrite(true).build();
  IsoSpec isoSpec=IsoSpec.builder().sourcePath(operatingSystemIso).installationScript("").build();
  NetworkSpec networkSpec=NetworkSpec.builder().build();
  MasterSpec machineSpec=MasterSpec.builder().iso(isoSpec).vm(vmSpec).network(networkSpec).build();
  try {
    Injector injector=view.utils().injector();
    injector.getInstance(CreateAndRegisterMachineFromIsoIfNotAlreadyExists.class).apply(machineSpec);
    fail();
  }
 catch (  VBoxException e) {
    ErrorCode errorCode=ErrorCode.valueOf(e);
    assertEquals(errorCode,ErrorCode.VBOX_E_OBJECT_NOT_FOUND);
  }
}
