{
  TerremarkOrgNetwork orgNetwork=client.getNetwork(arg0.getHref());
  TerremarkNetwork terremarkNetwork=client.getTerremarkNetwork(checkNotNull(checkNotNull(orgNetwork,"network at: " + arg0).getNetworkExtension(),"network extension for: " + arg0).getHref());
  return checkNotNull(terremarkNetwork,"terremark network extension at: " + orgNetwork.getNetworkExtension()).getNetworkType() == TerremarkNetwork.Type.DMZ;
}
