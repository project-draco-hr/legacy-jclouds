{
  sshKey=SshKeys.generate();
  String keyPairName="NewKeyPair";
  Vdc currentVDC=lazyGetVdc();
  Media keyPairsContainer=findOrCreateKeyPairContainerInVDCNamed(currentVDC,keyPairContainer,keyPairName);
  String keypairValue=mediaApi.getMetadataApi(keyPairsContainer.getId()).get(keyPairName);
  assertEquals(keypairValue,generateKeyPair(keyPairName));
}
