{
  ToStringHelper helper=super.string().add("org",org).add("description",description).add("configuration",configuration).add("networkPool",networkPool);
  if (allowedExternalIpAddresses.size() > 0)   helper.add("allowedExternalIpAddresses",allowedExternalIpAddresses);
  if (tasks.size() > 0)   helper.add("tasks",tasks);
  return helper;
}
