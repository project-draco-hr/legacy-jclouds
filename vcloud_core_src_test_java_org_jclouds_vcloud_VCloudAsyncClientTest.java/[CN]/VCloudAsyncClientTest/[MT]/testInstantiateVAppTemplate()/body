{
  Method method=VCloudAsyncClient.class.getMethod("instantiateVAppTemplateInVDC",String.class,String.class,String.class,Array.newInstance(InstantiateVAppTemplateOptions.class,0).getClass());
  GeneratedHttpRequest<VCloudAsyncClient> httpMethod=processor.createRequest(method,"1","my-vapp",3 + "");
  assertRequestLineEquals(httpMethod,"POST http://vcloud/vdc/1/action/instantiateVAppTemplate HTTP/1.1");
  assertHeadersEqual(httpMethod,"Accept: application/vnd.vmware.vcloud.vApp+xml\nContent-Length: 634\nContent-Type: application/vnd.vmware.vcloud.instantiateVAppTemplateParams+xml\n");
  assertPayloadEquals(httpMethod,Utils.toStringAndClose(getClass().getResourceAsStream("/newvapp-hosting.xml")));
  assertResponseParserClassEquals(method,httpMethod,ParseSax.class);
  assertSaxResponseParserClassEquals(method,VAppHandler.class);
  assertExceptionParserClassEquals(method,null);
  checkFilters(httpMethod);
}
