{
  identity=checkNotNull(System.getProperty("bluelock-vcloudexpress.identity"),"bluelock-vcloudexpress.identity");
  String credential=checkNotNull(System.getProperty("bluelock-vcloudexpress.credential"),"bluelock-vcloudexpress.credential");
  context=new RestContextFactory().createContext("bluelock-vcloudexpress",identity,credential,ImmutableSet.<Module>of(new Log4JLoggingModule()),new Properties());
  connection=context.getApi();
}
