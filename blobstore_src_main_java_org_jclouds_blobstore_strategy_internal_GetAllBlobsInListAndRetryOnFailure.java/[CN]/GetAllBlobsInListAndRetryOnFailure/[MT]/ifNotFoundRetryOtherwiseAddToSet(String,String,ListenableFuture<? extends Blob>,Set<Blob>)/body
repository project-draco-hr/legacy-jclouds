{
  for (int i=0; i < 3; i++) {
    Blob object=value.get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS);
    if (object == null) {
      Thread.sleep(requestRetryMilliseconds);
      value=connection.getBlob(container,key);
      continue;
    }
    object.getMetadata().setName(key);
    objects.add(object);
    return;
  }
}
