{
  for (int i=0; i < 3; i++) {
    Blob object=(maxTime != null) ? value.get(maxTime,TimeUnit.MILLISECONDS) : value.get();
    if (object == null) {
      retryHandler.imposeBackoffExponentialDelay(i + 1,String.format("blob %s/%s not found",container,key));
      value=connection.getBlob(container,key);
      continue;
    }
    object.getMetadata().setName(key);
    objects.add(object);
    return;
  }
}
