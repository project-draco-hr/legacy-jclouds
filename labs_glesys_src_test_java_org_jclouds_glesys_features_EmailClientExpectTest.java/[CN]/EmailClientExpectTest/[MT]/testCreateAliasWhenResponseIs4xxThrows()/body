{
  EmailClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint(URI.create("https://api.glesys.com/email/createalias/format/json")).headers(ImmutableMultimap.<String,String>builder().put("Accept","application/json").put("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").build()).payload(newUrlEncodedFormPayload(ImmutableMultimap.<String,String>builder().put("emailalias","test2@jclouds.org").put("goto","test@jclouds.org").build())).build(),HttpResponse.builder().statusCode(401).build()).getEmailClient();
  client.createAlias("test2@jclouds.org","test@jclouds.org");
}
