{
  EmailClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint("https://api.glesys.com/email/list/format/json").addHeader("Accept","application/json").addHeader("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").addFormParam("domainname","test").build(),HttpResponse.builder().statusCode(404).build()).getEmailClient();
  assertTrue(client.listAliases("test").isEmpty());
}
