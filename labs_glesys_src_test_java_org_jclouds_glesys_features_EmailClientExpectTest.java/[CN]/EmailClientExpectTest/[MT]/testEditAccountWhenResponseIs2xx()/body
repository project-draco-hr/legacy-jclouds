{
  EmailClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint(URI.create("https://api.glesys.com/email/editaccount/format/json")).headers(ImmutableMultimap.<String,String>builder().put("Accept","application/json").put("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").build()).payload(newUrlEncodedFormPayload(ImmutableMultimap.<String,String>builder().put("emailaccount","test@jclouds.org").put("password","anotherpass").build())).build(),HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/email_details.json",MediaType.APPLICATION_JSON)).build()).getEmailClient();
  assertEquals(client.editAccount("test@jclouds.org",EditAccountOptions.Builder.password("anotherpass")).toString(),getEmailAccountInDetails().toString());
}
