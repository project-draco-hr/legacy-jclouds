{
  EmailClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint(URI.create("https://api.glesys.com/email/delete/format/json")).headers(ImmutableMultimap.<String,String>builder().put("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").build()).payload(newUrlEncodedFormPayload(ImmutableMultimap.<String,String>builder().put("email","test2@jclouds.org").build())).build(),HttpResponse.builder().statusCode(200).build()).getEmailClient();
  client.delete("test2@jclouds.org");
}
