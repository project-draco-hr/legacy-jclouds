{
  EmailClient client=requestSendsResponse(HttpRequest.builder().method("POST").endpoint(URI.create("https://api.glesys.com/email/createaccount/format/json")).headers(ImmutableMultimap.<String,String>builder().put("Accept","application/json").put("Authorization","Basic aWRlbnRpdHk6Y3JlZGVudGlhbA==").build()).payload(newUrlEncodedFormPayload(ImmutableMultimap.<String,String>builder().put("emailaccount","test@jclouds.org").put("password","newpass").build())).build(),HttpResponse.builder().statusCode(404).build()).getEmailClient();
  client.createAccount("test@jclouds.org","newpass");
}
