{
  try {
    Set<Volume> list=gson.fromJson(new InputStreamReader(stream,"UTF-8"),VolumeListResponse.class).volumes;
    for (    Volume volume : list)     if (emptyString.equals(volume.getProductCodes()))     volume.getProductCodes().clear();
    return list;
  }
 catch (  UnsupportedEncodingException e) {
    throw new RuntimeException("jclouds requires UTF-8 encoding",e);
  }
}
