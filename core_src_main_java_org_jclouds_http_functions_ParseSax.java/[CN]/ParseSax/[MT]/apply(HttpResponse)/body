{
  InputStream input=null;
  try {
    input=from.getContent();
    if (input != null) {
      return parse(input);
    }
 else {
      throw new HttpException("No input to parse");
    }
  }
 catch (  Exception e) {
    Utils.<HttpException>rethrowIfRuntimeOrSameType(e);
    throw new HttpException("Error parsing input for " + from,e);
  }
}
