  assertEquals(new InstallRSAPrivateKey("/home/me/.ssh","-----BEGIN RSA PRIVATE KEY-----\n-----END RSA PRIVATE KEY-----\n").render(OsFamily.UNIX),"mkdir -p /home/me/.ssh\nrm /home/me/.ssh/id_rsa\ncat >> /home/me/.ssh/id_rsa <<'END_OF_FILE'\n-----BEGIN RSA PRIVATE KEY-----\n-----END RSA PRIVATE KEY-----\n\nEND_OF_FILE\nchmod 600 /home/me/.ssh/id_rsa\n");
