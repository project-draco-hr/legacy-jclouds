{
  Iterable<? extends Set<? extends Reservation<? extends RunningInstance>>> reservations=transformParallel(regions.get(),new Function<String,Future<? extends Set<? extends Reservation<? extends RunningInstance>>>>(){
    @Override public Future<Set<? extends Reservation<? extends RunningInstance>>> apply(    String from){
      return castToSpecificTypedFuture(client.getInstanceServices().describeInstancesInRegion(from));
    }
  }
,executor,null,logger,"reservations");
  return concat(concat(reservations));
}
