{
  Iterable<? extends Set<? extends Reservation<? extends RunningInstance>>> reservations=transformParallel(regions.get(),new Function<String,ListenableFuture<? extends Set<? extends Reservation<? extends RunningInstance>>>>(){
    @Override public ListenableFuture<? extends Set<? extends Reservation<? extends RunningInstance>>> apply(    String from){
      return client.getInstanceServices().describeInstancesInRegion(from);
    }
  }
,userExecutor,maxTime,logger,"reservations");
  return concat(concat(reservations));
}
