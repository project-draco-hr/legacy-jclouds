{
  this.creds=creds;
  checkState(OAUTH_ALGORITHM_NAMES_TO_KEYFACTORY_ALGORITHM_NAMES.containsKey(signatureOrMacAlgorithm),format("No mapping for key factory for algorithm: %s",signatureOrMacAlgorithm));
  this.keyCache=CacheBuilder.newBuilder().maximumSize(2).build(checkNotNull(loader,"loader"));
}
