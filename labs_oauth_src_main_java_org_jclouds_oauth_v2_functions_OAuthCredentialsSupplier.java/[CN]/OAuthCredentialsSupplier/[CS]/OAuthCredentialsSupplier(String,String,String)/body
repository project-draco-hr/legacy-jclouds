{
  this.identity=identity;
  this.privateKeyInPemFormat=privateKeyInPemFormat;
  checkState(OAUTH_ALGORITHM_NAMES_TO_KEYFACTORY_ALGORITHM_NAMES.containsKey(signatureOrMacAlgorithm),format("No mapping for key factory for algorithm: %s",signatureOrMacAlgorithm));
  this.keyFactoryAlgorithm=OAUTH_ALGORITHM_NAMES_TO_KEYFACTORY_ALGORITHM_NAMES.get(signatureOrMacAlgorithm);
}
