{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",vAppId + "/action/exitMaintenanceMode").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().httpResponseBuilder().statusCode(204).build());
  api.getVAppApi().exitMaintenanceMode(vAppURI);
}
