{
  if (from == null)   return null;
  try {
    return uriBuilder(cdnContainer.getUnchecked(from.getContainer())).clearQuery().appendPath(from.getName()).build();
  }
 catch (  NullPointerException e) {
    return null;
  }
catch (  CacheLoader.InvalidCacheLoadException e) {
    return null;
  }
}
