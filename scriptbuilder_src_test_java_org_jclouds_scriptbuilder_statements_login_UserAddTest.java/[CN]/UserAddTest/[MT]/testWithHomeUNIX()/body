{
  assertEquals(UserAdd.builder().login("me").home("/myhome/myme").build().render(OsFamily.UNIX),"mkdir -p /myhome/myme\nuseradd -s /bin/bash -m  -d /myhome/myme me\nchown -R me /myhome/myme\n");
  assertEquals(UserAdd.builder().login("me").home("/myhome/myme").defaultHome("/ignoreddefault").build().render(OsFamily.UNIX),"mkdir -p /myhome/myme\nuseradd -s /bin/bash -m  -d /myhome/myme me\nchown -R me /myhome/myme\n");
}
