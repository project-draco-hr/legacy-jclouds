{
  metadataValue=mediaClient.getMetadataValue(mediaRef,"key");
  checkResourceType(metadataValue);
  String value=metadataValue.getValue();
  assertNotNull(value,String.format(OBJ_FIELD_ATTRB_REQ,MEDIA,"MetadataEntry",metadataValue.toString(),metadataEntryValue.toString()));
  assertEquals(value,metadataEntryValue,String.format(OBJ_FIELD_EQ,MEDIA,"metadataEntry.value",metadataEntryValue,value));
}
