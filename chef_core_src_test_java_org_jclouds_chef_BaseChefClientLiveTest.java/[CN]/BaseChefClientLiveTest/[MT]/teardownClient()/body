{
  if (getValidatorConnection().clientExists(PREFIX))   getValidatorConnection().deleteClient(PREFIX);
  if (getAdminConnection().nodeExists(PREFIX))   getAdminConnection().deleteNode(PREFIX);
  if (getAdminConnection().roleExists(PREFIX))   getAdminConnection().deleteRole(PREFIX);
  if (getAdminConnection().databagExists(PREFIX))   getAdminConnection().deleteDatabag(PREFIX);
  closeContexts();
}
