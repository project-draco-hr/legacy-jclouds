{
  checkNotNull(virtualHardwareQuantity.get(ResourceType.PROCESSOR),"cpuCount must be present in instantiateVapp on terremark");
  checkNotNull(virtualHardwareQuantity.get(ResourceType.MEMORY),"memorySizeMegabytes must be present in instantiateVapp on terremark");
  return super.generateXml(name,template,properties,virtualHardwareQuantity,networkName,fenceMode,dhcp,network);
}
