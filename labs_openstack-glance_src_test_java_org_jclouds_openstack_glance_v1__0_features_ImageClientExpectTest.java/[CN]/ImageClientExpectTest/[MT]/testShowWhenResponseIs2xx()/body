{
  HttpRequest show=HttpRequest.builder().method("HEAD").endpoint(URI.create("https://glance.jclouds.org:9292/v1.0/images/fcc451d0-f6e4-4824-ad8f-70ec12326d07")).headers(ImmutableMultimap.<String,String>builder().put("X-Auth-Token",authToken).build()).build();
  HttpResponse showResponse=new ParseImageDetailsFromHeadersTest().response;
  GlanceClient clientWhenExist=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,show,showResponse);
  assertEquals(clientWhenExist.getConfiguredRegions(),ImmutableSet.of("az-1.region-a.geo-1"));
  assertEquals(clientWhenExist.getImageClientForRegion("az-1.region-a.geo-1").show("fcc451d0-f6e4-4824-ad8f-70ec12326d07").toString(),new ParseImageDetailsFromHeadersTest().expected().toString());
}
