{
  HttpRequest listInDetail=HttpRequest.builder().method("GET").endpoint(URI.create("https://glance.jclouds.org:9292/v1.0/images/detail")).headers(ImmutableMultimap.<String,String>builder().put("Accept","application/json").put("X-Auth-Token",authToken).build()).build();
  HttpResponse listInDetailResponse=HttpResponse.builder().statusCode(404).build();
  GlanceClient clientWhenNoExistInDetail=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,listInDetail,listInDetailResponse);
  assertTrue(clientWhenNoExistInDetail.getImageClientForRegion("az-1.region-a.geo-1").listInDetail().isEmpty());
}
