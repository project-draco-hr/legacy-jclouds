{
  if (response.getPayload() != null) {
    try {
      String content=Utils.toStringAndClose(response.getPayload().getInput());
      if (content != null && content.indexOf('<') >= 0)       return utils.parseAzureStorageErrorFromContent(command,response,Utils.toInputStream(content));
    }
 catch (    IOException e) {
      logger.warn(e,"exception reading error from response",response);
    }
  }
  return null;
}
