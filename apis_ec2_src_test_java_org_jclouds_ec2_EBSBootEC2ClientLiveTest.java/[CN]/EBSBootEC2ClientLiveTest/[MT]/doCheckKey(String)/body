{
  SshClient ssh=sshFactory.create(new IPSocket(address,22),LoginCredentials.builder().user("ubuntu").privateKey(keyPair.getKeyMaterial()).build());
  try {
    ssh.connect();
    ExecResponse hello=ssh.exec("echo hello");
    assertEquals(hello.getOutput().trim(),"hello");
  }
  finally {
    if (ssh != null)     ssh.disconnect();
  }
}
