{
  final String templateId="/vAppTemplate/vappTemplate-ef4415e6-d413-4cbb-9262-f9bbec5f2ea9";
  URI uri=URI.create(endpoint + templateId);
  MetadataApi api=orderedRequestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",templateId + "/metadata").acceptMedia(ANY).httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vapptemplate/error400.xml",ERROR).httpResponseBuilder().statusCode(400).build()).getMetadataApi(uri);
  api.get();
}
