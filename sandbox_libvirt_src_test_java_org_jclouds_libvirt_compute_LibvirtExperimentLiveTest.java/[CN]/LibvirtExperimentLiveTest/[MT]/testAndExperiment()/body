{
  ComputeServiceContext context=null;
  try {
    context=new ComputeServiceContextFactory().createContext(new StandaloneComputeServiceContextSpec<Domain,Domain,Image,Datacenter>("libvirt",endpoint,apiversion,identity,credential,new LibvirtComputeServiceContextModule(),ImmutableSet.<Module>of()));
    context.getComputeService().runNodesWithTag("ubuntu",1);
  }
 catch (  RunNodesException e) {
    e.printStackTrace();
  }
 finally {
    if (context != null)     context.close();
  }
}
