{
  TestDto dto=new TestDto();
  Method withUnexistingLink=TestEndpointLink.class.getMethod("withUnexistingLink",TestDto.class);
  GeneratedHttpRequest request=GeneratedHttpRequest.builder().declaring(TestEndpointLink.class).javaMethod(withUnexistingLink).args(ImmutableList.<Object>of(dto)).method(HttpMethod.GET).endpoint(URI.create("http://localhost")).build();
  BindToPath binder=new BindToPath();
  binder.bindToRequest(request,dto);
}
