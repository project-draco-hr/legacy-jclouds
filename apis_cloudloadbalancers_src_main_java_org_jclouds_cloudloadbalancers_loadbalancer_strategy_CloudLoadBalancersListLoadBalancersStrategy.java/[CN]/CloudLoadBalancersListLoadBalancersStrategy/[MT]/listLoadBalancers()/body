{
  return transform(concat(transformParallel(regions.get(),new Function<String,Future<Set<LoadBalancer>>>(){
    @Override public ListenableFuture<Set<LoadBalancer>> apply(    String from){
      return aclient.getLoadBalancerClient(from).listLoadBalancers();
    }
  }
,executor,null,logger,"loadbalancers")),converter);
