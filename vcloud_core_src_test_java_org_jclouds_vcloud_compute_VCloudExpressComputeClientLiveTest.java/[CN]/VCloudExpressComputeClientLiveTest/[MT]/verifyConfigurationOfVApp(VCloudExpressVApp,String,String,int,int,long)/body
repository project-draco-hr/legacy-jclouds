{
  assertEquals(Iterables.getOnlyElement(filter(vApp.getResourceAllocations(),resourceType(ResourceType.PROCESSOR))).getVirtualQuantity(),processorCount);
  assertEquals(Iterables.getOnlyElement(filter(vApp.getResourceAllocations(),resourceType(ResourceType.SCSI_CONTROLLER))).getVirtualQuantity(),1);
  assertEquals(Iterables.getOnlyElement(filter(vApp.getResourceAllocations(),resourceType(ResourceType.MEMORY))).getVirtualQuantity(),memory);
  assertEquals(Iterables.getOnlyElement(filter(vApp.getResourceAllocations(),resourceType(ResourceType.DISK_DRIVE))).getVirtualQuantity(),hardDisk);
}
