{
  if (parserClass == null)   assertEquals(RestAnnotationProcessor.createExceptionParserOrThrowResourceNotFoundOn404IfNoAnnotation(injector,method).getClass(),MapHttp4xxCodesToExceptions.class);
 else   assertEquals(RestAnnotationProcessor.createExceptionParserOrThrowResourceNotFoundOn404IfNoAnnotation(injector,method).getClass(),parserClass);
}
