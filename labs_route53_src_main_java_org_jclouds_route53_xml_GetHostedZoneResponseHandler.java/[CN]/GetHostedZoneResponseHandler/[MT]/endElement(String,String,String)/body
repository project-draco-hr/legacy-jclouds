{
  if (inZone) {
    if (qName.equals("HostedZone")) {
      inZone=false;
      zone=zoneHandler.getResult();
    }
 else {
      zoneHandler.endElement(uri,name,qName);
    }
  }
 else   if (qName.equals("NameServer")) {
    nameServers.add(currentOrNull(currentText));
  }
  currentText=new StringBuilder();
}
