{
  URI endpoint=URI.create("https://az-1.region-a.geo-1.compute.hpcloudsvc.com/v1.1/3456/servers/1/action");
  AdminActionsClient client=requestsSendResponses(keystoneAuthWithUsernameAndPasswordAndTenantName,responseWithKeystoneAccess,extensionsOfNovaRequest,extensionsOfNovaResponse,standardActionRequestBuilderVoidResponse(endpoint,"unlock").build(),HttpResponse.builder().statusCode(404).build()).getAdminActionsExtensionForZone("az-1.region-a.geo-1").get();
  assertFalse(client.unlockServer("1"));
}
