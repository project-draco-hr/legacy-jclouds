{
  return new NodeMetadataBuilder().providerId(node.getProviderId()).name(node.getName()).id(node.getId()).location(node.getLocation()).uri(node.getUri()).userMetadata(node.getUserMetadata()).tag(node.getTag()).hardware(node.getHardware()).imageId(node.getImageId()).operatingSystem(node.getOperatingSystem()).state(node.getState()).publicAddresses(node.getPublicAddresses()).privateAddresses(node.getPrivateAddresses()).credentials(node.getCredentials());
}
