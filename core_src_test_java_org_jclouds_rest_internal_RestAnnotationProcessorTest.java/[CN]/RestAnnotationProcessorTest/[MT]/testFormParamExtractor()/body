{
  Method method=TestPath.class.getMethod("oneFormParamExtractor",String.class);
  GeneratedHttpRequest<?> httpMethod=factory(TestPath.class).createRequest(method,new Object[]{"localhost"});
  assertRequestLineEquals(httpMethod,"POST http://localhost:8080/ HTTP/1.1");
  assertHeadersEqual(httpMethod,"Content-Length: 5\nContent-Type: application/x-www-form-urlencoded\n");
  assertPayloadEquals(httpMethod,"one=l");
}
