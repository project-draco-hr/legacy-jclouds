{
  Method method=TestConstantPathParam.class.getMethod("twoPaths",String.class,String.class);
  GeneratedHttpRequest<?> httpMethod=factory(TestConstantPathParam.class).createRequest(method,new Object[]{"1","localhost"});
  assertRequestLineEquals(httpMethod,"GET http://localhost:8080/v1/ralphie/1/localhost HTTP/1.1");
  assertHeadersEqual(httpMethod,"");
  assertPayloadEquals(httpMethod,null);
}
