{
  Method method=TestTransformers.class.getMethod("put",Payload.class);
  HttpRequest request=factory(TestQuery.class).createRequest(method,Payloads.newStringPayload("whoops"));
  assertEquals(request.getRequestLine(),"PUT http://localhost:9999?x-ms-version=2009-07-17 HTTP/1.1");
  assertEquals(request.getHeaders(),Multimaps.forMap(ImmutableMap.of("Content-Length","6","Content-Type","application/unknown")));
  assertEquals(request.getPayload().getRawContent(),"whoops");
}
