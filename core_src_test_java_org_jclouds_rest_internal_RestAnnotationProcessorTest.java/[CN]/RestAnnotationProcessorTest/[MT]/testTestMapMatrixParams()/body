{
  Method method=TestMapMatrixParams.class.getMethod("action",String.class,String.class,Map.class);
  GeneratedHttpRequest<TestMapMatrixParams> httpMethod=factory(TestMapMatrixParams.class).createRequest(method,new Object[]{"robot","kill",ImmutableMap.of("death","slow")});
  assertEquals(httpMethod.getRequestLine(),"POST http://localhost:8080/objects/robot/action/kill;death=slow HTTP/1.1");
  assertEquals(httpMethod.getHeaders().size(),0);
}
