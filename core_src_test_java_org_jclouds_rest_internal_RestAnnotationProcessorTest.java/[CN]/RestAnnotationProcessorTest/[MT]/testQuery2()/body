{
  Method method=TestQuery.class.getMethod("foo2");
  GeneratedHttpRequest<?> httpMethod=factory(TestQuery.class).createRequest(method,new Object[]{});
  assertEquals(httpMethod.getEndpoint().getHost(),"localhost");
  assertEquals(httpMethod.getEndpoint().getPath(),"");
  assertEquals(httpMethod.getEndpoint().getQuery(),"x-ms-version=2009-07-17&foo=bar&fooble=baz");
  assertEquals(httpMethod.getMethod(),"FOO");
}
