{
  RestContextSpec<Caller,AsyncCaller> contextSpec=contextSpec("test","http://localhost:9999","1","","","userfoo",null,Caller.class,AsyncCaller.class,ImmutableSet.<Module>builder().add(new MockModule(service)).add(new NullLoggingModule()).add(new CallerModule()).addAll(ImmutableSet.<Module>copyOf(modules)).build());
