{
  Map<String,BlobStoreContext> contexts=ImmutableMap.<String,BlobStoreContext>of("test1",new BlobStoreContextFactory().createContext("transient","dummy","dummy"),"test2",new BlobStoreContextFactory().createContext("transient","dummy","dummy"));
  for (  BlobStoreContext blobstore : contexts.values()) {
    blobstore.getAsyncBlobStore().createContainerInLocation(null,"favo").get();
  }
  return contexts;
}
