{
  String groupName=PREFIX + "ingress";
  try {
    client.getSecurityGroupServices().deleteSecurityGroup(groupName);
  }
 catch (  Exception e) {
  }
  client.getSecurityGroupServices().createSecurityGroup(groupName,groupName);
  client.getSecurityGroupServices().authorizeSecurityGroupIngress(groupName,IpProtocol.TCP,80,80,"0.0.0.0/0");
  assertEventually(new GroupHasPermission(client,groupName,new IpPermission(80,80,Sets.<UserIdGroupPair>newTreeSet(),IpProtocol.TCP,ImmutableSortedSet.of("0.0.0.0/0"))));
  client.getSecurityGroupServices().revokeSecurityGroupIngress(groupName,IpProtocol.TCP,80,80,"0.0.0.0/0");
  assertEventually(new GroupHasNoPermissions(client,groupName));
}
