{
  GlobalDomainClient client=requestSendsResponse(HttpRequest.builder().method("GET").endpoint(URI.create("http://localhost:8080/client/api?response=json&" + "command=updateDomain&id=10&name=test-2&apiKey=identity&signature=5t1eUf2Eyf%2FaB6qt%2BqIj%2BmcwFIo%3D")).headers(ImmutableMultimap.<String,String>builder().put("Accept","application/json").build()).build(),HttpResponse.builder().statusCode(200).payload(payloadFromResource("/updatedomainresponse.json")).build());
  assertEquals(client.updateDomain("10",name("test-2")),Domain.builder().id("10").name("test-2").level(1).parentDomainId("1").parentDomainName("ROOT").hasChild(false).build());
}
