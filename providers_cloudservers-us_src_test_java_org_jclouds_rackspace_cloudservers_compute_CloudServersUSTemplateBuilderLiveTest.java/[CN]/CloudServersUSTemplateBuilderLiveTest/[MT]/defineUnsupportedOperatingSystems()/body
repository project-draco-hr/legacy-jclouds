{
  return Predicates.not(new Predicate<OsFamilyVersion64Bit>(){
    @Override public boolean apply(    OsFamilyVersion64Bit input){
switch (input.family) {
case UBUNTU:
        return !(input.version.startsWith("11") || input.version.equals("8.04")) && input.is64Bit;
case DEBIAN:
      return input.is64Bit;
case CENTOS:
    return !(input.version.matches("5.[023]")) && input.is64Bit;
case WINDOWS:
  return input.version.equals("2008 SP2") || input.version.equals("") || (input.version.equals("2008 R2") && input.is64Bit);
default :
return false;
}
}
}
);
}
