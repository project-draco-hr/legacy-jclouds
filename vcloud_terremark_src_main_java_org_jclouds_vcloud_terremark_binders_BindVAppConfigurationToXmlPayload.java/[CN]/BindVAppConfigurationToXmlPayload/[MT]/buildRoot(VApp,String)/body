{
  XMLBuilder rootBuilder=XMLBuilder.create("VApp").a("type",vApp.getType()).a("name",name).a("status",vApp.getStatus().value()).a("size",vApp.getSize() + "").a("xmlns",ns).a("xmlns:xsi","http://www.w3.org/2001/XMLSchema-instance").a("xsi:schemaLocation",ns + " " + schema);
