{
  for (  Object arg : gRequest.getArgs()) {
    if (arg instanceof TerremarkVApp) {
      return (TerremarkVApp)arg;
    }
 else     if (arg instanceof TerremarkVApp[]) {
      TerremarkVApp[] vapps=(TerremarkVApp[])arg;
      return (vapps.length > 0) ? vapps[0] : null;
    }
  }
  return null;
}
