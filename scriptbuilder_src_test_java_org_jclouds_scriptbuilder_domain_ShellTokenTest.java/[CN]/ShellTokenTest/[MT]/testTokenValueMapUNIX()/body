{
  Map<String,String> expected=new ImmutableMap.Builder<String,String>().put("fs","/").put("ps",":").put("lf","\n").put("sh","bash").put("source",".").put("rem","#").put("args","$@").put("varstart","$").put("return","return").put("varend","").put("libraryPathVariable","LD_LIBRARY_PATH").put("beginScript","#!/bin/bash\nset +u\nshopt -s xpg_echo\nshopt -s expand_aliases\n").put("endScript","set -u\nreturn 0\n").put("vq","\"").put("beginFunctions","").put("endFunctions","").put("fncl","function ").put("fncr"," {\n").put("fnce","   return 0\n}\n").put("export","export").build();
  assertEquals(ShellToken.tokenValueMap(OsFamily.UNIX),expected);
}
