{
  Map<String,String> expected=new ImmutableMap.Builder<String,String>().put("fs","/").put("ps",":").put("lf","\n").put("sh","bash").put("source",".").put("rem","#").put("args","$@").put("varstart","$").put("return","return").put("varend","").put("libraryPathVariable","LD_LIBRARY_PATH").put("shebang","#!/bin/bash\n").build();
  assertEquals(ShellToken.tokenValueMap(OsFamily.UNIX),expected);
}
