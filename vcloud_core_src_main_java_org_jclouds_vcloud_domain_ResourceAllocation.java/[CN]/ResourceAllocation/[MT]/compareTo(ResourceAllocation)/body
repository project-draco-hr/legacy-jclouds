{
  final int BEFORE=-1;
  final int EQUAL=0;
  final int AFTER=1;
  if (this == that)   return EQUAL;
  if (this.id < that.id)   return BEFORE;
  if (this.id > that.id)   return AFTER;
  if (this.addressOnParent != null && that.addressOnParent != null) {
    if (this.addressOnParent < that.addressOnParent)     return BEFORE;
    if (this.addressOnParent > that.addressOnParent)     return AFTER;
  }
  return 1;
}
