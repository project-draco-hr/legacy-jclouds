{
  InputStream is=getClass().getResourceAsStream("/orglist.xml");
  Map<String,Link> result=factory.create(injector.getInstance(OrgListHandler.class)).parse(is);
  assertEquals(result,ImmutableMap.of("adrian@jclouds.org",new LinkImpl("adrian@jclouds.org",VCloudMediaType.ORG_XML,URI.create("https://services.vcloudexpress.terremark.com/api/v0.8/org/48"))));
}
