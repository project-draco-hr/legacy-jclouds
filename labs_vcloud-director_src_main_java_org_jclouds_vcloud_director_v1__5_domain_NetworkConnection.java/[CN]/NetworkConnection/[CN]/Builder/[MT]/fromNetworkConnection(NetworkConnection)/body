{
  return networkConnectionIndex(in.getNetworkConnectionIndex()).ipAddress(in.getIpAddress()).externalIpAddress(in.getExternalIpAddress()).isConnected(in.isConnected()).macAddress(in.getMACAddress()).ipAddressAllocationMode(in.getIpAddressAllocationMode()).network(in.getNetwork()).needsCustomization(in.needsCustomization());
}
