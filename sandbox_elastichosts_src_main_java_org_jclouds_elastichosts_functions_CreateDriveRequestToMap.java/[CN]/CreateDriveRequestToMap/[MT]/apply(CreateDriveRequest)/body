{
  ImmutableMap.Builder<String,String> builder=ImmutableMap.builder();
  builder.putAll(baseDriveToMap.apply(from));
  if (from.getAvoid().size() != 0)   builder.put("avoid",Joiner.on(' ').join(from.getAvoid()));
  if (from.getEncryptionCipher() != null)   builder.put("encryption:cipher",from.getEncryptionCipher());
  return builder.build();
}
