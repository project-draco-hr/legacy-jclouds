{
  CreateServerOptions options=withSecurityGroup("mygroup").withSecurityGroup("myothergroup");
  HttpRequest request=buildRequest(options);
  assertEquals(request.getPayload().getRawContent(),"{\"server\":{\"name\":\"foo\",\"imageRef\":\"1\",\"flavorRef\":\"2\",\"security_groups\":[{\"id\":0,\"name\":\"myothergroup\"},{\"id\":0,\"name\":\"mygroup\"}]}}");
}
