{
  assertOperationDoneSucessfully(api().delete(INSTANCE_NAME),TIME_WAIT);
  assertOperationDoneSucessfully(context.getApi().getDiskApiForProject(getUserProject()).delete(DISK_NAME),TIME_WAIT);
  assertOperationDoneSucessfully(context.getApi().getNetworkApiForProject(getUserProject()).delete(INSTANCE_NETWORK_NAME),TIME_WAIT);
}
