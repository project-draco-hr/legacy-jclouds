{
  assertOperationDoneSucessfully(context.getApi().getNetworkApiForProject(getUserProject()).createInIPv4Range(INSTANCE_NETWORK_NAME,IPV4_RANGE),TIME_WAIT);
  assertOperationDoneSucessfully(context.getApi().getDiskApiForProject(getUserProject()).createInZone("instance-live-test-disk",1,getDefaultZoneUrl(getUserProject())),TIME_WAIT);
  assertOperationDoneSucessfully(api().createInZone(INSTANCE_NAME,instance,DEFAULT_ZONE_NAME),TIME_WAIT);
}
