{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  SizeImpl other=(SizeImpl)obj;
  if (cores != other.cores)   return false;
  if (disk != other.disk)   return false;
  if (ram != other.ram)   return false;
  if (supportedArchitectures == null) {
    if (other.supportedArchitectures != null)     return false;
  }
 else   if (!supportedArchitectures.equals(other.supportedArchitectures))   return false;
  return true;
}
