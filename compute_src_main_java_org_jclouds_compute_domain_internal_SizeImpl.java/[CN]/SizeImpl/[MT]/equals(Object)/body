{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (getClass() != obj.getClass())   return false;
  SizeImpl other=(SizeImpl)obj;
  if (Double.doubleToLongBits(cores) != Double.doubleToLongBits(other.cores))   return false;
  if (disk != other.disk)   return false;
  if (ram != other.ram)   return false;
  return true;
}
