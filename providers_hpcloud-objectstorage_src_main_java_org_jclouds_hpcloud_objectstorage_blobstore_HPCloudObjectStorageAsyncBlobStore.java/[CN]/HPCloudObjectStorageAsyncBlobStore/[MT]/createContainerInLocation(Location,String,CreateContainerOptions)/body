{
  ListenableFuture<Boolean> returnVal=createContainerInLocation(location,container);
  if (options.isPublicRead())   return Futures.compose(createContainerInLocation(location,container),new Function<Boolean,Boolean>(){
    @Override public Boolean apply(    Boolean input){
      if (Boolean.TRUE.equals(input)) {
        return enableAndCache.apply(container) != null;
      }
      return false;
    }
  }
,service);
  return returnVal;
}
