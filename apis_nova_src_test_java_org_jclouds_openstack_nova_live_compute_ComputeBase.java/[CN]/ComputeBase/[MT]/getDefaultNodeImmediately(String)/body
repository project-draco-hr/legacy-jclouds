{
  for (  ComputeMetadata node : computeService.listNodes()) {
    if (((NodeMetadata)node).getGroup() != null)     if (((NodeMetadata)node).getGroup().equals(group))     if (((NodeMetadata)node).getState().equals(NodeState.PENDING) || ((NodeMetadata)node).getState().equals(NodeState.RUNNING))     return (NodeMetadata)node;
  }
  return createDefaultNode(group);
}
