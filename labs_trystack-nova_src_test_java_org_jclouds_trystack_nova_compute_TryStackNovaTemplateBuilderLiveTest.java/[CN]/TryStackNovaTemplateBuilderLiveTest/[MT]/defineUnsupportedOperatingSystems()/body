{
  return Predicates.not(new Predicate<OsFamilyVersion64Bit>(){
    @Override public boolean apply(    OsFamilyVersion64Bit input){
switch (input.family) {
case UBUNTU:
        return (input.version.equals("") || input.version.matches("^11.*")) && input.is64Bit;
default :
      return false;
  }
}
}
);
}
