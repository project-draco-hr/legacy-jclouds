{
  String queueName=PREFIX + "1";
  for (  final String region : Lists.newArrayList(null,Region.EU_WEST_1,Region.US_EAST_1,Region.US_WEST_1,Region.AP_SOUTHEAST_1)) {
    try {
      SortedSet<Queue> result=Sets.newTreeSet(client.listQueuesInRegion(region,queuePrefix(queueName)));
      if (result.size() >= 1) {
        client.deleteQueue(result.last());
        queueName+=1;
      }
    }
 catch (    Exception e) {
    }
    Queue queue=null;
    int tries=0;
    while (queue == null && tries < 5) {
      try {
        tries++;
        queue=client.createQueueInRegion(region,queueName);
      }
 catch (      AWSResponseException e) {
        queueName+="1";
        if (e.getError().getCode().equals("AWS.SimpleQueueService.QueueDeletedRecently"))         continue;
        throw e;
      }
    }
    if (region != null)     assertEquals(queue.getRegion(),region);
    assertEquals(queue.getName(),queueName);
    assertQueueInList(region,queue);
    queues.add(queue);
  }
}
