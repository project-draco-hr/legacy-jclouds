{
  checkArgument(checkNotNull(request,"input") instanceof GeneratedHttpRequest,"this binder is only valid for GeneratedHttpRequests!");
  checkArgument(checkNotNull(input,"input") instanceof String[],"this binder is only valid for String[] : " + input.getClass());
  String[] addressStrings=(String[])input;
  EC2Utils.indexStringArrayToFormValuesWithPrefix((GeneratedHttpRequest<?>)request,"PublicIp",addressStrings);
}
