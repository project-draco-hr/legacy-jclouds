{
  HttpCommand command=createCommand();
  HttpResponse response=new HttpResponse();
  assertEquals(handler.shouldRetryRequest(command,response),true);
  assertEquals(handler.shouldRetryRequest(command,response),true);
  assertEquals(handler.shouldRetryRequest(command,response),true);
  assertEquals(handler.shouldRetryRequest(command,response),true);
  assertEquals(handler.shouldRetryRequest(command,response),true);
  assertEquals(handler.shouldRetryRequest(command,response),false);
}
