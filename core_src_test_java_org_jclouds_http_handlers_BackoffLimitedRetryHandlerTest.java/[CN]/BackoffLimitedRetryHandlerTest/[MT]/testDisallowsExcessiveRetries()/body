{
  HttpFutureCommand<String> command=new HttpFutureCommand<String>("method","uri",new ReturnStringIf200());
  HttpResponse response=new HttpResponse();
  assertEquals(handler.retryRequest(command,response),true);
  assertEquals(handler.retryRequest(command,response),true);
  assertEquals(handler.retryRequest(command,response),true);
  assertEquals(handler.retryRequest(command,response),true);
  assertEquals(handler.retryRequest(command,response),true);
  assertEquals(handler.retryRequest(command,response),false);
}
