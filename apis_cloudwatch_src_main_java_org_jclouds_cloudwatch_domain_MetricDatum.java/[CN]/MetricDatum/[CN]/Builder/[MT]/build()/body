{
  Preconditions.checkNotNull(metricName,"metricName cannot be null.");
  Preconditions.checkNotNull(unit,"unit cannot be null.");
  Preconditions.checkArgument(metricName.length() > 1 && metricName.length() <= 255,"metricName cannot be empty and must be 255 characters or less.");
  Preconditions.checkArgument(statisticSet != null || value != null,"statisticSet or value must be set");
  return new MetricDatum(dimensions,metricName,statisticSet,timestamp,unit,value);
}
