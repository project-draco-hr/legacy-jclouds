{
  HttpResponse listResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/describe_policies.xml","text/xml")).build();
  ELBApi apiWhenExist=requestSendsResponse(list,listResponse);
  assertEquals(apiWhenExist.getPolicyApi().list().toString(),new DescribeLoadBalancerPoliciesResponseTest().expected().toString());
}
