{
  return PaginatedIterables.lazyContinue(instanceClient.list(options),new Function<Object,PaginatedIterable<Instance>>(){
    @Override public PaginatedIterable<Instance> apply(    Object input){
      return instanceClient.list(options.clone().afterMarker(input));
    }
    @Override public String toString(){
      return "listInstances(" + options + ")";
    }
  }
);
}
