{
  String script=ChefSolo.builder().cookbooksArchiveLocation("/tmp/cookbooks").installRecipe("apache2").installRecipe("mysql").build().render(OsFamily.UNIX);
  assertEquals(script,"setupPublicCurl || return 1\ninstallChefGems || return 1\nchef-solo -N `hostname` -r /tmp/cookbooks -o recipe[apache2],recipe[mysql]\n");
}
