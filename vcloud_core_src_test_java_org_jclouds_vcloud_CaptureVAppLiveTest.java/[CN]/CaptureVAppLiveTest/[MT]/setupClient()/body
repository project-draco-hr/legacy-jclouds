{
  setupCredentials();
  Properties props=new Properties();
  props.setProperty(Constants.PROPERTY_TRUST_ALL_CERTS,"true");
  props.setProperty(Constants.PROPERTY_RELAX_HOSTNAME,"true");
  client=new ComputeServiceContextFactory().createContext(provider,identity,credential,ImmutableSet.<Module>of(new Log4JLoggingModule()),props).getComputeService();
}
