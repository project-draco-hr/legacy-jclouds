{
  if (qName.equals("container")) {
    if (inContainer) {
      ignore=true;
      return;
    }
    inContainer=true;
    int index=attributes.getIndex("xlink:href");
    if (index != -1) {
      currentUrl=URI.create(attributes.getValue(index));
    }
  }
 else   if (qName.equals("parent") && !ignore) {
    int index=attributes.getIndex("xlink:href");
    if (index != -1) {
      currentParent=URI.create(attributes.getValue(index));
    }
  }
}
