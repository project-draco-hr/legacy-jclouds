{
  String bucketName=bucketPrefix + "filetestsforadrian";
  client.createBucketIfNotExists(bucketName).get(10,TimeUnit.SECONDS);
  context.createS3ObjectMap(bucketName).clear();
  assertEquals(client.getBucket(bucketName).get(10,TimeUnit.SECONDS).getContents().size(),0);
  S3Object object=new S3Object(key);
  object.getMetaData().setContentType(type);
  object.setData(content);
  assertNotNull(client.addObject(bucketName,object).get(10,TimeUnit.SECONDS));
  object=client.getObject(bucketName,object.getKey()).get(10,TimeUnit.SECONDS);
  returnedString=S3Utils.getContentAsStringAndClose(object);
  assertEquals(returnedString,realObject);
  assertEquals(client.getBucket(bucketName).get(10,TimeUnit.SECONDS).getContents().size(),1);
}
