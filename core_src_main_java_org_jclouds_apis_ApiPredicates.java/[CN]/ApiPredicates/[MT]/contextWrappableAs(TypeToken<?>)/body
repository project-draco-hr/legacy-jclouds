{
  checkNotNull(type,"context must be defined");
  return new Predicate<ApiMetadata>(){
    /** 
 * {@inheritDoc}
 */
    @Override public boolean apply(    ApiMetadata apiMetadata){
      for (      TypeToken<? extends Wrapper> to : apiMetadata.getWrappers())       if (type.isAssignableFrom(to))       return true;
      return false;
    }
    /** 
 * {@inheritDoc}
 */
    @Override public String toString(){
      return "contextWrappableAs(" + type + ")";
    }
  }
;
}
