{
  Map<String,String> copy=Maps.newHashMap();
  copy.putAll(postParams);
  if (postParams.get("count") == null)   copy.put("count","1");
  if (postParams.get("megabytes") == null)   copy.put("megabytes","512");
  if (postParams.get("network") == null)   copy.put("network",defaultNetwork.toASCIIString());
  super.bindToRequest(request,copy);
}
