{
  return new MemoizedRetryOnTimeOutButNotOnAuthorizationExceptionSupplier<Map<URI,? extends org.jclouds.vcloud.domain.VDC>>(authException,seconds,new Supplier<Map<URI,? extends org.jclouds.vcloud.domain.VDC>>(){
    @Override public Map<URI,? extends org.jclouds.vcloud.domain.VDC> get(){
      return supplier.get();
    }
  }
);
}
