{
  return new MemoizedRetryOnTimeOutButNotOnAuthorizationExceptionSupplier<Map<String,Map<String,Map<String,? extends org.jclouds.vcloud.domain.CatalogItem>>>>(authException,seconds,new Supplier<Map<String,Map<String,Map<String,? extends org.jclouds.vcloud.domain.CatalogItem>>>>(){
    @Override public Map<String,Map<String,Map<String,? extends org.jclouds.vcloud.domain.CatalogItem>>> get(){
      return supplier.get();
    }
  }
);
}
