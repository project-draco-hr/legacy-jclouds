{
  return new MemoizedRetryOnTimeOutButNotOnAuthorizationExceptionSupplier<Map<String,Map<String,? extends org.jclouds.vcloud.domain.Catalog>>>(authException,seconds,new Supplier<Map<String,Map<String,? extends org.jclouds.vcloud.domain.Catalog>>>(){
    @Override public Map<String,Map<String,? extends org.jclouds.vcloud.domain.Catalog>> get(){
      return supplier.get();
    }
  }
);
}
