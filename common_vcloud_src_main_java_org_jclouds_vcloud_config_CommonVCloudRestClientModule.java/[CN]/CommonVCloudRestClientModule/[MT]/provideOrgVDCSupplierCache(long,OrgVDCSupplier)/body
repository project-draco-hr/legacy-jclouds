{
  return new RetryOnTimeOutButNotOnAuthorizationExceptionSupplier<Map<String,Map<String,? extends org.jclouds.vcloud.domain.VDC>>>(authException,seconds,new Supplier<Map<String,Map<String,? extends org.jclouds.vcloud.domain.VDC>>>(){
    @Override public Map<String,Map<String,? extends org.jclouds.vcloud.domain.VDC>> get(){
      return supplier.get();
    }
  }
);
}
