{
  if (this == o)   return true;
  if (o instanceof StorageController) {
    StorageController other=(StorageController)o;
    return Objects.equal(name,other.name) && Objects.equal(bus,other.bus) && Objects.equal(hardDisks,other.hardDisks)&& Objects.equal(isoImages,other.isoImages);
  }
  return false;
}
