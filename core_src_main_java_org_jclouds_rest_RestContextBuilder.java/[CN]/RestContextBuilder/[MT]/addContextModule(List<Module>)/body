{
  modules.add(new AbstractModule(){
    @SuppressWarnings("unchecked") @Override protected void configure(){
      bind((TypeLiteral)TypeLiteral.get(Types.newParameterizedType(RestContext.class,syncClientType,asyncClientType))).to(TypeLiteral.get(Types.newParameterizedType(RestContextImpl.class,syncClientType,asyncClientType))).in(Scopes.SINGLETON);
    }
  }
);
}
