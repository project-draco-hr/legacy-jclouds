{
  try {
    return connection.getObjectMetaData(bucket,key.toString()).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS) != S3Object.MetaData.NOT_FOUND;
  }
 catch (  Exception e) {
    Utils.<S3RuntimeException>rethrowIfRuntimeOrSameType(e);
    throw new S3RuntimeException(String.format("Error searching for %1s:%2s",bucket,key),e);
  }
}
