{
  URI networkUri=URI.create(endpoint + "/network/f3ba8256-6f48-4512-aad6-600e85b4dc38");
  VCloudDirectorUserClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/network/f3ba8256-6f48-4512-aad6-600e85b4dc38").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/network/network.xml",VCloudDirectorMediaType.ORG_NETWORK).httpResponseBuilder().build());
  OrgNetwork expected=orgNetwork();
  assertEquals(Network.<OrgNetwork>toSubType(client.getNetworkClient().getNetwork(networkUri)),expected);
}
