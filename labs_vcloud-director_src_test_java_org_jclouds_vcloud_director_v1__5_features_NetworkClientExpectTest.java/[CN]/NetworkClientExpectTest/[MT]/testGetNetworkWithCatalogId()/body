{
  URI networkUri=URI.create(endpoint + "/network/9e08c2f6-077a-42ce-bece-d5332e2ebb5c");
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/network/9e08c2f6-077a-42ce-bece-d5332e2ebb5c").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/network/error403-catalog.xml",VCloudDirectorMediaType.ERROR).httpResponseBuilder().statusCode(403).build());
  assertNull(client.getNetworkClient().getNetwork(networkUri));
}
