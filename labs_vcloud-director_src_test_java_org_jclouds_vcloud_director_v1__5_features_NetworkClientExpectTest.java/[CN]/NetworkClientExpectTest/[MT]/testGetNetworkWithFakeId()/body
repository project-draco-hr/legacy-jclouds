{
  URI networkUri=URI.create(endpoint + "/network/aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeee");
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/network/aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeee").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/network/error403-fake.xml",VCloudDirectorMediaType.ERROR).httpResponseBuilder().statusCode(403).build());
  assertNull(client.getNetworkClient().getNetwork(networkUri));
}
