{
  URI orgUri=URI.create("https://vcloudbeta.bluelock.com/api/org/6f312e42-cd2b-488d-a2bb-97519cd57ed0");
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/org/6f312e42-cd2b-488d-a2bb-97519cd57ed0/metadata/KEY").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/org/orgMetadataValue.xml",VCloudDirectorMediaType.METADATA_VALUE).httpResponseBuilder().build());
  MetadataValue expected=metadataValue();
  assertEquals(api.getOrgApi().getMetadataApi().getValue(orgUri,"KEY"),expected);
}
