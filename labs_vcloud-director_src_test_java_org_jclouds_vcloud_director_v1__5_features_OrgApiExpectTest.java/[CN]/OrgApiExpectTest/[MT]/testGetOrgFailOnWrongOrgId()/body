{
  URI orgUri=URI.create(endpoint + "/org/9e08c2f6-077a-42ce-bece-d5332e2ebb5c");
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,getStandardRequest("GET","/org/9e08c2f6-077a-42ce-bece-d5332e2ebb5c"),getStandardPayloadResponse(403,"/org/error403-catalog.xml",VCloudDirectorMediaType.ERROR));
  assertNull(api.getOrgApi().getOrg(orgUri));
}
