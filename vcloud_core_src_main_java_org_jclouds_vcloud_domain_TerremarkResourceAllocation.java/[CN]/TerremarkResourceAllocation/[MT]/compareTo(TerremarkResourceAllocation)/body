{
  final int BEFORE=-1;
  final int EQUAL=0;
  final int AFTER=1;
  if (this == that)   return EQUAL;
  int comparison=this.resourceType.compareTo(that.resourceType);
  if (comparison != EQUAL)   return comparison;
  if (this.instanceID < that.instanceID)   return BEFORE;
  if (this.instanceID > that.instanceID)   return AFTER;
  return EQUAL;
}
