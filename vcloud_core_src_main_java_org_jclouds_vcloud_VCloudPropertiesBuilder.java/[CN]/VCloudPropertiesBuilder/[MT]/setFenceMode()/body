{
  if (properties.getProperty(PROPERTY_VCLOUD_DEFAULT_FENCEMODE) == null) {
    if (properties.getProperty(PROPERTY_VCLOUD_VERSION_SCHEMA).startsWith("0.8"))     properties.setProperty(PROPERTY_VCLOUD_DEFAULT_FENCEMODE,"allowInOut");
 else     properties.setProperty(PROPERTY_VCLOUD_DEFAULT_FENCEMODE,FenceMode.BRIDGED);
  }
}
