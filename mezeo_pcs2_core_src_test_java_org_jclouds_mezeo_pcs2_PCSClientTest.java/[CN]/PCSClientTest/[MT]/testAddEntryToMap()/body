{
  Method method=PCSAsyncClient.class.getMethod("addMetadataItemToMap",URI.class,String.class,Map.class);
  GeneratedHttpRequest<PCSAsyncClient> httpMethod=processor.createRequest(method,new Object[]{URI.create("http://localhost/pow"),"newkey",ImmutableMap.of("key","value")});
  assertEquals(httpMethod.getRequestLine(),"GET http://localhost/pow/metadata/newkey HTTP/1.1");
  assertEquals(httpMethod.getHeaders().size(),0);
  assertEquals(processor.createExceptionParserOrNullIfNotFound(method),null);
  assertEquals(processor.createResponseParser(method,httpMethod).getClass(),AddMetadataItemIntoMap.class);
}
