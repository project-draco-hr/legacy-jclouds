{
  String schemafileName="master.xsd";
  HttpResponse response=context.getUtils().getHttpClient().invoke(HttpRequest.builder().method("GET").endpoint(endpoint + "/v1.5/schema/" + schemafileName).addHeader("x-vcloud-authorization",sessionWithToken.getToken()).addHeader("Accept","*/*").build());
