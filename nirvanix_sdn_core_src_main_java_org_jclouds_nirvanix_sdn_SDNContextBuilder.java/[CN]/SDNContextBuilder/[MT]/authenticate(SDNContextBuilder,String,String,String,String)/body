{
  builder.getProperties().setProperty(SDNConstants.PROPERTY_SDN_APPKEY,checkNotNull(appkey,"appkey"));
  builder.getProperties().setProperty(SDNConstants.PROPERTY_SDN_APPNAME,checkNotNull(appname,"appname"));
  builder.getProperties().setProperty(SDNConstants.PROPERTY_SDN_USERNAME,checkNotNull(username,"username"));
  builder.getProperties().setProperty(SDNConstants.PROPERTY_SDN_PASSWORD,checkNotNull(password,"password"));
}
