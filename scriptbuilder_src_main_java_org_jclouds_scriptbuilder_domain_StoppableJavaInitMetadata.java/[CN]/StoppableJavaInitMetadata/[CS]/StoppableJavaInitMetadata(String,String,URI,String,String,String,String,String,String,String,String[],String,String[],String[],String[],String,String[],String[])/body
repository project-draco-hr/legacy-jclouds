{
  super(name,platformHome,endPoint,startDir,stopDir,configDir,dataDir,logDir,goldDir,javaHome,classpath,mainClass,opts,args);
  this.stopClasspath=checkNotNull(stopClasspath,"stopClasspath");
  this.stopClass=checkNotNull(stopClass,"stopClass");
