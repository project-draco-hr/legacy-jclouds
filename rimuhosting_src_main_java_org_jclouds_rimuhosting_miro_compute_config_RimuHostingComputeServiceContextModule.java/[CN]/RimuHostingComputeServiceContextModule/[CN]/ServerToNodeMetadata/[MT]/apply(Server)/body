{
  String locationId="//TODO";
  String tag=from.getName().replaceAll("-[0-9]+","");
  Credentials creds=null;
  NodeState state=runningStateToNodeState.get(from.getState());
  return new NodeMetadataImpl(from.getId() + "",from.getName(),locationId,null,ImmutableMap.<String,String>of(),tag,state,getPublicAddresses.apply(from),ImmutableList.<InetAddress>of(),ImmutableMap.<String,String>of(),creds);
}
