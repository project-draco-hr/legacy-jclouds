{
  Iterable<? extends AWSRunningInstance> spots=filter(transform(concat(transformParallel(regions,new Function<String,Future<Set<SpotInstanceRequest>>>(){
    @SuppressWarnings("unchecked") @Override public Future<Set<SpotInstanceRequest>> apply(    String from){
      return (Future<Set<SpotInstanceRequest>>)client.getSpotInstanceServices().describeSpotInstanceRequestsInRegion(from);
    }
  }
,executor,null,logger,"reservations")),spotConverter),notNull());
  return concat(super.pollRunningInstances(),spots);
}
