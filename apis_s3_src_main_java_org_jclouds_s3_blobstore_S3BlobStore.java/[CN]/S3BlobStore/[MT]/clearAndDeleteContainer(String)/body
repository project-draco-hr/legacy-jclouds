{
  try {
    if (!Assertions.eventuallyTrue(new Supplier<Boolean>(){
      public Boolean get(){
        clearContainer(container);
        return sync.deleteBucketIfEmpty(container);
      }
    }
,30000)) {
      throw new IllegalStateException(container + " still exists after deleting!");
    }
  }
