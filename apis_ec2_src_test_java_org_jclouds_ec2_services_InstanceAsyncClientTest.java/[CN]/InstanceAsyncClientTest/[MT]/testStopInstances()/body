{
  Invokable<?,?> method=method(InstanceAsyncClient.class,"stopInstancesInRegion",String.class,boolean.class,String[].class);
  GeneratedHttpRequest request=processor.createRequest(method,Lists.<Object>newArrayList(null,true,"1","2"));
  assertRequestLineEquals(request,"POST https://ec2.us-east-1.amazonaws.com/ HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"Host: ec2.us-east-1.amazonaws.com\n");
  assertPayloadEquals(request,"Action=StopInstances&Force=true&InstanceId.1=1&InstanceId.2=2","application/x-www-form-urlencoded",false);
  assertResponseParserClassEquals(method,request,ParseSax.class);
  assertSaxResponseParserClassEquals(method,InstanceStateChangeHandler.class);
  assertFallbackClassEquals(method,null);
  checkFilters(request);
}
