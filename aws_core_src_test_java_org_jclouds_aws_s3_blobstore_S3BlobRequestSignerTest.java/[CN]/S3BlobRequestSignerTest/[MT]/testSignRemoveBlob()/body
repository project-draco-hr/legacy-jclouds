{
  HttpRequest request=signer.signRemoveBlob("container","name");
  assertRequestLineEquals(request,"DELETE https://container.s3.amazonaws.com/name HTTP/1.1");
  assertNonPayloadHeadersEqual(request,"Authorization: AWS identity:4FnyjdX/ULdDMRbVlLNjZfEo9RQ=\nDate: Thu, 05 Jun 2008 16:38:19 GMT\nHost: container.s3.amazonaws.com\n");
  assertPayloadEquals(request,null,null,false);
  assertEquals(request.getFilters().size(),0);
}
