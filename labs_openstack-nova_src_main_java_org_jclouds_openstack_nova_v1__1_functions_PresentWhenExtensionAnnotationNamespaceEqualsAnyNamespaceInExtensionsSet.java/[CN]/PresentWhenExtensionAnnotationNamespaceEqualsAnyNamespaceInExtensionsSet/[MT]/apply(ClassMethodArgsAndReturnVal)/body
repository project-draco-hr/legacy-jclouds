{
  Optional<org.jclouds.openstack.services.Extension> ext=Optional.fromNullable(input.getClazz().getAnnotation(org.jclouds.openstack.services.Extension.class));
  if (ext.isPresent()) {
    checkState(input.getArgs() != null && input.getArgs().length == 1,"expecting an arg %s",input);
    URI namespace=URI.create(ext.get().namespace());
    if (Iterables.any(extensions.getUnchecked(checkNotNull(input.getArgs()[0],"arg[0] in %s",input).toString()),ExtensionPredicates.namespaceEquals(namespace)))     return Optional.of(input.getReturnVal());
  }
  return Optional.absent();
}
