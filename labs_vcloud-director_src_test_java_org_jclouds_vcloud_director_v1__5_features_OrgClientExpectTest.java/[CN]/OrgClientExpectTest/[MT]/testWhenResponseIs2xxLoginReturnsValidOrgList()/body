{
  HttpRequest orgListRequest=HttpRequest.builder().method("GET").endpoint(URI.create("http://localhost/api/org/")).headers(ImmutableMultimap.<String,String>builder().put("Accept","*/*").put("x-vcloud-authorization",token).build()).build();
  HttpResponse orgListResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/orglist.xml",VCloudDirectorMediaType.ORGLIST_XML + ";version=1.5")).build();
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,orgListRequest,orgListResponse);
  assertEquals(client.getOrgClient().getOrgList(),OrgList.builder().addOrg(OrgLink.builder().type("application/vnd.vmware.vcloud.org+xml").name("JClouds").href(URI.create("https://vcloudbeta.bluelock.com/api/org/6f312e42-cd2b-488d-a2bb-97519cd57ed0")).build()).build());
}
