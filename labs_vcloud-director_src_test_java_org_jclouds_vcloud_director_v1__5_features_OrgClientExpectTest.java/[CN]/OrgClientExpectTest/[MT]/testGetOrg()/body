{
  URI orgUri=URI.create(endpoint + "/org/6f312e42-cd2b-488d-a2bb-97519cd57ed0");
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,getStandardRequest("GET","/org/6f312e42-cd2b-488d-a2bb-97519cd57ed0"),getStandardPayloadResponse("/org/org.xml",VCloudDirectorMediaType.ORG));
  Org expected=org();
  assertEquals(client.getOrgClient().getOrg(orgUri),expected);
}
