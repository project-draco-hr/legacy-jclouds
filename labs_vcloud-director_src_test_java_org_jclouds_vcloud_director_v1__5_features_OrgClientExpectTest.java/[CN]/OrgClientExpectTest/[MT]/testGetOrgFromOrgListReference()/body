{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,getStandardRequest("GET","/org/"),getStandardPayloadResponse("/org/orglist.xml",VCloudDirectorMediaType.ORG_LIST));
  Reference org=Iterables.getOnlyElement(client.getOrgClient().getOrgList().getOrgs());
  client=requestsSendResponses(loginRequest,sessionResponse,getStandardRequest("GET",org.getHref()),getStandardPayloadResponse("/org/org.xml",VCloudDirectorMediaType.ORG));
  Org expected=org();
  assertEquals(client.getOrgClient().getOrg(org),expected);
}
