{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vmId + "/virtualHardwareSection/memory").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getVirtualHardwareSectionMemory.xml",VCloudDirectorMediaType.OVF_RASD_ITEM).httpResponseBuilder().build());
  RasdItem expected=getVirtualHardwareSectionMemory();
  assertEquals(api.getVmApi().getVirtualHardwareSectionMemory(vmURI),expected);
}
