{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",vmId + "/action/discardSuspendedState").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/discardSuspendedStateTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=discardSuspendedStateTask();
  assertEquals(api.getVmApi().discardSuspendedState(vmURI),expected);
}
