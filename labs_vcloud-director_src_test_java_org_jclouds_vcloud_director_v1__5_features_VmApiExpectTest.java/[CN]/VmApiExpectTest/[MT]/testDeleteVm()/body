{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("DELETE",vmId).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vm/deleteVmTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=deleteVmTask();
  assertEquals(api.getVmApi().deleteVm(vmURI),expected);
}
