{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vmId + "/virtualHardwareSection/cpu").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getVirtualHardwareSectionCpu.xml",VCloudDirectorMediaType.OVF_RASD_ITEM).httpResponseBuilder().build());
  RasdItem expected=getVirtualHardwareSectionCpu();
  assertEquals(api.getVmApi().getVirtualHardwareSectionCpu(vmURI),expected);
}
