{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT",vmId + "/productSections").xmlFilePayload("/vApp/editProductSections.xml",VCloudDirectorMediaType.PRODUCT_SECTION_LIST).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/editProductSections.xml",VCloudDirectorMediaType.VAPP).httpResponseBuilder().build());
  Task expected=editProductSectionsTask();
  assertEquals(api.getVmApi().editProductSections(vmURI,null),expected);
}
