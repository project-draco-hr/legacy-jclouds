{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vmId + "/networkConnectionSection").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getNetworkConnectionSection.xml",VCloudDirectorMediaType.NETWORK_CONNECTION_SECTION).httpResponseBuilder().build());
  NetworkConnectionSection expected=getNetworkConnectionSection();
  assertEquals(api.getVmApi().getNetworkConnectionSection(vmURI),expected);
}
