{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",vmId + "/power/action/reset").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/resetTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=resetTask();
  assertEquals(api.getVmApi().reset(vmURI),expected);
}
