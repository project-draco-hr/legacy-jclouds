{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vmId + "/operatingSystemSection").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getOperatingSystemSection.xml",VCloudDirectorMediaType.OPERATING_SYSTEM_SECTION).httpResponseBuilder().build());
  OperatingSystemSection expected=getOperatingSystemSection();
  assertEquals(api.getVmApi().getOperatingSystemSection(vmURI),expected);
}
