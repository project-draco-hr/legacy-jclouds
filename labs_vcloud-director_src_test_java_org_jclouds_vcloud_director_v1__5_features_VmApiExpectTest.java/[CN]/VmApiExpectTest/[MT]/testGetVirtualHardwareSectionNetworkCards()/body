{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET",vmId + "/virtualHardwareSection/networkCards").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/getVirtualHardwareSectionNetworkCards.xml",VCloudDirectorMediaType.OVF_RASD_ITEMS_LIST).httpResponseBuilder().build());
  RasdItemsList expected=getVirtualHardwareSectionNetworkCards();
  assertEquals(api.getVmApi().getVirtualHardwareSectionNetworkCards(vmURI),expected);
}
