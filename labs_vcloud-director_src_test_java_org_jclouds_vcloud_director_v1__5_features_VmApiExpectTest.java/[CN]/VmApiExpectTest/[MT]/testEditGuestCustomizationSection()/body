{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT",vmId + "/guestCustomizationSection").xmlFilePayload("/vApp/editGuestCustomizationSection.xml",VCloudDirectorMediaType.GUEST_CUSTOMIZATION_SECTION).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/editGuestCustomizationSectionTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  GuestCustomizationSection section=getGuestCustomizationSection().toBuilder().build();
  Task expected=editGuestCustomizationSectionTask();
  assertEquals(api.getVmApi().editGuestCustomizationSection(vmURI,section),expected);
}
