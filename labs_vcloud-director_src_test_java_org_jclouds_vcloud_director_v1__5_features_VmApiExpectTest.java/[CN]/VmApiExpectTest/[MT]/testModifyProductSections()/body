{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("PUT",vmId + "/productSections").xmlFilePayload("/vApp/modifyProductSections.xml",VCloudDirectorMediaType.PRODUCT_SECTION_LIST).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/modifyProductSections.xml",VCloudDirectorMediaType.VAPP).httpResponseBuilder().build());
  Task expected=modifyProductSectionsTask();
  assertEquals(api.getVmApi().modifyProductSections(vmURI,null),expected);
}
