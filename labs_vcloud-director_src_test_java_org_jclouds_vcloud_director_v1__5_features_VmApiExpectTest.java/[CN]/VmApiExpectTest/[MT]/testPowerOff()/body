{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",vmId + "/power/action/powerOff").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/powerOffTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=powerOffTask();
  assertEquals(api.getVmApi().powerOff(vmURI),expected);
}
