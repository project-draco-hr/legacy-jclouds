{
  VCloudDirectorApi api=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST",vmId + "/action/installVMwareTools").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vApp/installVMwareToolsTask.xml",VCloudDirectorMediaType.TASK).httpResponseBuilder().build());
  Task expected=installVMwareToolsTask();
  assertEquals(api.getVmApi().installVMwareTools(vmURI),expected);
}
