{
  HttpRequest get=HttpRequest.builder().method("GET").endpoint(OPERATIONS_URL_PREFIX + "/operation-1352178598164-4cdcc9d031510-4aa46279").addHeader("Accept","application/json").addHeader("Authorization","Bearer " + TOKEN).build();
