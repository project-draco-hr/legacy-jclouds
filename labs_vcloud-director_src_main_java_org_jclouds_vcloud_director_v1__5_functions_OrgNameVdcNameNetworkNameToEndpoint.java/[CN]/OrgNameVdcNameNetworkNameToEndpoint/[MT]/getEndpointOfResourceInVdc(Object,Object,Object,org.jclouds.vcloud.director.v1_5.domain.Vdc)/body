{
  Reference resourceEntity=Iterables.find(VdcObject.getAvailableNetworks(),nameEquals((String)Vdc));
  if (resourceEntity == null)   throw new NoSuchElementException("network " + resource + " in Vdc "+ Vdc+ ", org "+ org+ " not found in "+ VdcObject.getAvailableNetworks());
  return resourceEntity.getHref();
}
