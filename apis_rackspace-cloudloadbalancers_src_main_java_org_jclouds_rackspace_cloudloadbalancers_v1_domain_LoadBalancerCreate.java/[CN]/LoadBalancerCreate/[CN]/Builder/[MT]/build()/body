{
  if (virtualIps == null) {
    return new LoadBalancerCreate(name,protocol,port,nodes,algorithm,timeout,halfClosed,sessionPersistence,connectionLogging,connectionThrottle,healthMonitor,accessRules,metadata,virtualIPType,virtualIPId);
  }
 else {
    return new LoadBalancerCreate(name,protocol,port,nodes,algorithm,timeout,halfClosed,sessionPersistence,connectionLogging,connectionThrottle,healthMonitor,accessRules,metadata,virtualIps);
  }
}
