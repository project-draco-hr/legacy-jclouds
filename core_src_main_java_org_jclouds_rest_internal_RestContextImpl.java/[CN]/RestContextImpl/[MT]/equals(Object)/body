{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  RestContextImpl other=(RestContextImpl)obj;
  if (apiVersion == null) {
    if (other.apiVersion != null)     return false;
  }
 else   if (!apiVersion.equals(other.apiVersion))   return false;
  if (endpoint == null) {
    if (other.endpoint != null)     return false;
  }
 else   if (!endpoint.equals(other.endpoint))   return false;
  if (identity == null) {
    if (other.identity != null)     return false;
  }
 else   if (!identity.equals(other.identity))   return false;
  if (provider == null) {
    if (other.provider != null)     return false;
  }
 else   if (!provider.equals(other.provider))   return false;
  return true;
}
