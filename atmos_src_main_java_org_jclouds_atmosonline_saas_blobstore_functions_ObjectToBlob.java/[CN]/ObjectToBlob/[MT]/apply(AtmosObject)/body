{
  if (from == null)   return null;
  Blob blob=blobFactory.create(object2BlobMd.apply(from));
  if (from.getContentMetadata().getContentLength() != null)   blob.setContentLength(from.getContentMetadata().getContentLength());
  blob.setPayload(checkNotNull(from.getPayload(),"payload: " + from));
  blob.setAllHeaders(from.getAllHeaders());
  return blob;
}
