{
  return fromVdc(in).resourceGuaranteedMemory(in.getResourceGuaranteedMemory()).resourceGuaranteedCpu(in.getResourceGuaranteedCpu()).vCpuInMhz(in.getVCpuInMhz()).isThinProvision(in.isThinProvision()).networkPoolReference(in.getNetworkPoolReference()).providerVdcReference(in.getProviderVdcReference()).usesFastProvisioning(in.usesFastProvisioning());
}
