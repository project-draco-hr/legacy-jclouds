{
  if (context.getConsistencyModel() == ConsistencyModels.STRICT) {
    assertion.run();
    return;
  }
 else {
    AssertionError error=null;
    for (int i=0; i < 30; i++) {
      try {
        assertion.run();
        return;
      }
 catch (      AssertionError e) {
        error=e;
      }
      Thread.sleep(INCONSISTENCY_WINDOW / 30);
    }
    if (error != null)     throw error;
  }
}
