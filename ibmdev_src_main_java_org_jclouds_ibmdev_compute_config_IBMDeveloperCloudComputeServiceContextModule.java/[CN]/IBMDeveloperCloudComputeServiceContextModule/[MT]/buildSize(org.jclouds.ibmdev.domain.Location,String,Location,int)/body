{
  return new SizeImpl(id,id,location.getId() + "/" + id,assignedLocation,null,ImmutableMap.<String,String>of(),multiplier,multiplier * 1024,multiplier * 10,new Predicate<Image>(){
    @Override public boolean apply(    Image input){
      if (input instanceof IBMImage)       return IBMImage.class.cast(input).rawImage.getSupportedInstanceTypes().contains(id);
      return false;
    }
  }
);
}
