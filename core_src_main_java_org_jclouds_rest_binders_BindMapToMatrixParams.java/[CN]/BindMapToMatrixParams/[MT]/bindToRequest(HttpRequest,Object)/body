{
  checkArgument(checkNotNull(request,"input") instanceof GeneratedHttpRequest,"this binder is only valid for GeneratedHttpRequests!");
  checkArgument(checkNotNull(input,"input") instanceof Map,"this binder is only valid for Maps!");
  Map<String,String> map=(Map<String,String>)input;
  for (  Entry<String,String> entry : map.entrySet()) {
    ((GeneratedHttpRequest)request).replaceMatrixParam(entry.getKey(),entry.getValue());
  }
}
