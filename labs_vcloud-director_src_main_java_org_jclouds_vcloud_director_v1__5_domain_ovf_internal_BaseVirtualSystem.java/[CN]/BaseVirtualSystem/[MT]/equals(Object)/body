{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  BaseVirtualSystem other=(BaseVirtualSystem)obj;
  return super.equals(other) && equal(id,other.id) && equal(name,other.name)&& equal(operatingSystem,other.operatingSystem)&& equal(virtualHardwareSections,other.virtualHardwareSections)&& equal(productSections,other.productSections)&& equal(additionalSections,other.additionalSections);
}
