{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("GET","/vdc/e9cd3387-ac57-4d27-a481-9bee75e0690f").acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vdc/vdc.xml",VCloudDirectorMediaType.VDC).httpResponseBuilder().build());
  Vdc expected=getVdc();
  assertEquals(client.getVdcClient().getVdc(vdcURI),expected);
}
