{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST","/vdc/e9cd3387-ac57-4d27-a481-9bee75e0690f/action/media").xmlFilePayload("/vdc/params/createMedia.xml",VCloudDirectorMediaType.MEDIA).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vdc/createMedia.xml",VCloudDirectorMediaType.MEDIA).httpResponseBuilder().build());
  Media expected=createMedia();
  Media createMedia=Media.builder().build();
  assertEquals(client.getVdcClient().createMedia(vdcURI,createMedia),expected);
}
