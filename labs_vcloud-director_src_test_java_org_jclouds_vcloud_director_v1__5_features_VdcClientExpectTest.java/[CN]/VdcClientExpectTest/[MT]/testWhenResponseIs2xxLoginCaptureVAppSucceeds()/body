{
  URI vdcUri=URI.create(endpoint + "/vdc/e9cd3387-ac57-4d27-a481-9bee75e0690f/action/captureVApp");
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,getStandardPayloadRequest("POST","/vdc/e9cd3387-ac57-4d27-a481-9bee75e0690f/action/captureVApp","/vdc/params/captureVApp",VCloudDirectorMediaType.CAPTURE_VAPP_PARAMS),getStandardPayloadResponse("/vdc/vdc.xml",VCloudDirectorMediaType.VDC));
  Vdc expected=vdc();
  Reference vdcRef=Reference.builder().href(vdcUri).build();
  assertEquals(client.getVdcClient().captureVApp(vdcRef,params),expected);
}
