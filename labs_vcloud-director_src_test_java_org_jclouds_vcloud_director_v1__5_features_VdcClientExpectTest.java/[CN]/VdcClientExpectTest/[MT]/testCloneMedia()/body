{
  VCloudDirectorClient client=requestsSendResponses(loginRequest,sessionResponse,new VcloudHttpRequestPrimer().apiCommand("POST","/vdc/e9cd3387-ac57-4d27-a481-9bee75e0690f/action/cloneMedia").xmlFilePayload("/vdc/params/cloneMedia.xml",VCloudDirectorMediaType.CLONE_MEDIA_PARAMS).acceptAnyMedia().httpRequestBuilder().build(),new VcloudHttpResponsePrimer().xmlFilePayload("/vdc/cloneMedia.xml",VCloudDirectorMediaType.MEDIA).httpResponseBuilder().build());
  Media expected=cloneMedia();
  CloneMediaParams params=CloneMediaParams.builder().build();
  assertEquals(client.getVdcClient().cloneMedia(vdcRef,params),expected);
}
