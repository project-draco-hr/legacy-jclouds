{
  HttpRequest listFlavors=HttpRequest.builder().method("GET").endpoint("https://az-1.region-a.geo-1.compute.hpcloudsvc.com/v1.1/3456/flavors").addHeader("Accept","application/json").addHeader("X-Auth-Token",authToken).build();
  HttpResponse listFlavorsResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResource("/flavor_list.json")).build();
  NovaClient clientWhenFlavorsExist=requestsSendResponses(keystoneAuthWithUsernameAndPasswordAndTenantName,responseWithKeystoneAccess,listFlavors,listFlavorsResponse);
  assertEquals(clientWhenFlavorsExist.getConfiguredZones(),ImmutableSet.of("az-1.region-a.geo-1"));
  assertEquals(clientWhenFlavorsExist.getFlavorClientForZone("az-1.region-a.geo-1").listFlavors().toString(),new ParseFlavorListTest().expected().toString());
}
