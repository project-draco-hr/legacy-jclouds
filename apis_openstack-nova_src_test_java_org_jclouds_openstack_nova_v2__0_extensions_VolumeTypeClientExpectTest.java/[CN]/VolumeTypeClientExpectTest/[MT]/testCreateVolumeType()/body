{
  URI endpoint=URI.create("https://az-1.region-a.geo-1.compute.hpcloudsvc.com/v1.1/3456/os-volume-types");
  VolumeTypeClient client=requestsSendResponses(keystoneAuthWithUsernameAndPassword,responseWithKeystoneAccess,extensionsOfNovaRequest,extensionsOfNovaResponse,standardRequestBuilder(endpoint).method("POST").payload(payloadFromStringWithContentType("{\"volume_type\":{\"name\":\"jclouds-test-1\"}}",MediaType.APPLICATION_JSON)).build(),standardResponseBuilder(200).payload(payloadFromResource("/volume_type.json")).build()).getVolumeTypeExtensionForZone("az-1.region-a.geo-1").get();
  VolumeType type=client.createVolumeType("jclouds-test-1");
  assertEquals(type,testVolumeType());
}
