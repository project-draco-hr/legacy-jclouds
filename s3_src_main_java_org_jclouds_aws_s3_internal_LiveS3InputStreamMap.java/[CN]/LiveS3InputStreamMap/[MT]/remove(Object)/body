{
  InputStream old=get(o);
  try {
    connection.deleteObject(bucket,o.toString()).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS);
  }
 catch (  Exception e) {
    Utils.<S3RuntimeException>rethrowIfRuntimeOrSameType(e);
    throw new S3RuntimeException(String.format("Error removing object %1s:%2s",bucket,o),e);
  }
  return old;
}
