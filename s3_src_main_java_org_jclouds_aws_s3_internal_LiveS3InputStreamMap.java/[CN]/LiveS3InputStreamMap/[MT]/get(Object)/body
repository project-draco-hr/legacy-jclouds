{
  try {
    return (InputStream)(connection.getObject(bucket,o.toString()).get(requestTimeoutMilliseconds,TimeUnit.MILLISECONDS)).getContent();
  }
 catch (  Exception e) {
    Utils.<S3RuntimeException>rethrowIfRuntimeOrSameType(e);
    throw new S3RuntimeException(String.format("Error geting object %1s:%2s",bucket,o),e);
  }
}
