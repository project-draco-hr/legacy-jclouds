{
  String toParse=reader.nextString();
  try {
    return standardDateService.iso8601SecondsDateParse(toParse);
  }
 catch (  Exception ex) {
    try {
synchronized (dateFormat) {
        return dateFormat.parse(toParse);
      }
    }
 catch (    ParseException e) {
      throw Throwables.propagate(e);
    }
  }
}
