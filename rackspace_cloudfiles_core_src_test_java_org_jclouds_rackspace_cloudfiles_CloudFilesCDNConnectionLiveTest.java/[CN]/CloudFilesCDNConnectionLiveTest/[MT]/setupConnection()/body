{
  String account=System.getProperty("jclouds.test.user");
  String key=System.getProperty("jclouds.test.key");
  cdnConnection=CloudFilesCDNContextBuilder.newBuilder(account,key).withModule(new Log4JLoggingModule()).withJsonDebug().buildContext().getApi();
  filesConnection=CloudFilesContextBuilder.newBuilder(account,key).withModule(new Log4JLoggingModule()).withJsonDebug().buildContext().getApi();
}
