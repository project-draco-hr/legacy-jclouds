{
  String account=checkNotNull(properties.getProperty(AzureStorageConstants.PROPERTY_AZURESTORAGE_ACCOUNT),"azureStorageAccount");
  properties.setProperty(AzureBlobConstants.PROPERTY_AZUREBLOB_ENDPOINT,checkNotNull(endpoint,"endpoint").toString());
  properties.setProperty(AzureBlobConstants.PROPERTY_AZUREBLOB_ENDPOINT,endpoint.toString().replaceAll("\\{account\\}",account));
  return this;
}
