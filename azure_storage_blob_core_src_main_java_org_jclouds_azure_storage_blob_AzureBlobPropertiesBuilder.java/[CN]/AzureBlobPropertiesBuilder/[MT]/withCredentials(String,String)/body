{
  properties.setProperty(AzureStorageConstants.PROPERTY_AZURESTORAGE_ACCOUNT,checkNotNull(id,"azureStorageAccount"));
  properties.setProperty(AzureStorageConstants.PROPERTY_AZURESTORAGE_KEY,checkNotNull(secret,"azureStorageKey"));
  String endpoint=properties.getProperty(AzureBlobConstants.PROPERTY_AZUREBLOB_ENDPOINT);
  properties.setProperty(AzureBlobConstants.PROPERTY_AZUREBLOB_ENDPOINT,endpoint.replaceAll("\\{account\\}",id));
  return this;
}
