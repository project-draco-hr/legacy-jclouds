{
  return new FutureBase<List<S3Bucket.Metadata>>(){
    public List<S3Bucket.Metadata> get() throws InterruptedException, ExecutionException {
      List<S3Bucket.Metadata> list=new ArrayList<S3Bucket.Metadata>();
      for (      String name : bucketToContents.keySet())       list.add(new S3Bucket.Metadata(name));
      return list;
    }
  }
;
}
