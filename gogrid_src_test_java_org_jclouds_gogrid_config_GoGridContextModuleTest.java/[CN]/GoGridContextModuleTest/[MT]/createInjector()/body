{
  return Guice.createInjector(new GoGridRestClientModule(),new GoGridContextModule(),new RestModule(),new ExecutorServiceModule(sameThreadExecutor(),sameThreadExecutor()),new AbstractModule(){
    @Override protected void configure(){
      Names.bindProperties(binder(),checkNotNull(new GoGridPropertiesBuilder("user","key").build(),"properties"));
      bind(TransformingHttpCommandExecutorService.class).toInstance(createMock(TransformingHttpCommandExecutorService.class));
    }
  }
);
}
