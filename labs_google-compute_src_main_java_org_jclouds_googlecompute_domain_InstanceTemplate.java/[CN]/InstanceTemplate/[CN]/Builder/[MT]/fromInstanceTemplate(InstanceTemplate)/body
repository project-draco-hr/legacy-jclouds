{
  return InstanceTemplate.builder().forMachineTypeAndNetwork(instanceTemplate.getMachineType(),instanceTemplate.getNetwork()).name(instanceTemplate.getName()).description(instanceTemplate.getDescription()).zone(instanceTemplate.getZone()).image(instanceTemplate.getImage()).tags(instanceTemplate.getTags()).disks(instanceTemplate.getDisks()).metadata(instanceTemplate.getMetadata()).serviceAccounts(instanceTemplate.getServiceAccounts());
}
