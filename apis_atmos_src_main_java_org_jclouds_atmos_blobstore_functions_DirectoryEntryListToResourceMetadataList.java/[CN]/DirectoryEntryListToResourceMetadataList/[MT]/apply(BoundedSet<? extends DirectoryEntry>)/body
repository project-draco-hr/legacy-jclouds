{
  return new PageSetImpl<StorageMetadata>(Iterables.transform(from,new Function<DirectoryEntry,StorageMetadata>(){
    public StorageMetadata apply(    DirectoryEntry from){
      StorageType type=from.getType() == FileType.DIRECTORY ? StorageType.FOLDER : StorageType.BLOB;
      if (type == StorageType.FOLDER)       return new StorageMetadataImpl(type,from.getObjectID(),from.getObjectName(),defaultLocation.get(),null,null,null,ImmutableMap.<String,String>of());
 else       return new BlobMetadataImpl(from.getObjectID(),from.getObjectName(),defaultLocation.get(),null,null,null,ImmutableMap.<String,String>of(),null,null,new BaseMutableContentMetadata());
    }
  }
),from.getToken());
}
