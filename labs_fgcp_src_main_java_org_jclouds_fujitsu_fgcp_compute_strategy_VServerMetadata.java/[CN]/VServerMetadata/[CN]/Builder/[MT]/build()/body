{
  if (initialPassword == null)   initialPassword="";
  if (server != null) {
    return new VServerMetadata(server,initialPassword,status,image,publicIps);
  }
 else   if (serverWithDetails != null) {
    return new VServerMetadata(serverWithDetails,initialPassword,status,image,publicIps);
  }
 else {
    if (id == null)     id="dummy-id";
    if (name == null)     name="dummy-name";
    return new VServerMetadata(id,name,template,status);
  }
}
