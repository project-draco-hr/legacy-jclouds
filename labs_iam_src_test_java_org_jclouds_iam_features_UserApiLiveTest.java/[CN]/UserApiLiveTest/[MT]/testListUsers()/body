{
  ImmutableList<User> users=api().list().concat().toList();
  getAnonymousLogger().info("users: " + users.size());
  for (  User user : users) {
    checkUser(user);
    assertEquals(api().get(user.getId()),user);
    if (user.getPath().isPresent())     assertEquals(api().listPathPrefix(user.getPath().get()).toSet(),ImmutableSet.of(user));
  }
}
