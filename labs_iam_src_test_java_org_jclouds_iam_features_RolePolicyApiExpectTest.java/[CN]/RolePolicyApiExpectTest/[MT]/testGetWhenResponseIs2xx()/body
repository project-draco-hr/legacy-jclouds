{
  HttpResponse getResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/get_role_policy.xml","text/xml")).build();
  IAMApi apiWhenExist=requestSendsResponse(get,getResponse);
  assertEquals(apiWhenExist.getPolicyApiForRole("S3Access").get("S3AccessPolicy").toString(),new GetRolePolicyResponseTest().expected().toString());
}
