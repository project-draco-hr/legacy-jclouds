{
  HttpResponse listResponse=HttpResponse.builder().statusCode(200).payload(payloadFromResourceWithContentType("/list_role_policies.xml","text/xml")).build();
  IAMApi apiWhenExist=requestSendsResponse(list,listResponse);
  assertEquals(apiWhenExist.getPolicyApiForRole("S3Access").list().get(0).toString(),new ListRolePoliciesResponseTest().expected().toString());
}
