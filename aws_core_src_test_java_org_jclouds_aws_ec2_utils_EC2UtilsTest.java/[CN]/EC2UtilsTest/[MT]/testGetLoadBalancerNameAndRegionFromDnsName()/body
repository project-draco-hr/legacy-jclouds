{
  assertEquals(ImmutableMap.<String,String>of("us-east-1","my-load-balancer"),EC2Utils.getLoadBalancerNameAndRegionFromDnsName("my-load-balancer-1277832914.us-east-1.elb.amazonaws.com"));
  assertEquals(ImmutableMap.<String,String>of("us-east-1","ec2lb"),EC2Utils.getLoadBalancerNameAndRegionFromDnsName("ec2lb-915583419.us-east-1.elb.amazonaws.com"));
}
