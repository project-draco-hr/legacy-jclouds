{
  if (qName.equalsIgnoreCase("Task")) {
    this.task=new TaskImpl(taskLink.getHref(),operation,status,startTime,endTime,expiryTime,owner,error);
    operation=null;
    taskLink=null;
    status=null;
    startTime=null;
    endTime=null;
    owner=null;
    error=null;
  }
 else   if (qName.equalsIgnoreCase("Owner")) {
    inOwner=false;
  }
}
