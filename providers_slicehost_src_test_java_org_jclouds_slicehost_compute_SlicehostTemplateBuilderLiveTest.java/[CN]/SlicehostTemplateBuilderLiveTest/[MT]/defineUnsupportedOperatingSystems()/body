{
  return new Predicate<OsFamilyVersion64Bit>(){
    @Override public boolean apply(    OsFamilyVersion64Bit input){
switch (input.family) {
case UBUNTU:
        return !input.version.equals("") && !(input.version.equals("10.04") || input.version.endsWith(".10"));
case RHEL:
      return !(input.version.equals("") && input.is64Bit);
case CENTOS:
    return !input.version.equals("") && input.version.matches("5.[23]") || (input.version.equals("5.0") && !input.is64Bit);
case WINDOWS:
  return !input.version.equals("") && input.version.startsWith("2008") && !(input.version.startsWith("2008 R2") && input.is64Bit || input.version.startsWith("2008 SP2") && !input.is64Bit) || input.version.indexOf("2003") != -1;
default :
return true;
}
}
}
;
}
