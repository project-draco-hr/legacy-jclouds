{
  computeContext=new ComputeServiceContextFactory().createContext(computeServiceProvider,ImmutableSet.of(new Log4JLoggingModule(),new JschSshClientModule()),setupCredentials(computeServiceProvider));
  blobContext=new BlobStoreContextFactory().createContext(blobStoreProvider,ImmutableSet.of(new Log4JLoggingModule()),setupCredentials(blobStoreProvider));
  blobContext.getAsyncBlobStore().createContainerInLocation(null,tag);
  computeContext.getComputeService().destroyNodesMatching(NodePredicates.withTag(tag));
}
